;-------------------------------------------------
IF LOCAL && NOWEX:射精
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL && NOWEX:放尿
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 0
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;四重绝顶
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:四重绝顶
		PRINTFORMW 
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;三重绝顶
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:三重绝顶
		SELECTCASE NOWEX:三重绝顶
				;Ｃ＆Ｖ＆Ａ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 0
				;Ｂ＆Ｖ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ｂ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ｖ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;二重绝顶
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 0
	;-------------------------------------------------
	IF NOWEX:二重绝顶
		SELECTCASE NOWEX:二重绝顶
				;Ｃ＆Ｖ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 0
				;Ｖ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 0
				;Ｖ＆Ｂ绝顶
			CASE 5
				PRINTFORMW 
				RETURN 0
				;Ａ＆Ｂ绝顶
			CASE 6
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	IF NOWEX:Ｃ绝顶
		;強絶頂Ｃ
		IF NOWEX:Ｃ绝顶 == 2
			PRINTFORMW 
			;絶頂Ｃ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｖ绝顶
		;強絶頂Ｖ
		IF NOWEX:Ｖ绝顶 == 2
			PRINTFORMW 
			;絶頂Ｖ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ａ绝顶
		;強絶頂Ａ
		IF NOWEX:Ａ绝顶 == 2
			PRINTFORMW 
			;絶頂Ａ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
	IF NOWEX:Ｂ绝顶
		;強絶頂Ｂ
		IF NOWEX:Ｂ绝顶 == 2
			PRINTFORMW 
			;絶頂Ｂ
		ELSE
			PRINTFORMW 
		ENDIF
	ENDIF
ENDIF


