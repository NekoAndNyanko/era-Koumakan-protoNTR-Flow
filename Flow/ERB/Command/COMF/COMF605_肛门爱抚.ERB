;-------------------------------------------------
;肛门爱抚
;自慰系コマンド、レベル2
;-------------------------------------------------
@COM605

TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_左尻)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 40
DOWNBASE:气力 += 100

SOURCE:快Ａ = 100
SOURCE:苦痛 = 100
SOURCE:屈从 = 100
SOURCE:逸脱 = 80
SOURCE:反感 = 60
CALL 締り具合変動, TARGET, 1, 1
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 100

;Aへの苦痛は先に処理しておく
CALL PAIN_CHECK_A(TARGET)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＡ⇔調教者の指の汚れが移动
STAIN:PLAYER:手 |= STAIN:Ａ
STAIN:PLAYER:手 |= STAIN:腸内
STAIN:Ａ |= STAIN:PLAYER:手
STAIN:腸内 |= STAIN:PLAYER:手
;-------------------------------------------------
;経験の処理
;-------------------------------------------------

;自慰经验
EXP:PLAYER:自慰经验 ++

RETURN 1
