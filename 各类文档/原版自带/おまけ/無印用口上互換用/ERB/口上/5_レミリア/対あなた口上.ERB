	#DIM 奴隷
	#DIM 対象
	#DIMS 呼称

	呼称 = %CALLNAME:対象%
	IF CALLNAME:奴隷 == "レミリア"
		IF CALLNAME:対象 == "パチュリー"
			呼称 = パチェ
		ELSE
		ENDIF
