;妊娠パッチ系DEBUG======================================

@子宫内体积と内部性資料DEBUG(ARG)
;ARG 対象
CALL COLOR_GG
PRINT 子宮内精子量:
LOCAL:1 = 0
FOR LOCAL, 0, 301
	;全キャラ回せば次に訪問者の振り分けを行うようにする
	SIF LOCAL == CHARANUM
		LOCAL = 300
		
	LOCAL:1 += DA:ARG:LOCAL
NEXT

BAR LOCAL:1, CFLAG:ARG:350, 16
PRINTFORM ({LOCAL:1,4}/{CFLAG:ARG:350,4})({(LOCAL:1 * 100 / CFLAG:ARG:350)}％)
PRINTL

RESETCOLOR


@精子取得DEBUG, ARG, ARG:1, ARG:2, ARG:3, ARG:4
;ARG   精力
;ARG:1 ストック番号
;ARG:2 対象
;ARG:3 加射精者
;ARG:4 カウント
CALL COLOR_GG
PRINTFORM 精力={ARG}　TA:%CALLNAME:(ARG:2)%:
IF ARG:3 == 精子主_訪問者
	PRINT 訪問者
ELSEIF ARG:3 == 精子主_客
	PRINT 客
ELSEIF ARG:3 == 精子主_村人
	PRINT 村人
ELSE
	PRINTFORM %CALLNAME:(ARG:3)%
ENDIF
PRINTFORML :{ARG:4} = {ARG:1}
RESETCOLOR
;N_T_R(ARG, ARG:1)

;AFTERTRA.ERB〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜〜
@精子処理DEBUG2, ARG, ARG:1, ARG:2
;ARG 射精対象
;ARG:1 射精した側
;ARG:2 ARG子宮内のARG:1の精子総量
