@NTR_KOJO_MESSAGE_COM_K10_302
CALL TRAIN_MESSAGE
; NTR対応口上実行
CALL NTR_KOJO_MESSAGE_COM_K10_302_1
IF 1 == RESULT
	; 戻り値が1であれば、非NTR対応口上を実行
	TRYCALL KOJO_MESSAGE_COM_K10_302_1
ENDIF
RETURN RESULT

@NTR_KOJO_MESSAGE_COM_K10_302_1
#DIM 奴隷
#DIM RET_VAL

奴隷 = 人物_魔理沙
RET_VAL = 0

IF TALENT:奴隷:公众便所

	IF TALENT:奴隷:恋慕 && TALENT:奴隷:亲爱
		PRINTFORML “…啊，这段时间可以和%CALLNAME:MASTER%互相接触…。好开心啊……”
		;TranslatedMark 「…ああ、%CALLNAME:MASTER%と触れ合えるこの時間…。嬉しいぜ…」
	ELSEIF TALENT:奴隷:恋慕
		IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
			PRINTDATAL
				DATAFORM “…如果没有钥匙的话，就到此为止了”
				;TranslatedMark 「…鍵なしでは、ここまでだな」
				DATAFORM “…不能再这样了。没有钥匙的服务就到此为止了”
				;TranslatedMark 「…これ以上は駄目だ。鍵なしでサービスできるのはここまでだ」
				DATAFORM “…到此为止……你明白的吧？”
				;TranslatedMark 「…これ以上は…わかってるよな？」
			ENDDATA
		ELSE
			PRINTDATAL
				DATAFORM “…啊，如果只是这样的话没问题的”
				;TranslatedMark 「…ああ、これくらいならきっと大丈夫だ」
				DATAFORM “…到此为止了，把钥匙带来吧”
				;TranslatedMark 「…これ以上は、鍵を持ってきてくれ」
				DATAFORM “这样的话……”
				;TranslatedMark 「これくらいなら…」
			ENDDATA
		ENDIF
	ELSE
		PRINTDATAL
			DATAFORM “不行”
			;TranslatedMark 「駄目だ」
			DATAFORM “…别这样！”
			;TranslatedMark 「…やめてくれ」
		ENDDATA
	ENDIF

ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取り返し寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取り返し中) 

	PRINTDATAL
		DATAFORM “…如果只是这样的话，…可以啊”
		;TranslatedMark 「…これくらいなら、…いいぜ」
		DATAFORM “…要对那家伙保密哦」
		;TranslatedMark 「…あいつには内緒だぞ」
		DATAFORM “…这是我们小小的抵抗，是吧？”
		;TranslatedMark 「…これは私達の細やかな抵抗。だよな？」
		DATAFORM “…不能再继续了。如果没有钥匙的话，我会倒大霉的”
		;TranslatedMark 「…これ以上は駄目だ。鍵がなきゃ私が酷い目に合う」
	ENDDATA

ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ) 

	PRINTDATAL
		DATAFORM “住手”
		;TranslatedMark 「やめろ」
		DATAFORM “别这样”
		;TranslatedMark 「やめてくれ」
		DATAFORM “不行”
		;TranslatedMark 「駄目だ」
	ENDDATA

ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)

	PRINTFORML “……”
	;TranslatedMark 「…」
	PRINTFORML %CALLNAME:MASTER%的手碰触到的瞬间，%CALLNAME:奴隷%全身都僵硬了。
	;TranslatedMark %CALLNAME:MASTER%の手が触れた瞬間、%CALLNAME:奴隷%は一瞬身を固くした。

ELSE
	; それ以外の場合、非NTR口上を実行する
	RET_VAL = 1
ENDIF 

RETURN RET_VAL


