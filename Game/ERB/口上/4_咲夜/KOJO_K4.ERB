;==================================================
;おやすみ口上
;==================================================
@KOJO_EVENT_K4_7(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;眠っている
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF CFLAG:睡眠
		IF LOCAL:1 && ARG == 2
			PRINTFORMW 「呼唔…。」
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;眠いので部屋に帰った
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW 「差不多该睡觉了、%CALLNAME:MASTER%也早点休息吧，别影响到明天的工作了。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;寝るので%CALLNAME:MASTER%を追い出した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「%CALLNAME:MASTER%也该睡觉了哦、我先去睡了，晚安。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃった
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「呼啊啊………我才没有打哈欠…。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃったのでベッドまでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「我感觉...好困…。」
		PRINTFORMW 帮看起来相当疲惫的%CALLNAME:人物_咲夜%送回房间睡觉去了。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の部屋で寝ちゃったのでそのままおやすみ
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW %CALLNAME:MASTER%帮正在沉睡的%CALLNAME:人物_咲夜%盖了一条毯子。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%が寝ちゃったのでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 0
	ENDIF
	;その他
	PRINTFORMW
ENDIF
RETURN 0

