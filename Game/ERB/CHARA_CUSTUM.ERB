@CHARA_CUSTUM

PRINTL 可以改变角色的设定
FOR LOCAL, 0, CHARANUM
	PRINTFORML [{LOCAL}] - %CALLNAME:LOCAL%
NEXT
PRINTL [999] 設定完了
INPUT

IF RESULT >= 0 && RESULT < CHARANUM
	CALL CUSTOM_TERMINAL(RESULT)
ELSEIF RESULT == 999
	PRINTL 这样可以吗？
	PRINTL [0] 是的
	PRINTBUTTON "[ELSE] 不要", 1
	PRINTL
	ONEINPUT
	IF RESULT == 0
		CALL CLOTHES_CUSTOM
		RETURN
	ENDIF
ENDIF
RESTART
RETURN 0

