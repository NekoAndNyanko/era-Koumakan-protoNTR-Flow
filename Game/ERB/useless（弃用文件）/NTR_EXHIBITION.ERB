;NTR野外調教関係
;　訪問者による住人野外調教関係の処理

;-------------------------------------------------------------------------------
;弱み獲得1 自慰行為を目撃されることによる訪問者への弱み獲得
@NTR_GET_WEAKNESS_1(奴隷)
#DIM 奴隷

;再表示防止
IF !TALENT:奴隷:被访问者抓到把柄
	IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
		PRINTFORMW %CALLNAME:奴隷%は自慰行為を%NTR_NAME(0)%に撮影されてしまったようだ
		PRINTFORMW %CALLNAME:奴隷%は%NTR_NAME(0)%に弱みを握られた。
	ENDIF
	TALENT:奴隷:被访问者抓到把柄 = 1
ENDIF
RETURN

;-------------------------------------------------------------------------------
;弱み獲得2 恋人以外との性行為による訪問者への弱み獲得
@NTR_GET_WEAKNESS_2(奴隷)
#DIM 奴隷
;恋慕でなければチェックしない
IF TALENT:奴隷:恋慕
	;再表示防止
	IF !TALENT:奴隷:被访问者抓到把柄
		IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
			PRINTFORMW %CALLNAME:奴隷%与%ANATA_LOVER(奴隷)%以外的%NTR_NAME(3)%身体重叠。
			PRINTFORMW %CALLNAME:奴隷%被%NTR_NAME(0)%掌握了弱点。
		ENDIF
		TALENT:奴隷:被访问者抓到把柄 = 1
	ENDIF
ENDIF
RETURN

;-------------------------------------------------------------------------------
;NTR野外調教開始
@NTR_EXHIBITION_START(奴隷)
#DIM 奴隷
IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
	PRINTFORMW 因为%CALLNAME:奴隷%被抓住了弱点，所以无法阻止%NTR_NAME(0)%
ENDIF
;念のため同行解除
IF CFLAG:奴隷:同行 != 0
	CFLAG:奴隷:同行 = 0
	IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
		PRINTFORMW %CALLNAME:奴隷%远离%CALLNAME:MASTER%向%NTR_NAME(0)%的方向走去。
	ENDIF
ENDIF
;訪問者の同行も解除
FLAG:訪問者同行フラグ = 0
IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
	IF CFLAG:奴隷:脅迫度 > 野外調教閾値_放置プレイ
		PRINTFORMW %CALLNAME:奴隷%穿着内衣和%NTR_NAME(0)%一起出去了。
		TRYCALLLIST
			FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_17_0(奴隷)
			FUNC NTR_KOJO_K_17_0(奴隷)
		ENDFUNC
	ELSE
		PRINTFORMW %CALLNAME:奴隷%不穿内裤、并且穿着很曝露的衣服与%NTR_NAME(0)%一起出去了
		TRYCALLLIST
			FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_17_1(奴隷)
			FUNC NTR_KOJO_K_17_1(奴隷)
		ENDFUNC
	ENDIF
ENDIF
CFLAG:奴隷:NTR野外調教 = 1
CFLAG:奴隷:自慰中 = 0			
FLAG:訪問者の現在位置 = 位置_薄暗い路地裏
CFLAG:奴隷:現在位置 = 位置_薄暗い路地裏
CFLAG:奴隷:NTR訪問者との行為の終了時刻 = DATETIME() + RAND:60 + 60 + RAND(CFLAG:奴隷:脅迫度+1)
RETURN

;-------------------------------------------------------------------------------
;NTR野外調教
@NTR_EXHIBITION(奴隷)
#DIM 奴隷
IF CFLAG:奴隷:脅迫度 > 野外調教閾値_放置プレイ
	IF !CFLAG:奴隷:村人
		;村人がまだいないとき
		CALL NTR_EXHIBITION_LEAVE_0(奴隷)
	ELSEIF !CFLAG:奴隷:村人のセクハラ
		;村人がいるけどまだ見てるだけ
		CALL NTR_EXHIBITION_LEAVE_1(奴隷)
	ELSEIF !CFLAG:奴隷:下着剥ぎ取り
		;村人が手を出してきた（下着着用）
		CALL NTR_EXHIBITION_LEAVE_2(奴隷)
	ELSE
		;村人が手を出してきた（全裸）
		CALL NTR_EXHIBITION_LEAVE_3(奴隷)
	ENDIF
ELSE
	CALL NTR_EXHIBITION_STAY(奴隷)
ENDIF

;-------------------------------------------------------------------------------
;NTR野外調教
@NTR_EXHIBITION_STAY(奴隷)
#DIM 奴隷

IF RAND:100 < 20
	IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
		PRINTFORMW %CALLNAME:奴隷%受到%NTR_NAME(0)%的威胁，开始羞耻地自慰
		PRINTFORMW %NTR_NAME(0)%在%CALLNAME:奴隷%绝顶之前一直看着%CALLNAME:奴隷%
		TRYCALLLIST
			FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_17_3(奴隷)
			FUNC NTR_KOJO_K_17_3(奴隷)
		ENDFUNC
	ENDIF
	EXP:奴隷:浮気露出快乐经验 ++
	EXP:奴隷:自慰经验 ++
	EXP:奴隷:浮気调教自慰经验 ++	
	EXP:奴隷:浮気绝顶经验 ++
	CALL 改变小穴色素(奴隷,1)
	CFLAG:奴隷:脅迫度 ++
	SOURCE:奴隷:露出 = 200
	CALL NTR_EXH_SOURCE_OP(奴隷)
ELSEIF RAND:100 < 80 || CFLAG:奴隷:NTR訪問者との行為の終了時刻 >= DATETIME()
	IF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置)
		PRINTFORMW %NTR_NAME(0)%让%CALLNAME:奴隷%拉起裙子，开始拍摄暴露出来的秘密场所
		TRYCALLLIST
			FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_17_4(奴隷)
			FUNC NTR_KOJO_K_17_4(奴隷)
		ENDFUNC
	ENDIF
	EXP:奴隷:浮気露出快乐经验 ++
	EXP:奴隷:浮気照相经验 ++
	CFLAG:奴隷:脅迫度 ++
	SOURCE:奴隷:露出 = 200
	CALL NTR_EXH_SOURCE_OP(奴隷)
ELSE
	IF NTR_CHK_VISIBLE(CFLAG:奴隷:現在位置)
		PRINTFORMW %CALLNAME:奴隷%不知道从哪里回来了
	ENDIF
	CALL NTR_EXHIBITION_END(奴隷)
ENDIF
RETURN

