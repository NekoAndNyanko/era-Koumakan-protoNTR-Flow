;-------------------------------------------------------------------------------
;実行可否判定を行う
@WC_ACTABLE(加害者,カウンター行為)
#FUNCTION
#DIM 加害者
#DIM カウンター行為
VARSET LOCAL
;前のキャラが何をしたかをLOCAL:100～に出力
FOR LOCAL,1,CHARANUM
	SIF TARGET:LOCAL <= 0
		CONTINUE
	SIF TCVAR:(TARGET:LOCAL):20
		LOCAL:(100 + TCVAR:(TARGET:LOCAL):20) ++
NEXT
;実行可否判定本体
SELECTCASE カウンター行為
	CASE CNT_色っぽい仕草
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		RETURNF 1
	CASE CNT_見つめる
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		RETURNF 1
	CASE CNT_体を摺り寄せる
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		RETURNF 1
	CASE CNT_裙子を捲る
		;;大浴場では全裸扱い
		SIF CFLAG:MASTER:現在位置 == 位置_红魔馆_大浴场
			RETURNF 0
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;裙子着用
		SIF !EQUIP:MASTER:9
			RETURNF 0
		SIF TEQUIP:MASTER:0 > 3
			RETURNF 0
		;全裸
		SIF !EQUIP:MASTER:0
			RETURNF 0
		SIF !ONCE("裙子めくられ",1,加害者)
			RETURNF 0
		RETURNF 1
	CASE CNT_耳元に息を吹きかける
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;会话中はダメ
		SIF SELECTCOM == 300
			RETURNF 0
		RETURNF 1
	CASE CNT_抚摸臀部
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		RETURNF 1
	CASE CNT_囁く
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;好感度が多少はないとだめ
		SIF CFLAG:加害者:好感度 <= 500
			RETURNF 0
		RETURNF 1
	CASE CNT_お尻いじめ
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		RETURNF 1
	CASE CNT_パンツを脱がす
		;;大浴場では全裸扱い
		SIF CFLAG:MASTER:現在位置 == 位置_红魔馆_大浴场
			RETURNF 0
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;「推到」コマンド選択時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		;;他の着せ替えから、２時間くらいはだめ
		SIF TIME_PROGRESS(CFLAG:MASTER:WC_着せ替え時間) < 120 && CFLAG:MASTER:WC_着せ替え時間 != 0
			RETURNF 0
		;;ズボンとかだとだめ
		SIF TEQUIP:MASTER:0 > 3
			RETURNF 0
		;;はいてなきゃだめ
		SIF !EQUIP:MASTER:下半身下着２
			RETURNF 0
		;;贞操帯は専用が別にある
		SIF EQUIP:MASTER:下半身下着１ == 100
			RETURNF 0
		RETURNF 1
	CASE CNT_パンツ選択
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;「推到」コマンド選択時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		;;前のキャラが「パンツを脱がされる」だとだめ
		SIF LOCAL:116
			RETURNF 0
		;;他の着せ替えから、２時間くらいはだめ
		SIF TIME_PROGRESS(CFLAG:MASTER:WC_着せ替え時間) < 120 && CFLAG:MASTER:WC_着せ替え時間 != 0
			RETURNF 0
		;;贞操帯は専用が別にある
		SIF EQUIP:MASTER:下半身下着１ == 100
			RETURNF 0
		;;好感度が多少はないとだめ
		SIF CFLAG:加害者:好感度 <= 500
			RETURNF 0
		;;蕾米莉亚は、恋慕か管理人の時に、あなたが公众便所かＮＴＲだとしてくれない
		SIF 加害者 == 人物_蕾米莉亚 && (TALENT:加害者:恋慕 || TALENT:加害者:管理人) && (TALENT:MASTER:公众便所 || TALENT:MASTER:NTR)
			RETURNF 0
		;;オトコだと美鈴／パチュリー／蕾米莉亚／フラン／チルノ／大妖精は無し
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_美铃
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_帕秋莉
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_蕾米莉亚
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_芙兰朵露
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_琪露诺
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_大妖精
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_魔理沙
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_灵梦
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_露米娅
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_爱丽丝
			RETURNF 0
		;;咲夜さんはオトコでも小柄以下なら女装させる
		SIF !HAS_VAGINA(MASTER) && 加害者 == 人物_咲夜 && TALENT:MASTER:体型 >= 0
			RETURNF 0
		;;「换衣服」コマンド選択時は、加害者の部屋じゃなくても、はいててもＯＫ
		SIF SELECTCOM == 430
			RETURNF 1
		;;ズボンとかだとだめ
		SIF TEQUIP:MASTER:0 > 3
			RETURNF 0
		;;はいてたらだめ
		SIF EQUIP:MASTER:下半身下着２
			RETURNF 0
		SIF EQUIP:MASTER:下半身下着１
			RETURNF 0
		;;加害者自身の部屋以外はダメ
		SIF 加害者 == 人物_美铃 && CFLAG:人物_美铃:現在位置 != 位置_红魔馆_守卫小屋
			RETURNF 0
		SIF 加害者 == 人物_小恶魔 && CFLAG:人物_小恶魔:現在位置 != 位置_红魔馆_小恶魔私室
			RETURNF 0
		SIF 加害者 == 人物_帕秋莉 && CFLAG:人物_帕秋莉:現在位置 != 位置_红魔馆_帕秋莉私室
			RETURNF 0
		SIF 加害者 == 人物_咲夜 && CFLAG:人物_咲夜:現在位置 != 位置_红魔馆_咲夜私室
			RETURNF 0
		SIF 加害者 == 人物_蕾米莉亚 && CFLAG:人物_蕾米莉亚:現在位置 != 位置_红魔馆_蕾米莉亚私室
			RETURNF 0
		SIF 加害者 == 人物_芙兰朵露 && CFLAG:人物_芙兰朵露:現在位置 != 位置_红魔馆_地下室
			RETURNF 0
		SIF 加害者 == 人物_艾丽娅 && CFLAG:人物_艾丽娅:現在位置 != 位置_红魔馆_小恶魔私室
			RETURNF 0
		SIF 加害者 == 人物_琪露诺 && CFLAG:人物_琪露诺:現在位置 != 位置_雾之湖_琪露诺的家
			RETURNF 0
		SIF 加害者 == 人物_大妖精 && CFLAG:人物_大妖精:現在位置 != 位置_雾之湖_大妖精的家
			RETURNF 0
		SIF 加害者 == 人物_魔理沙 && CFLAG:人物_魔理沙:現在位置 != 位置_魔法之森_雾雨魔法店
			RETURNF 0
		SIF 加害者 == 人物_灵梦 && CFLAG:人物_灵梦:現在位置 != 位置_博麗神社 && CFLAG:人物_灵梦:現在位置 != 位置_博麗神社周辺
			RETURNF 0
		SIF 加害者 == 人物_露米娅 && CFLAG:人物_露米娅:現在位置 != 位置_魔法之森_露米娅的树屋
			RETURNF 0
		SIF 加害者 == 人物_爱丽丝 && CFLAG:人物_爱丽丝:現在位置 != 位置_魔法之森_爱丽丝的家
			RETURNF 0
		RETURNF 1
	CASE CNT_アイテム装着される
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;開始早々はダメ
		SIF DATETIME() < 10
			RETURNF 0
		;;「推到」コマンド選択時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		;;贞操帯は、着けた本人じゃないとダメ
		SIF EQUIP:MASTER:下半身下着１ == 100 && CFLAG:MASTER:WC_あなたに贞操帯を着けた人 != 加害者
			RETURNF 0
		;;抖Ｓ之气が１は無いとだめ
		SIF ABL:加害者:抖Ｓ之气 < 1
			RETURNF 0
		;;跳蛋か阴蒂夹か乳头夹を持っているか、アイテムなし設定でないとダメ
		SIF ITEM:0 == 0 && ITEM:2 == 0 && ITEM:10 == 0 && NOITEM == 0
			RETURNF 0
		;;他の着せ替えから、２時間くらいはだめ
		SIF TIME_PROGRESS(CFLAG:MASTER:WC_着せ替え時間) < 120 && CFLAG:MASTER:WC_着せ替え時間 != 0
			RETURNF 0
		;;管理人か蕾米莉亚なら、着け直し可能
		SIF TALENT:加害者:管理人 || 加害者 == 人物_蕾米莉亚
			RETURNF 1
		;;止まってるのを着けた人なら、外し可能
		SIF CFLAG:MASTER:WC_乳头夹装着 == 1 && CFLAG:MASTER:WC_道具Ｂ装着者 == 加害者
			RETURNF 1
		SIF CFLAG:MASTER:WC_阴蒂夹装着 == 1 && CFLAG:MASTER:WC_道具Ｃ装着者 == 加害者
			RETURNF 1
		SIF CFLAG:MASTER:WC_跳蛋Ｃ装着 == 1 && CFLAG:MASTER:WC_道具Ｃ装着者 == 加害者
			RETURNF 1
		SIF CFLAG:MASTER:WC_跳蛋Ｖ装着 == 1 && CFLAG:MASTER:WC_道具Ｖ装着者 == 加害者
			RETURNF 1
		SIF CFLAG:MASTER:WC_跳蛋Ａ装着 == 1 && CFLAG:MASTER:WC_道具Ａ装着者 == 加害者
			RETURNF 1
		SIF CFLAG:MASTER:WC_振动棒装着 == 1 && CFLAG:MASTER:WC_道具Ａ装着者 == 加害者
			RETURNF 1
		SIF CFLAG:MASTER:WC_肛门振动棒装着 == 1 && CFLAG:MASTER:WC_道具Ａ装着者 == 加害者
			RETURNF 1
		;;着けたのが動いてたら、だめ
		SIF CFLAG:MASTER:WC_乳头夹装着 > 1 && CFLAG:MASTER:WC_道具Ｂ装着者 == 加害者
			RETURNF 0
		SIF CFLAG:MASTER:WC_阴蒂夹装着 > 1 && CFLAG:MASTER:WC_道具Ｃ装着者 == 加害者
			RETURNF 0
		SIF CFLAG:MASTER:WC_跳蛋Ｃ装着 > 1 && CFLAG:MASTER:WC_道具Ｃ装着者 == 加害者
			RETURNF 0
		SIF CFLAG:MASTER:WC_跳蛋Ｖ装着 > 1 && CFLAG:MASTER:WC_道具Ｖ装着者 == 加害者
			RETURNF 0
		SIF CFLAG:MASTER:WC_跳蛋Ａ装着 > 1 && CFLAG:MASTER:WC_道具Ａ装着者 == 加害者
			RETURNF 0
		SIF CFLAG:MASTER:WC_振动棒装着 > 1 && CFLAG:MASTER:WC_道具Ａ装着者 == 加害者
			RETURNF 0
		SIF CFLAG:MASTER:WC_肛门振动棒装着 >= 2 && CFLAG:MASTER:WC_道具Ａ装着者 == 加害者
			RETURNF 0
		;;装着箇所が埋まってたらダメ
		SIF CFLAG:MASTER:WC_道具Ｂ装着者 != 0 && CFLAG:MASTER:WC_道具Ｃ装着者 != 0 && CFLAG:MASTER:WC_道具Ｖ装着者 != 0 && CFLAG:MASTER:WC_道具Ａ装着者 != 0
			RETURNF 0
		SIF TALENT:MASTER:处女 && CFLAG:MASTER:WC_道具Ｂ装着者 != 0 && CFLAG:MASTER:WC_道具Ｃ装着者 != 0 && CFLAG:MASTER:WC_道具Ａ装着者 != 0
			RETURNF 0
		SIF !HAS_VAGINA(MASTER) && CFLAG:MASTER:WC_道具Ｂ装着者 != 0 && CFLAG:MASTER:WC_道具Ｃ装着者 != 0 && CFLAG:MASTER:WC_道具Ａ装着者 != 0
			RETURNF 0
		RETURNF 1
	CASE CNT_裙子たくし上げ命令
		;;大浴場では全裸扱い
		SIF CFLAG:MASTER:現在位置 == 位置_红魔馆_大浴场
			RETURNF 0
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;「推到」コマンド選択時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		;;他の着せ替えから、１時間くらいはだめ
		SIF TIME_PROGRESS(CFLAG:MASTER:WC_着せ替え時間) < 60 && CFLAG:MASTER:WC_着せ替え時間 != 0
			RETURNF 0
		;;裙子着用
		SIF !EQUIP:MASTER:9
			RETURNF 0
		SIF TEQUIP:MASTER:0 > 3
			RETURNF 0
		RETURNF 1
	CASE CNT_贞操帯関連
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;「推到」コマンド選択時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		;;管理人さんじゃないとだめ
		SIF !TALENT:加害者:管理人
			RETURNF 0
		;;NTRの時は、撃破してないとだめ
		SIF TALENT:MASTER:NTR && !CFLAG:加害者:WC_訪問者撃破
			RETURNF 0
		;;他の着せ替えから、２時間くらいはだめ
		SIF TIME_PROGRESS(CFLAG:MASTER:WC_着せ替え時間) < 120 && CFLAG:MASTER:WC_着せ替え時間 != 0
			RETURNF 0
		;;あなたが处女で、处女予約も予約奪取でも出来てなかったら、だめ
		SIF TALENT:MASTER:处女 && CFLAG:MASTER:WC_处女予約 != 加害者 && CFLAG:MASTER:WC_予約奪取 != 加害者
			RETURNF 0
		RETURNF 1
	CASE CNT_飲尿命令
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;「推到」コマンド選択時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		;;女の子が贞操帯をつけてたらだめ
		SIF EQUIP:加害者:下半身下着１ == 100
			RETURNF 0
		;;女の子がトイレに行きたくないならだめ
		SIF REST_GetUrge(加害者,1) < 60
			RETURNF 0
		;;			;;前の飲尿から、１時間くらいはダメ。尿意がわかったらそっちに判定文を切り替え
		;;			SIF TIME_PROGRESS(CFLAG:加害者:WC_飲尿時間) < 60
		;;				RETURNF 0
		RETURNF 1
	CASE CNT_乳搾り手コキ
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		SIF !(HAS_PENIS(MASTER))
			RETURNF 0
		SIF CFLAG:MASTER:阴茎用贞操帯着用 != 0
			RETURNF 0
		;押し倒し時はされない
		SIF SELECTCOM == 350
			RETURNF 0
		RETURNF 1
	CASE CNT_キス
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		;;肉便器には、好感度が高くないとファーストキスはしてもらえない
		SIF CFLAG:加害者:好感度 < 500 && CSTR:加害者:M处女丧失记录 == ""
			RETURNF 0
		SIF !COM417_is_Protected_CheckOnly(MASTER,加害者,淫纹_右頬) == 0
			RETURNF 0
		SIF CHK_COM312_STAIN(加害者, MASTER)
			RETURNF 0
		SIF TFLAG:60
			RETURNF 0
		RETURNF 1
	CASE CNT_背面抱擁
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		SIF LOCAL:131
			RETURNF 0
		RETURNF 1
	CASE CNT_対面抱擁
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		SIF LOCAL:132
			RETURNF 0
		;キス
		SIF LOCAL:131
			RETURNF 0
		RETURNF 1
	CASE CNT_胸を揉ませる
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		SIF !COM417_is_Protected_CheckOnly(MASTER,加害者,淫纹_胸元) == 0
			RETURNF 0
		SIF TFLAG:61
			RETURNF 0
		RETURNF 1
	CASE CNT_股間をまさぐる
		;;前々回、前回と同じ行為はしない
		SIF CFLAG:加害者:WC_前々回セクハラ == カウンター行為
			RETURNF 0
		SIF CFLAG:加害者:WC_前回セクハラ == カウンター行為
			RETURNF 0
		SIF TEQUIP:MASTER:0 <= 3
			RETURNF 0
		SIF TFLAG:62
			RETURNF 0
		RETURNF 1
	CASE CNT_裙子の中を見せる
		;;前々回、前回と同じ行為はしない
