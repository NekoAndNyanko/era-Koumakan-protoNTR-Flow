PRINTL
FOR LOCAL, 1, CHARANUM
	PRINTFORML [{LOCAL,3}] %CALLNAME:LOCAL%（%寝間着名称(CFLAG:LOCAL:寝間着)%）
NEXT
PRINTL [999] OK
INPUT
IF RESULT >= 1 && RESULT < CHARANUM
	LOCAL:1 = RESULT
	PRINTFORML 对%CALLNAME:RESULT%的睡衣的设定
	FOR LOCAL, 0, 開始時人数
		PRINTFORML [{LOCAL}] %寝間着名称(LOCAL)%
	NEXT
	INPUT
	IF RESULT >= 0 && RESULT <= 開始時人数
		CFLAG:(LOCAL:1):寝間着 = RESULT
	ENDIF
ELSEIF RESULT == 999
	PRINTL 真的确定了吗？
	PRINTL [0] 是的
	PRINTBUTTON "[ELSE] 不要", 1
	ONEINPUT
	SIF RESULT == 0
		RETURN
ENDIF
RESTART
RETURN 0