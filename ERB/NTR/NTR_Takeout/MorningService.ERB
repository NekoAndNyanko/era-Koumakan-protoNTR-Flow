;-------------------------------------------------------------------------------
;	お持ち帰りからの帰宅時処理
;-------------------------------------------------------------------------------
;-------------------------------------------------------------------------------
;朝のご侍奉(口交)
@NTR_MORNING_SERVICE_FELLATIO(奴隷)
#DIM 奴隷
CALL EJACULATION_M(奴隷, 人物_訪問者)
CALL NTR_EXP_ADD_M_SEX(奴隷,1)
; IF !TALENT:奴隷:亲爱
; CALL NTR_EXP_ADD_M_SEX(奴隷,1)
; ENDIF
CALL LOST_VIRGIN_M(奴隷, 人物_訪問者,1)
IF NTR_CHK_VISIBLE(900)
	CALL NTR_MESSAGE_14_0(奴隷)
	TRYCALLLIST
		FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_14_0(奴隷)
		FUNC NTR_KOJO_K_14_0(奴隷)
	ENDFUNC
ENDIF
;手
CALL STAIN_ADD_SEMEN_HAND(奴隷, 人物_訪問者)
;好感度吸収
CALL NTR_DRAIN_LOVE(奴隷, 20)

;-------------------------------------------------------------------------------
;朝のご侍奉(性交)
@NTR_MORNING_SERVICE_VSEX(奴隷)
#DIM 奴隷
CALL EJACULATION_V(奴隷, 人物_訪問者)
CALL NTR_EXP_ADD_V_SEX_EX(奴隷,1)
IF 訪問者_Ｐ大きさ == 2
	CALL NTR_EXP_ADD_V_EXPANSION(奴隷,1)
	SIF GETBIT(FLAG:ＮＴＲパッチ設定,32) || CFLAG:MASTER:現在位置 == CFLAG:奴隷:現在位置
		PRINTFORML %CALLNAME:奴隷%的小穴被%NTR_NAME(0)%怒挺的大肉棒蹂躏着…
	IF ( TALENT:奴隷:受虐狂 || ABL:奴隷:抖Ｍ之气 > 2 ) && EXP:奴隷:Ｖ扩张经验 + EXP:奴隷:浮気Ｖ扩张经验 < 450
		CUP:奴隷:欲情 += 500
		CUP:奴隷:苦痛 += 250
	ELSEIF EXP:奴隷:Ｖ扩张经验 + EXP:奴隷:浮気Ｖ扩张经验 < 450
		CUP:奴隷:苦痛 += 500
	ENDIF
ENDIF
; IF !TALENT:奴隷:亲爱
; CALL NTR_EXP_ADD_V_SEX(奴隷,1)
; EXP:奴隷:浮気绝顶经验 ++
; ENDIF
CALL 改变小穴色素(奴隷,1)
CALL 締り具合変動, 奴隷, 3, 0
CALL LOST_VIRGIN(奴隷,PLAYER)

CALL NTR_GET_WEAKNESS_2(奴隷)
;V感覚が高いと绝顶经验加算（強絶頂扱い）
CALL NTR_V_EXP_PLUS(奴隷)
IF NTR_CHK_VISIBLE(900)
	CALL NTR_MESSAGE_14_2(奴隷)
	TRYCALLLIST
		FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_14_2(奴隷)
		FUNC NTR_KOJO_K_14_2(奴隷)
	ENDFUNC
ENDIF
;好感度吸収
CALL NTR_DRAIN_LOVE(奴隷, 50)
;快楽経験追加
CALL EXP_GOT_CHECK(奴隷)
CFLAG:奴隷:NTR訪問者と最後にセックスした日時 = DATETIME()
CFLAG:奴隷:NTR他の男と最後にセックスした日時 = DATETIME()

;-------------------------------------------------------------------------------
;朝のご侍奉(Ａ性交)
@NTR_MORNING_SERVICE_ASEX(奴隷)
