CALL NTR_EXP_ADD_A_SEX(奴隷,1)
CALL NTR_EXP_ADD_A_EX(奴隷,1)
IF 訪問者_Ｐ大きさ == 2
	CALL NTR_EXP_ADD_A_EXPANSION(奴隷,1)
	SIF GETBIT(FLAG:ＮＴＲパッチ設定,32) || CFLAG:MASTER:現在位置 == CFLAG:奴隷:現在位置
		PRINTFORML %CALLNAME:奴隷%的肛穴被%NTR_NAME(0)%坚挺的大肉棒狠狠地操弄着…
	IF ( TALENT:奴隷:受虐狂 || ABL:奴隷:抖Ｍ之气 > 2 ) && EXP:奴隷:Ａ扩张经验 + EXP:奴隷:浮気Ａ扩张经验 < 450
		CUP:奴隷:欲情 += 500
		CUP:奴隷:苦痛 += 250
	ELSEIF EXP:奴隷:Ａ扩张经验 + EXP:奴隷:浮気Ａ扩张经验 < 450
		CUP:奴隷:苦痛 += 500
	ENDIF
ENDIF
; IF !TALENT:奴隷:亲爱
; CALL NTR_EXP_ADD_A_SEX(奴隷,1)
; EXP:奴隷:浮気绝顶经验 ++
; ENDIF
CALL 改变肛门色素(奴隷,1)
CALL 締り具合変動, 奴隷, 3, 1
CALL LOST_VIRGIN_A(奴隷,PLAYER)

CALL NTR_GET_WEAKNESS_2(奴隷)
;A感覚が高いと绝顶经验加算（強絶頂扱い）
CALL NTR_A_EXP_PLUS(奴隷)
IF NTR_CHK_VISIBLE(900)
	CALL NTR_MESSAGE_14_1(奴隷)
	TRYCALLLIST
		FUNC NTR_KOJO_K{CFLAG:奴隷:口上パターン}_14_1(奴隷)
		FUNC NTR_KOJO_K_14_1(奴隷)
	ENDFUNC
ENDIF
;好感度吸収
CALL NTR_DRAIN_LOVE(奴隷, 50)
;快楽経験追加
CALL EXP_GOT_CHECK(奴隷)
CFLAG:奴隷:NTR訪問者と最後にセックスした日時 = DATETIME()
CFLAG:奴隷:NTR他の男と最後にセックスした日時 = DATETIME()
