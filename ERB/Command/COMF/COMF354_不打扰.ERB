@COM354
;不打扰
;%NTR_NAME(0)%と住人１人同席、かつ主人が空気の時の時間進行用コマンド
;会话
IF FLAG:25 == 0
	IF TALENT:NTR
		PRINTFORML %CALLNAME:TARGET%完全无视了%CALLNAME:MASTER%，与%NTR_NAME(0)%很开心地在交谈。
	ELSE
		PRINTFORML %CALLNAME:TARGET%想要来搭话，但是被%NTR_NAME(0)%困扰着，没办法好好说话。
	ENDIF
	;キス
ELSEIF FLAG:25 == 1
	IF TALENT:NTR
		PRINTFORML %CALLNAME:TARGET%正期待着%CALLNAME:MASTER%惊讶的反应
		PRINTFORML %CALLNAME:MASTER%也的确看着跟%NTR_NAME(0)%接吻的%CALLNAME:TARGET%惊讶地发愣。
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%看着在自己面前跟%NTR_NAME(0)%接吻的%CALLNAME:TARGET%惊讶地发愣。
		ELSE
			PRINTFORML 想要阻止%CALLNAME:TARGET%继续接吻，但是被%NTR_NAME(0)%察觉，身体变得动弹不得。
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 20 + 20 * ABL:MASTER:欲望 + 10 * TALENT:TARGET:浮気公認
	;爱抚 or 胸爱抚
ELSEIF FLAG:25 == 2 || FLAG:25 == 3
	IF TALENT:NTR
		PRINTFORML %CALLNAME:MASTER%惊讶地望着完全无视自己，正在接受%NTR_NAME(0)%爱抚的%CALLNAME:TARGET%。
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%看着在自己面前被%NTR_NAME(0)%爱抚的%CALLNAME:TARGET%惊讶地发愣。
		ELSE
			PRINTFORML 想要阻止%NTR_NAME(0)%继续爱抚%CALLNAME:TARGET%，可身体却动弹不得。
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 30 + 30 * ABL:MASTER:欲望 + 20 * TALENT:TARGET:浮気公認
	;フェラ or 69
ELSEIF FLAG:25 == 4 || FLAG:25 == 5 || FLAG:25 == 6
	IF TALENT:NTR
		PRINTFORML %CALLNAME:TARGET%完全无视了目瞪口呆的%CALLNAME:MASTER%，正在认真地为%NTR_NAME(0)%口交侍奉。
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%目瞪口呆地看着为%NTR_NAME(0)%口交侍奉的%CALLNAME:TARGET%。
		ELSE
			PRINTFORML 想要阻止强迫%CALLNAME:TARGET%侍奉自己阴茎的%NTR_NAME(0)%，但怎么也无法行动。
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 50 + 50 * ABL:MASTER:欲望 + 30 * TALENT:TARGET:浮気公認
	;セーックス！おああなる
ELSE
	IF TALENT:NTR
		PRINTFORML %CALLNAME:MASTER%目瞪口呆得望着像是在表演性交一样的两人。
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%看见%CALLNAME:TARGET%的身体正吞吐着%NTR_NAME(0)%的阴茎，但没办法说出什么来。	
		ELSE
			PRINTFORML %CALLNAME:MASTER%看见%NTR_NAME(0)%的阴茎正在%CALLNAME:TARGET%的身体里扑哧扑哧得抽插，被打击地说不出话来。	
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 100 + 100 * ABL:MASTER:欲望 + 50 * TALENT:TARGET:浮気公認
ENDIF

LOSEBASE:气力 += 100
DOWNBASE:MASTER:气力 += 100

