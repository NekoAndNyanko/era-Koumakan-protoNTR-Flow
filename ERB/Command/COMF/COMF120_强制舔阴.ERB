;-------------------------------------------------
;强制舔阴
;助手によるコマンド、及びレズプレイ、レベル1
;-------------------------------------------------
@COM120

TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_右頬)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:射精判定用 += LOCALS
TCVAR:PLAYER:2 = 4
;強度
TCVAR:PLAYER:4 = 1000
;侍奉快楽
TFLAG:30 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 10
DOWNBASE:气力 += 100

SOURCE:性行动 = 300
SOURCE:达成 = 150
SOURCE:屈从 = 500
SOURCE:逸脱 = 200
SOURCE:反感 = 200

;上のほうで計算した汚れデータ
SOURCE:肮脏 = GET_STAINCOUNT(PLAYER, 部位_Ｖ, TARGET) * 80 + 50


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 400

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の口⇔調教者のＶの汚れが移动
STAIN:口 |= STAIN:PLAYER:Ｖ
STAIN:PLAYER:Ｖ |= STAIN:口

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 6
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 6
ENDIF

;奴隷が猫舌持ちの場合は、調教者に嗜虐快乐经验を追加
SIF TALENT:TARGET:猫舌
	EXP:嗜虐快乐经验 ++
EXP:口淫经验 ++
CALL 改变小穴色素(PLAYER,1)
RETURN 1

@CAN_COM120(ARG)
SKIPDISP ARG
PRINTL 强制舔阴
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:侍奉精神
IF ABL:侍奉精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:侍奉精神 * 4
	PRINT 侍奉精神
	PRINTV 'LV,ABL:侍奉精神,'(,ABL:侍奉精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快乐刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快乐刻印 * 2
	PRINT 快乐刻印
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞耻心 > 0
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 恥じらい
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭鈍感
IF TALENT:污臭耐性 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 3
	PRINT 汚臭鈍感
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭敏感
IF TALENT:污臭耐性 < 0
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 献身的
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感应答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 快感の否定
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 淫乱
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;娼婦
IF TALENT:淫乱 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 娼婦
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｖ, TARGET)
;汚れあり
IF LOCAL
	PRINT  - 
	TFLAG:実行値 -= LOCAL
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:污臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:污臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(, LOCAL, ')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;合計を表示(23以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 23
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0
RETURN 1
