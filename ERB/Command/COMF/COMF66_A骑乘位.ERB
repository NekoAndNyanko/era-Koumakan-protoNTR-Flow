;-------------------------------------------------
;肛交骑乘位
;セックス系コマンド、レベル2
;-------------------------------------------------
@COM66

TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_左尻)
SIF RESULT
	RETURN 0

;前回の調教が3Pのとき3Pへ
IF PREVCOM == 71
	CALL COM_ABLE71
	SIF RESULT
		JUMP COM71
ENDIF
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:射精判定用 += LOCALS
TCVAR:PLAYER:2 = 2
;強度
TCVAR:PLAYER:4 = 2200

TFLAG:30 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 80
DOWNBASE:气力 += 200

SOURCE:快Ａ = 500
SOURCE:情爱 = 250
SOURCE:性行动 = 250
SOURCE:达成 = 250
SOURCE:苦痛 = 800
SOURCE:露出 = 120
SOURCE:逸脱 = 700
SOURCE:反感 = 900

;上のほうで計算した汚れデータ
SOURCE:肮脏 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) * 10 + 60

CALL 締り具合変動, TARGET, 3, 1
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 900

;Aへの苦痛は先に処理しておく
CALL PAIN_CHECK_A(TARGET)

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;奴隷のＡ⇔調教者のＰの汚れが移动
STAIN:PLAYER:Ｐ |= STAIN:Ａ
STAIN:PLAYER:Ｐ |= STAIN:腸内
STAIN:Ａ |= STAIN:PLAYER:Ｐ
STAIN:腸内 |= STAIN:PLAYER:Ｐ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;A経験
EXP:Ａ经验 ++

;性交経験
EXP:Ａ性交经验 ++

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 7
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 7
ENDIF


;爱情经验
SIF !ASSIPLAY
	EXP:爱情经验 += TALENT:恋慕
CALL 改变肛门色素(TARGET,1)
CALL 确认小穴颜色(TARGET)
CALL 确认小穴松弛度(TARGET)
CALL 确认肛门颜色(TARGET)
CALL 确认肛门松弛度(TARGET)
CALL LOST_VIRGIN_A(TARGET,PLAYER)
RETURN 1

@CAN_COM66(ARG)
SKIPDISP ARG
PRINTL 肛交骑乘位
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 3
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:Ａ感觉
IF ABL:Ａ感觉
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:Ａ感觉 * 2
	PRINT Ａ感觉
	PRINTV 'LV,ABL:Ａ感觉,'(,ABL:Ａ感觉 * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:侍奉精神
IF ABL:侍奉精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:侍奉精神 * 4
	PRINT 侍奉精神
	PRINTV 'LV,ABL:侍奉精神,'(,ABL:侍奉精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
IF MARK:快乐刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快乐刻印 * 3
	PRINT 快乐刻印
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;PALAM:润滑（の不足）
IF PALAM:润滑 < PALAMLV:3
	PRINT  - 
	TFLAG:実行値 -= 5
	PRINT 润滑不足
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 3
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞耻心 > 0
	PRINT  - 
	TFLAG:実行値 -= 2
	PRINT 恥じらい
	PRINTV '(,2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感应答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 5
	PRINT 快感の否定
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;男嫌い（恋慕、亲爱で無効）
IF TALENT:性別嗜好 & 1 && !(HAS_VAGINA(PLAYER)) && (!TALENT:恋慕 || PLAYER != MASTER)
	PRINT  - 
	TFLAG:実行値 -= 12
	PRINT 男嫌い
	PRINTV '(,12,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 15
	PRINT 淫乱
	PRINTV '(,15,')
	TFLAG:実行値表示用フラグ = 15
ENDIF
;娼婦
IF TALENT:淫乱 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 娼婦
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF


;A経験
IF EXP:Ａ经验 < EXPLV:2
	PRINT  - 
	TFLAG:実行値 -= 30
	PRINT Ａ经验
	PRINT 不足
	PRINTV '(,30,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;調教者がふたなり
IF GET_SEXNAME(PLAYER) == "ふたなり"
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 4
	PRINT ふたなり
	PRINTV '(,4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;媚药
IF TEQUIP:媚药
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 媚药
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET)
;汚れあり
IF LOCAL
	PRINT  - 
	TFLAG:実行値 -= LOCAL
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:污臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:污臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,LOCAL,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;合計を表示(45以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 45
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0
RETURN 1


