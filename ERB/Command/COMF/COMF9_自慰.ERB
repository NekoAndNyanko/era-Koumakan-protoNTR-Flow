;-------------------------------------------------
;自慰
;爱抚系コマンド、レベル3
;-------------------------------------------------
@COM9
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）
IF HAS_PENIS(TARGET)
	LOCALS = {TARGET}/
	CSTR:TARGET:射精判定用 += LOCALS
	TCVAR:TARGET:2 = 3
	;強度
	TCVAR:TARGET:4 = 500
ENDIF

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 5
DOWNBASE:气力 += 50

SOURCE:快Ｃ = 80
SOURCE:快Ｂ = 80
SOURCE:性行动 = 100
SOURCE:恐怖 = 200
SOURCE:露出 = 400
SOURCE:屈从 = 500
SOURCE:逸脱 = 300
SOURCE:反感 = 250
;振动棒
IF TEQUIP:振动棒
	SOURCE:快Ｖ = 80
	SOURCE:屈从 += 100
	SOURCE:反感 += 100
	TIMES SOURCE:快Ｃ , 0.70
	TIMES SOURCE:快Ｂ , 0.70
ENDIF
;肛门振动棒
IF TEQUIP:肛门振动棒
	SOURCE:屈从 += 150
	SOURCE:反感 += 150
	TIMES SOURCE:快Ｃ , 0.70
	TIMES SOURCE:快Ｂ , 0.70
ENDIF
;淋浴
IF TEQUIP:32
	SOURCE:快Ｃ += 50
	SOURCE:露出 += 100
	SOURCE:屈从 += 50
	SOURCE:反感 += 80
ENDIF
;ビデオ
IF TEQUIP:28
	SOURCE:液体 += 50
	SOURCE:欲情 += 100
	SOURCE:反感 += 200
ENDIF
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 400
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷の指⇔奴隷のＢの汚れが移动
STAIN:手 |= STAIN:Ｂ
STAIN:Ｂ |= STAIN:手

;奴隷の指⇔奴隷のＶの汚れが移动
STAIN:手 |= STAIN:Ｖ
STAIN:Ｖ |= STAIN:手

;淋浴オナニーの場合汚れをリセット、润滑更に半分。
IF TEQUIP:32 == 1
	STAIN:手 = 汚れ_なし
	STAIN:Ｐ = 汚れ_阴茎
	STAIN:Ｖ = 汚れ_愛液
	STAIN:Ａ = 汚れ_菊花
	STAIN:膣内 = 汚れ_なし
	PALAM:4 /= 2
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;录像摄影時は経験にプラス
IF TEQUIP:录像摄影
	EXP:自慰经验 += 2
	EXP:调教自慰经验 += 2
ELSE
	EXP:自慰经验 += 1
	EXP:调教自慰经验 += 1
ENDIF
;公開オナニー時は异常经验付加
IF TEQUIP:录像摄影 && !GETBIT(CFLAG:3,0)
	EXP:异常经验 ++
	SETBIT CFLAG:异常经验,0
ENDIF
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
ENDIF
CALL 改变小穴色素(TARGET,1)
CALL 改变乳头色素(TARGET,1)
CALL 改变乳头大小(TARGET,1)
CALL 确认小穴颜色(TARGET)
CALL 确认小穴松弛度(TARGET)
CALL 确认乳头颜色(TARGET)
CALL 确认乳头大小(TARGET)
RETURN 1


@CAN_COM9(ARG)
SKIPDISP ARG
SIF TEQUIP:录像摄影
	PRINT 公開
SIF TEQUIP:淋浴
	PRINT 淋浴
IF TEQUIP:振动棒 && TEQUIP:肛门振动棒
	PRINT 二穴振动棒
ELSEIF TEQUIP:振动棒
	PRINT 振动棒
ELSEIF TEQUIP:肛门振动棒
	PRINT 肛门振动棒
ENDIF

IF TEQUIP:录像摄影 || TEQUIP:淋浴 || TEQUIP:振动棒 || TEQUIP:肛门振动棒
	PRINTL オナニー
ELSE
	PRINTL 自慰
ENDIF
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
;VARSET TFLAG:実行値
TFLAG:実行値 = 0
;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 3
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:露出癖
IF ABL:露出癖
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:露出癖 * 4
	PRINT 露出癖
	PRINTV 'LV,ABL:露出癖,'(,ABL:露出癖 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:自慰中毒
IF ABL:自慰中毒
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:自慰中毒 * 3
	PRINT 自慰中毒
	PRINTV 'LV,ABL:自慰中毒,'(,ABL:自慰中毒 * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快乐刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快乐刻印 * 3
	PRINT 快乐刻印
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;自制心
IF TALENT:自制心
	PRINT  - 
	TFLAG:実行値 -= 5
	PRINT 自制心
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞耻心 > 0
	PRINT  - 
	TFLAG:実行値 -= 5
	PRINT 恥じらい
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥薄い
IF TALENT:羞耻心 < 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 2
	PRINT 恥薄い
	PRINTV '(,2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;容易自慰
IF TALENT:容易自慰
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 容易自慰
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感に素直
IF TALENT:快感应答 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 快感に素直
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感应答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 5
	PRINT 快感の否定
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;媚药
IF TEQUIP:媚药
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 8
	PRINT 媚药
	PRINTV '(,8,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;合計を表示(33以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

;難易度上昇
;公開で+10、振动棒で+5、肛门振动棒で+5、淋浴で＋3
LOCAL = 33
SIF TEQUIP:录像摄影
	LOCAL += 10
SIF TEQUIP:淋浴
	LOCAL += 3
SIF TEQUIP:振动棒
	LOCAL += 5
SIF TEQUIP:肛门振动棒
	LOCAL += 5

SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0
RETURN 1

