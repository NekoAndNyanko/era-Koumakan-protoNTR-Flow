;-------------------------------------------------
;授乳手コキ
;爱抚系派生コマンド
;-------------------------------------------------
@SCOM16
TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_胸元)
SIF RESULT
	RETURN 0
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:PLAYER:行為者 = TARGET
TCVAR:TARGET:行為者 = PLAYER

LOCALS = {TARGET}/
CSTR:PLAYER:射精判定用 += LOCALS
TCVAR:PLAYER:2 = 3
;強度
TCVAR:PLAYER:4 = 500
;侍奉快楽
TFLAG:30 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 10
DOWNBASE:气力 += 100

SOURCE:快Ｂ = 150
SOURCE:情爱 = 150
SOURCE:苦痛 = 50
SOURCE:露出 = 30
SOURCE:肮脏 = 40
SOURCE:反感 = 70
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 10

;調教者が［幼児退行］
IF TALENT:PLAYER:幼儿／幼儿退行
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:性行动 , 1.20
ENDIF

;調教者が［幼稚］
IF TALENT:PLAYER:幼稚
	TIMES SOURCE:快Ｂ , 1.20
	TIMES SOURCE:性行动 , 1.20
ENDIF

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者の口の汚れが移动
STAIN:Ｂ |= STAIN:PLAYER:口
STAIN:PLAYER:口 |= STAIN:Ｂ
;射精に関係なく行われる処理はここから
;奴隷の指⇔調教者のＰの汚れが移动
STAIN:手 |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:手
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 5
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 5
ENDIF

;調教者が猫舌持ちの場合は、に嗜虐快乐经验を追加
SIF TALENT:猫舌
	EXP:嗜虐快乐经验 += 1

;手淫经验
EXP:手淫经验 ++

RETURN 1

@CAN_SCOM16(ARG)
SKIPDISP ARG
PRINTL 授乳手コキ
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:侍奉精神
IF ABL:侍奉精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:侍奉精神 * 4
	PRINT 侍奉精神
	PRINTV 'LV,ABL:侍奉精神,'(,ABL:侍奉精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:精液中毒 * 1
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快乐刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快乐刻印 * 1
	PRINT 快乐刻印
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞耻心 > 0
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 恥じらい
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭鈍感
IF TALENT:污臭耐性 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭敏感
IF TALENT:污臭耐性 < 0
	TFLAG:実行値 -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 献身的
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感应答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 快感の否定
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;男嫌い（恋慕、亲爱で無効）
IF TALENT:性別嗜好 & 1 && !(HAS_VAGINA(PLAYER)) && (!TALENT:恋慕 || PLAYER != MASTER)
	PRINT  - 
	TFLAG:実行値 -= 5
	PRINT 男嫌い
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 恋慕
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;亲爱
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 亲爱
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF

;調教者がふたなり
IF GET_SEXNAME(PLAYER) == "ふたなり"
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 8
	PRINT ふたなり
	PRINTV '(,8,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;手扱きでは汚れがそれほど影響しない
LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) / 3
;汚れあり
IF LOCAL
	PRINT  - 
	TFLAG:実行値 -= LOCAL
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:污臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:污臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,LOCAL,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;鍵を持っていると実行可能
IF FLAG:贞操帯鍵購入フラグ == TARGET
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 100
	PRINT 贞操帯の鍵
	PRINTV '(,100,')
	TFLAG:実行値表示用フラグ = 100
ENDIF	


;合計を表示(14以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 14
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0

RETURN 1