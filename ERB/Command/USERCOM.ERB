@TARGET_CHANGE
PRINTFORML %CALLNAME:TARGET%の相手をしています
PRINTL [0] - 返回
FOR LOCAL,1,CHARANUM
	SIF CFLAG:LOCAL:同室
		PRINTFORML [{LOCAL}] - %CALLNAME:LOCAL%
NEXT
INPUT
IF RESULT <= 0 || RESULT >= CHARANUM
	RETURN 0
ELSEIF CFLAG:RESULT:同室
	TFLAG:104 = RESULT
ENDIF

