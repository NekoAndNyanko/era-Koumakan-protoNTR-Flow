@COM_ABLE0
;爱抚実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(0)
	RETURN RESULT
RETURN 1

@COM_ABLE1
;舔阴実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(1)
	RETURN RESULT
;膣镜を使用しているとダメ
SIF TEQUIP:膣镜
	RETURN 0
;性器が汚れているとMASTERの場合舔阴はできない（調教者が汚れ無視か愛液、母乳のよごれならＯＫ）
SIF STAIN:Ｖ >= 汚れ_阴茎 && (STAIN:Ｖ != 汚れ_母乳 && STAIN:Ｖ != 汚れ_母乳_愛液) && ASSIPLAY == 0 && TALENT:MASTER:污臭耐性 < 2
	RETURN 0
;性器が汚れており、汚臭敏感で顺从3以下のASSIの場合舔阴はできない（調教者が汚れ無視か愛液、母乳のよごれならＯＫ）
IF STAIN:Ｖ >= 汚れ_阴茎 && (STAIN:Ｖ != 汚れ_母乳 && STAIN:Ｖ != 汚れ_母乳_愛液) && ASSIPLAY
	SIF ABL:ASSI:顺从 <= 3 && TALENT:ASSI:污臭耐性 < 0
		RETURN 0
ENDIF
;下半身着衣
SIF TEQUIP:0 & 4
	RETURN 0
RETURN 1

@COM_ABLE2
;调教者口交実行判定
SIF !TFLAG:100
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(2)
	RETURN RESULT
;性器が汚れているとMASTERの場合调教者口交はできない（調教者が汚れ無視か愛液、Ｐ、母乳のよごれならＯＫ）
SIF STAIN:Ｐ >= 汚れ_精液 && (STAIN:Ｐ != 汚れ_母乳_阴茎 && STAIN:Ｐ != 汚れ_母乳_阴茎_愛液) && ASSIPLAY == 0 && TALENT:MASTER:污臭耐性 < 2
	RETURN 0
;性器が汚れており、汚臭敏感で顺从3以下のASSIの場合フェラはできない（調教者が汚れ無視か愛液、Ｐ、母乳のよごれならＯＫ）
IF STAIN:Ｐ >= 汚れ_精液 && (STAIN:Ｐ != 汚れ_母乳_阴茎 && STAIN:Ｐ != 汚れ_母乳_阴茎_愛液) && ASSIPLAY
	SIF ABL:ASSI:顺从 <= 3 && TALENT:ASSI:污臭耐性 < 0
		RETURN 0
ENDIF
;猫舌の調教者の場合は技巧Lv2以上必要
SIF TALENT:PLAYER:猫舌 && ABL:PLAYER:技巧 < 2
	RETURN 0
;オトコかふたなりじゃないとダメ
SIF !HAS_PENIS(TARGET)
	RETURN 0
;下半身着衣
SIF TEQUIP:0 & 4
	RETURN 0
;飞机杯使用中はダメ
SIF TEQUIP:12
	RETURN 0
RETURN 1

@COM_ABLE3
