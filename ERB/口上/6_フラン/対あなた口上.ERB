;==================================================
;300ex - 浮気公認について
;==================================================
@KOJO_MESSAGE_COM_K6_300_10_02(奴隷, 調教者)
#DIM 奴隷
#DIM 調教者
#DIM RSLT_LV		; 处女喪失会话戻り値

RSLT_LV= 0
;最後にセックスした日時で分岐。その中でさらに好感度、屈服度で分岐
IF TALENT:奴隷:妊娠
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		PRINTFORML 「吼啊、お腹が大きいうちはエッチしてもらえないの。ざーんねん」
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
		PRINTFORML 「吼啊、お腹が大きいうちはエッチしてもらえないの」
	ELSE
		PRINTFORML 「お腹が大きいうちはエッチしないんだって」
	ENDIF
ELSEIF (CFLAG:奴隷:NTR訪問者と最後にセックスした日時/(24*60)) == (DATETIME()/(24*60))
	;同じ日にセックスしてたら
	SELECTCASE CFLAG:奴隷:NTR訪問者と最後にセックスした日時
		CASE IS > (DATETIME() - 1*01*60) ;１時間以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「聞きたいの？ たった今あの人にエッチしてもらったばかりだよ。えへっ・・」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「聞きたいの？ たった今あの人とエッチしたばかりだよ。えへっ・・」
			ELSE
				PRINTFORML 「・・たった今あの人とエッチしたばかりなの・・これでよかったの？」
			ENDIF
		CASE IS > (DATETIME() - 1*04*60) ;４時間以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「聞きたいの？ さっきあの人にエッチしてもらったばかりだよ。えへっ・・」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「聞きたいの？ さっきあの人とエッチしたばかりだよ。えへっ・・」
			ELSE
				PRINTFORML 「・・さっきあの人とエッチしたばかりなの・・これでよかったの？」
			ENDIF
		CASEELSE
			PRINTFORML 今日もセックスしたのだと答えた。
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「聞きたいの？ 今日もあの人にエッチしてもらったよ」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「聞きたいの？ 今日もあの人とエッチしたよ」
			ELSE
				PRINTFORML 「・・今日もあの人とエッチしたの・・これでよかったの？」
			ENDIF
	ENDSELECT
	CALL KOJO_MESSAGE_COM_K6_300_10_02LV(奴隷, 調教者)
	RSLT_LV = RESULT
ELSE
	SELECTCASE CFLAG:奴隷:NTR訪問者と最後にセックスした日時
		CASE IS > (DATETIME() - 1*01*60) ;１時間以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「聞きたいの？ たった今あの人にエッチしてもらったばかりだよ。えへっ・・」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「聞きたいの？ たった今あの人とエッチしたばかりだよ。えへっ・・」
			ELSE
				PRINTFORML 「・・たった今あの人とエッチしたばかりなの・・これでよかったの？」
			ENDIF
			CALL KOJO_MESSAGE_COM_K6_300_10_02LV(奴隷, 調教者)
			RSLT_LV = RESULT
		CASE IS > (DATETIME() - 1*08*60) ;８時間以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「聞きたいの？ さっきあの人にエッチしてもらったばかりだよ。えへっ・・」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「聞きたいの？ さっきあの人とエッチしたばかりだよ。えへっ・・」
			ELSE
				PRINTFORML 「・・さっきあの人とエッチしたばかりなの・・これでよかったの？」
			ENDIF
			CALL KOJO_MESSAGE_COM_K6_300_10_02LV(奴隷, 調教者)
			RSLT_LV = RESULT
		CASE IS > (DATETIME() - 1*24*60) ;１日以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「聞きたいの？ 昨日もあの人にエッチしてもらったよ」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「聞きたいの？ 昨日もあの人とエッチしたよ」
			ELSE
				PRINTFORML 「・・昨日もあの人とエッチしたの・・これでよかったの？」
			ENDIF
			CALL KOJO_MESSAGE_COM_K6_300_10_02LV(奴隷, 調教者)
			RSLT_LV = RESULT
		CASE IS > (DATETIME() - 3*24*60) ;２日以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「昨日はあの人にエッチしてもらえなかったの」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「昨日はあの人とエッチしてないの」
			ELSE
				PRINTFORML 「・・昨日はあの人とエッチしてないの・・」
			ENDIF
		CASEELSE
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「このところごゴブサタなの・・疼いちゃう」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「このところごゴブサタなの・・」
			ELSE
				PRINTFORML 「・・ずいぶんあの人とエッチしてないの・・」
			ENDIF
	ENDSELECT
ENDIF
;膣が訪問者の精液で汚れている場合の分岐
IF CFLAG:奴隷:射精者V == 人物_訪問者 && (STAIN:奴隷:Ｖ & 汚れ_精液)
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		IF 1 == RSLT_LV
			PRINTFORML 「それでね、今%CALLNAME:奴隷%のアソコは、あの人のでいっぱいなんだよ♪」
		ELSE
			PRINTFORML 「そうだ、教えてあげる・・今%CALLNAME:奴隷%のアソコは、あの人のでいっぱいなんだよ♪」
		ENDIF
		IF TALENT:奴隷:人妻
			PRINTFORML 「自分のお嫁さんがほかの人とエッチして、本当に嬉しいんだ？」
		ELSEIF TALENT:奴隷:恋慕
			PRINTFORML 「コイビトがほかの人とエッチして、本当に嬉しいんだ？」
		ENDIF
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
		IF 1 == RSLT_LV
			PRINTFORML 「それでね、今%CALLNAME:奴隷%のアソコは、あの人ので汚れてるんだよ」
		ELSE
			PRINTFORML 「そうだ、教えてあげる・・今%CALLNAME:奴隷%のアソコは、あの人ので汚れてるんだよ」
		ENDIF
		IF TALENT:奴隷:人妻
			PRINTFORML 「自分のお嫁さんがほかの人とエッチして、嬉しいの？」
		ELSEIF TALENT:奴隷:恋慕
			PRINTFORML 「コイビトがほかの人とエッチして、嬉しいの？」
		ENDIF
	ELSE
		IF 1 == RSLT_LV
			PRINTFORML 「・・それで・・今%CALLNAME:奴隷%のアソコは、あの人ので汚れてるんだよ」
		ELSE
			PRINTFORML 「・・聞いて・・今%CALLNAME:奴隷%のアソコは、あの人ので汚れてるんだよ」
		ENDIF
		IF TALENT:奴隷:人妻
			PRINTFORML 「・・自分のお嫁さんがこんなことされて・・本当に平気なの・・？」
		ELSEIF TALENT:奴隷:恋慕
			PRINTFORML 「・・コイビトがこんなことされて・・本当に平気なの・・？」
		ENDIF
	ENDIF
ENDIF
RETURN 0

