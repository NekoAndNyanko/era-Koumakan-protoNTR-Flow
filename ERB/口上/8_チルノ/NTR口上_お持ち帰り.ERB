	SELECTCASE TALENT:奴隷:浮気公認
		CASE IS >= 4	; 外泊公認
			PRINTFORML 「・・うん・・行こ・・」
			PRINTFORML %CALLNAME:奴隷%は大人しく%NTR_NAME(0)%の家に同行した。
			PRINTFORML 外泊してくると%MSG_LOVER(奴隷)%相手に大見得を切ってしまったせいもあり、%CALLNAME:奴隷%は半ば居直っていた。
			PRINTFORML （・・%CALLNAME:MASTER%が・・悪いんだから・・）
		CASE IS >= 1	; 浮気公認
			PRINTFORML 「・・」
			PRINTFORML %CALLNAME:奴隷%は大人しく%NTR_NAME(0)%の家に同行した。
			IF TALENT:奴隷:恋慕
				PRINTFORML （・・%CALLNAME:MASTER%・・本当に・・いいんだよね・・）
			ENDIF
		CASEELSE		; 非公認
			PRINTFORML 「・・」
			PRINTFORML %CALLNAME:奴隷%は表面上大人しく%NTR_NAME(0)%の家に同行した。
			IF TALENT:奴隷:恋慕
				PRINTFORML （・・許して・・%CALLNAME:MASTER%・・）
			ENDIF
	ENDSELECT

ENDIF


RETURN 0

