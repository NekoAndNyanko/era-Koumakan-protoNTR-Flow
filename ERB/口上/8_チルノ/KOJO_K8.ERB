;うふふカウンター
;-------------------------------------------------
;まだ

;-------------------------------------------------
;奴隷の絶頂時の口上
;NOWEX:0～3 CVAB絶頂
;NOWEX:4 二重绝顶
;NOWEX:5 三重绝顶
;NOWEX:6 四重绝顶
;NOWEX:10 喷乳
;NOWEX:11 射精
;NOWEX:12 放尿
;適当に組み合わせて使ってください
;-------------------------------------------------
@KOJO_MESSAGE_PALAMCNG_B_K8
CALL PALAM_MESSAGE_B
;-------------------------------------------------
;喷乳
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:10
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;射精
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:11
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;放尿
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL && NOWEX:12
	PRINTFORMW 
ENDIF
;-------------------------------------------------
;絶頂
;記入チェック（=0, 非表示、1, 表示）
LOCAL = 1
;-------------------------------------------------
IF LOCAL
	;-------------------------------------------------
	;四重绝顶
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:6
		PRINTFORMW 「…！」
		PRINTFORMW 躰の全てに快楽が巡り、%CALLNAME:人物_琪露诺%は最高の絶頂を朧気な意識で味わった。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;三重绝顶
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:5
		SELECTCASE NOWEX:5
				;Ｃ＆Ｖ＆Ａ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 0
				;Ｂ＆Ｖ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ｂ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ｖ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	;-------------------------------------------------
	;二重绝顶
	;記入チェック（=0, 非表示、1, 表示）
	LOCAL:1 = 1
	;-------------------------------------------------
	IF NOWEX:4
		SELECTCASE NOWEX:4
				;Ｃ＆Ｖ绝顶
			CASE 1
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ａ绝顶
			CASE 2
				PRINTFORMW 
				RETURN 0
				;Ｖ＆Ａ绝顶
			CASE 3
				PRINTFORMW 
				RETURN 0
				;Ｃ＆Ｂ绝顶
			CASE 4
				PRINTFORMW 
				RETURN 0
				;Ｖ＆Ｂ绝顶
			CASE 5
				PRINTFORMW 
				RETURN 0
				;Ａ＆Ｂ绝顶
			CASE 6
				PRINTFORMW 
				RETURN 0
		ENDSELECT
	ENDIF
	IF NOWEX:0
		;強絶頂Ｃ
		IF NOWEX:0 == 2
			PRINTFORMW 「んぅぅぅ！お豆が、熱くて、あたいおかしくっ・・・！」
			;絶頂Ｃ
		ELSE
			PRINTFORMW 「はぁあぅう・・・頭がふわってなったぁ・・・。」
		ENDIF
	ENDIF
	IF NOWEX:1
		;強絶頂Ｖ
		IF NOWEX:1 == 2
			PRINTFORMW 「ん、んんん！？お腹の下が、ビクンって…！」
			;絶頂Ｖ
		ELSE
			PRINTFORMW 「あ、はぁ、あ・・・奥がビクンってなったぁ、なにこれ・・・。」
		ENDIF
	ENDIF
	IF NOWEX:2
		;強絶頂Ａ
		IF NOWEX:2 == 2
			PRINTFORMW 「はぅぁぁぁぁぁ！おしりの穴が、気持ちいいよぉぉぉ！」
			;絶頂Ａ
		ELSE
			PRINTFORMW 「や、やだぁ・・・おかしいよぉ・・・こっちでこうなるなんてぇ！」
		ENDIF
	ENDIF
	IF NOWEX:3
		;強絶頂Ｂ
		IF NOWEX:3 == 2
			PRINTFORMW 胸への快楽が最高潮に達した時、%CALLNAME:人物_琪露诺%は背を仰け反らせて達した快楽に浸った。
			;絶頂Ｂ
		ELSE
			PRINTFORMW 「先っぽが・・・ジンジンする、取れちゃうかと思った・・・。」
		ENDIF
	ENDIF
