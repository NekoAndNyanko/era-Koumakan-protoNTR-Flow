;==================================================
;おやすみ口上
;==================================================
@KOJO_EVENT_K8_7(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;眠っている
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF CFLAG:睡眠
		IF LOCAL:1 && ARG == 2
			PRINTFORMW 「あたいは…サイキョー…。」
			PRINTFORMW なんの夢を見ているんだろうか？
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;眠いので部屋に帰った
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW 「むにぃ…眠いから帰るねぇ…。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;寝るので%CALLNAME:MASTER%を追い出した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「あたい寝るから、また明日ねぇ…。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃった
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「ふぁぁ…眠い…くぅ…。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃったのでベッドまでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「あたい…眠いよぉ…%CALLNAME:MASTER%…。」
		PRINTFORMW %CALLNAME:MASTER%は眠る%CALLNAME:人物_琪露诺%を家まで運んだ…。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の部屋で寝ちゃったのでそのままおやすみ
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 眠る%CALLNAME:人物_琪露诺%に%CALLNAME:MASTER%は布団をかけた。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%が寝ちゃったのでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「う、うぐ、%CALLNAME:MASTER%を運べなくて何がサイキョーよ！」
		RETURN 0
	ENDIF
	;その他
	PRINTFORMW
ENDIF
RETURN 0

