@NTR_KOJO_K10_1(奴隷)
#DIM 奴隷
#DIMS 呼称
#DIMS 阴茎

呼称 = %MSG_NTR_SEX_10(奴隷)%
阴茎 = %MSG_CALL_PENIS_10(奴隷)%
;大浴場
IF CFLAG:奴隷:当前位置 == 位置_红魔馆_大浴场
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ)
		IF TALENT:奴隷:淫乱 && TALENT:奴隷:淫乱
			PRINTDATAL
				DATAFORM 「%呼称%の%阴茎%が気持ちよくなれる部分を研究しておくぜ」
				DATAFORM 「お腹にこの子がいるうちはこんなことしかできないけど…」
			ENDDATA
		ELSEIF TALENT:奴隷:淫乱
			PRINTDATAL
				DATAFORM 「%呼称%の%阴茎%が気持ちよくなれる部分を研究するぜ」
				DATAFORM 「このいやらしい%阴茎%がいつも私をいじめているんだよな…」
			ENDDATA
		ELSE
			PRINTDATAL
				DATAFORM 「ここがいいのか？」
				DATAFORM 「すごい…こいつにいつもいじめられてるから、今日はこっちがいじめる番だぜ！」
			ENDDATA
		ENDIF
		PRINTFORML %CALLNAME:奴隷%は己の胸で%NTR_NAME(0)%の背中を流しながら、%NTR_NAME(4)%の股間を手で扱いている。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
		PRINTDATAL
			DATAFORM 「んっ…ああっ…」
			DATAFORM 「あ…ん…いい…」
		ENDDATA
		PRINTFORML %NTR_NAME(0)%の隣で入浴している%CALLNAME:奴隷%は時折肌に触れられても逃げようともしない。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_うふふする程度)
		PRINTDATAL
			DATAFORM 「あんまり見るなよな…」
			DATAFORM 「もう…そんなに見るなよ…」
		ENDDATA
		IF IS_NTR_SOFT()
			PRINTFORML %NTR_NAME(0)%のすぐ近くで入浴している%CALLNAME:奴隷%は%NTR_NAME(4)%の視線から裸身を隠そうともしない。
		ELSE
			PRINTFORML %NTR_NAME(0)%のすぐ近くで入浴している%CALLNAME:奴隷%は舐めるような視線から裸身を隠そうともしない。
		ENDIF
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_侍奉する程度)
		PRINTDATAL
			DATAFORM 「あんまり見るなよな…」
			DATAFORM 「もう…そんなに見るなよ…」
		ENDDATA
		PRINTFORML %NTR_NAME(0)%の近くで入浴している%CALLNAME:奴隷%は視線から裸身を隠そうともしない。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_体を触らせる程度)
		PRINTDATAL
			DATAFORM 「あまりじろじろ見るなよな」
			DATAFORM 「そんなにじろじろ見るなよな」
		ENDDATA
		PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%の不躾な視線を咎めた。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_接吻程度)
		PRINTFORML 「…」
		PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%の視線から逃れようと姿勢を変えた。
	ELSE
		PRINTFORML 「…」
		PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%の視線から逃れようと姿勢を変えた。
	ENDIF
	;处女特別分岐
	;条件が「寝取られそう」なので寝取られ寸前も含む
ELSEIF TALENT:奴隷:处女 && NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう) && RAND:10 == 0
	PRINTDATAL
		DATAFORM 「私のここはまだ处女なんだが、いつ使ってくれるんだ？」
		DATAFORM 「そろそろ私の处女を奪う気になったか？早くここでも侍奉できるようになりたいぜ」
	ENDDATA
	PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%の耳元でなまめかしく囁いて、秘裂を差し出すように見せつけた。
	IF TALENT:奴隷:恋慕
		PRINTFORML %CALLNAME:奴隷%は%ANATA_LOVER(奴隷)%以外の相手に处女を捧げてしまうつもりなのだろうか。
	ENDIF
	;その他
ELSE
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ)
		IF TALENT:奴隷:妊娠
			PRINTDATAL
				DATAFORM 「ああ…この子が生まれたら、おまんこを%阴茎%でいっぱい愛してくれ…」
				DATAFORM 「うん…この子も元気だぜ」
				DATAFORM 「この子がいるとはいえ、%阴茎%が恋しいよ」
			ENDDATA
		ELSE
			PRINTDATAL
				DATAFORM 「ああ…分かってる…」
				DATAFORM 「了解だ…へへっ、今度だな？」
				DATAFORM 「いつものように…だな？ ああ、分かってるぜ」
			ENDDATA
		ENDIF
		PRINTFORML %CALLNAME:奴隷%はうっとりと頬を染め微笑している。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		IF TALENT:奴隷:妊娠
			PRINTDATAL
				DATAFORM 「ああ…この子が生まれたら、おまんこを%阴茎%でいっぱい愛してくれ…」
				DATAFORM 「うん…この子も元気だぜ」
				DATAFORM 「この子がいるとはいえ、%阴茎%が恋しいよ」
			ENDDATA
		ELSE
			PRINTDATAL
				DATAFORM 「ああ…分かってる…」
				DATAFORM 「了解だ…へへっ、今度だな？」
				DATAFORM 「いつものように…だな？ ああ、分かってるぜ」
			ENDDATA
		ENDIF
		PRINTFORML %CALLNAME:奴隷%はうっとりと頬を染め微笑している。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		PRINTDATAL
			DATAFORM 「ははっ、そんなこともあるんだな！」
			DATAFORM 「あー、そうそう、こんな話を知ってるか！？」
			DATAFORM 「へー、それは面白そうだな！」
		ENDDATA
		PRINTFORML %CALLNAME:奴隷%は食いつくように%NTR_NAME()%と話をしている。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
		PRINTDATAL
			DATAFORM 「ははっ、そんなこともあるんだな」
			DATAFORM 「あー、そうそう、こんな話を知ってるか？」
			DATAFORM 「へー、それは面白そうだな！」
		ENDDATA
		PRINTFORML %CALLNAME:奴隷%は楽しそうに笑っている。
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_うふふする程度)
		IF IS_NTR_SOFT()
			PRINTDATAL
				DATAFORM 「これは、私が前に聞いた話なんだが」
				DATAFORM 「へえ、それでどうなるんだ？」
				DATAFORM 「なるほど、それは初耳だ」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME()%と楽しそうに会话している。
		ELSE
			PRINTDATAL
				DATAFORM 「あっ、あの、もういいかっ…？」
				DATAFORM 「あの、もう…他の用事がっ…」
				DATAFORM 「あの、私…仕事がっ…」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は何かに怯えるように言葉を飲み込んだ。
		ENDIF
		;ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_侍奉する程度)
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_体を触らせる程度)
		IF IS_NTR_SOFT()
			PRINTDATAL
				DATAFORM 「そんなこともあるのか」
				DATAFORM 「え、そうなのか」
				DATAFORM 「どうしたんだ？」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME()%の話に興味を持ち始めたようだ。
		ELSE
			PRINTDATAL
				DATAFORM 「そっ、そう…」
				DATAFORM 「そっ、そうなのか…」
				DATAFORM 「そう…」
			ENDDATA
			PRINTFORML %CALLNAME:奴隷%は不安そうに答えている。
		ENDIF
		;↓以下はELSEと同じ
		;ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_接吻程度)
	ELSE
		PRINTDATAL
			DATAFORM 「ほーう、そうなのか」
			DATAFORM 「ふーん、そうなのか」
			DATAFORM 「へえ。大変だな」
		ENDDATA
		PRINTFORML %CALLNAME:奴隷%は興味なさそうに答えている。
	ENDIF
ENDIF
RETURN 0

