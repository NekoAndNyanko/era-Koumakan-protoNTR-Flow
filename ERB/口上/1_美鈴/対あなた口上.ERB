		CASEELSE
			PRINTFORML %"今日もセックスしたのだと答えた。"%
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「啊哈哈，今天也和那个人抱了哦」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「啊哈哈，今天也和那个人抱了哦」
			ELSE
				PRINTFORML 「・・今天也和那个人做爱了・・・」
			ENDIF
	ENDSELECT
ELSE
	SELECTCASE CFLAG:奴隷:NTR訪問者と最後にセックスした日時
		CASE IS > (DATETIME() - 1*01*60) ;１時間以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「呀哈哈，刚刚和那个人做了呢」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「啊哈哈，刚刚被那个人抱着呢」
			ELSE
				PRINTFORML 「・・刚才・・刚刚和那个人做爱了・・・」
			ENDIF
		CASE IS > (DATETIME() - 1*08*60) ;８時間以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「啊哈哈，刚刚和那个人做了呢」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「啊哈哈，刚刚和那个人做了呢」
			ELSE
				PRINTFORML 「・・刚才・・刚刚和那个人做爱了・・・」
			ENDIF
		CASE IS > (DATETIME() - 1*24*60) ;１日以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「啊哈哈，昨天那个人也抱了我」
			ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
				PRINTFORML 「啊哈哈，昨天也被那个人抱了」
			ELSE
				PRINTFORML 「・・昨天也和那个人做爱了」
			ENDIF
		CASE IS > (DATETIME() - 3*24*60) ;２日以内
			IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
				PRINTFORML 「昨天没有被那个人抱着呢」
