;==================================================
;「恋人」「伴侶」生成
;　奴隷から見たときの「あなた」の立場を生成する
;　　恋慕→恋人　人妻→夫、など
;==================================================
@MSG_LOVER(奴隷)
#FUNCTIONS
#DIM 奴隷
IF TALENT:奴隷:人妻
	;同性カップルの場合
	IF (HAS_VAGINA(奴隷) && HAS_VAGINA(MASTER)) || (!HAS_VAGINA(奴隷) && !HAS_VAGINA(MASTER))
		IF RAND(2)== 0
			RETURNF "伴侶"
		ELSE
			RETURNF "配偶者"
		ENDIF
	ELSEIF HAS_VAGINA(MASTER)
		RETURNF "妻子"
	ELSE
		RETURNF "丈夫"
	ENDIF
ELSEIF TALENT:奴隷:恋慕
	;「大人しい」だと1/2で「想い人」
	IF TALENT:奴隷:应答 == -1
		RETURNF "思念之人"
	ELSE
		RETURNF "恋人"
	ENDIF
ELSE
	;現状ではフェールセーフ
	RETURNF "熟人"
ENDIF

;==================================================
;「恋人」「伴侶」生成（逆)
;　「あなた」から見たときの奴隷の立場を生成する
;　　恋慕→恋人　人妻→夫、など
;==================================================
@MSG_RE_LOVER(奴隷)
#FUNCTIONS
#DIM 奴隷
IF TALENT:奴隷:人妻
	;同性カップルの場合
	IF (HAS_VAGINA(奴隷) && HAS_VAGINA(MASTER)) || (!HAS_VAGINA(奴隷) && !HAS_VAGINA(MASTER))
		IF RAND(2)== 0
			RETURNF "伴侶"
		ELSE
			RETURNF "配偶者"
		ENDIF
	ELSEIF HAS_VAGINA(奴隷)
		RETURNF "妻"
	ELSE
		RETURNF "夫"
	ENDIF
ELSEIF TALENT:奴隷:恋慕
	RETURNF "恋人"
ELSE
	;現状ではフェールセーフ
	RETURNF "知人"
ENDIF

