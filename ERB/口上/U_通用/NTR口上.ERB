#DIM 奴隷

SELECTCASE 絶頂強度
	CASE 0
		RETURN 0
	CASE IS < 3
		PRINTDATAL
			DATAFORM %"「んっ、あっ、ああんっ。はっ、っあ・・いい・・いくぅ・・！」"%
			DATAFORM %"「んんっ、あんっ。はっ、っいい・・い、いくぅ・・！」"%
			DATAFORM %"「いいっ、いいのっ。い・・い、いくぅ・・！」"%
		ENDDATA
		PRINTDATAL
			DATAFORM %"（・・ごめんなさい・・でも、イけなかったなんて知られたら、"+CALLNAME:MASTER+"は・・）"%
			DATAFORM %"（・・ごめんなさい・・でも、イけなかったなんて、"+CALLNAME:MASTER+"には言えない・・）"%
			DATAFORM %"（・・許して・・でも、"+CALLNAME:MASTER+"には、イけなかったなんて、言えない・・）"%
		ENDDATA
	CASEELSE
		PRINTDATAL
			DATAFORM %"「はぁ・・はっ・・あっ、はっ・ああっ・・いい・・いく・・いくっ・・ああっ！」"%
			DATAFORM %"「はああんっ・・はっ・・あっ、はっ・ああっ・・いい・・イく・・イくイくううっ！」"%
			DATAFORM %"「いっ、イくっ！・・いい・・いいっ、・・いくぅぅっ！」"%
		ENDDATA
		PRINTDATAL
			DATAFORM %"（・・イキたい・・でも、"+CALLNAME:MASTER+"じゃ・・ああっ、ダメ・・そんな事考えちゃ・・）"%
			DATAFORM %"（・・ああ・でも、"+CALLNAME:MASTER+"じゃイけない・・ああっ、ダメ・・そんな、そんな事・・）"%
			DATAFORM %"（・・ああっ、"+CALLNAME:MASTER+"じゃ足りない・・イきたい、イきたいのに・・）"%
		ENDDATA
ENDSELECT

RETURN 0

