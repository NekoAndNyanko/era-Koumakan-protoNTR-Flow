@EVENT_COUNTER_SOURCE(ARG,ARG:1)
SIF TCVAR:ARG:カウンター行動決定フラグ > 1
	RETURN 0
IF ARG:1 == 1

ENDIF

IF TEQUIP:PLAYER:Ｖ性爱 == ARG && TCVAR:ARG:カウンター行動 != 80 && CFLAG:ARG:うふふ == 2
