	SOURCE:ARG:性行动 += 150
	SOURCE:ARG:征服 += 400
	SOURCE:PLAYER:液体 += 100
	SOURCE:ARG:虐待 += 250
	SOURCE:ARG:强迫 += 400
	SOURCE:ARG:与快Ｖ += 500
	CALL PAIN_CHECK_V_MASTER(ARG)
	LOCALS = {ARG}/
	CSTR:PLAYER:射精判定用 += LOCALS
	TCVAR:PLAYER:射精箇所フラグ = 1
	;強度
	TCVAR:PLAYER:射精快感強度 += 400
	STAIN:ARG:Ｐ |= STAIN:PLAYER:膣内
	STAIN:PLAYER:膣内 |= STAIN:ARG:Ｐ
	EXP:PLAYER:Ｖ性交经验 ++
	CALL 締り具合変動, PLAYER, 3, 0
ENDIF
