	iAnimeCon = FLAG:图像_动画/2?FLAG:图像_动画/2#1
	CALL PRINT_IMAGE_ANIMATION("後背位_挿入_",0,6,iAnimeCon,1,,iSize,sAlign)

	iAnimeCon = (FLAG:图像_动画-iAnimeCon)?FLAG:图像_动画-iAnimeCon#1
	CALL PRINT_IMAGE_ANIMATION("後背位_挿入2_",0,12,iAnimeCon,0)
ELSE
	IF sRes_Name == "後背位_絶頂"
		iAnimeCon = FLAG:图像_动画/2?FLAG:图像_动画/2#1
		CALL PRINT_IMAGE_ANIMATION("後背位_挿入2_",0,12,iAnimeCon,0)
	ENDIF

	CALL PRINT_IMAGE_ANIMATION("後背位_絶頂_",0,4,FLAG:图像_动画,0)
	CALL PRINT_IMAGE_ANIMATION("後背位_絶頂_",0,2,1,0)
ENDIF
IF sRes_Name == "後背位_射精"
	sAnime_Res_Name = 後背位_射精_{FLAG:图像_射精图像类型}_
	CALL PRINT_IMAGE_ANIMATION(sAnime_Res_Name,0,8,1,0,,iSize,sAlign)
	CALL PRINT_IMAGE_ANIMATION(sAnime_Res_Name,6,2,FLAG:图像_动画,1,,iSize,sAlign)

	;大量なら追加で
	IF NOWEX:MASTER:射精 > 1
		CALL PRINT_IMAGE_ANIMATION(sAnime_Res_Name,8,2,FLAG:图像_动画,1,,iSize,sAlign)
	ENDIF

