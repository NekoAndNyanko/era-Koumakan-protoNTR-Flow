; RETURN 0 コマンドの実行不可
; RETURN 1 コマンドの実行可能
@COM300_11_ENABLE
#DIM 奴隷

奴隷 = TARGET:0
RESULTS:0 = %"[11]-"+CALLNAME:(PLAYER:0)+"の好きなプレイについて"%
;作りかけなのです
RETURN 0
IF TCVAR:奴隷:会话不能
	RETURN 0
ELSEIF TALENT:奴隷:恋慕 == 0
	RETURN 0
ELSE
	RETURN 1
ENDIF
RETURN 0

