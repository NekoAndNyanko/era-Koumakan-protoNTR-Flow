;==============================================================================
@EVENT_WC_COUNTER_POSE(ARG,ARG:1)
VARSET LOCAL
SIF TCVAR:ARG:30 > 1
	RETURN 0
SELECTCASE TCVAR:ARG:20
		;キス
	CASE 30
		TFLAG:60 ++
		;胸を揉ませる
	CASE 33
		TFLAG:61 ++
		;股間をまさぐる
	CASE 34
		TFLAG:62 ++
		;胸を揉む
	CASE 36
		TFLAG:65 ++
		;手淫
	CASE 50
		TFLAG:62 ++
		;口交
	CASE 51
		TFLAG:62 ++
		;素股
	CASE 52
		TFLAG:62 ++
		;乳交
	CASE 53
		TFLAG:62 ++
		;足交
	CASE 54
		TFLAG:62 ++
		;肛门爱抚
	CASE 55
		TFLAG:64 ++
		;舔舐肛门
	CASE 56
		TFLAG:64 ++
		;指挿入
	CASE 57
		TFLAG:63 ++
		;胸爱抚
	CASE 58
		TFLAG:65 ++
		;吮吸乳首
	CASE 59
		TFLAG:65 ++
		;ディープキス
	CASE 60
		TFLAG:60 ++
		;强制舔阴
	CASE 70
		TFLAG:60 ++
		;口交強制
	CASE 71
		TFLAG:60 ++
		;舔肛侍奉
	CASE 72
		TFLAG:60 ++
		;ぱふぱふ
	CASE 73
		TFLAG:60 ++
		;乳合わせ
	CASE 74
		TFLAG:65 ++
		;タワシ洗い
	CASE 75
		TFLAG:61 ++
		;挿入される
	CASE 80
		TFLAG:63 ++
		;肛交骑乘位
	CASE 81
		TFLAG:64 ++
		;肛交后背位
	CASE 82
		TFLAG:64 ++
		;肛交背面座位
	CASE 83
		TFLAG:64 ++
		;骑乘位(逆强奸）
	CASE 91
		TFLAG:62 ++
ENDSELECT
;69体勢
SELECTCASE TCVAR:ARG:20
	CASE 50
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;口交
	CASE 51
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;乳交
	CASE 53
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;肛门爱抚
	CASE 55
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;指挿入
	CASE 57
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;强制舔阴
	CASE 70
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;口交強制
	CASE 71
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
		;舔肛侍奉
	CASE 72
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,2)
	CASEELSE
		CALL EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,0)
ENDSELECT

;引数1 対象のキャラ番号 2 行動順 3 六九式移行への確率
@EVENT_WC_COUNTER_POSE_69(ARG,ARG:1,ARG:2)
IF ARG:2 == 0
	TEQUIP:ARG:六九式 = 0
	RETURN 0
ENDIF
IF TEQUIP:ARG:六九式
	TFLAG:60 ++
	RETURN 0
ENDIF
SIF TFLAG:60
	RETURN 0
IF RAND:(ARG:2) == 0
	TEQUIP:ARG:六九式 = 1
	TCVAR:ARG:27 = 1
ENDIF

