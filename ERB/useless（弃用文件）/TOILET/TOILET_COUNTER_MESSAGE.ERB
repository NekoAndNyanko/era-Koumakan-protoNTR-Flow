@WC_LOVEranking(ARG)		;;力技で好感度順位
;;ARG=誰？
#FUNCTION
#DIM LOOP_CHR
#DIM 勝ち星
勝ち星 = 0
;;同点だったときは、館内地位で勝敗。館内地位さえ同点なら、勝ち星無し

FOR LOOP_CHR,1,CHARANUM
	IF CFLAG:ARG:好感度 > CFLAG:LOOP_CHR:好感度 && ARG != LOOP_CHR
		勝ち星 += 1
	ELSEIF CFLAG:ARG:好感度 == CFLAG:LOOP_CHR:好感度 && CFLAG:ARG:館内地位 > CFLAG:LOOP_CHR:館内地位 && ARG != LOOP_CHR
		勝ち星 += 1
	ENDIF
NEXT

LOCAL:20 = (CHARANUM - 1) - 勝ち星

RETURNF LOCAL:20
