@EVENT_WC_COUNTER_PUNISHMENT(ARG,ARG:1)
IF NO:ARG == 999
	;CALL COM461_PUNISHMENT_FINISH
	RETURN 0
ENDIF
VARSET LOCAL
LOCAL = 1
WHILE LOCAL
	LOCAL:1 = RAND:10
	RESULT = 1
	TRYCALLFORM WC_PUNISHMENT_{LOCAL:1}(ARG,ARG:1)
	LOCAL = RESULT
WEND
DRAWLINE
WAIT
;パンツを脱がせる
@WC_PUNISHMENT_0(ARG,ARG:1)
SIF !CFLAG:PLAYER:服装_下半身下着２
	RETURN 1
CFLAG:PLAYER:服装_下半身下着２ = 0

PRINTFORML %CALLNAME:ARG%把%CALLNAME:PLAYER%的内裤夺去了
PRINTFORML 下达了今天一天不准穿内裤的命令，然后整理了弄乱的衣服…
RETURN 0

;跳蛋挿入
@WC_PUNISHMENT_1(ARG,ARG:1)
SIF !HAS_VAGINA(PLAYER)
	RETURN 1

CFLAG:PLAYER:跳蛋挿入 = 240
CFLAG:PLAYER:跳蛋挿入者 = ARG
PRINTFORML %CALLNAME:ARG%在%CALLNAME:PLAYER%的小穴里插入跳蛋然后固定住
PRINTFORML 命令暂时保持这个样子，然后整理了弄乱的衣服…
RETURN 0

;跳蛋A挿入
@WC_PUNISHMENT_2(ARG,ARG:1)

CFLAG:PLAYER:跳蛋A挿入 = 240
