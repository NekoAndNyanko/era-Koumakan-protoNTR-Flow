@绝顶确认(ARG,M绝顶强度,C绝顶强度,B绝顶强度,V绝顶强度,A绝顶强度,行为者)
#DIM M绝顶强度
#DIM C绝顶强度
#DIM B绝顶强度
#DIM V绝顶强度
#DIM A绝顶强度
#DIM 行为者
SELECTCASE TALENT:ARG:Ｍ感度
	CASE -1
		TIMES M绝顶强度 , 0.5
		M绝顶强度 = M绝顶强度 < 1 ? 1 # M绝顶强度
	CASE 1
		TIMES M绝顶强度 , 1.5
	CASE 2
		TIMES M绝顶强度 , 2
	CASE -2
		M绝顶强度 = 0
ENDSELECT
SELECTCASE TALENT:ARG:Ｃ感度
	CASE -1
		TIMES C绝顶强度 , 0.5
		C绝顶强度 = C绝顶强度 < 1 ? 1 # C绝顶强度
	CASE 1
		TIMES C绝顶强度 , 1.5
	CASE 2
		TIMES C绝顶强度 , 2
	CASE -2
		C绝顶强度 = 0
ENDSELECT
SELECTCASE TALENT:ARG:Ｂ感度
	CASE -1
		TIMES B绝顶强度 , 0.5
		B绝顶强度 = B绝顶强度 < 1 ? 1 # B绝顶强度
	CASE 1
		TIMES B绝顶强度 , 1.5
	CASE 2
		TIMES B绝顶强度 , 2
	CASE -2
		B绝顶强度 = 0
ENDSELECT
SELECTCASE TALENT:ARG:Ｖ感度
	CASE -1
		TIMES V绝顶强度 , 0.5
		V绝顶强度 = V绝顶强度 < 1 ? 1 # V绝顶强度
	CASE 1
		TIMES V绝顶强度 , 1.5
	CASE 2
		TIMES V绝顶强度 , 2
	CASE -2
		V绝顶强度 = 0
ENDSELECT
SELECTCASE TALENT:ARG:Ａ感度
	CASE -1
		TIMES A绝顶强度 , 0.5
		A绝顶强度 = A绝顶强度 < 1 ? 1 # A绝顶强度
	CASE 1
		TIMES A绝顶强度 , 1.5
	CASE 2
		TIMES A绝顶强度 , 2
	CASE -2
		A绝顶强度 = 0
ENDSELECT

IF M绝顶强度+C绝顶强度+B绝顶强度+V绝顶强度+A绝顶强度 > 0
	IF 行为者 == 人物_訪問者 || (行为者 != 0 && 行为者 >= CHARANUM && (TALENT:ARG:恋慕 || TALENT:ARG:亲爱 || TALENT:ARG:人妻 || TALENT:ARG:恋人))
		EXP:ARG:浮気Ｍ绝顶经验 += M绝顶强度
		EXP:ARG:浮気Ｃ绝顶经验 += C绝顶强度
		EXP:ARG:浮気Ｂ绝顶经验 += B绝顶强度
		EXP:ARG:浮気Ｖ绝顶经验 += V绝顶强度
		EXP:ARG:浮気Ａ绝顶经验 += A绝顶强度
	ELSE
		EXP:ARG:Ｍ绝顶经验 += M绝顶强度
		EXP:ARG:Ｃ绝顶经验 += C绝顶强度
		EXP:ARG:Ｂ绝顶经验 += B绝顶强度
		EXP:ARG:Ｖ绝顶经验 += V绝顶强度
		EXP:ARG:Ａ绝顶经验 += A绝顶强度
	ENDIF
	;口上呼出
	IF FLAG:睡姦フラグ == 1 && NTR_CHK_VISIBLE(FLAG:訪問者の現在位置) || IS_NTR_SHOW_ALL()
		IF CFLAG:ARG:睡眠
			PRINTFORMW %CALLNAME:ARG%的蜜穴喷出了晶莹的液体，看来是绝顶了…

		ENDIF
	ELSE

	ENDIF
	CFLAG:ARG:性欲 -= (M绝顶强度+C绝顶强度+B绝顶强度+V绝顶强度+A绝顶强度) * 20
	SIF CFLAG:ARG:性欲 < 0
		CFLAG:ARG:性欲 = 0
	SIF CFLAG:ARG:性欲 < 500
		CFLAG:ARG:覚醒 += 3
ENDIF




