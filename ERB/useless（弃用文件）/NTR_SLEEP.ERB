;-------------------------------------------------------------------------------
;睡姦実行
@VISITER_FRIENDSHIP_SLEEP(奴隷)
#DIM 奴隷
#DIM 觉醒等待 = 0
SIF CFLAG:奴隷:性欲 >= 80
	CFLAG:奴隷:挿入 += 1
IF FLAG:睡姦フラグ == 1
	TCVAR:LOCAL:行為者 = 人物_訪問者
	IF CFLAG:奴隷:睡眠
		IF 觉醒等待 == 1 && CFLAG:奴隷:覚醒 > 0
			SIF NTR_CHK_VISIBLE(FLAG:訪問者の現在位置) || IS_NTR_SHOW_ALL()
				PRINTFORML %NTR_NAME(0)%正在等待%CALLNAME:奴隷%的睡眠恢复稳定、
			CFLAG:奴隷:覚醒 -= 1
			SIF CFLAG:奴隷:覚醒 == 0
				觉醒等待 = 0
			CALL 睡奸觉醒度状态表现(奴隷)
			RETURN
		ENDIF
		SIF CFLAG:奴隷:覚醒 < 0
			CFLAG:奴隷:覚醒 = 0
		IF CFLAG:奴隷:覚醒 >= 10
			觉醒等待 = 1
			CALL 睡奸觉醒(奴隷)
		ELSEIF CFLAG:奴隷:挿入 < 5
			CFLAG:奴隷:挿入 += 1
			IF CSTR:奴隷:M处女丧失记录 == ""
				CALL NTR_SLEEP_口交(奴隷)
			ELSEIF RAND(100) < 30
				CALL NTR_SLEEP_接吻(奴隷)
			ELSEIF RAND(100) < 50
				CALL NTR_SLEEP_爱抚(奴隷)
			ELSEIF RAND(100) < 50
				CALL NTR_SLEEP_胸爱抚(奴隷)
			ELSEIF RAND(100) < 20
				CALL NTR_SLEEP_舔菊(奴隷)
			ELSEIF RAND(100) < 15
				CALL NTR_SLEEP_脱内裤(奴隷)
				RESTART
			ELSEIF RAND(100) < 15
				CALL NTR_SLEEP_脱内裤(奴隷)
				CALL NTR_SLEEP_摄影(奴隷)
				RESTART
			ELSE
				SELECTCASE RAND(3)
					CASE 0
						CALL NTR_SLEEP_接吻(奴隷)
					CASE 1
						CALL NTR_SLEEP_爱抚(奴隷)
					CASE 2
						CALL NTR_SLEEP_胸爱抚(奴隷)
				ENDSELECT
			ENDIF
		ELSEIF CFLAG:奴隷:挿入 >= 5 && CFLAG:奴隷:挿入 < 10
			CFLAG:奴隷:挿入 += 1
			SELECTCASE RAND(3)
				CASE 0
					CALL NTR_SLEEP_口交(奴隷)
				CASE 1
					CALL NTR_SLEEP_69(奴隷)
				CASE 2
					CALL NTR_SLEEP_素股(奴隷)
			ENDSELECT
		ELSEIF CFLAG:奴隷:挿入 >= 10
			CFLAG:奴隷:挿入 += 1
			SIF RAND(100) < 10
				CFLAG:奴隷:挿入 /= 2
			IF TALENT:奴隷:处女 && IS_NTR_KEEP_VIRGIN()
				CALL NTR_SLEEP_A_SEX(奴隷)
			ELSEIF RAND(100) < 75
				CALL NTR_SLEEP_SEX(奴隷)
			ELSE
				CALL NTR_SLEEP_A_SEX(奴隷)
			ENDIF
		ENDIF
		CALL 睡奸觉醒度状态表现(奴隷)
	ELSE
		FLAG:睡姦フラグ = 0
		CFLAG:奴隷:NTR睡姦 = 0
		CFLAG:奴隷:覚醒 = 0
		CFLAG:奴隷:挿入 = 0
		FLAG:訪問者との行為 = NTR_行为_睡奸_无
	ENDIF
	[IF_DEBUG]
	PRINTFORML %CALLNAME:奴隷%的觉醒度为 {CFLAG:奴隷:覚醒} 
	[ENDIF]

ELSE
ENDIF
