		SELECTCASE CFLAG:MASTER:WC_訪問者の前回セクハラ
			CASE 40,41,42,43,44,45
				RETURNF 0
		ENDSELECT
		RETURNF 1
	CASE 44		;;人里・肉便器
		;;受け入れた　未満だとだめ
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			RETURNF 0
		;;通常移动範囲以外だとだめ
		SIF CFLAG:MASTER:現在位置 > 場所番号最大値
			RETURNF 0
		;;あなたが处女だとだめ
		SIF TALENT:MASTER:处女 && HAS_VAGINA(MASTER)
			RETURNF 0
		;;あなたの卖春经验が１０ないとだめ
		SIF EXP:MASTER:卖春经验 < 10
			RETURNF 0
		;;Ｂにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ｂ装着者
			RETURNF 0
		;;Ｃにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ｃ装着者
			RETURNF 0
		;;Ｖにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ｖ装着者
			RETURNF 0
		;;Ａにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ａ装着者
			RETURNF 0
		;;贞操帯つけてたらだめ
		SIF EQUIP:MASTER:下半身下着１ == 100 && CFLAG:MASTER:WC_あなたに贞操帯を着けた人 != 人物_訪問者
			RETURNF 0
		;;前回、前々回が特殊（外出）系だったらだめ
		SELECTCASE CFLAG:MASTER:WC_訪問者の前々回セクハラ
			CASE 40,41,42,43,44,45
				RETURNF 0
		ENDSELECT
		SELECTCASE CFLAG:MASTER:WC_訪問者の前回セクハラ
			CASE 40,41,42,43,44,45
				RETURNF 0
		ENDSELECT
		RETURNF 1
	CASE 45		;;人里・お披露目プレイ
		;;受け入れた　未満だとだめ
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			RETURNF 0
		;;通常移动範囲以外だとだめ
		SIF CFLAG:MASTER:現在位置 > 場所番号最大値
			RETURNF 0
		;;Ｂにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ｂ装着者
			RETURNF 0
		;;Ｃにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ｃ装着者
			RETURNF 0
		;;Ｖにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ｖ装着者
			RETURNF 0
		;;Ａにえっちなお道具つけてたらだめ
		SIF CFLAG:MASTER:WC_道具Ａ装着者
			RETURNF 0
		;;贞操帯つけてたらだめ
		SIF EQUIP:MASTER:下半身下着１ == 100 && CFLAG:MASTER:WC_あなたに贞操帯を着けた人 != 人物_訪問者
			RETURNF 0
		;;テクニック系の総和が１３以上ないとだめ
		SIF ABL:MASTER:指 + ABL:MASTER:舌 + ABL:MASTER:胸 + ABL:MASTER:腰 + ABL:MASTER:膣 + ABL:MASTER:菊花 < 12
			RETURNF 0
		;;前回、前々回が特殊（外出）系だったらだめ
		SELECTCASE CFLAG:MASTER:WC_訪問者の前々回セクハラ
			CASE 40,41,42,43,44,45
				RETURNF 0
		ENDSELECT
		SELECTCASE CFLAG:MASTER:WC_訪問者の前回セクハラ
			CASE 40,41,42,43,44,45
				RETURNF 0
		ENDSELECT
		RETURNF 1
ENDSELECT
RETURNF 0


;;-------------------------------------------------------------------------------
;;屈服度は、ナンバーワン？
;;順位では無く、誰にも負けてないか、だけを判定
@WC_topOFsurrender(ARG)
;;ARG   = 対象者
#FUNCTION
#DIM LOOP_CHR
#DIM 対象者
対象者 = ARG

FOR LOOP_CHR,0,CHARANUM
	IF LOOP_CHR != 対象者
		SIF CFLAG:対象者:屈服度 < CFLAG:LOOP_CHR:屈服度
			RETURNF 0
	ENDIF
NEXT
RETURNF 1


;;-------------------------------------------------------------------------------
;;屈服度は、勝ってる？
;;同席している女の子と比較して、屈服度が勝っているか判定
@WC_winOFsurrender(ARG)
;;ARG   = 対象者
#FUNCTION
#DIM LOOP_CHR
#DIM 対象者
対象者 = ARG

;同席者の判定は最大64まで
FOR LOOP_CHR,0,CHARANUM
	SIF LOOP_CHR >= 64
		BREAK
	IF LOOP_CHR != 対象者
		SIF CFLAG:対象者:屈服度 < CFLAG:LOOP_CHR:屈服度 && GETBIT(CFLAG:MASTER:WC_同席者,LOOP_CHR)
			RETURNF 0
	ENDIF
NEXT
RETURNF 1


