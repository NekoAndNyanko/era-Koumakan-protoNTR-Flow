;;屈服度の上昇値を決定
;;訪問者は、とりあえず男としておく
@WC_SURRENDER(ARG,ARG:1)
;;ARG   = 対象者
;;ARG:1 = プレイ内容
;;= 1,ライト
;;= 2,ソフト
;;= 3,ハード
;;= 4,特殊
#FUNCTION
#DIM LOOP_CHR
#DIM 対象者
#DIM プレイ内容
#DIM 上昇予定値
#DIM 訪問者凄味
対象者 = ARG
プレイ内容 = ARG:1
上昇予定値 = 0
訪問者凄味 = 0

FOR LOOP_CHR,0,CHARANUM
	訪問者凄味 += MARK:LOOP_CHR:浮気快乐刻印
NEXT

上昇予定値 = プレイ内容 * 5
上昇予定値 += TALENT:対象者:NTR + TALENT:対象者:公众便所 - TALENT:対象者:胆量 - TALENT:対象者:态度 - TALENT:対象者:应答 - TALENT:対象者:自尊心 - TALENT:対象者:贞操

SELECTCASE TALENT:対象者:性別嗜好
	CASE 1,3	;;男嫌い／人間不信
		上昇予定値 -= 1
ENDSELECT

IF !HAS_VAGINA(対象者)
	SELECTCASE ABL:対象者:断背之气
		CASE 0
			上昇予定値 -= 1
		CASE 1

		CASE 2
			上昇予定値 += 1
		CASE 3
			上昇予定値 += 2
		CASE 4
			上昇予定値 += 3
		CASEELSE	;;5以上
			上昇予定値 += 4
	ENDSELECT
ENDIF

IF CFLAG:対象者:WC_訪問者セクハラ失敗 == 1
	上昇予定値 += プレイ内容
	SIF 上昇予定値 < 0
		上昇予定値 = 0
ELSE
	IF 対象者 == MASTER
		上昇予定値 += CFLAG:対象者:WC_訪問者受け入れ具合 + LIMIT(訪問者凄味 / 5,1,9)
	ELSE
		上昇予定値 += MARK:対象者:浮気快乐刻印 + LIMIT(訪問者凄味 / 5,1,9)
	ENDIF

	IF 上昇予定値 < プレイ内容
		SELECTCASE プレイ内容
			CASE 1
				上昇予定値 = プレイ内容 + 1
			CASE 2
				上昇予定値 = プレイ内容 + 2
			CASE 3
				上昇予定値 = プレイ内容 + 3
		ENDSELECT
	ENDIF
ENDIF

RETURNF 上昇予定値


