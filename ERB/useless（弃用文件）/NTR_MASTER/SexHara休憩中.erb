		;		THROW 反応の決定で問題が発生しました。
		;;		RETURN 1
		;ENDSELECT
		SELECTCASE CFLAG:被害者:NTR訪問者との行為 
			CASE 0,1,2,3,4,5
				TRYCALLLIST
					FUNC SexHara休息中_Lunatic_K{CFLAG:加害者:297}_93_1_{CFLAG:被害者:NTR訪問者との行為}(加害者, 被害者)
					FUNC SexHara休息中_Lunatic_KU_93_1_{CFLAG:被害者:NTR訪問者との行為}(加害者, 被害者)
				ENDFUNC
			CASEELSE
				THROW 反応の決定で問題が発生しました。
				RETURN 1
		ENDSELECT
	ENDIF
	;PRINTFORMW その姿を見下ろしながら%CALLNAME:被害者%は呟いた
	;PRINTFORMW 「足もとがお留守ですよ、門番さん」
	TRYCALLLIST
