;403,休息中に受けるセクハラ
;-------------------------------------------------------------------------------
;被害者は常にMASTERなので引数にはしない
@SexHara休息中(加害者)
#DIM 加害者
IF 加害者 == MASTER
	RETURN 0
ENDIF
TRYCALL SexHara休息中_Easy(加害者, MASTER)
SIF RESULT == 1
	RETURN 0
WAIT
TRYCALL SexHara休息中_Normal(加害者, MASTER)
SIF RESULT == 1
	RETURN 0
WAIT
TRYCALL SexHara休息中_Extra(加害者, MASTER)
SIF RESULT == 1
	RETURN 0
WAIT
TRYCALL SexHara休息中_Hard(加害者, MASTER)
SIF RESULT == 1
	RETURN 0
WAIT
TRYCALL SexHara休息中_Lunatic(加害者, MASTER)
SIF RESULT == 1
	RETURN 0
WAIT
TRYCALL SexHara休息中_Phantasm(加害者, MASTER)
RETURN 0

