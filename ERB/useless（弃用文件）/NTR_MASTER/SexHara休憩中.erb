;-------------------------------------------------------------------------------
;あなたを管理しちゃう - 阴茎用贞操帯をつけさせる
@SexHara休息中_Phantasm_Route1(加害者, 被害者)
#DIM 加害者
#DIM 被害者
#DIM V_MIN
#DIM V_MAX
VARSET LOCAL
IF CFLAG:被害者:阴茎用贞操帯着用 == 0
	;つけていない場合
	;PRINTFORML %CALLNAME:加害者%は%CALLNAME:被害者%の目の前で、小さな阴茎用の贞操帯を取りだした
	TRYCALLLIST
		FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_0_0(加害者, 被害者)
		FUNC SexHara休息中_Phantasm_KU_0_0_0(加害者, 被害者)
	ENDFUNC
	V_MIN = 0
	V_MAX = 1
	IF CFLAG:被害者:屈服度 > 2000 ;屈服度が高い場合は拒否できない
		PRINT [1] - 穿上
		V_MIN = 1
	ELSE
		PRINT [0] - 那是绝对不被允许的！ [1] - 做出反应就输了……做出反应就输了……
	ENDIF
	$INPUT_LOOP_1
	INPUT
	SIF V_MIN == V_MAX
		RESULT = V_MAX
	SIF RESULT < V_MIN || V_MAX < RESULT
		GOTO INPUT_LOOP_1
	IF !RESULT
		;PRINTFORML %CALLNAME:被害者%が腕を掴んで止めると、%CALLNAME:加害者%は贞操帯を片付け離れていった……
		TRYCALLLIST
			FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_0_1(加害者, 被害者)
			FUNC SexHara休息中_Phantasm_KU_0_0_1(加害者, 被害者)
		ENDFUNC
		RETURN 1
	ELSE
		;PRINTFORML %CALLNAME:被害者%は%CALLNAME:被害者%の阴茎に贞操帯を着けると、２４時間後に外してあげると言って離れていった……
		TRYCALLLIST
			FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_0_2(加害者, 被害者)
			FUNC SexHara休息中_Phantasm_KU_0_0_2(加害者, 被害者)
		ENDFUNC
		;屈服度が劇的に増える。満たない場合は強制的に1500に
		CFLAG:被害者:屈服度 = MAX (CFLAG:被害者:屈服度 + 20 + RAND:20,1500)
		;阴茎用贞操帯装着
		CALL MOUNT_P_CHASTITY_BELT(被害者, 加害者, 1)
	ENDIF
ELSEIF CFLAG:被害者:阴茎用贞操帯着用 > 1000 && CHK_DISMOUNT_P_CHASTITY_BELT(被害者, 加害者)
	;すでに着けている場合。ちょっとクソ管理だけど、1000未満は別で使う
	;PRINTFORML %CALLNAME:加害者%は%CALLNAME:被害者%の贞操帯を指でコンコンと叩き、鍵をちらつかせた
	TRYCALLLIST
		FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_1_0(加害者, 被害者)
		FUNC SexHara休息中_Phantasm_KU_0_1_0(加害者, 被害者)
	ENDFUNC
	V_MIN = 0
	V_MAX = 1
	IF CFLAG:被害者:屈服度 > 4000
		;屈服度が非常に高い場合は耐えることしかできない
		PRINT [1] - 直到摘下来位置都要忍耐
		V_MIN = 1
	ELSE
		PRINT [0] - 求求你把它摘下来 [1] - 直到摘下来位置都要忍耐
	ENDIF
	$INPUT_LOOP_2
	INPUT
	SIF V_MIN == V_MAX
		RESULT = V_MAX
	SIF RESULT < V_MIN || V_MAX < RESULT
		GOTO INPUT_LOOP_2
	IF RESULT==0 ;もう外してくれと頼んだ
		IF TIME_PROGRESS(CFLAG:被害者:阴茎用贞操帯着用) < 時間_1DAY
			;１日経過していない
			;PRINTFORML %CALLNAME:加害者%は、いつ着けたのか覚えていないのなら、今日から数え直しなさいと言い離れていった……
			TRYCALLLIST
				FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_1_1(加害者, 被害者)
				FUNC SexHara休息中_Phantasm_KU_0_1_1(加害者, 被害者)
			ENDFUNC
			;立場をわからされる事で屈服度が増加
			SOURCE:被害者:被动 += 100 + 200 * ABL:被害者:顺从
			CALL NTR_ADD_SURRENDER(被害者, (20 + RAND:20) )
			;着用日をリセット
			CFLAG:被害者:阴茎用贞操帯着用 = TIME_PROGRESS(0)
		ELSE
			;１日我慢できた
			;PRINTFORML %CALLNAME:加害者%はそろそろ許してあげると言い、%CALLNAME:被害者%の贞操帯を取り外した。
			TRYCALLLIST
				FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_1_2(加害者, 被害者)
				FUNC SexHara休息中_Phantasm_KU_0_1_2(加害者, 被害者)
			ENDFUNC
			;阴茎用贞操帯解除
			CALL DISMOUNT_P_CHASTITY_BELT(被害者)
			SOURCE:被害者:被动 += 100 + 200 * ABL:被害者:顺从
		ENDIF
	ELSE ;外して貰えるまで我慢している
		IF TIME_PROGRESS(CFLAG:被害者:阴茎用贞操帯着用) < 時間_1DAY
			;７日経過していない
			;PRINTFORML %CALLNAME:加害者%は意地悪く笑い、贞操帯の鍵をポケットにしまい込んだ
			TRYCALLLIST
				FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_1_3(加害者, 被害者)
				FUNC SexHara休息中_Phantasm_KU_0_1_3(加害者, 被害者)
			ENDFUNC
			CALL NTR_ADD_SURRENDER(被害者, RAND:20)
		ELSEIF TIME_PROGRESS(CFLAG:被害者:阴茎用贞操帯着用) < RAND(86400)
			;７日以上経過しているがもう少し付けさせておきたい場合
			;PRINTFORML %CALLNAME:加害者%は贞操帯が気に入ったようだからもう少し着けてなさいと言い、贞操帯の鍵をポケットにしまい込んだ
			TRYCALLLIST
				FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_1_4(加害者, 被害者)
				FUNC SexHara休息中_Phantasm_KU_0_1_4(加害者, 被害者)
			ENDFUNC
			CALL NTR_ADD_SURRENDER(被害者, RAND:20)
		ELSE
			;７日以上経過していて気が向いた場合(最大で60日我慢させる)
			;PRINTFORML %CALLNAME:加害者%は、贞操帯を着けてから{TIME_PROGRESS(CFLAG:被害者:阴茎用贞操帯着用)/時間_1DAY}日になるし、そろそろ許してあげると言い、%CALLNAME:被害者%の贞操帯を取り外した。
			TRYCALLLIST
				FUNC SexHara休息中_Phantasm_K{CFLAG:加害者:297}_0_1_5(加害者, 被害者)
				FUNC SexHara休息中_Phantasm_KU_0_1_5(加害者, 被害者)
			ENDFUNC
			;阴茎用贞操帯解除
			CALL DISMOUNT_P_CHASTITY_BELT(被害者)
			SOURCE:被害者:被动 += 100 + 200 * ABL:被害者:顺从
		ENDIF
	ENDIF
ENDIF
PRINTFORML
RETURN 0

