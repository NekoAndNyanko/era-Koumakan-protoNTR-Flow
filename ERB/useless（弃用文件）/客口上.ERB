@CALL_K_NAME(奴隷)
#FUNCTIONS
#DIM 奴隷
#DIMS 呼称

呼称 = %CALLNAME:奴隷+"たん"%
SELECTCASE 奴隷
	CASE 人物_美铃
		IF CALLNAME:奴隷 == "美鈴"
			呼称 = "メイリンたん"
		ENDIF
	CASE 人物_小恶魔
		IF CALLNAME:奴隷 == "小恶魔"
			呼称 = "こぁ"
		ENDIF
	CASE 人物_帕秋莉
		IF CALLNAME:奴隷 == "パチュリー"
			呼称 = "ぱっちぇたん"
		ENDIF
	CASE 人物_咲夜
		IF CALLNAME:奴隷 == "咲夜"
			呼称 = "さっきゅん"
		ENDIF
	CASE 人物_蕾米莉亚
		IF CALLNAME:奴隷 == "蕾米莉亚"
			呼称 = "れみぃたん"
		ENDIF
	CASE 人物_芙兰朵露
		IF CALLNAME:奴隷 == "フラン" || CALLNAME:奴隷 == "芙兰朵露"
			呼称 = "フランたん"
		ENDIF
	CASE 人物_艾丽娅
		IF CALLNAME:奴隷 == "小恶魔"
			呼称 = "ここぁ"
		ENDIF
	CASE 人物_琪露诺
		IF CALLNAME:奴隷 == "チルノ"
			呼称 = "ちるのたん"
		ENDIF
	CASE 人物_大妖精
		IF CALLNAME:奴隷 == "大妖精"
			呼称 = "だいちゃん"
		ENDIF
ENDSELECT
RETURNF 呼称


