	LOCAL:1 = 0
	FOR LOCAL,0,CHARANUM
		IF DOWNBASE:LOCAL:体力 > 0
			PRINTFORM 体力-{DOWNBASE:LOCAL:体力} 
			LOCAL:1 ++
		ELSEIF DOWNBASE:LOCAL:体力 < 0
