	;-------------------------------------------------
	;性行动のソース
	;-------------------------------------------------
	CALL SOURCE_性行动(LOCAL)
	;-------------------------------------------------
	;达成のソース
	;-------------------------------------------------
	CALL SOURCE_达成(LOCAL)
	;-------------------------------------------------
	;痛みのソース
	;-------------------------------------------------
	CALL SOURCE_苦痛(LOCAL)
	;-------------------------------------------------
	;恐れのソース
