;NTR贞操帯有効期限チェック
@ChastityBelt_Check
#DIM 奴隷

;NTR贞操帯カウンタ処理
IF FLAG:贞操帯鍵購入フラグ
	奴隷 = FLAG:贞操帯鍵購入フラグ
	IF FLAG:贞操帯鍵有効カウンタ < TIME_PROGRESS()
		IF CFLAG:奴隷:うふふ>0 && CFLAG:MASTER:うふふ>0
			;うふふ中に切れた場合
			PRINTFORMW %CALLNAME:奴隷+"把"+CALLNAME:MASTER+"推开了、拿走了贞操带的钥匙。好像是钥匙的有效期到了。"%
			TRYCALLLIST
				FUNC NTR_MESSAGE_COM_K{奴隷}_350_0_3(奴隷)
				FUNC NTR_MESSAGE_COM_K_350_0_3(奴隷)
			ENDFUNC
			IF TALENT:奴隷:公众便所
				CFLAG:奴隷:うふふ = 0
				CFLAG:MASTER:うふふ = 0
			ENDIF
		ELSE
			PRINTFORMW %NTR_NAME(0)+"那里买来的"+CALLNAME:奴隷+"的贞操带钥匙好像过期了。"%
		ENDIF
		CALL CLOTHES_Change_Knickers(奴隷)
		FLAG:贞操帯鍵購入フラグ = 0
	ENDIF
ENDIF
RETURN 0


