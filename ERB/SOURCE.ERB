IF (TCVAR:行為者 < CHARANUM)
	LOCAL:0 = RELATION:TARGET:(NO:PLAYER)
	SIF LOCAL:0 != 0
		PRINTFORML ＜相性{LOCAL / 100}.{LOCAL % 100}倍＞
	SIF SELECTCOM == PREVCOM && TFLAG:50 == TFLAG:151 && (SELECTCOM < 350 || SELECTCOM >= 500)
		PRINTL ＜同一コマンド連続実行＞
	;气力０
	SIF BASE:1 <= 0
