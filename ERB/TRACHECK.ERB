@memorial_m_append_isPenis(奴隷, 相手编号)
#DIM 奴隷
#DIM 相手编号
#DIMS 内容
#DIMS 日時
#DIMS 場所
#DIMS 相手 = ""

IF 奴隷 == 相手编号
	RETURN
ENDIF
IF CSTR:奴隷:M处女丧失记录 == ""
	日時 = 第{年份记录}年 %日付_月(暦法月)%{暦法日}日 %天候(天候値)%%" "%
	場所 = %"在"+GETPLACENAME(CFLAG:奴隷:現在位置)+"，"%
	IF CFLAG:奴隷:睡眠
		場所 += "在睡梦中"
	ENDIF
	SELECTCASE 相手编号
		CASE IS == -1
			RETURN
		CASE IS < CHARANUM
			相手 = %CALLNAME:相手编号+"的肉棒"%
		CASE IS == 人物_訪問者
			相手 = %NTR_NAME(0)+"强壮的肉棒"%
		CASE IS == 人物_客, 人物_村人
			相手 = %"不知名的男人的肉棒"%
		CASEELSE
			相手 = %NTR_NAME(0)+"的肉棒"%
	ENDSELECT

	IF 内容 == ""
		IF CFLAG:奴隷:睡眠
			内容 = %"被强迫地吸吮了"%
		ELSE
			SELECTCASE TCVAR:奴隷:行為者
				CASE IS == 0
					; 行為者があなた
					IF TALENT:奴隷:恋慕
						内容 = %"满怀爱意地吸吮了"%
					ELSEIF TALENT:奴隷:恋人
						内容 = %"吸吮了"%
					ELSE
						内容 = %"半推半就地吸吮了"%
					ENDIF
				CASE IS == 人物_訪問者
					IF TALENT:奴隷:NTR || (奴隷 == 人物_あなた && (FLAG:1840 >= 11 && FLAG:1840 <= 15))
						内容 = %"满怀爱意地吸吮了"%
					ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || (奴隷 == 人物_あなた && FLAG:1840 == 10)
						内容 = %"满怀爱意地吸吮了"%
					ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
						内容 = %"主动地吸吮了"%
					ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
						内容 = %"吸吮了"%
					ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_うふふする程度) 
						内容 = %"半推半就地吸吮了"%
					ELSEIF TALENT:奴隷:浮気公認
						内容 = %"取走的"%
					ELSE
						内容 = %"被强迫着吸吮了"%
					ENDIF
				CASE IS == 人物_客
					内容 = %"买去的"%
				CASEELSE
					; 行為者が住人
					; 住人からの行動なので「奪われた」は無い。
					IF 奴隷 ==  TCVAR:奴隷:行為者
						;奪った側
						IF 相手编号 == 0
							;相手があなた
							IF TALENT:相手编号:恋慕
								内容 = %"满怀爱意地吸吮了"%
							ELSE
								内容 = %"半推半就地吸吮了"%
							ENDIF
						ELSEIF 相手编号 == 人物_訪問者
							IF TALENT:相手编号:NTR
								内容 = %"满怀爱意地吸吮了"%
							ELSEIF NTR_CHK_FAVORABLY(相手编号, FAV_寝取られ寸前) 
								内容 = %"满怀爱意地吸吮了"%
							ELSE
								内容 = %"半推半就地吸吮了"%
							ENDIF
						ELSE
							; 住人同士
							内容 = %"半推半就地吸吮了"%
						ENDIF
					ELSE
						;奪われた側（たぶんここは死に分岐）
						IF 相手编号 == 0
							;相手があなた
							IF TALENT:奴隷:恋慕
								内容 = %"满怀爱意地吸吮了"%
							ELSEIF TALENT:奴隷:恋人
								内容 = %"吸吮了"%
							ELSE
								内容 = %"半推半就地吸吮了"%
							ENDIF
						ELSEIF 相手编号 == 人物_訪問者
							IF TALENT:奴隷:NTR
								内容 = %"满怀爱意地吸吮了"%
							ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
								内容 = %"满怀爱意地吸吮了"%
							ELSEIF TALENT:奴隷:浮気公認
								内容 = %"取走的"%
							ELSE
								内容 = %"半推半就地吸吮了"%
							ENDIF
						ELSE
							; 住人同士
							内容 = %"半推半就地吸吮了"%
						ENDIF
					ENDIF
			ENDSELECT
		ENDIF
	ENDIF
	CSTR:奴隷:M处女丧失记录 = %日時%%場所%%内容%%相手%
ENDIF


