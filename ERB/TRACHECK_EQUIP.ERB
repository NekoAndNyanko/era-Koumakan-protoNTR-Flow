;挿入継続の処理
@EQUIP_INCERT_V(ARG)
;-------------------------------------------------
;行為者
;-------------------------------------------------
TCVAR:ARG:116 = TEQUIP:ARG:Ｖ性爱
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 ）
;TCVAR:4 射精快感強度

LOCALS = {ARG}/
CSTR:(TEQUIP:ARG:Ｖ性爱):射精判定用 += LOCALS
TCVAR:(TEQUIP:ARG:Ｖ性爱):射精箇所フラグ = 1
;強度
TCVAR:(TEQUIP:ARG:Ｖ性爱):射精快感強度 = 700

PRINTFORML ＜%CALLNAME:(TEQUIP:9)%Ｖ插入中＞
DOWNBASE:ARG:体力 += 20
DOWNBASE:ARG:气力 += 50

SOURCE:ARG:快Ｖ += 150
SOURCE:ARG:情爱 += 75
SOURCE:ARG:露出 += 25

;Vへの苦痛は先に処理しておく
;EXP:Ｖ经验をみる（处女はありえない）
LOCAL = 200
LOCAL:1 = 150
IF EXP:ARG:Ｖ经验 < EXPLV:2
	TIMES LOCAL , 0.80
	TIMES LOCAL:1 , 1.20
ELSEIF EXP:ARG:Ｖ经验 < EXPLV:3
	TIMES LOCAL , 0.40
ELSEIF EXP:ARG:Ｖ经验 < EXPLV:4
	TIMES LOCAL , 0.10
ELSEIF EXP:ARG:Ｖ经验 < EXPLV:5
	TIMES LOCAL , 0.00
ELSEIF EXP:ARG:Ｖ经验 >= EXPLV:5
	TIMES LOCAL , 0.00
ENDIF

;PALAM:润滑をみる
IF PALAM:ARG:润滑 < PALAMLV:1
	TIMES LOCAL , 1.20
	TIMES LOCAL:1 , 1.50
ELSEIF PALAM:ARG:润滑 < PALAMLV:2
	TIMES LOCAL , 0.70
	TIMES LOCAL:1 , 1.20
ELSEIF PALAM:ARG:润滑 < PALAMLV:3
	TIMES LOCAL , 0.40
ELSEIF PALAM:ARG:润滑 < PALAMLV:4
	TIMES LOCAL , 0.10
ELSEIF PALAM:ARG:润滑 >= PALAMLV:4
	TIMES LOCAL , 0.10
ENDIF

SOURCE:ARG:苦痛 += LOCAL
SOURCE:ARG:反感 += LOCAL:1
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:ARG:Ｖ性交经验 ++
RETURN 1

@EQUIP_INCERT_A(ARG)
;-------------------------------------------------
;行為者
;-------------------------------------------------
TCVAR:ARG:116 = TEQUIP:ARG:Ａ性爱
;TCVAR:0 射精対象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI 4=触手 5=その他）
;TCVAR:1 予備射精対象フラグ（ビットを使いたくない人用、CFLAG:100に翻訳されます）
;	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI -1=触手 -2=その他）
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 ）
;TCVAR:4 射精快感強度
LOCALS = {ARG}/
CSTR:(TEQUIP:ARG:Ａ性爱):10 += LOCALS
SETBIT TCVAR:(TEQUIP:ARG:Ａ性爱):0, 0
TCVAR:(TEQUIP:ARG:Ａ性爱):2 = 2
;強度
TCVAR:(TEQUIP:ARG:Ａ性爱):4 = 900
PRINTFORML ＜%CALLNAME:(TEQUIP:9)%Ａ插入中＞
DOWNBASE:ARG:体力 += 30
DOWNBASE:ARG:气力 += 60

SOURCE:ARG:快Ａ += 200
SOURCE:ARG:情爱 += 50
SOURCE:ARG:露出 += 30
SOURCE:ARG:逸脱 += 100

LOCAL = 400
LOCAL:1 = 250
;Aへの苦痛は先に処理しておく
IF EXP:ARG:Ａ经验 < EXPLV:1
	TIMES LOCAL , 3.00
	TIMES LOCAL:1 , 3.00
ELSEIF EXP:ARG:Ａ经验 < EXPLV:2
	TIMES LOCAL , 1.20
	TIMES LOCAL:1 , 2.00
ELSEIF EXP:ARG:Ａ经验 < EXPLV:3
	TIMES LOCAL , 0.80
	TIMES LOCAL:1 , 1.50
ELSEIF EXP:ARG:Ａ经验 < EXPLV:4
	TIMES LOCAL , 0.60
	TIMES LOCAL:1 , 1.20
ELSEIF EXP:ARG:Ａ经验 < EXPLV:5
	TIMES LOCAL , 0.40
ELSEIF EXP:ARG:Ａ经验 >= EXPLV:5
	TIMES LOCAL , 0.20
ENDIF

;PALAM:润滑をみる
IF PALAM:ARG:润滑 < PALAMLV:1
	TIMES LOCAL, 1.20
	TIMES LOCAL:1 , 2.00
ELSEIF PALAM:ARG:润滑 < PALAMLV:2
	TIMES LOCAL , 1.00
	TIMES LOCAL:1 , 1.50
ELSEIF PALAM:ARG:润滑 < PALAMLV:3
	TIMES LOCAL , 0.60
ELSEIF PALAM:ARG:润滑 < PALAMLV:4
	TIMES LOCAL , 0.30
ELSEIF PALAM:ARG:润滑 >= PALAMLV:4
	TIMES LOCAL , 0.20
ENDIF

SOURCE:ARG:苦痛 += LOCAL
SOURCE:ARG:反感 += LOCAL:1
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:ARG:Ａ性交经验 ++
EXP:ARG:Ａ经验 ++
RETURN 1

