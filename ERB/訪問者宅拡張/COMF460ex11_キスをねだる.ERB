	DATAFORM %"在"+NTR_NAME(0)+"温柔地亲吻下"+CALLNAME:奴隷+"意乱情迷地抱紧了"+NTR_NAME(0)+"……"%
ENDDATA
; 手抜き。そのうち修正。
CALL VISITOR_TAKEOUT_10_02(奴隷)
CALL NTR_SOURCE_CHECK(奴隷,人物_訪問者)
CALL ORGASM_ADD(奴隷,人物_訪問者)
RETURN 1
RETURN 0

