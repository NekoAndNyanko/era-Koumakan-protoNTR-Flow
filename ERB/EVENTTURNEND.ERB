RETURN 0

;-------------------------------------------------------------------------------
;体力回復
@CHK_BODY_POWER_RECOVERY(奴隷)
#DIM 奴隷
#DIM 体力回復分

体力回復分 = MAXBASE:奴隷:体力 / 3
;回復早い、回復遅い
体力回復分 = 体力回復分 * 3 / (3 - TALENT:奴隷:恢复速度)
BASE:奴隷:体力 = MIN(MAXBASE:奴隷:体力, BASE:奴隷:体力 + 体力回復分)
;CFLAG:奴隷:衰弱 = 0
RETURN 0

