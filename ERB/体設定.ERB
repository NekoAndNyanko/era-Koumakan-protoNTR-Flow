	CFLAG:子:Ｐオプション１ = CFLAG:(ARG:2):Ｐオプション１
	CFLAG:子:Ｐオプション２ = CFLAG:(ARG:2):Ｐオプション２

	CFLAG:子:子供Ｐ大きさ保管 = CFLAG:子:Ｐ大きさ
	CFLAG:子:Ｐ大きさ = -2

	;一つのフラグで設定
	CFLAG:子:目色右 = CFLAG:(ARG:2):目色右
	CFLAG:子:目色左 = CFLAG:(ARG:2):目色左

	;設定後に決まるもの
	CFLAG:子:髪の長さ = CFLAG:(ARG:2):髪の長さ
	CFLAG:子:髪の長さ指定 = CFLAG:(ARG:2):髪の長さ指定
	CFLAG:子:肌色 = CFLAG:(ARG:2):肌色
	CFLAG:子:髪色 = CFLAG:(ARG:2):髪色

