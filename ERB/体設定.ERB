@体詳細整頓(ARG)
;同じものがあるなら片方を消す
IF CFLAG:ARG:髪オプション１ != 0
	SIF CFLAG:ARG:髪オプション１ == CFLAG:ARG:髪オプション２
		CFLAG:ARG:髪オプション２ = 0
ENDIF
IF CFLAG:ARG:瞳オプション１ != 0
	SIF CFLAG:ARG:瞳オプション１ == CFLAG:ARG:瞳オプション２
		CFLAG:ARG:瞳オプション２ = 0
ENDIF
IF CFLAG:ARG:体オプション１ != 0
	SIF CFLAG:ARG:体オプション１ == CFLAG:ARG:体オプション２
		CFLAG:ARG:体オプション２ = 0
	SIF CFLAG:ARG:体オプション１ == CFLAG:ARG:体オプション３
		CFLAG:ARG:体オプション３ = 0
	SIF CFLAG:ARG:体オプション１ == CFLAG:ARG:体オプション４
		CFLAG:ARG:体オプション４ = 0
ENDIF
IF CFLAG:ARG:体オプション２ != 0
	SIF CFLAG:ARG:体オプション２ == CFLAG:ARG:体オプション３
		CFLAG:ARG:体オプション３ = 0
	SIF CFLAG:ARG:体オプション２ == CFLAG:ARG:体オプション４
		CFLAG:ARG:体オプション４ = 0
ENDIF
IF CFLAG:ARG:体オプション３ != 0
	SIF CFLAG:ARG:体オプション３ == CFLAG:ARG:体オプション４
		CFLAG:ARG:体オプション４ = 0
ENDIF

;体オプションは複数持ち禁止の番号がある場合、番号の大きいほうを消す
SIF 体詳細オプション設定(ARG, CFLAG:ARG:体オプション４, "体４") == 0
	CFLAG:ARG:体オプション４ = 0
SIF 体詳細オプション設定(ARG, CFLAG:ARG:体オプション３, "体３") == 0
	CFLAG:ARG:体オプション３ = 0
SIF 体詳細オプション設定(ARG, CFLAG:ARG:体オプション２, "体２") == 0
	CFLAG:ARG:体オプション２ = 0
SIF 体詳細オプション設定(ARG, CFLAG:ARG:Ｐオプション２, "Ｐ２") == 0
	CFLAG:ARG:Ｐオプション２ = 0

;前の番号が開いているなら詰める
IF CFLAG:ARG:髪オプション２ > 0
	IF CFLAG:ARG:髪オプション１ <= 0
		CFLAG:ARG:髪オプション１ = CFLAG:ARG:髪オプション２
		CFLAG:ARG:髪オプション２ = 0
	ENDIF
ENDIF
IF CFLAG:ARG:瞳オプション２ > 0
	IF CFLAG:ARG:瞳オプション１ <= 0
		CFLAG:ARG:瞳オプション１ = CFLAG:ARG:瞳オプション２
		CFLAG:ARG:瞳オプション２ = 0
	ENDIF
ENDIF
IF CFLAG:ARG:体オプション４ > 0
	IF CFLAG:ARG:体オプション１ <= 0
		CFLAG:ARG:体オプション１ = CFLAG:ARG:体オプション４
		CFLAG:ARG:体オプション４ = 0
	ELSEIF CFLAG:ARG:体オプション２ <= 0
		CFLAG:ARG:体オプション２ = CFLAG:ARG:体オプション４
		CFLAG:ARG:体オプション４ = 0
	ELSEIF CFLAG:ARG:体オプション３ <= 0
		CFLAG:ARG:体オプション３ = CFLAG:ARG:体オプション４
		CFLAG:ARG:体オプション４ = 0
	ENDIF
ENDIF
IF CFLAG:ARG:体オプション３ > 0
	IF CFLAG:ARG:体オプション１ <= 0
		CFLAG:ARG:体オプション１ = CFLAG:ARG:体オプション３
		CFLAG:ARG:体オプション３ = 0
	ELSEIF CFLAG:ARG:体オプション２ <= 0
		CFLAG:ARG:体オプション２ = CFLAG:ARG:体オプション３
		CFLAG:ARG:体オプション３ = 0
	ENDIF
ENDIF
IF CFLAG:ARG:体オプション２ > 0
	IF CFLAG:ARG:体オプション１ <= 0
		CFLAG:ARG:体オプション１ = CFLAG:ARG:体オプション２
		CFLAG:ARG:体オプション２ = 0
	ENDIF
ENDIF
IF CFLAG:ARG:Ｖオプション２ > 0
	IF CFLAG:ARG:Ｖオプション１ <= 0
		CFLAG:ARG:Ｖオプション１ = CFLAG:ARG:Ｖオプション２
		CFLAG:ARG:Ｖオプション２ = 0
	ENDIF
ENDIF
IF CFLAG:ARG:Ｐオプション２ > 0
	IF CFLAG:ARG:Ｐオプション１ <= 0
		CFLAG:ARG:Ｐオプション１ = CFLAG:ARG:Ｐオプション２
		CFLAG:ARG:Ｐオプション２ = 0
	ENDIF
ENDIF
RETURN

