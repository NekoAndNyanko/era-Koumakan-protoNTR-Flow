	ENDIF
	CFLAG:子:肌色 = CFLAG:子:肌原色
	CFLAG:子:髪色 = CFLAG:子:髪原色
	CALL 体詳細整頓(子)

ENDIF
RETURN


@体設定_子供Ｐ成長(子供)
#DIM 子供

;幼児
IF CFLAG:子供:成長度 == 成長度_幼児
	;遺伝上短小なら短小のままになる
	SIF CFLAG:子供:Ｐ大きさ != CFLAG:子供:子供Ｐ大きさ保管
		CFLAG:子供:Ｐ大きさ ++

	;成長期(1/4で遺伝上の最大の大きさに)
ELSEIF CFLAG:子供:成長度 == 成長度_成長期
	IF CFLAG:子供:Ｐ大きさ != CFLAG:子供:子供Ｐ大きさ保管
		IF !RAND:4
			CFLAG:子供:Ｐ大きさ = CFLAG:子供:子供Ｐ大きさ保管
		ELSE
			CFLAG:子供:Ｐ大きさ ++
		ENDIF
	ENDIF
	;大人
ELSEIF CFLAG:子供:成長度 == 成長度_成人
	CFLAG:子供:Ｐ大きさ = CFLAG:子供:子供Ｐ大きさ保管
ENDIF
RETURN

@体設定_子供髪変更(ARG)
;長さ変更
IF !RAND:4
	;息子
	IF NO:ARG == 148
		SELECTCASE RAND:100
				;だいぶ短め 20%
			CASE 0 TO 19
				CFLAG:ARG:髪の長さ指定 = 0
				;短め 45%
			CASE 20 TO 64
				CFLAG:ARG:髪の長さ指定 = RAND(1, 4)
				;ちょいと長め 20%
			CASE 65 TO 84
				CFLAG:ARG:髪の長さ指定 = RAND(4, 6)
				;長め 10%
			CASE 85 TO 94
				CFLAG:ARG:髪の長さ指定 = RAND(6, 9)
				;超長め 5%
			CASE IS >= 95
				CFLAG:ARG:髪の長さ指定 = 9
		ENDSELECT
		;娘
	ELSE
		SELECTCASE RAND:100
				;だいぶ短め 5%
			CASE 0 TO 4
				CFLAG:ARG:髪の長さ指定 = 0
				;短め 30%
			CASE 5 TO 34
				CFLAG:ARG:髪の長さ指定 = RAND(1, 4)
				;ちょいと長め 35%
			CASE 35 TO 69
				CFLAG:ARG:髪の長さ指定 = RAND(4, 6)
				;長め 20%
			CASE 70 TO 89
				CFLAG:ARG:髪の長さ指定 = RAND(6, 9)
				;超長め 10%
			CASE IS >= 90
				CFLAG:ARG:髪の長さ指定 = 9
		ENDSELECT
	ENDIF
ENDIF
;髪型変更
IF CFLAG:ARG:髪の長さ >= 200 && !RAND:10
	LOCAL = RAND:6
	;オプションが被らない
	IF LOCAL != CFLAG:ARG:髪オプション２
		;既に１がある場合、1/5の確率で２に追加
		IF CFLAG:ARG:髪オプション１ && !RAND:5
			CFLAG:ARG:髪オプション２ = LOCAL
		ELSE
			CFLAG:ARG:髪オプション１ = LOCAL
		ENDIF
	ENDIF
ELSEIF CFLAG:ARG:髪の長さ < 200
	CFLAG:ARG:髪オプション１ = 0
	CFLAG:ARG:髪オプション２ = 0
ENDIF
RETURN


