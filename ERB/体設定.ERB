;体の詳細設定
@体詳細設定１
PRINTFORML キャラ達の体の詳細設定（後で変更できません）
FOR LOCAL, 0, CHARANUM
	CALL 肌肤等颜色改变(LOCAL)
	PRINTFORML [{LOCAL,3}] - %CALLNAME:LOCAL%
NEXT
PRINTL [999] 設定完了
INPUT

IF RESULT >= 0 && RESULT < CHARANUM
	CALL 体詳細設定２(RESULT)
ELSEIF RESULT == 999
	PRINTL 这样可以吗？
	PRINTL [0] 是的
	PRINTBUTTON "[ELSE] 不要", 1
	PRINTL
	INPUT

	IF RESULT == 0
		RETURN
	ENDIF
ENDIF
RESTART

@体詳細設定２(ARG)
#DIM LCOUNT, 3  ;ループ用
CALL 肌肤等颜色改变(ARG)
PRINTFORML %CALLNAME:ARG%の体の詳細設定
PRINTL 《   髪   》--------------------------------
PRINTFORM 　　[ 0] %"色",8,LEFT% ： 
CALL 汎用色SET(CFLAG:ARG:髪原色)
PRINTFORM ■　
RESETCOLOR
PRINTFORML %色汎用(CFLAG:ARG:髪原色)%
PRINTFORML 　　[ 1] %"长度",8,LEFT% ： %髪の長さ(CFLAG:ARG:髪の長さ)%
PRINTFORML 　　[ 2] %"长度的界限",8,LEFT% ： %髪の長さ(CFLAG:ARG:髪の長さ*100)%
PRINTFORML 　　[ 3] %"特徴1",8,LEFT% ： %髪オプション(CFLAG:ARG:髪オプション１)%
PRINTFORML 　　[ 4] %"特徴2",8,LEFT% ： %髪オプション(CFLAG:ARG:髪オプション２)%
PRINTL
PRINTL 《   目   》--------------------------------
PRINTFORML 　　[ 5] %"形状",8,LEFT% ： %目つき(CFLAG:ARG:目つき)%
PRINTL 　　※特徴にオッドアイがない場合は色(右)の情報のみ表示されます
PRINTFORM 　　[ 6] %"色(右)",8,LEFT% ： 
CALL 汎用色SET(CFLAG:ARG:目色右)
PRINTFORM ■　
RESETCOLOR
PRINTFORML %色汎用(CFLAG:ARG:目色右)%
PRINTFORM 　　[ 7] %"色(左)",8,LEFT% ： 
CALL 汎用色SET(CFLAG:ARG:目色左)
