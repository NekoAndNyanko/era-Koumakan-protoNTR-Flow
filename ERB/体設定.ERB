	;基本半分ずつ取得
	;LOCAL 父親の遺伝子(残りは母親)
	;LOCAL:N 振り分ける番号
	VARSET LOCAL
	;1/15の確率で変異
	IF !RAND:15
		LOCAL = RAND(1, 16)
	ELSE
		;IF NO:子 == 148
		;	LOCAL = 6
		;ELSE
		LOCAL = 8
		;ENDIF
	ENDIF

	WHILE 20
		SLOTM = (RAND:16 + 1)
		IF !LOCAL:SLOTM
			LOCAL:SLOTM += 1
			LOCAL -= 1
			SIF LOCAL == 0
				BREAK
		ENDIF
	WEND

	;設定開始
	IF LOCAL:1
		;父親が訪問者で、訪問者の外見を設定しているならそれを
		IF CFLAG:子:父親 == 人物_訪問者 && GETBIT(FLAG:雑多設定, 3)
			CFLAG:子:髪原色 = 訪問者_髪原色
