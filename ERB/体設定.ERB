			CFLAG:子:Ｖオプション１ = 訪問者_Ｖオプション１
		ELSEIF CFLAG:子:父親 >= 900
			CFLAG:子:Ｖオプション１ = RAND:3
		ELSE
			CFLAG:子:Ｖオプション１ = CFLAG:父:Ｖオプション１
		ENDIF
	ELSE
		CFLAG:子:Ｖオプション１ = CFLAG:母:Ｖオプション１
	ENDIF
	IF LOCAL:13
		IF CFLAG:子:父親 == 人物_訪問者 && GETBIT(FLAG:雑多設定, 3)
			CFLAG:子:Ｖオプション２ = 訪問者_Ｖオプション２
		ELSEIF CFLAG:子:父親 >= 900
			CFLAG:子:Ｖオプション２ = RAND:3
		ELSE
			CFLAG:子:Ｖオプション２ = CFLAG:父:Ｖオプション２
		ENDIF
	ELSE
		CFLAG:子:Ｖオプション２ = CFLAG:母:Ｖオプション２
	ENDIF

	IF LOCAL:14
		IF CFLAG:子:父親 == 人物_訪問者 && GETBIT(FLAG:雑多設定, 3)
			CFLAG:子:Ｐ大きさ = 訪問者_Ｐ大きさ
		ELSEIF CFLAG:子:父親 >= 900
			CFLAG:子:Ｐ大きさ = RAND:5 - 2
		ELSE
			CFLAG:子:Ｐ大きさ = CFLAG:父:Ｐ大きさ
		ENDIF
	ELSE
		CFLAG:子:Ｐ大きさ = CFLAG:母:Ｐ大きさ
	ENDIF
	IF LOCAL:15
		IF CFLAG:子:父親 == 人物_訪問者 && GETBIT(FLAG:雑多設定, 3)
			CFLAG:子:Ｐオプション１ = 訪問者_Ｐオプション１
		ELSEIF CFLAG:子:父親 >= 900
			CFLAG:子:Ｐオプション１ = RAND:8
		ELSE
			CFLAG:子:Ｐオプション１ = CFLAG:父:Ｐオプション１
		ENDIF
	ELSE
		CFLAG:子:Ｐオプション１ = CFLAG:母:Ｐオプション１
	ENDIF
	IF LOCAL:16
		IF CFLAG:子:父親 == 人物_訪問者 && GETBIT(FLAG:雑多設定, 3)
			CFLAG:子:Ｐオプション２ = 訪問者_Ｐオプション２
		ELSEIF CFLAG:子:父親 >= 900
			CFLAG:子:Ｐオプション２ = RAND:8
		ELSE
			CFLAG:子:Ｐオプション２ = CFLAG:父:Ｐオプション２
		ENDIF
	ELSE
		CFLAG:子:Ｐオプション２ = CFLAG:母:Ｐオプション２
	ENDIF

	;生まれたばかりなので小さく
	CFLAG:子:子供Ｐ大きさ保管 = CFLAG:子:Ｐ大きさ
	CFLAG:子:Ｐ大きさ = -2

	;遺伝しない情報を削除
	SIF CFLAG:子:瞳オプション１ == 5
		CFLAG:子:瞳オプション１ = 0
	SIF CFLAG:子:瞳オプション２ == 5
		CFLAG:子:瞳オプション２ = 0


	;残りを設定
	IF NO:子 == 148
		CFLAG:子:髪の長さ指定 = 2
	ELSE
