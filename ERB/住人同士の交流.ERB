;-------------------------------------------------------------------------------
;Ｍ初体験を記録する
;すでに初体験が記録されている場合は無視される
@INHABITANT_MEMORIAL_M(キャラ番号,相手,内容)
#DIM キャラ番号
#DIMS 相手
#DIMS 内容

IF キャラ番号 == TCVAR:キャラ番号:行為者
	IF 相手 == ""
		相手 = %CALLNAME:(CFLAG:キャラ番号:住人同士イベント相手)+"に"%
	ENDIF
ENDIF
IF 内容 == ""
	SELECTCASE TCVAR:キャラ番号:行為者
		CASE IS < CHARANUM
			IF CFLAG:キャラ番号:住人同士イベント内容%10
				;攻め側
				内容 = %"捧げた"%
			ELSE
				LOCAL = CFLAG:キャラ番号:体を許す好感度
				TIMES LOCAL , 0.60
				IF TA:キャラ番号:(TCVAR:キャラ番号:行為者):TA_好感度 < LOCAL
					内容 = %"奪われた"%
				ELSE
					内容 = %"許した"%
				ENDIF
			ENDIF
		CASEELSE
			RETURN 0
	ENDSELECT
ENDIF
IF CSTR:キャラ番号:M处女丧失记录 == ""
	CSTR:キャラ番号:M处女丧失记录 = %INHABITANT_MEMORIAL_CREATE(キャラ番号,相手,内容)%
ENDIF
RETURN 0

;-------------------------------------------------------------------------------
;初体験の文字列を作成する
@INHABITANT_MEMORIAL_CREATE(キャラ番号,相手,内容)
#FUNCTIONS
#DIM キャラ番号
#DIMS 日時
#DIMS 場所
#DIMS 相手
#DIMS 内容

日時 = %日付_月(暦法月)+"の"+天候(天候値)+"の日に"%
場所 = %GETPLACENAME(CFLAG:キャラ番号:現在位置)+"で"%

IF キャラ番号 == TCVAR:キャラ番号:行為者
	IF 相手 == ""
		相手 = %CALLNAME:(CFLAG:キャラ番号:住人同士イベント相手)+"に"%
	ENDIF
ENDIF
IF 相手 == ""
	SELECTCASE TCVAR:キャラ番号:行為者
		CASE IS < CHARANUM
			相手 = %CALLNAME:(TCVAR:キャラ番号:行為者)+"に"%
		CASEELSE
			相手 = "誰かに"
	ENDSELECT
ENDIF

IF 内容 == ""
	SELECTCASE TCVAR:キャラ番号:行為者
		CASE IS < CHARANUM
			IF CFLAG:キャラ番号:住人同士イベント内容%10
				;攻め側
				内容 = %"捧げた"%
			ELSE
				IF TA:キャラ番号:(TCVAR:キャラ番号:行為者):TA_好感度 < CFLAG:キャラ番号:体を許す好感度
					内容 = %"奪われた"%
				ELSE
					;嫌がると表示されてからの好感度計算によりここに来る可能性あり
					内容 = %"許した"%
				ENDIF
			ENDIF
		CASEELSE
			内容 = %"奪われた"%
	ENDSELECT
ENDIF
RETURNF 日時+場所+相手+内容

;-------------------------------------------------------------------------------
;イベント内容、終了時間設定
@INHABITANT_EVENT_SET(攻め側,受け側,内容,終了時間)
#DIM 攻め側
#DIM 受け側
#DIM 内容		; -1の時は設定しない
#DIM 終了時間	; -1の時は設定しない

IF -1 != 内容
	CFLAG:攻め側:住人同士イベント内容 = 内容 * 10 + 1
	CFLAG:受け側:住人同士イベント内容 = 内容 * 10 + 0
ENDIF
IF -1 != 終了時間
	CFLAG:攻め側:住人同士イベント終了時間 = 終了時間
	CFLAG:受け側:住人同士イベント終了時間 = 終了時間
ENDIF
