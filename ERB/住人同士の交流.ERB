@INHABITANT_MEMORIAL_V(キャラ番号,相手,内容)
#DIM キャラ番号
#DIMS 相手
#DIMS 内容

IF (TEQUIP:キャラ番号:Ｖ性爱 >= 0) && (TEQUIP:キャラ番号:Ｖ性爱 != TCVAR:キャラ番号:行為者)
	SELECTCASE TEQUIP:キャラ番号:Ｖ性爱
		CASE IS < CHARANUM
