;-------------------------------------------------
;G点刺激
;爱抚系派生コマンド
;-------------------------------------------------
@SCOM3

TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_太股)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 50
DOWNBASE:气力 += 100

SOURCE:快Ｖ = 180
SOURCE:苦痛 = 20
SOURCE:液体 = 50
SOURCE:露出 = 80
SOURCE:逸脱 = 100
SOURCE:反感 = 100

CALL 締り具合変動, TARGET, 3, 0

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 100

;处女だった場合は、苦痛のソースと反発のソースを追加する
IF TALENT:处女 == 1
	SOURCE:苦痛 += 2000
	SOURCE:反感 += 3000
ENDIF

;再生处女だった場合は、苦痛のソースと反発のソースを追加する
IF TALENT:处女 == 2
	SOURCE:苦痛 += 500
	SOURCE:反感 += 1000
ENDIF

;Vへの苦痛は先に処理しておく
CALL PAIN_CHECK_V(TARGET)
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;膣内に破瓜の血の汚れを付加
SIF TALENT:处女
	STAIN:膣内 |= 汚れ_破瓜の血
;奴隷の膣内⇔調教者の指の汚れが移动
STAIN:膣内 |= STAIN:PLAYER:手
STAIN:PLAYER:手 |= STAIN:膣内

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;V経験
EXP:Ｖ经验 += 1
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 3
ENDIF

RETURN 1
