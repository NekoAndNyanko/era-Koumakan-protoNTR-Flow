;-------------------------------------------------
;交互插入
;セックス、助手系派生コマンド
;-------------------------------------------------
@SCOM13

TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_太股)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;TCVAR:0 射精対象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI）
;TCVAR:1 予備射精対象フラグ（ビットを使いたくない人用、CFLAG:100に翻訳されます）	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI）
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）
;二人目
LOCAL = MASTER_POSE(6,1,1)

LOCALS = {TARGET}/{LOCAL}/
CSTR:PLAYER:射精判定用 += LOCALS
TCVAR:MASTER:2 = 1
;強度
TCVAR:MASTER:4 = 2500

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 50
DOWNBASE:气力 += 110

SOURCE:快Ｖ = 460
SOURCE:情爱 = 210
SOURCE:苦痛 = 510
SOURCE:露出 = 80
SOURCE:反感 = 350

SOURCE:LOCAL:快Ｖ = 230
SOURCE:LOCAL:情爱 = 100
SOURCE:LOCAL:苦痛 = 510
SOURCE:LOCAL:露出 = 40
SOURCE:LOCAL:反感 = 350
;上のほうで計算した汚れデータ
SOURCE:肮脏 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) * 10 + 60
SOURCE:LOCAL:肮脏 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, LOCAL) * 10 + 60


CALL 締り具合変動, TARGET, 3, 0
CALL 締り具合変動, LOCAL, 3, 0
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
FOR LOCAL:1,0,2
	IF LOCAL:1 == 0
		LOCAL:2 = TARGET
	ELSE
		LOCAL:2 = LOCAL
	ENDIF
	;抑圧、抵抗持ちには、抑郁ソース追加
	SIF TALENT:(LOCAL:2):自己爱 < 0 || TALENT:(LOCAL:2):抵抗
		SOURCE:(LOCAL:2):抑郁 += 310

	;贞操観念持ちで恋慕、亲爱持ち以外には常に反発のソースを追加する（セックス系のみ）
	SIF TALENT:(LOCAL:2):贞操 > 0 && !TALENT:(LOCAL:2):恋慕
		SOURCE:(LOCAL:2):反感 += 1300

	;处女だった場合は、苦痛のソースと反発のソースを追加する
	IF TALENT:(LOCAL:2):处女 == 1
		SOURCE:(LOCAL:2):苦痛 += 2040
		SOURCE:(LOCAL:2):反感 += 3500
	ENDIF

	;再生处女だった場合は、苦痛のソースと反発のソースを追加する
	IF TALENT:(LOCAL:2):处女 == 2
		SOURCE:(LOCAL:2):苦痛 += 510
		SOURCE:(LOCAL:2):反感 += 1100
	ENDIF

	;Vへの苦痛は先に処理しておく
	CALL PAIN_CHECK_V(LOCAL:2)
NEXT

;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;膣内に破瓜の血の汚れを付加
SIF TALENT:处女
	STAIN:膣内 |= 汚れ_破瓜の血
;奴隷のＶ⇔調教者のＰの汚れが移动
STAIN:PLAYER:Ｐ |= STAIN:Ｖ
STAIN:PLAYER:Ｐ |= STAIN:膣内
STAIN:Ｖ |= STAIN:PLAYER:Ｐ
STAIN:膣内 |= STAIN:PLAYER:Ｐ
;助手のＶ⇔調教者のＰの汚れが移动
STAIN:PLAYER:Ｐ |= STAIN:LOCAL:Ｖ
STAIN:PLAYER:Ｐ |= STAIN:LOCAL:膣内
STAIN:LOCAL:Ｖ |= STAIN:PLAYER:Ｐ
STAIN:LOCAL:膣内 |= STAIN:PLAYER:Ｐ
;奴隷のＶ⇔調教者のＰの汚れが移动
STAIN:PLAYER:Ｐ |= STAIN:Ｖ
STAIN:PLAYER:Ｐ |= STAIN:膣内
STAIN:Ｖ |= STAIN:PLAYER:Ｐ
STAIN:膣内 |= STAIN:PLAYER:Ｐ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ性交经验 ++
EXP:LOCAL:Ｖ性交经验 ++
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 3
ENDIF
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,LOCAL)
	EXP:LOCAL:(50 + HETEROSEX(TARGET,LOCAL)) += 3
	EXP:TARGET:(50 + HETEROSEX(TARGET,LOCAL)) += 3
ENDIF
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(LOCAL,PLAYER)
	EXP:LOCAL:(50 + HETEROSEX(LOCAL,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(LOCAL,PLAYER)) += 3
ENDIF

IF !ASSIPLAY
	EXP:爱情经验 += TALENT:恋慕 * 2
	;处女
	SIF TALENT:处女 && TALENT:恋慕
		EXP:爱情经验 += 4
ENDIF
RETURN 1

@CAN_SCOM13(ARG)
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
;绳で縛られている場合は無視（绳が無い時のみ実行判定を行う）
IF TEQUIP:绳 == 0
	SKIPDISP ARG
	PRINTL 交互插入
	TFLAG:実行値 = 0
	TFLAG:実行値表示用フラグ = 0
	;すべての命令に共通の要素を考慮
	;(顺从が高いと命令に従いやすいなど)
	CALL COM_ORDER

	;ABL:欲望
	IF ABL:欲望
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += ABL:欲望 * 3
		PRINT 欲望
		PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 3,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;ABL:Ｖ感觉
	IF ABL:Ｖ感觉
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += ABL:Ｖ感觉 * 2
		PRINT Ｖ感觉
		PRINTV 'LV,ABL:Ｖ感觉,'(,ABL:Ｖ感觉 * 2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;快乐刻印
	IF MARK:快乐刻印
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += MARK:快乐刻印 * 3
		PRINT 快乐刻印
		PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 3,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;PALAM:润滑（の不足）
	IF PALAM:润滑 < PALAMLV:3
		PRINT  - 
		TFLAG:実行値 -= 5
		PRINT 润滑不足
		PRINTV '(,5,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;PALAM:欲情
	IF GETPALAMLV(PALAM:欲情,5)
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 3
		PRINT 欲情
		PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 3,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;恥じらい
	IF TALENT:羞耻心 > 0
		PRINT  - 
		TFLAG:実行値 -= 2
		PRINT 恥じらい
		PRINTV '(,2,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;快感の否定
	IF TALENT:快感应答 < 0 && !TALENT:恋慕
		PRINT  - 
		TFLAG:実行値 -= 5
		PRINT 快感の否定
		PRINTV '(,5,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;男嫌い（恋慕、亲爱で無効）
	IF TALENT:性別嗜好 & 1 && !(HAS_VAGINA(PLAYER)) && (!TALENT:恋慕 || PLAYER != MASTER)
		PRINT  - 
		TFLAG:実行値 -= 12
		PRINT 男嫌い
		PRINTV '(,12,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;恋慕
	IF TALENT:恋慕 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 12
		PRINT 恋慕
		PRINTV '(,12,')
		TFLAG:実行値表示用フラグ = 12
	ENDIF
	;亲爱
	IF TALENT:恋慕 > 1 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 2
		PRINT 亲爱
		PRINTV '(,2,')
		TFLAG:実行値表示用フラグ = 2
	ENDIF
	;淫乱
	IF TALENT:淫乱 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 3
		PRINT 淫乱
		PRINTV '(,3,')
		TFLAG:実行値表示用フラグ = 3
	ENDIF
	;娼婦
	IF TALENT:淫乱 > 1 && ASSIPLAY == 0
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 3
		PRINT 娼婦
		PRINTV '(,3,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF
	;处女
	LOCAL = MAX(10,40 - EXP:Ｖ经验)
	IF TALENT:处女
		PRINT  - 
		TFLAG:実行値 -= LOCAL
		PRINT 处女
		PRINTV '(,LOCAL,')
		TFLAG:実行値表示用フラグ = 1
		;V経験
	ELSEIF EXP:Ｖ经验 < EXPLV:2
		PRINT  - 
		TFLAG:実行値 -= 15
		PRINT Ｖ经验
		PRINT 不足
		PRINTV '(,15,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;調教者がふたなり
	IF GET_SEXNAME(PLAYER) == "ふたなり"
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 4
		PRINT ふたなり
		PRINTV '(,4,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;媚药
	IF TEQUIP:媚药
		SIF TFLAG:実行値表示用フラグ
			PRINT  + 
		TFLAG:実行値 += 6
		PRINT 媚药
		PRINTV '(,6,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET)
	;汚れあり
	IF LOCAL
		PRINT  - 
		TFLAG:実行値 -= LOCAL
		PRINT 汚れあり
		;汚臭鈍感
		IF TALENT:污臭耐性 > 0
			PRINT 、汚臭鈍感
		ELSEIF TALENT:污臭耐性 < 0
			PRINT 、汚臭敏感
		ENDIF
		PRINTV '(,LOCAL,')
		TFLAG:実行値表示用フラグ = 1
	ENDIF

	;合計を表示(25以上で実行)
	PRINT  = 
	PRINTV TFLAG:実行値

	LOCAL = 25
	SIF TFLAG:実行値 < LOCAL
		PRINT  < 
	SIF TFLAG:実行値 == LOCAL
		PRINT  = 
	SIF TFLAG:実行値 > LOCAL
		PRINT  > 
	PRINT 実行値
	PRINTV LOCAL

	WAIT

	SKIPDISP 0
	;実行できない
	SIF TFLAG:実行値 < LOCAL
		RETURN 0
ENDIF
RETURN 1
