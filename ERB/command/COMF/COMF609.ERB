;-------------------------------------------------
;手淫
;自慰系コマンド、レベル3
;-------------------------------------------------
@COM609

TRYCALL COM417_is_Protected(PLAYER,TARGET,淫纹_太股)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:射精判定用 += LOCALS
TCVAR:PLAYER:2 = 3
;強度
TCVAR:PLAYER:4 = 500
;侍奉快楽
TFLAG:30 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 10
DOWNBASE:气力 += 100

SOURCE:情爱 = 50
SOURCE:性行动 = 250
SOURCE:达成 = 50
SOURCE:屈从 = 350
SOURCE:逸脱 = 100
SOURCE:反感 = 100

;上のほうで計算した汚れデータ
SOURCE:肮脏 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) / 3 * 10 + 60


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 200
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;奴隷の指⇔調教者のＰの汚れが移动
STAIN:手 |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:手

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;手淫经验
EXP:手淫经验 ++

;自慰经验
EXP:PLAYER:自慰经验 ++

RETURN 1
