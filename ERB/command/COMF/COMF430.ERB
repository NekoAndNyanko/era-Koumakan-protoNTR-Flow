	PRINTFORML %"[   ] - 配件     ："+CLOTHNAME(GETNUM(EQUIP,"配件")    ,EQUIP:奴隷:配件)+"(変更不可)"%
	PRINTFORML %"[  0] - 帽子           ："+CLOTHNAME(GETNUM(EQUIP,"帽子")          ,EQUIP:奴隷:帽子)%
	PRINTFORML %"[  1] - 鞋子             ："+CLOTHNAME(GETNUM(EQUIP,"鞋子")            ,EQUIP:奴隷:鞋子  )%
	PRINTFORML %"[  2] - 脚             ："+CLOTHNAME(GETNUM(EQUIP,"袜子")          ,EQUIP:奴隷:袜子)%
