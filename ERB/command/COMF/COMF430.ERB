	PRINTFORML %"[  8] - 全身上着       ："+CLOTHNAME(GETNUM(EQUIP,"赛车服"),EQUIP:奴隷:赛车服)+CLOTHNAME(GETNUM(EQUIP,"连衣裙")  ,EQUIP:奴隷:连衣裙)+CLOTHNAME(GETNUM(EQUIP,"着物"),EQUIP:奴隷:着物)+CLOTHNAME(GETNUM(EQUIP,"紧身衣"),EQUIP:奴隷:紧身衣)%
	PRINTFORML %"[  9] - 预设     "%
	PRINTFORML %"[ 10] - cosplay       "%
	PRINTFORML %"[ 11] - 全部脱掉     "%
	PRINTFORML %"[100] - 这样就好了     "%
	INPUT
	SELECTCASE RESULT
		CASE 0
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Head(奴隷)
		CASE 1
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Foot(奴隷)
		CASE 2
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Leg(奴隷)
		CASE 3
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Hand(奴隷)
		CASE 4
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Knickers(奴隷)
		CASE 5
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Botoms(奴隷)
		CASE 6
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Bra(奴隷)
		CASE 7
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Tops(奴隷)
		CASE 8
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Suit(奴隷)
		CASE 9
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Preset(奴隷)
		CASE 10
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_CHOOSE_Cosplay(奴隷)
		CASE 11
			CLEARLINE (LINECOUNT - 描画行数)
			CALL CLOTHES_Preset_NUDE(奴隷)
		CASE 100
			RETURN 0
	ENDSELECT
	;EQUIPからCFLAGへセーブ
	CALL CLOTHES_Save(奴隷)
LOOP 1
RETURN 0

