;-------------------------------------------------
;换衣服
;-------------------------------------------------
@COM430
#DIM  着替え対象
#DIMS 変更前下着名
LOCAL:1 = 0
;お着替え対象が何人いるか
FOR 着替え対象,1,CHARANUM
	IF CFLAG:着替え対象:同室 && TALENT:着替え対象:恋慕
		LOCAL:1 ++
	ENDIF
NEXT
PRINTFORML %"服を换衣服事ができます"%
;対象があなた以外にもいる場合選択させる
IF LOCAL:1 >= 1
	PRINTFORML %"誰のお着替えをしますか?"%
	;着替え対象を探す
	PRINTFORML %"[0] - "+CALLNAME:(MASTER)%
	FOR 着替え対象,1,CHARANUM
		IF CFLAG:着替え対象:同室 && TALENT:着替え対象:恋慕
			PRINTFORML %"["+TOSTR(着替え対象)+"] - "+CALLNAME:着替え対象%
		ENDIF
	NEXT
	PRINTFORML %"[999] - 返回"%
	;入力ループ
	DO
		INPUT
		着替え対象 = RESULT
		SELECTCASE 着替え対象
			CASE MASTER
				CALL CLOTHES_CHOOSE(MASTER)
				;女やふたなりでない時服装によって[女装]を与える
				IF HAS_VAGINA(MASTER) == 0 && ( EQUIP:MASTER:裙子 && ( EQUIP:MASTER:上半身上着１ || EQUIP:MASTER:上半身上着２ ) || EQUIP:MASTER:连衣裙 && !EQUIP:MASTER:下半身上着１ )
					TALENT:MASTER:女装 = 1
					;[女装]を持っていて裙子、连衣裙、上半身上着を着ていない、または连衣裙を着ているが下半身上着を履いている(チャイナ服+ズボン)とき[女装]を消す
				ELSEIF TALENT:MASTER:女装 && ( ( !EQUIP:MASTER:裙子 || !EQUIP:MASTER:连衣裙 || !EQUIP:MASTER:上半身上着１ || !EQUIP:MASTER:上半身上着２ ) || ( EQUIP:MASTER:连衣裙 && EQUIP:MASTER:下半身上着１ ) )
					TALENT:MASTER:女装 = 0
				ENDIF
				RETURN 1
			CASE 0 TO (CHARANUM-1)
				;有効な範囲内の入力
				IF CFLAG:着替え対象:同室 && TALENT:着替え対象:恋慕
					変更前下着名 = 下着名１_２(着替え対象)
					CALL CLOTHES_CHOOSE(着替え対象)
					SIF 変更前下着名 != 下着名１_２(着替え対象)
						CFLAG:着替え対象:ぱんつ確認 = 3
					RETURN 1
				ENDIF
			CASE 999
				RETURN 0
			CASEELSE
		ENDSELECT
		CLEARLINE 1
	LOOP 1
ELSE
	;誰もいないならそのままあなたの服装変え
	CALL CLOTHES_CHOOSE(MASTER)
	;女やふたなりでない時服装によって[女装]を与える
	IF HAS_VAGINA(MASTER) == 0 && ( EQUIP:MASTER:裙子 && ( EQUIP:MASTER:上半身上着１ || EQUIP:MASTER:上半身上着２ ) || EQUIP:MASTER:连衣裙 && !EQUIP:MASTER:下半身上着１ )
		TALENT:MASTER:女装 = 1
		;[女装]を持っていて裙子、连衣裙、上半身上着を着ていない、または连衣裙を着ているが下半身上着を履いている(チャイナ服+ズボン)とき[女装]を消す
	ELSEIF TALENT:MASTER:女装 && ( ( !EQUIP:MASTER:裙子 || !EQUIP:MASTER:连衣裙 || !EQUIP:MASTER:上半身上着１ || !EQUIP:MASTER:上半身上着２ ) || ( EQUIP:MASTER:连衣裙 && EQUIP:MASTER:下半身上着１ ) )
		TALENT:MASTER:女装 = 0
	ENDIF
ENDIF
RETURN 0

