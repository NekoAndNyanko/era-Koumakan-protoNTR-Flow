;-------------------------------------------------
;足交
;助手によるコマンド、及びレズプレイ、レベル1
;-------------------------------------------------
@COM123
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）

LOCALS = {PLAYER}/
CSTR:TARGET:射精判定用 += LOCALS
TCVAR:TARGET:2 = 7
;強度
TCVAR:TARGET:4 = 1000
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 30
DOWNBASE:气力 += 150

SOURCE:快Ｃ = 250
SOURCE:达成 = 200
SOURCE:露出 = 300
SOURCE:屈从 = 300
SOURCE:逸脱 = 350
SOURCE:反感 = 350


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 400

;苦痛を伴わない抖Ｍ之气及び受虐狂の処理は先にしておく
;抖Ｍ之气を見る
SELECTCASE ABL:抖Ｍ之气
	CASE 0
		TIMES SOURCE:达成 , 0.80
		TIMES SOURCE:屈从 , 0.80
		SOURCE:液体 += 0
		SOURCE:欲情 += 0
	CASE 1
		TIMES SOURCE:达成 , 1.00
		TIMES SOURCE:屈从 , 1.00
		SOURCE:液体 += 20
		SOURCE:欲情 += 30
	CASE 2
		TIMES SOURCE:达成 , 1.20
		TIMES SOURCE:屈从 , 1.20
		SOURCE:液体 += 40
		SOURCE:欲情 += 70
	CASE 3
		TIMES SOURCE:达成 , 1.40
		TIMES SOURCE:屈从 , 1.40
		SOURCE:液体 += 70
		SOURCE:欲情 += 120
	CASE 4
		TIMES SOURCE:达成 , 1.50
		TIMES SOURCE:屈从 , 1.50
		SOURCE:液体 += 110
		SOURCE:欲情 += 180
	CASE 5
		TIMES SOURCE:达成 , 1.70
		TIMES SOURCE:屈从 , 1.70
		SOURCE:液体 += 150
		SOURCE:欲情 += 250
	CASE 6
		TIMES SOURCE:达成 , 1.90
		TIMES SOURCE:屈从 , 1.90
		SOURCE:液体 += 200
		SOURCE:欲情 += 300
	CASE 7
		TIMES SOURCE:达成 , 2.10
		TIMES SOURCE:屈从 , 2.10
		SOURCE:液体 += 250
		SOURCE:欲情 += 350
	CASE 8
		TIMES SOURCE:达成 , 2.30
		TIMES SOURCE:屈从 , 2.30
		SOURCE:液体 += 300
		SOURCE:欲情 += 400
	CASE 9
		TIMES SOURCE:达成 , 2.50
		TIMES SOURCE:屈从 , 2.50
		SOURCE:液体 += 350
		SOURCE:欲情 += 450
	CASEELSE
		TIMES SOURCE:达成 , 3.00
		TIMES SOURCE:屈从 , 3.00
		SOURCE:液体 += 500
		SOURCE:欲情 += 500
ENDSELECT

;受虐狂を見る
IF TALENT:受虐狂
	TIMES SOURCE:达成 , 2.50
	TIMES SOURCE:屈从 , 2.50
	TIMES SOURCE:液体 , 1.20
	TIMES SOURCE:欲情 , 1.20
ENDIF

;倒错的
SIF TALENT:倒错的
	TIMES SOURCE:达成 , 1.80

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
ENDIF

RETURN 1

