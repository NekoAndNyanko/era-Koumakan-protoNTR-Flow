;-------------------------------------------------
;助手を犯させる
;助手によるコマンド、及びレズプレイ、レベル4
;-------------------------------------------------
@COM122
PRINTL 助手を犯させる
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(顺从が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 2
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:侍奉精神
IF ABL:侍奉精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:侍奉精神 * 4
	PRINT 侍奉精神
	PRINTV 'LV,ABL:侍奉精神,'(,ABL:侍奉精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快乐刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快乐刻印 * 2
	PRINT 快乐刻印
	PRINTV 'LV,MARK:快乐刻印,'(,MARK:快乐刻印 * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 2
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞耻心 > 0
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 恥じらい
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 献身的
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感に素直
IF TALENT:快感应答 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 2
	PRINT 快感に素直
	PRINTV '(,2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感应答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 2
	PRINT 快感の否定
	PRINTV '(,2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 淫乱
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;娼婦
IF TALENT:淫乱 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 娼婦
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF

;助手が处女
IF TALENT:PLAYER:处女
	PRINT  - 
	TFLAG:実行値 -= 15
	PRINT 助手が处女
	PRINTV '(,15,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;合計を表示(40以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 40
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT

;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0

;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = 助手を犯させる
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = TARGET
;TCVAR:0 射精対象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI）
;TCVAR:1 予備射精対象フラグ（ビットを使いたくない人用、CFLAG:100に翻訳されます）	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI）
;TCVAR:2 射精箇所フラグ（-1=避孕套 1=膣内 2=菊花 3=手淫 4=口淫 5=乳交 6=素股 7=足交 8=体表 9=舔肛侍奉 10=道具 11=触手）

SETBIT TCVAR:TARGET:0, 3
TCVAR:TARGET:2 = 1
;強度
TCVAR:TARGET:4 = 2000

TEQUIP:ASSI:Ｖ性爱 = TARGET
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、气力
DOWNBASE:体力 += 50
DOWNBASE:气力 += 150

SOURCE:快Ｃ = 400
SOURCE:性行动 = 500
SOURCE:达成 = 400
SOURCE:屈从 = 700
SOURCE:逸脱 = 500
SOURCE:反感 = 500


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑郁ソース追加
SIF TALENT:自己爱 < 0 || TALENT:抵抗
	SOURCE:抑郁 += 500

;助手が处女の場合、屈服逸脱が倍化し、顺从が2になる。ただし一度きり
IF TALENT:PLAYER:处女 == 1
	TIMES SOURCE:屈从 , 5.00
	TIMES SOURCE:逸脱 , 3.00

	CALL LOST_VIRGIN(PLAYER, TARGET)
	IF !GETBIT(CFLAG:3,4)
		;异常经验追加（一回きり）
		EXP:异常经验 += 1
		SETBIT CFLAG:异常经验,4
	ENDIF
	IF ABL:顺从 < 2
		PRINTFORML そして%ABLNAME:0%が2になった
		ABL:顺从 = 2
	ENDIF
ENDIF
EXP:ASSI:Ｖ经验 += 1
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
IF HAS_PENIS(TARGET)
	;奴隷のＰ⇔調教者のＶの汚れが移动
	STAIN:Ｐ |= STAIN:PLAYER:Ｖ
	STAIN:PLAYER:Ｖ |= STAIN:Ｐ
ENDIF

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
SIF HETEROSEX(TARGET,ASSI)
	EXP:(50 + HETEROSEX(TARGET,ASSI)) += 10

RETURN 1

