;;-------------------------------------------------------------------------------
@WC_SexHara_SOURCE(ARG)
;;ARG=セクハラ内容
#DIM セクハラ内容
#DIM セクハラ成立
#DIM 言い訳対象
#DIM 堕ち時間
#DIM 経過時間
#DIM 興奮要素
#DIM 客人数
#DIM テク合計
	セクハラ内容 = ARG
	セクハラ成立 = 0
	言い訳対象 = 0
	堕ち時間 = 0
	経過時間 = 0
	興奮要素 = 0
	客人数 = 0
	テク合計 = 0

	SELECTCASE セクハラ内容
	CASE 11		;;触られる
		SOURCE:MASTER:恐怖 += 50
		SOURCE:MASTER:屈従 += 100
	CASE 12		;;ぱんつ見られ
		SOURCE:MASTER:露出 += 200
		SOURCE:MASTER:屈従 += 100
	CASE 13		;;飲尿
		;;対象設定
		IF CFLAG:MASTER:WC_管理人は誰 != 0
			言い訳対象 = CFLAG:MASTER:WC_管理人は誰
		ELSE
			言い訳対象 = 人物_レミリア
		ENDIF
		;;可否判定
		IF !GET_TARGETNUM()
			IF CFLAG:MASTER:WC_訪問者受け入れ具合 > 1
				セクハラ成立 = 1
			ELSEIF CFLAG:言い訳対象:WC_訪問者飲尿許可 == 2
				セクハラ成立 = 1
			ELSEIF CFLAG:MASTER:屈服度 > 200
				セクハラ成立 = 1
			ELSE
				SELECTCASE CFLAG:MASTER:現在位置
					CASE 場所_屋外トイレ,場所_1Fトイレ,場所_2Fトイレ
						セクハラ成立 = 1
				ENDSELECT
			ENDIF
		ELSE
			IF CFLAG:言い訳対象:現在位置 == CFLAG:MASTER:現在位置
				;;サド系がまるでないと許可しない
				IF TALENT:言い訳対象:サド || ABL:言い訳対象:サドっ気
					;;恋慕の時は、屈服度が好感度の２倍を超えてないと許可しない
					IF TALENT:言い訳対象:恋慕 && CFLAG:言い訳対象:屈服度 > CFLAG:言い訳対象:好感度 * 2
						セクハラ成立 = 1
						CFLAG:言い訳対象:WC_訪問者飲尿許可 = 4
					;;管理人の時は、屈服度が好感度を超えてないと許可しない
					ELSEIF TALENT:言い訳対象:管理人 && CFLAG:言い訳対象:屈服度 > CFLAG:言い訳対象:好感度
						セクハラ成立 = 1
						CFLAG:言い訳対象:WC_訪問者飲尿許可 = 4
					;;それ以外（言い訳対象がレミリア）の時は、許可されちゃう
					ELSEIF 言い訳対象 == 人物_レミリア
						セクハラ成立 = 1
						CFLAG:言い訳対象:WC_訪問者飲尿許可 = 4
					ENDIF
				ENDIF
			ELSEIF WC_SexHara_MESSAGE_intercept_CHECK(13) == 0
				IF CFLAG:MASTER:WC_訪問者受け入れ具合 > 1
					セクハラ成立 = 1
				ELSEIF CFLAG:言い訳対象:WC_訪問者飲尿許可 == 2
					セクハラ成立 = 1
				ELSEIF CFLAG:MASTER:屈服度 > 200
					セクハラ成立 = 1
				ENDIF
			ENDIF
		ENDIF
		;;数値設定
		IF !セクハラ成立
			SOURCE:MASTER:屈従 += 100
		ELSE
			SOURCE:MASTER:達成 += 100
			SOURCE:MASTER:屈従 += 300
			IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
				SELECTCASE TALENT:MASTER:汚臭耐性
					CASE -2		;;潔癖症
						SOURCE:MASTER:不潔 += 50
					CASE -1		;;汚臭敏感
						SOURCE:MASTER:不潔 += 40
					CASE 0		;;普通
						SOURCE:MASTER:不潔 += 20
					CASE 1		;;汚臭鈍感
						SOURCE:MASTER:不潔 += 10
				ENDSELECT
			ENDIF
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 14		;;小便シャワー
		SOURCE:MASTER:恐怖 += 100
		SOURCE:MASTER:屈従 += 400
		IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
			SELECTCASE TALENT:MASTER:汚臭耐性
				CASE -2		;;潔癖症
					SOURCE:MASTER:不潔 += 50
				CASE -1		;;汚臭敏感
					SOURCE:MASTER:不潔 += 40
				CASE 0		;;普通
					SOURCE:MASTER:不潔 += 20
				CASE 1		;;汚臭鈍感
					SOURCE:MASTER:不潔 += 10
			ENDSELECT
		ENDIF
		TIME += 10	;;時間加算
	CASE 15		;;キス
		SOURCE:MASTER:恭順 += 150
		SOURCE:MASTER:屈従 += 250
		SELECTCASE CFLAG:MASTER:WC_訪問者受け入れ具合
			CASE 0,1,2	;;飲尿・口奉仕　以下
				SOURCE:MASTER:反感 += 100
			CASE IS >= 5	;;受け入れた　以上
				SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
		ENDSELECT
		IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SELECTCASE TALENT:MASTER:汚臭耐性
				CASE -2		;;潔癖症
					SOURCE:MASTER:不潔 += 30
				CASE -1		;;汚臭敏感
					SOURCE:MASTER:不潔 += 15
			ENDSELECT
		ENDIF
	CASE 20		;;ぱんつ脱がす
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:露出 += 50
			SOURCE:MASTER:屈従 += 100
		ELSE
			SOURCE:MASTER:露出 += 200
			SOURCE:MASTER:屈従 += 200
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 21		;;Ｂ愛撫
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:快Ｂ += 100
			SOURCE:MASTER:欲情 += 100
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 50
		ELSE
			SIF CFLAG:MASTER:WC_ニプルキャップ装着
				SOURCE:MASTER:快Ｂ += 100
			SOURCE:MASTER:快Ｂ += 200
			SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:欲情 += 200
			SOURCE:MASTER:恭順 += 150
			SOURCE:MASTER:屈従 += 100
			SOURCE:MASTER:露出 += 100
			IF !HAS_VAGINA(MASTER)
				SOURCE:MASTER:苦痛 += 50
			ENDIF
			経過時間 = 5 + RAND:5
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 22		;;Ｃ愛撫
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:快Ｃ += 100
			SOURCE:MASTER:欲情 += 150
			SOURCE:MASTER:恭順 += 50
			SOURCE:MASTER:屈従 += 50
		ELSE
			SIF CFLAG:MASTER:WC_クリキャップ装着 || CFLAG:MASTER:WC_ローターＣ装着
				SOURCE:MASTER:快Ｃ += 100
			SOURCE:MASTER:快Ｃ += 200
			SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:欲情 += 250
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 100
			SOURCE:MASTER:露出 += 100
			SOURCE:MASTER:液体 += 10
			経過時間 = 5 + RAND:5
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 23		;;Ｖ愛撫
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:快Ｖ += 100
			SOURCE:MASTER:欲情 += 150
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 50
		ELSE
			SIF CFLAG:MASTER:WC_バイブ装着 || CFLAG:MASTER:WC_ローターＶ装着
				SOURCE:MASTER:快Ｖ += 100
			SOURCE:MASTER:快Ｖ += 200
			SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:欲情 += 250
			SOURCE:MASTER:恭順 += 150
			SOURCE:MASTER:屈従 += 100
			SOURCE:MASTER:露出 += 100
			SOURCE:MASTER:液体 += 20
			CALL 締り具合変動, MASTER, 1, 0
			SIF TALENT:MASTER:浮気な蜜壷
				CFLAG:MASTER:WC_訪問者セクハラ屈服上昇値 += WC_SURRENDER(MASTER,ARG:1)
			経過時間 = 5 + RAND:5
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 24		;;Ａ愛撫
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:快Ａ += 100
			SOURCE:MASTER:欲情 += 150
			SOURCE:MASTER:恭順 += 50
			SOURCE:MASTER:屈従 += 100
		ELSE
			SIF CFLAG:MASTER:WC_アナルバイブ装着 || CFLAG:MASTER:WC_ローターＡ装着
				SOURCE:MASTER:快Ａ += 100
			SOURCE:MASTER:快Ａ += 200
			SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:欲情 += 250
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 150
			SOURCE:MASTER:露出 += 100
			SOURCE:MASTER:液体 += 10
			CALL 締り具合変動, MASTER, 1, 1
			SIF TALENT:MASTER:浮気な尻穴
				CFLAG:MASTER:WC_訪問者セクハラ屈服上昇値 += WC_SURRENDER(MASTER,ARG:1)
			経過時間 = 5 + RAND:5
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 25		;;手扱き
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 100
		ELSE
			SOURCE:MASTER:性行動 += 250
			SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:達成 += 200
			SOURCE:MASTER:欲情 += 200
			SOURCE:MASTER:恭順 += 200
			SOURCE:MASTER:屈従 += 300
			IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
				SELECTCASE TALENT:MASTER:汚臭耐性
					CASE -2		;;潔癖症
						SOURCE:MASTER:不潔 += 40
					CASE -1		;;汚臭敏感
						SOURCE:MASTER:不潔 += 10
				ENDSELECT
			ENDIF
			経過時間 = 10 + RAND:( MAX( (10 - ABL:MASTER:指), 1) )
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 26		;;口奉仕
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 100
		ELSE
			SOURCE:MASTER:性行動 += 300
			SOURCE:MASTER:情愛 += 100 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:達成 += 200
			SOURCE:MASTER:欲情 += 200
			SOURCE:MASTER:恭順 += 250
			SOURCE:MASTER:屈従 += 400
			IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
				SELECTCASE TALENT:MASTER:汚臭耐性
					CASE -2		;;潔癖症
						SOURCE:MASTER:不潔 += 50
					CASE -1		;;汚臭敏感
						SOURCE:MASTER:不潔 += 40
					CASE 1		;;汚臭鈍感
						SOURCE:MASTER:不潔 += 10
					CASE 2		;;汚れ無視
						SOURCE:MASTER:不潔 += 0
					CASEELSE
						SOURCE:MASTER:不潔 += 20
				ENDSELECT
			ENDIF
			経過時間 = 5 + RAND:( MAX( (10 - ABL:MASTER:舌) , 1) )
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 27		;;パイズリ
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 100
		ELSE
			SOURCE:MASTER:快Ｂ += 150
			SOURCE:MASTER:性行動 += 400
			SOURCE:MASTER:情愛 += 100 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:達成 += 150
			SOURCE:MASTER:欲情 += 200
			SOURCE:MASTER:恭順 += 200
			SOURCE:MASTER:屈従 += 400
			IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
				SELECTCASE TALENT:MASTER:汚臭耐性
					CASE -2		;;潔癖症
						SOURCE:MASTER:不潔 += 40
					CASE -1		;;汚臭敏感
						SOURCE:MASTER:不潔 += 30
					CASE 1		;;汚臭鈍感
						SOURCE:MASTER:不潔 += 10
					CASE 2		;;汚れ無視
						SOURCE:MASTER:不潔 += 0
					CASEELSE
						SOURCE:MASTER:不潔 += 20
				ENDSELECT
			ENDIF
			経過時間 = 10 - TALENT:MASTER:バストサイズ + RAND:( MAX( (10 - ABL:MASTER:胸) , 1) )
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 28		;;素股
		IF WC_SexHara_MESSAGE_intercept_CHECK(20) > 0
			SOURCE:MASTER:恭順 += 100
			SOURCE:MASTER:屈従 += 100
		ELSE
			SOURCE:MASTER:快Ｃ += 150
			SOURCE:MASTER:性行動 += 300
			SOURCE:MASTER:情愛 += 50 + (10 * CFLAG:MASTER:WC_訪問者受け入れ具合)
			SOURCE:MASTER:達成 += 150
			SOURCE:MASTER:欲情 += 250
			SOURCE:MASTER:恭順 += 250
			SOURCE:MASTER:屈従 += 500
			IF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
				SELECTCASE TALENT:MASTER:汚臭耐性
					CASE -2		;;潔癖症
						SOURCE:MASTER:不潔 += 50
					CASE -1		;;汚臭敏感
						SOURCE:MASTER:不潔 += 40
					CASE 1		;;汚臭鈍感
						SOURCE:MASTER:不潔 += 10
					CASE 2		;;汚れ無視
						SOURCE:MASTER:不潔 += 0
					CASEELSE
						SOURCE:MASTER:不潔 += 20
				ENDSELECT
			ENDIF
			経過時間 = 10 - GETPALAMLV(PALAM:MASTER:潤滑,5) + RAND:( MAX( (10 - ABL:MASTER:腰), 1) )
			TIME += 経過時間
		ENDIF
		SIF CFLAG:MASTER:WC_訪問者受け入れ具合 < 5
			SOURCE:MASTER:反感 += 100
	CASE 29		;;屈服レイプ
		IF GET_TARGETNUM() > 0
			SOURCE:MASTER:屈従 += 100
		ELSEIF FLAG:訪問者の現在位置 != CFLAG:MASTER:WC_屈服レイプ継続中 && CFLAG:MASTER:WC_屈服レイプ継続中 > 0 && FLAG:訪問者の現在位置 != CFLAG:MASTER:現在位置
			SOURCE:MASTER:屈従 += 100
		ELSE
			SELECTCASE CFLAG:MASTER:WC_屈服レイプステップ
			CASE 0,9
				SELECTCASE WC_sensPOINT(MASTER,人物_訪問者)
					CASE 0	;;Ｃ
						堕ち時間 = 5 - ABL:MASTER:Ｃ感覚
					CASE 1	;;Ｖ
						堕ち時間 = 5 - ABL:MASTER:Ｖ感覚
					CASE 2	;;Ａ
						堕ち時間 = 5 - ABL:MASTER:Ａ感覚
					CASE 3	;;Ｂ
						堕ち時間 = 5 - ABL:MASTER:Ｂ感覚
				ENDSELECT
				SIF 堕ち時間 < 1
					堕ち時間 = 1
				SELECTCASE WC_sensPOINT(MASTER,人物_訪問者)
					CASE 0	;;Ｃ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:快Ｃ,5) + TALENT:MASTER:Ｃ感度 - GETPALAMLV(PALAM:MASTER:欲情,5)
					CASE 1	;;Ｖ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:快Ｖ,5) + TALENT:MASTER:Ｖ感度 - GETPALAMLV(PALAM:MASTER:欲情,5)
					CASE 2	;;Ａ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:快Ａ,5) + TALENT:MASTER:Ａ感度 - GETPALAMLV(PALAM:MASTER:欲情,5)
					CASE 3	;;Ｂ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:快Ｂ,5) + TALENT:MASTER:Ｂ感度 - GETPALAMLV(PALAM:MASTER:欲情,5)
				ENDSELECT
				SIF 経過時間 < 2
					経過時間 = 2
				経過時間 += 堕ち時間
				SELECTCASE WC_sensPOINT(MASTER,人物_訪問者)
					CASE 0	;;Ｃ
						SOURCE:MASTER:快Ｃ += 1000 + (2 * 経過時間)
					CASE 1	;;Ｖ
						SOURCE:MASTER:快Ｖ += 1000 + (2 * 経過時間)
						CALL 締り具合変動, TARGET, 1, 0
					CASE 2	;;Ａ
						SOURCE:MASTER:快Ａ += 1000 + (2 * 経過時間)
						CALL 締り具合変動, TARGET, 1, 1
					CASE 3	;;Ｂ
						SOURCE:MASTER:快Ｂ += 1000 + (2 * 経過時間)
				ENDSELECT
				SOURCE:MASTER:恐怖 += 100 - (5 * 堕ち時間)
				SOURCE:MASTER:欲情 += 300 + (2 * 経過時間)
				SOURCE:MASTER:露出 += 100
				SOURCE:MASTER:恭順 += 300 + (2 * 経過時間)
				SOURCE:MASTER:屈従 += 500 + (2 * 経過時間)
				TIME += 経過時間
				DOWNBASE:MASTER:体力 += 経過時間 + 50
				DOWNBASE:MASTER:気力 += 経過時間 + 80
			CASE 1
				SELECTCASE WC_dullPOINT(MASTER,人物_訪問者)
					CASE 0	;;Ｃ
						堕ち時間 = 5 - TALENT:MASTER:Ｃ感度
					CASE 1	;;Ｖ
						堕ち時間 = 5 - TALENT:MASTER:Ｖ感度
					CASE 2	;;Ａ
						堕ち時間 = 5 - TALENT:MASTER:Ａ感度
					CASE 3	;;Ｂ
						堕ち時間 = 5 - TALENT:MASTER:Ｂ感度
				ENDSELECT
				SIF 堕ち時間 < 1
					堕ち時間 = 1
				SELECTCASE WC_dullPOINT(MASTER,人物_訪問者)
					CASE 0	;;Ｃ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:欲情,5) - GETPALAMLV(PALAM:MASTER:快Ｃ,5) - TALENT:MASTER:Ｃ感度
					CASE 1	;;Ｖ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:欲情,5) - GETPALAMLV(PALAM:MASTER:快Ｖ,5) - TALENT:MASTER:Ｖ感度
					CASE 2	;;Ａ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:欲情,5) - GETPALAMLV(PALAM:MASTER:快Ａ,5) - TALENT:MASTER:Ａ感度
					CASE 3	;;Ｂ
						経過時間 = 15 + GETPALAMLV(PALAM:MASTER:欲情,5) - GETPALAMLV(PALAM:MASTER:快Ｂ,5) - TALENT:MASTER:Ｂ感度
				ENDSELECT
				SIF 経過時間 < 2
					経過時間 = 2
				経過時間 += 堕ち時間
				SELECTCASE WC_dullPOINT(MASTER,人物_訪問者)
					CASE 0	;;Ｃ
						SOURCE:MASTER:快Ｃ += 1000 + (2 * 経過時間)
					CASE 1	;;Ｖ
						SOURCE:MASTER:快Ｖ += 1000 + (2 * 経過時間)
						CALL 締り具合変動, TARGET, 1, 0
					CASE 2	;;Ａ
						SOURCE:MASTER:快Ａ += 1000 + (2 * 経過時間)
						CALL 締り具合変動, TARGET, 1, 1
					CASE 3	;;Ｂ
						SOURCE:MASTER:快Ｂ += 1000 + (2 * 経過時間)
				ENDSELECT
				SOURCE:MASTER:恐怖 += 150 - (5 * 堕ち時間)
				SOURCE:MASTER:欲情 += 200 + (2 * 経過時間)
				SOURCE:MASTER:露出 += 100
				SOURCE:MASTER:恭順 += 200 + (2 * 経過時間)
				SOURCE:MASTER:屈従 += 700 + (2 * 経過時間)
				TIME += 経過時間
				DOWNBASE:MASTER:体力 += 経過時間 + 50
				DOWNBASE:MASTER:気力 += 経過時間 + 80
			CASE 2
				SELECTCASE BASE:MASTER:Ａ緩さ
					CASE IS >= 750	;;ぽっかり
						堕ち時間 = 5 - ABL:MASTER:Ａ感覚
					CASE IS >= 450	;;がばがば
						堕ち時間 = 6 - ABL:MASTER:Ａ感覚
					CASE IS >= 250	;;ゆるゆる
						堕ち時間 = 7 - ABL:MASTER:Ａ感覚
					CASE IS >= 100	;;きゅっきゅっ
						堕ち時間 = 8 - ABL:MASTER:Ａ感覚
					CASEELSE		;;ぎちぎち
						堕ち時間 = 9 - ABL:MASTER:Ａ感覚
				ENDSELECT
				SIF 堕ち時間 < 1
					堕ち時間 = 1
				経過時間 = 15 + GETPALAMLV(PALAM:MASTER:欲情,5) + GETPALAMLV(PALAM:MASTER:快Ａ,5) + TALENT:MASTER:Ａ感度
