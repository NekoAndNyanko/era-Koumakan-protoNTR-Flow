@EXP_DEMAND_10(パターン,経験番号,キャラ番号)
	#FUNCTION
	#DIM パターン
	#DIM 経験番号
	#DIM キャラ番号
	#DIM 異常経験 = GETNUM(EXP,"異常経験")
	VARSET LOCAL
	;ＬＶ４から５に上げるときは異常経験必要（素質：[臆病][素直]なら無視できる）
	IF ABL:キャラ番号:従順 == 4 && TALENT:キャラ番号:度胸 < 0 && TALENT:キャラ番号:態度 < 0
		LOCAL:異常経験 = 1
		;ＬＶ７から８に上げるときは異常経験3以上必要（素質：[臆病][素直]なら無視できる）
	ELSEIF ABL:キャラ番号:従順 == 7 && TALENT:キャラ番号:度胸 < 0 && TALENT:キャラ番号:態度 < 0
		LOCAL:異常経験 = 3
	ENDIF
RETURNF LOCAL:(経験番号)

;-------------------------------------------------------------------------------
;欲望の上昇にJUELが必要ならその値を返す
@JUEL_DEMAND_11(パターン,珠番号,キャラ番号)
	#FUNCTION
	#DIM パターン
	#DIM 珠番号
	#DIM キャラ番号
	#DIM 欲情 = GETNUM(JUEL,"欲情")
	#DIM 優越 = GETNUM(JUEL,"優越")
	SIF パターン > 1
		RETURNF -1
	;欲情の珠で上げる
	VARSET LOCAL
	IF パターン == 0
		SELECTCASE ABL:キャラ番号:欲情
			CASE 0
				LOCAL:欲情 = 1
			CASE 1
				LOCAL:欲情 = 20
			CASE 2
				LOCAL:欲情 = 1000
			CASE 3
				LOCAL:欲情 = 8000
			CASE 4
				LOCAL:欲情 = 24000
			CASE 5
				LOCAL:欲情 = 30000
			CASE 6
				LOCAL:欲情 = 36000
			CASE 7
				LOCAL:欲情 = 45000
			CASE 8
				LOCAL:欲情 = 55000
			CASE 9
				LOCAL:欲情 = 70000
			CASEELSE
				LOCAL:欲情 = 10000 + 20000 * (ABL:キャラ番号:欲情 - 6)
		ENDSELECT
	ELSEIF パターン == 1
		SELECTCASE ABL:キャラ番号:欲情
			CASE 0
				LOCAL:優越 = 50
			CASE 1
				LOCAL:優越 = 200
			CASE 2
				LOCAL:優越 = 1000
			CASE 3
				LOCAL:優越 = 8000
			CASE 4
				LOCAL:優越 = 24000
			CASE 5
				LOCAL:優越 = 30000
			CASE 6
				LOCAL:優越 = 36000
			CASE 7
				LOCAL:優越 = 45000
			CASE 8
				LOCAL:優越 = 55000
			CASE 9
				LOCAL:優越 = 70000
			CASEELSE
				LOCAL:優越 = 10000 + 20000 * (ABL:キャラ番号:欲情 - 6)
		ENDSELECT
	ENDIF
	;恋慕
	LOCAL:欲情 = LOCAL:欲情 * (10 - 2 * TALENT:キャラ番号:恋慕) / 10
	LOCAL:優越 = LOCAL:優越 * (10 - 2 * TALENT:キャラ番号:恋慕) / 10
	SIF LOCAL:欲情 == 0
		LOCAL:欲情 = 1
	;一線越えない
	IF TALENT:キャラ番号:一線越えない && !TALENT:キャラ番号:恋慕
		TIMES LOCAL:欲情 , 2.00
		TIMES LOCAL:優越 , 2.00
	ENDIF
RETURNF LOCAL:(珠番号)

