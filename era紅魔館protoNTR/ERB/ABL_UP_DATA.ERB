;-------------------------------------------------------------------------------
;Ｃ感覚の上昇にJUELが必要ならその値を返す
@JUEL_DEMAND_0(パターン,珠番号,キャラ番号)
	#FUNCTION
	#DIM パターン
	#DIM 珠番号
	#DIM キャラ番号
	#DIM 快Ｃ = GETNUM(JUEL,"快Ｃ")
	SIF パターン > 0
		RETURNF -1
	VARSET LOCAL
	SELECTCASE ABL:キャラ番号:Ｃ感覚
		CASE 0
			LOCAL:快Ｃ = 1
		CASE 1
			LOCAL:快Ｃ = 20
		CASE 2
			LOCAL:快Ｃ = 400
		CASE 3
			LOCAL:快Ｃ = 5000
		CASE 4
			LOCAL:快Ｃ = 15000
		CASE 5
			LOCAL:快Ｃ = 30000
		CASE 6
			LOCAL:快Ｃ = 40000
		CASE 7
			LOCAL:快Ｃ = 50000
		CASE 8
			LOCAL:快Ｃ = 60000
		CASE 9
			LOCAL:快Ｃ = 70000
		CASEELSE
			LOCAL:快Ｃ = 20000 * (ABL:キャラ番号:Ｃ感覚 - 6)
	ENDSELECT
	;経験補正
	LOCAL:快Ｃ = LOCAL:快Ｃ * 10 / (5 + GETEXPLV(EXP:キャラ番号:Ｃ経験,8))
	LOCAL:快Ｃ = LOCAL:快Ｃ * (1 + ABL:キャラ番号:Ｃ感覚) / (1 + GETEXPLV(EXP:キャラ番号:Ｃ絶頂経験,8))
	;一線越えない
	SIF TALENT:キャラ番号:一線越えない && !TALENT:キャラ番号:恋慕
		TIMES LOCAL:快Ｃ , 2.00
RETURNF LOCAL:(珠番号)

