;-------------------------------------------------
;愛撫
;自慰系コマンド　レベル2
;-------------------------------------------------
@COM600

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_恥丘)
SIF RESULT
	RETURN 0
TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_胸元)
SIF RESULT
	RETURN 0

PRINTFORML 愛撫実行
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:116 = PLAYER

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 5
DOWNBASE:気力 += 50

SOURCE:快Ｃ = 80
SOURCE:快Ｂ = 80
SOURCE:情愛 = 50
SOURCE:性行動 = 60
SOURCE:露出 = 20
SOURCE:不潔 = 30
SOURCE:逸脱 = 20
SOURCE:反感 = 20

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 20
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
IF (FIRSTTIME(TOSTR(312),1) || FIRSTTIME(TOSTR(20),1)) || (MASTER_POSE(4,4) != TARGET && MASTER_POSE(0,4)) || (STAIN:口 >= 汚れ_ペニス && (STAIN:口 != 汚れ_母乳 && STAIN:口 != 汚れ_母乳_愛液) && ASSIPLAY == 0 && TALENT:MASTER:汚臭耐性 < 2) || TEQUIP:20
;奴隷の口の汚れがあるときキスはない（愛液と母乳よごれはＯＫ、汚れ無視か助手調教時は問題なし）
;ボールギャグ使用中もなし
	SOURCE:不潔 = 0
	SOURCE:情愛 /= 4
	SOURCE:液体 /= 2
ELSE
	;汚臭鈍感
	SIF TALENT:汚臭耐性 > 0
		SOURCE:不潔 /= 4
	;汚臭敏感
	SIF TALENT:汚臭耐性 < 0
		SOURCE:不潔 *= 3
	;プライド高い
	SIF TALENT:プライド > 0
		SOURCE:不潔 *= 2
ENDIF


;-------------------------------------------------
;経験の処理
;-------------------------------------------------

;手淫経験
EXP:PLAYER:手淫経験 ++
;自慰経験
EXP:PLAYER:自慰経験 ++

RETURN 1
