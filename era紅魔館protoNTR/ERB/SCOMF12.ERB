;-------------------------------------------------
;パイズリフェラ
;奉仕系コマンド、レベル3
;-------------------------------------------------
@SCOM12

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_左尻)
SIF RESULT
	RETURN 0
TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_胸元)
SIF RESULT
	RETURN 0


;-------------------------------------------------
;実行決定
;-------------------------------------------------
STR:0 = パイズリフェラ
;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:PLAYER:行為者 = TARGET
;TCVAR:0 射精対象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI）
;TCVAR:1 予備射精対象フラグ（ビットを使いたくない人用、CFLAG:100に翻訳されます）	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI）
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 5
;強度
TCVAR:PLAYER:4 = 3000
;奉仕快楽
TFLAG:30 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 30
DOWNBASE:気力 += 160

SOURCE:快Ｂ = 100
SOURCE:情愛 = 250
SOURCE:性行動 = 500
SOURCE:達成 = 300
SOURCE:屈従 = 1000
SOURCE:逸脱 = 400
SOURCE:反感 = 400

;上のほうで計算した汚れデータ
SOURCE:不潔 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) * 10 + 60


;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 600
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;奴隷のＢ⇔調教者のＰの汚れが移動
STAIN:Ｂ |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:Ｂ
;奴隷の口⇔調教者のＰの汚れが移動
STAIN:口 |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:口
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:口淫経験 ++
EXP:パイズリ経験 ++
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 7
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 7
ENDIF

;愛情経験
SIF TALENT:恋慕 && ASSIPLAY == 0
	EXP:愛情経験 += 1

;奴隷が猫舌持ちの場合は、調教者に嗜虐快楽経験を追加
SIF TALENT:TARGET:猫舌
	EXP:嗜虐快楽経験 ++

RETURN 1

@CAN_SCOM12(ARG)
SKIPDISP ARG
PRINTL パイズリフェラ
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:精液中毒 * 3
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快楽刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快楽刻印 * 1
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 恥じらい
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭耐性 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭耐性 < 0
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 汚臭敏感
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 献身的
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 快感の否定
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:性別嗜好 & 1 && !(HAS_VAGINA(PLAYER)) && (!TALENT:恋慕 || PLAYER != MASTER)
	PRINT  - 
	TFLAG:実行値 -= 12
	PRINT 男嫌い
	PRINTV '(,12,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 恋慕
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 親愛
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF

;調教者がふたなり
IF GET_SEXNAME(PLAYER) == "ふたなり"
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 8
	PRINT ふたなり
	PRINTV '(,8,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET)
;汚れあり
IF LOCAL
	PRINT  - 
	TFLAG:実行値 -= LOCAL
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:汚臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,LOCAL,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;合計を表示(30以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 35
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0
RETURN 1
