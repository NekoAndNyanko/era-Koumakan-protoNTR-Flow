;娼婦紋の色取得
@COM417_GetProstituteMarkColor(人物)
	#FUNCTIONS
	#DIM 人物
	
	IF CFLAG:人物:娼婦紋
		SELECTCASE (RESULT:1 * RESULT:0)
			CASE IS >= 200	; 娼婦紋が鮮やかなピンク色
				RETURNF "鮮やかなピンク色"
			CASE IS >= 100	; 娼婦紋がピンク色
				RETURNF "ピンク色"
			CASE 50 TO 99	; 娼婦紋が赤
				RETURNF "赤色"
			CASE 1 TO 49	; 娼婦紋が赤紫
				RETURNF "赤紫色"
			CASEELSE		; 娼婦紋が紫
				RETURNF "紫色"
		ENDSELECT
	ELSE
		RETURNF ""
	ENDIF
