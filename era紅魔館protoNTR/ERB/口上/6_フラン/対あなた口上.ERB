	#DIMS 呼称

	呼称 = %CALLNAME:対象%
	IF CALLNAME:奴隷 == "フラン" || CALLNAME:奴隷 == "フランドール"
		IF CALLNAME:対象 == "レミリア"
			呼称 = お姉さま
		ELSE
		ENDIF
	ELSE
		; 名称変更時はデフォルトのまま
	ENDIF
	RETURNF 呼称
