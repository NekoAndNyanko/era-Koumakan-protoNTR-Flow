;==================================================
;300ex - 浮気について話す - 許せない
;==================================================
@KOJO_MESSAGE_COM_K4_300_10_01_01(奴隷, 調教者)
	#DIM 奴隷
	#DIM 調教者
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		;寝取られ寸前ならもう手遅れ
		IF TALENT:奴隷:人妻
			PRINTFORML 「・・今更？ 私のことを構ってもくれないくせに、夫婦気取りなんてよしてちょうだい」
			PRINTFORML 「この際だからはっきり言っておくわ。あの人との関係をやめるつもりはないわ。これからもキスだって、セックスだってするわ」
			PRINTFORML 「許せない？ まだ夫婦気取りなのかしら？ もう潮時とは思わないの？ 未練がましいわね・・」
		ELSE
			PRINTFORML 「・・今更？ 私のことを構ってもくれないくせに、恋人気取りなんてよしてちょうだい」
			PRINTFORML 「この際だからはっきり言っておくわ。あの人との関係をやめるつもりはないわ。これからもキスだって、セックスだってするわ」
			PRINTFORML 「許せない？ まだ恋人気取りなのかしら？ もう潮時とは思わないの？ 未練がましいわね・・」
		ENDIF
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		;寝取られそうなぐらいなら、しぶしぶ頭を下げる
		PRINTFORML 「・・わかったわ・・ええ・・ごめんなさい・・」
	ELSE
		;それ以外なら泣いて謝る
		PRINTFORML 「ごめんなさい・・ごめんなさい・・もう、もう二度と、二度とあんなことしないわ・・」
	ENDIF
RETURN 0

;==================================================
;300ex - 浮気について話す - 許せる
;==================================================
@KOJO_MESSAGE_COM_K4_300_10_01_02(奴隷, 調教者)
	#DIM 奴隷
	#DIM 調教者
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		;寝取られ寸前
		PRINTFORML 「ふふ、公認ということでいいのかしら？」
		PRINTFORML 「でももっとスリルのあるコトしてみたくない・・？ たとえば、%MSG_RE_LOVER(奴隷)%を寝取られる側ではなくて他人の恋人を寝取る側になってみるとか・・ふふ・・」
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		;寝取られそうなら、別れ話と思い少し不安を感じる
		PRINTFORML 「・・浮気をしてもいいだなんて・・わ、私のことが嫌いになったの？ もうどうでもいいと思っているの！？」
		PRINTFORML 「・・そうじゃなくて、怒ってないって・・？ 信じてくれているの・・？」
	ELSE
		;それ以外なら自分のしたことを悔いる
		PRINTFORML 「ち、違うの、浮気とかそういうのではないのよ。あ、あの%NTR_OTOKO()%に脅されて・・」
		PRINTFORML 「・・本当に・・本当に、怒ってない・・の・・？」
	ENDIF
RETURN 0

;==================================================
;300ex - 浮気について話す - 寝取られたい
;==================================================
@KOJO_MESSAGE_COM_K4_300_10_01_03(奴隷, 調教者)
	#DIM 奴隷
	#DIM 調教者
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		;寝取られ寸前なら
		PRINTFORML 「ふふっ、%CALLNAME:調教者%ならそう言うと思ってたわ」
		PRINTFORML 「%MSG_RE_LOVER(奴隷)%が他の%NTR_OTOKO()%に犯られるのに興奮する変態さんですものね？」
	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
		;寝取られそうなら
		PRINTFORML 「・・本気なのね？ いいわ、%CALLNAME:調教者%がそう言うのなら遠慮なく、%NTR_KARE()%と親交を深めることにするわ」
	ELSE
		;それ以外なら
		PRINTFORML 「・・正気なの？ %CALLNAME:調教者%が何を考えているのか理解できないわ・・」
		PRINTFORML 「・・本当に、本当にそれでいいのね？」
	ENDIF
RETURN 0

;==================================================
;300ex - 浮気公認について
;==================================================
@KOJO_MESSAGE_COM_K4_300_10_02(奴隷, 調教者)
	#DIM 奴隷
	#DIM 調教者
	#DIM RSLT_LV		; 処女喪失会話戻り値
	#DIM 特殊フラグ1	;特殊パターン1 恋慕かつ親愛ではない浮気公認の公衆便所
	#DIMS 語尾

	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		語尾 = %MSG_HEART()%
	ELSE
		語尾 = %"♪"%
	ENDIF
	特殊フラグ1 = 0
	RSLT_LV = 0
	;最後にセックスした日時で分岐。その中でさらに好感度、屈服度で分岐
	IF TALENT:奴隷:妊娠
		IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
			PRINTFORML 「ふふっ、今こんな身体でしょう？ 子供が産まれるまではお預けというわけ・・残念だわ」
		ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
			PRINTFORML 「今こんな身体でしょう？ 子供が産まれるまではさすがにお預けだわ」
		ELSE
			PRINTFORML 「今こんな身体だから、子供が産まれるまではさすがにお預けね」
		ENDIF
	ELSEIF (CFLAG:奴隷:NTR訪問者と最後にセックスした日時/(24*60)) == (DATETIME()/(24*60))
		;同じ日にセックスしてたら
		SELECTCASE CFLAG:奴隷:NTR訪問者と最後にセックスした日時
			CASE IS > (DATETIME() - 1*01*60) ;１時間以内
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ふふっ、たった今%NTR_KARE()%に抱いてもらったばかりなのよ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ふふっ、たった今%NTR_KARE()%に抱かれたばかりなのよ」
				ELSE
					PRINTFORML 「・・たった今%NTR_KARE()%とセックスしたばかりなの・・」
				ENDIF
			CASE IS > (DATETIME() - 1*04*60) ;４時間以内
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ふふっ、さっき%NTR_KARE()%に抱いてもらったばかりなのよ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ふふっ、さっき%NTR_KARE()%に抱かれたばかりなのよ」
				ELSE
					PRINTFORML 「・・さっき%NTR_KARE()%とセックスしたばかりなの・・」
				ENDIF
			CASEELSE
				PRINTFORML %"今日もセックスしたのだと答えた。"%
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ふふっ、今日も%NTR_KARE()%に抱いてもらったのよ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ふふっ、今日も%NTR_KARE()%に抱かれたのよ」
				ELSE
					PRINTFORML 「・・今日も%NTR_KARE()%とセックスしたわ・・」
				ENDIF
		ENDSELECT
		CALL KOJO_MESSAGE_COM_K4_300_10_02LV(奴隷, 調教者)
		RSLT_LV = RESULT
	ELSE
		SELECTCASE CFLAG:奴隷:NTR訪問者と最後にセックスした日時
			CASE IS > (DATETIME() - 1*01*60) ;１時間以内
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ふふっ、たった今%NTR_KARE()%に抱いてもらったばかりなのよ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ふふっ、たった今%NTR_KARE()%に抱かれたばかりなのよ」
				ELSE
					PRINTFORML 「・・たった今%NTR_KARE()%とセックスしたばかりなの・・」
				ENDIF
				CALL KOJO_MESSAGE_COM_K4_300_10_02LV(奴隷, 調教者)
				RSLT_LV = RESULT
			CASE IS > (DATETIME() - 1*08*60) ;８時間以内
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ふふっ、さっき%NTR_KARE()%に抱いてもらったばかりなのよ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ふふっ、さっき%NTR_KARE()%に抱かれたばかりなのよ」
				ELSE
					PRINTFORML 「・・さっき%NTR_KARE()%とセックスしたばかりなの・・」
				ENDIF
				CALL KOJO_MESSAGE_COM_K4_300_10_02LV(奴隷, 調教者)
				RSLT_LV = RESULT
			CASE IS > (DATETIME() - 1*24*60) ;１日以内
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ふふっ、昨日も%NTR_KARE()%に抱いてもらったのよ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ふふっ、昨日も%NTR_KARE()%に抱かれたのよ」
				ELSE
					PRINTFORML 「・・昨日も%NTR_KARE()%とセックスしたわ・・」
				ENDIF
				CALL KOJO_MESSAGE_COM_K4_300_10_02LV(奴隷, 調教者)
				RSLT_LV = RESULT
			CASE IS > (DATETIME() - 3*24*60) ;２日以内
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「昨日は%NTR_KARE()%に抱いてもらえなかったわ」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「昨日は%NTR_KARE()%に抱かれなかったわ」
				ELSE
					PRINTFORML 「・・昨日は%NTR_KARE()%とセックスしていないわ・・」
				ENDIF
				CALL KOJO_MESSAGE_COM_K4_300_10_02LV(奴隷, 調教者)
				RSLT_LV = RESULT
			CASEELSE
				IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
					PRINTFORML 「ずいぶん%NTR_KARE()%に抱いてもらえてないわ・・」
				ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)
					PRINTFORML 「ずいぶん%NTR_KARE()%に抱かれてないわ・・」
				ELSE
					PRINTFORML 「・・ずいぶん%NTR_KARE()%とセックスしていないわ・・」
				ENDIF
				CALL KOJO_MESSAGE_COM_K4_300_10_02LV(奴隷, 調教者)
				RSLT_LV = RESULT
		ENDSELECT
	ENDIF
	;特殊パターン1 恋慕かつ親愛ではない浮気公認の公衆便所
	IF TALENT:奴隷:恋慕 && !TALENT:奴隷:親愛 && TALENT:奴隷:浮気公認 && TALENT:奴隷:公衆便所 
		特殊フラグ1 = 1
	ELSE
		特殊フラグ1 = 0
	ENDIF

	;特殊パターンかつ膣が訪問者の精液で汚れており、ローター挿入の場合の分岐
	IF 特殊フラグ1 == 1 && CFLAG:奴隷:射精者V == 人物_訪問者 && (STAIN:奴隷:Ｖ & 汚れ_精液) && CFLAG:奴隷:ローター挿入 != 0
	    PRINTFORML %CALLNAME:奴隷%は%CALLNAME:MASTER%にそっと抱きつくと、耳元で囁いた。
		IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)
			IF 1 == RSLT_LV
				PRINTFORML 「それでね、ふふっ・・今私のアソコは、%NTR_KARE()%に注がれた精液が溢れているのよ」
			ELSE
