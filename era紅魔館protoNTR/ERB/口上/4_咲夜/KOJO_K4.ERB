;==================================================
;おやすみ口上
;==================================================
@KOJO_EVENT_K4_7(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;眠っている
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF CFLAG:睡眠
		IF LOCAL:1 && ARG == 2
			PRINTFORMW 「すぅ…。」
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;眠いので部屋に帰った
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW 「そろそろ寝るわ、%CALLNAME:MASTER%も明日の仕事に支障が出ない程度にね。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;寝るので%CALLNAME:MASTER%を追い出した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「%CALLNAME:MASTER%もそろそろ寝なさい、私はもう寝るわ。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃった
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「ふぁぁ………欠伸なんて…してないわよ…。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃったのでベッドまでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「私としたことが…眠くて…。」
		PRINTFORMW 眠そうにしている%CALLNAME:人物_咲夜%を%CALLNAME:人物_咲夜%の私室まで運んだ。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の部屋で寝ちゃったのでそのままおやすみ
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORMW 眠る%CALLNAME:人物_咲夜%に%CALLNAME:MASTER%は毛布をかけた。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%が寝ちゃったのでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 
		RETURN 0
	ENDIF
	;その他
	PRINTFORMW
ENDIF
RETURN 0

