			DATAFORM 「・・あの人には内緒よ」
			DATAFORM 「・・これは二人だけの秘密。いいわね？」
		ENDDATA

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ) 

		PRINTDATAL
			DATAFORM 「やめなさい」
			DATAFORM 「やめて」
			DATAFORM 「駄目よ」
		ENDDATA

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前) || NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)

		PRINTFORML 「・・・」
		PRINTFORML %CALLNAME:MASTER%の手が触れた瞬間、%CALLNAME:奴隷%は一瞬身を固くした。

	ELSE
		; それ以外の場合、非NTR口上を実行する
		RET_VAL = 1
	ENDIF 

