	#DIM 奴隷
	#DIMS S_HEART = ""	; 寝取られ具合でハート表示の頻度変更

	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ)

		S_HEART = %MSG_HEART()%
		IF TALENT:奴隷:処女 || CFLAG:奴隷:処女喪失中
			PRINTFORML 「あっ、ふふっ・・初めてなんだから、やさしくして・・ねっ%S_HEART%・・んっ、つぅ！」
		;非処女
		ELSE
			IF CHK_NTR_SATISFACTORY(奴隷) 
				;あなたとのエッチで満足できない身体の時
				PRINTDATAL
					DATAFORM 「あああっ！ これ、これをちょうだい！ これが欲しいのぉ%S_HEART%」
					DATAFORM 「ああっ・・き、きてっ%S_HEART%・・極太チンポきてっ！！」
					DATAFORM 「んんっ・・ああっ！ すごいのちょうだいっ%S_HEART%」
				ENDDATA
			ELSE
				PRINTDATAL
					DATAFORM 「んんんっ・・はぁぅんっあっ・・は、はいってくるぅ・・%S_HEART%」
					DATAFORM 「ああっ%S_HEART%・・き、きてっ・・！」
					DATAFORM 「んんっ・・ああっ！ ちょうだいっ%S_HEART%」
				ENDDATA
			ENDIF
		ENDIF

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)

		S_HEART = %MSG_HEART()%
		IF TALENT:奴隷:処女 || CFLAG:奴隷:処女喪失中
			PRINTFORML 「あっ、ふふっ・・初めてなんだから、やさしくして・・ねっ%S_HEART%・・んっ、つぅ！」
		;非処女
		ELSE
			IF CHK_NTR_SATISFACTORY(奴隷) 
				;あなたとのエッチで満足できない身体の時
				PRINTDATAL
					DATAFORM 「あああっ！ これ、これをちょうだい！ これが欲しいのぉ%S_HEART%」
					DATAFORM 「ああっ・・き、きてっ%S_HEART%・・極太チンポきてっ！！」
					DATAFORM 「んんっ・・ああっ！ すごいのちょうだいっ%S_HEART%」
				ENDDATA
			ELSE
				PRINTDATAL
					DATAFORM 「んんんっ・・はぁぅんっあっ・・は、はいってくるぅ・・%S_HEART%」
					DATAFORM 「ああっ%S_HEART%・・き、きてっ・・！」
					DATAFORM 「んんっ・・ああっ！ ちょうだいっ%S_HEART%」
				ENDDATA
			ENDIF
		ENDIF

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)

		IF (RAND:2 == 0)
			S_HEART = %MSG_HEART()%
		ENDIF
		IF TALENT:奴隷:処女 || CFLAG:奴隷:処女喪失中
			PRINTFORML 「ええ・・いいわ・・初めてなんだから、やさしくして・・ねっ%S_HEART%・・んっ、つぅ！」
		;非処女
		ELSE
			IF CHK_NTR_SATISFACTORY(奴隷) 
				;あなたとのエッチで満足できない身体の時
				PRINTDATAL
					DATAFORM 「あああっ！ これ、これよ！ これが欲しかったのぉ%S_HEART%」
					DATAFORM 「ああっ・・い、いいっ%S_HEART%・・極太チンポいいっ！！」
					DATAFORM 「んんっ・・ああっ！ すごいっ%S_HEART%」
				ENDDATA
			ELSE
				PRINTDATAL
					DATAFORM 「いいわ・・きて・・%S_HEART%」
					DATAFORM 「ええ、そこよ・・そこにちょうだい・・%S_HEART%」
					DATAFORM 「ああ・・そうよ・・きて・・%S_HEART%」
				ENDDATA
			ENDIF
		ENDIF

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い)

		IF (RAND:3 == 0)
			S_HEART = %MSG_HEART()%
		ENDIF
		IF TALENT:奴隷:浮気公認
			PRINTFORML 押し倒された%CALLNAME:奴隷%は何かを決心したかのように、%NTR_NAME(0)%の背中に腕を回した。
		ELSE
			PRINTFORML 押し倒された%CALLNAME:奴隷%は弱々しく抵抗している。
		ENDIF
		IF TALENT:奴隷:処女 || CFLAG:奴隷:処女喪失中
			PRINTFORML 「こ、これ以上はだめっ・・そ、それだけは・・いっ・・やぁぁァァァアアアア！！」
			IF TALENT:奴隷:恋慕 && TALENT:奴隷:浮気公認
				PRINTFORML 「・・いいわ・・でもせめて・・痛くしないで・・」
				PRINTFORML %CALLNAME:MASTER%が望んだことだからと、%CALLNAME:奴隷%は%NTR_NAME(0)%に処女を捧げようとしている。
			ELSEIF TALENT:奴隷:恋慕
				PRINTFORML 「ああっ・・だめ・・だめよ・・」
				PRINTFORML %NTR_NAME(0)%に組み敷かれた%CALLNAME:奴隷%は抜け出すことができずにいる。
				PRINTFORML 彼女は他の%NTR_OTOKO()%に処女を奪われることを受け入れようとしている。
			ELSE
				PRINTFORML 「ああっ・・だめ・・だめよ・・」
				PRINTFORML %NTR_NAME(0)%に組み敷かれた%CALLNAME:奴隷%は抜け出すことができずにいる。
				PRINTFORML 彼女は%NTR_NAME(0)%に処女を奪われることを受け入れようとしている。
			ENDIF
		;非処女
		ELSE
			PRINTDATAL
				DATAFORM 「だっ、だめ、まって・・それはっ・・・うっ・・・くぅぅぅ%S_HEART%」
				DATAFORM 「だめ、だめよっ・・これ以上はっ・・ああっ%S_HEART%」
				DATAFORM 「お、お願い、許して・・それは・・だめなのっ・・あああっ%S_HEART%」
			ENDDATA
		ENDIF

	ELSE

		IF TALENT:奴隷:処女 || CFLAG:奴隷:処女喪失中
			PRINTFORML 「だっ、だめっ！！そ、それだけは・・いっ・・やぁぁァァァアアアア！！」
		;非処女
		ELSE
			PRINTDATAL
				DATAFORM 「いっ、嫌っ、そ、それはっ・・・うっ・・・くぅぅぅ」
				DATAFORM 「だめ、だめよっ・・これ以上は嫌あっ・・ああっ」
				DATAFORM 「ゆ、許して・・それは・・だめなのっ・・あああっ！」
			ENDDATA
		ENDIF

	ENDIF
RETURN 0

