		ELSE
			PRINTFORML %"「私は"+CALLNAME:PLAYER+"がそう願うから"+NTR_KARE()+"に抱かれてくるわ」"%
			PRINTDATAL
				DATAFORM %"「でもその間、"+CALLNAME:PLAYER+"のセックスは禁止よ。"+MSG_RE_LOVER(奴隷)+"としてのお願い」"%
				DATAFORM %"「だからその間、"+CALLNAME:PLAYER+"は誰かとセックスしてほしくないわ」"%
				DATAFORM %"「だからその間、"+CALLNAME:PLAYER+"が誰かとセックスするのは嫌よ」"%
