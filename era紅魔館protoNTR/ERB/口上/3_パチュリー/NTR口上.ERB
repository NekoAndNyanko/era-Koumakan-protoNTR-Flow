		PRINTDATAL
			DATAFORM 「んっ・・んぅっ」
			DATAFORM 「なに・・ん・・んんっ・・」
			DATAFORM 「ん・・んんっ・・」
		ENDDATA
		IF NTR_CHK_PEEP_CONT(奴隷)
			PRINTFORML %CALLNAME:奴隷%は%ANATA_LOVER(奴隷)%の視線の先で%NTR_NAME(0)%に唇を重ねられた。
			IF TALENT:奴隷:浮気公認
				PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%に抵抗をせず、口内に送り込まれた%NTR_KARE()%の舌を受け入れた。
			ELSE
				PRINTFORML %CALLNAME:奴隷%は驚いていたが、%NTR_NAME(0)%の舌が口内に侵入すると目を閉じて彼の舌を受け入れた。
			ENDIF
		ELSE
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%に唇を重ねられても抗わずにその舌を受け入れた。
		ENDIF

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_奉仕する程度)

		PRINTDATAL
			DATAFORM 「えっ？　んっ・・んむぅっ！！」
			DATAFORM 「なに・・んっ！ んんっ・・！」
			DATAFORM 「んっ！？ ん・・んんっ・・」
		ENDDATA
		IF NTR_CHK_PEEP_CONT(奴隷)
			PRINTFORML %CALLNAME:奴隷%は%ANATA_LOVER(奴隷)%の視線の先で乱暴に唇を奪われた。
			IF TALENT:奴隷:浮気公認
				PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%に抵抗をせず、口内に送り込まれた%NTR_KARE()%の舌を受け入れた。
			ELSE
				PRINTFORML %CALLNAME:奴隷%は弱々しく抵抗していたが、%NTR_NAME(0)%の舌が口内に侵入すると次第に大人しくなりついには彼の舌を受け入れた。
			ENDIF
		ELSE
			PRINTFORML %CALLNAME:奴隷%は乱暴に唇を奪われ、弱々しく抗うも%NTR_NAME(0)%を振り払うことができなかった。
		ENDIF

	ELSE

		PRINTDATAL
			DATAFORM 「えっ？　んっ、んんぅっ！！」
			DATAFORM 「やっ・・いやっ・・んんっ！」
			DATAFORM 「んんっ！？ やっ・・ん・・」
		ENDDATA
		IF NTR_CHK_PEEP_CONT(奴隷)
			PRINTFORML %CALLNAME:奴隷%は%ANATA_LOVER(奴隷)%の視線の先で乱暴に唇を奪われた。
		ELSE
			IF TALENT:奴隷:恋慕
				PRINTFORML %CALLNAME:奴隷%は%CALLNAME:MASTER%以外の%NTR_OTOKO()%に乱暴に唇を奪われた。	
			ELSE
				PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%に乱暴に唇を奪われた。	
			ENDIF
		ENDIF

	ENDIF
RETURN 0

