@NTR_KOJO_K3_2_0(奴隷)
	#DIM 奴隷
	IF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ)

		PRINTDATAL
			DATAFORM 「ん・・ちゅ・・%MSG_HEART()%」
			DATAFORM 「ちゅ・・素敵・・んっ・・%MSG_HEART()%」
			DATAFORM 「キス・・したいわ・・ん・・%MSG_HEART()%」
		ENDDATA
		IF NTR_CHK_PEEP_CONT(奴隷) && NTR_CHK_MISETUKE(奴隷)
			PRINTFORML %CALLNAME:奴隷%は自分から%NTR_NAME(0)%を抱きしめながら、%CALLNAME:MASTER%に見せつけるように唇と舌を捧げた。
		ELSE
			PRINTFORML %CALLNAME:奴隷%は%NTR_NAME(0)%に抱きつき、自ら唇と舌を捧げた。
		ENDIF

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)

		PRINTDATAL
			DATAFORM 「ん・・ちゅ・・%MSG_HEART()%」
			DATAFORM 「ちゅ・・素敵・・んっ・・%MSG_HEART()%」
			DATAFORM 「キス・・したいわ・・ん・・%MSG_HEART()%」
		ENDDATA
		IF TALENT:奴隷:恋慕
			IF NTR_CHK_PEEP_CONT(奴隷) && NTR_CHK_MISETUKE(奴隷)
				PRINTFORML %CALLNAME:奴隷%は自分から%NTR_NAME(0)%を抱きしめながら、%ANATA_LOVER(奴隷)%に見せつけるように唇と舌を捧げた。
			ELSE
				PRINTFORML %CALLNAME:奴隷%は%CALLNAME:MASTER%のことも忘れ自分から%NTR_NAME(0)%に唇と舌を捧げた。
			ENDIF
		ELSE
			IF NTR_CHK_PEEP_CONT(奴隷) && NTR_CHK_MISETUKE(奴隷)
				PRINTFORML %CALLNAME:奴隷%は自分から%NTR_NAME(0)%を抱きしめながら、%CALLNAME:MASTER%に見せつけるように唇と舌を捧げた。
			ELSE
				PRINTFORML %CALLNAME:奴隷%は自分から%NTR_NAME(0)%に唇と舌を捧げた。
			ENDIF
		ENDIF

	ELSEIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう)

		PRINTDATAL
			DATAFORM 「んっ！ ・・ん・・ちゅ・・♪」
			DATAFORM 「んっ！ ・・ちゅ・・素敵・・んっ・・♪」
			DATAFORM 「ん！ ん・・ええ・・いいわ・・ん・・♪」
		ENDDATA
