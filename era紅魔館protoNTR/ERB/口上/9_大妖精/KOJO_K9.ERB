;==================================================
;おやすみ口上
;==================================================
@KOJO_EVENT_K9_7(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;眠っている
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF CFLAG:睡眠
		IF LOCAL:1 && ARG == 2
			PRINTFORMW 「ん…。」
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;眠いので部屋に帰った
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW 「おやすみなさい%CALLNAME:MASTER%」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;寝るので%CALLNAME:MASTER%を追い出した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「それでは%CALLNAME:MASTER%、また明日。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃった
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORMW 「すぅ…。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃったのでベッドまでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「…%CALLNAME:MASTER%」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の部屋で寝ちゃったのでそのままおやすみ
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「…むにゃ、%CALLNAME:MASTER%」
		PRINTFORMW ベットで話をしていたらそのまま寝てしまった…。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%が寝ちゃったのでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW
		RETURN 0
	ENDIF
	;その他
	PRINTFORMW
ENDIF
RETURN 0

