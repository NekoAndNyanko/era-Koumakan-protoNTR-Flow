;==================================================
;おやすみ口上
;==================================================
@KOJO_EVENT_K10_7(ARG,ARG:1)
;口上主はTARGET
;--------------------------------------------------
LOCAL = 1
IF LOCAL
	;-------------------------------------------------
	;小分岐
	;-------------------------------------------------
	;眠っている
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF CFLAG:睡眠
		IF LOCAL:1 && ARG == 2
			PRINTFORMW 「すぅ…。」
			PRINTFORMW 金色の髪を揺らして眠りについている。
		ENDIF
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;眠いので部屋に帰った
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 0
		PRINTFORMW 「さて、いい加減私も帰るか。」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;寝るので%CALLNAME:MASTER%を追い出した
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 1
		PRINTFORMW 「そろそろ%CALLNAME:MASTER%も寝たほうがいいぜ」
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃった
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 2
		PRINTFORML 「や、やばい…瞼が重い…。」
		PRINTFORMW 体制を崩して%CALLNAME:人物_魔理沙%は眠ってしまった
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の見てる前で寝ちゃったのでベッドまでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 3
		PRINTFORMW 「眠い…もう、此処で寝る…。」
		PRINTFORMW 深い眠りについてしまった%CALLNAME:人物_魔理沙%を家のベッドまで運んだ。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%の部屋で寝ちゃったのでそのままおやすみ
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 4
		PRINTFORML 「だ、だめだ…私、もう限界…。」
		PRINTFORMW 眠る%CALLNAME:人物_魔理沙%に%CALLNAME:MASTER%は毛布をかけた。
		RETURN 0
	ENDIF
	;-------------------------------------------------
	;%CALLNAME:MASTER%が寝ちゃったのでお運び
	;記入チェック
	LOCAL:1 = 1
	;-------------------------------------------------
	IF LOCAL:1 && ARG == 5
		PRINTFORMW 「なんだかこうしてみるときれいな顔してるなー。」
		RETURN 0
	ENDIF
	;その他
	PRINTFORMW
ENDIF
RETURN 0

