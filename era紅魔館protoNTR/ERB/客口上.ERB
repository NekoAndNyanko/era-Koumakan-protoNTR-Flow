@CALL_K_NAME(奴隷)
	#FUNCTIONS
	#DIM 奴隷
	#DIMS 呼称
	
	呼称 = %CALLNAME:奴隷+"たん"%
	SELECTCASE 奴隷
		CASE 人物_美鈴
			IF CALLNAME:奴隷 == "美鈴"
				呼称 = "メイリンたん"
			ENDIF
		CASE 人物_小悪魔
			IF CALLNAME:奴隷 == "小悪魔"
				呼称 = "こぁ"
			ENDIF
		CASE 人物_パチュリー
			IF CALLNAME:奴隷 == "パチュリー"
				呼称 = "ぱっちぇたん"
			ENDIF
		CASE 人物_咲夜
			IF CALLNAME:奴隷 == "咲夜"
				呼称 = "さっきゅん"
			ENDIF
		CASE 人物_レミリア
			IF CALLNAME:奴隷 == "レミリア"
				呼称 = "れみぃたん"
			ENDIF
		CASE 人物_フラン
			IF CALLNAME:奴隷 == "フラン" || CALLNAME:奴隷 == "フランドール"
				呼称 = "フランたん"
			ENDIF
		CASE 人物_子悪魔
			IF CALLNAME:奴隷 == "小悪魔"
				呼称 = "ここぁ"
			ENDIF
		CASE 人物_チルノ
			IF CALLNAME:奴隷 == "チルノ"
				呼称 = "ちるのたん"
			ENDIF
		CASE 人物_大妖精
			IF CALLNAME:奴隷 == "大妖精"
				呼称 = "だいちゃん"
			ENDIF
		ENDSELECT
	RETURNF 呼称


