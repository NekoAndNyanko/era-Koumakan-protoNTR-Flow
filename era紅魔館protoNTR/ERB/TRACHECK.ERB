;-------------------------------------------------
;好感度ボーナス
;-------------------------------------------------
@MASTER_FAVOR_CHECK(ARG,ARG:1)
	#FUNCTION
	SIF ASSIPLAY
		RETURNF 100
	SELECTCASE ARG:1
		CASE 0 TO 3
			RETURNF GET_REVISION(CFLAG:ARG:好感度, 200 , 20000) + 100
		CASE 10
			RETURNF GET_REVISION(CFLAG:ARG:好感度, 200 , 15000) + 100
		CASE 30 TO 33
			RETURNF MAX(50,100 - CFLAG:ARG:好感度 / 200)
		CASEELSE
			RETURNF 100
	ENDSELECT

;-------------------------------------------------
;技巧のチェック
;-------------------------------------------------
@TECHNIQUE_CHECK(ARG,ARG:1)
	#FUNCTION
	SIF TCVAR:ARG:行為者 < 0
		RETURNF 100
	SIF TCVAR:ARG:行為者 > CHARANUM
		RETURNF 100
	SELECTCASE ARG:1
		CASE 0 TO 3
			RETURNF GET_REVISION(ABL:(TCVAR:ARG:行為者):技巧 + 2, 500, 10)
		CASEELSE
			RETURNF 100
	ENDSELECT

;-------------------------------------------------
;ムード
;-------------------------------------------------
@MOOD_CHECK(ARG,ARG:1)
	#FUNCTION
	SELECTCASE ARG:1
		CASE 10,12,15,16,18,20,21,22
			RETURNF GET_REVISION(BASE:ARG:ムード, 50 , 500) + 100
		CASE 30 TO 33
			RETURNF 200 - BASE:ARG:ムード / 100
		CASEELSE
			RETURNF 100
	ENDSELECT

;-------------------------------------------------
;理性
;-------------------------------------------------
@REASON_CHECK(ARG,ARG:1)
	#FUNCTION
	SELECTCASE ARG:1
		CASE 0,1,2,3,10,12,15,16,18
			RETURNF GET_REVISION(1000 - BASE:ARG:理性, 50 , 500) + 100
		CASE 30 TO 33
			RETURNF 50 + BASE:ARG:理性 / 100
		CASEELSE
			RETURNF 100
	ENDSELECT

;-------------------------------------------------
;処女喪失時の処理
;事前に下記変数の設定が必要
; TEQUIP:奴隷:Ｖセックス 挿入しているキャラ(挿入させられている場合もこっち)
; TCVAR:奴隷:行為者     破らせたキャラ(自分で挿入している場合Ｖセックスと同一)
; TEQUIP:奴隷:バイブ    バイブを使ってるなら1
; TFLAG:50  =3であればGスポット刺激による処女喪失。相手はあなたとする。
;-------------------------------------------------
@LOST_VIRGIN(奴隷, 相手)
	#DIM 奴隷
	#DIM 相手

	IF TALENT:奴隷:処女 == 1 && EXP_UP(1,奴隷)

		;純正処女だった場合喪失を記録
		IF TEQUIP:奴隷:バイブ
			IF 奴隷 == 相手 || 相手 == -1
				IF TCVAR:奴隷:行為者 == 奴隷
					; 自分自身の手で
					TRYCALL memorial_v_append(奴隷,-1,"バイブで失った")
				ELSE
					TRYCALL memorial_v_append(奴隷, TCVAR:奴隷:行為者,"バイブで奪われた")
				ENDIF
			ELSE
				TRYCALL memorial_v_append(奴隷,相手,"バイブで奪われた")
			ENDIF
			TRYCALL COM417_Lost_Virgin(奴隷,-1)
		ELSEIF TFLAG:50 == 3
			; Gスポット刺激の場合の例外処理
			TRYCALL memorial_v_append(奴隷,PLAYER,"指で奪われた")
			TRYCALL COM417_Lost_Virgin(奴隷,PLAYER)
		ELSE
			TRYCALL memorial_v_append(奴隷, 相手)
			TRYCALL COM417_Lost_Virgin(奴隷,TEQUIP:奴隷:Ｖセックス)
			; 訪問者相手にＶ処女喪失時に記録
			IF 相手 == 人物_訪問者
				CFLAG:奴隷:NTR処女喪失 = 人物_訪問者
			ELSE
				CFLAG:奴隷:NTR処女喪失 = 0
			ENDIF
		ENDIF
		
		PRINTW 処女喪失
		;処女喪失フラグ(今回のコマンド)
		TCVAR:奴隷:破瓜 = 1
		;ビデオ撮影なら記録
		IF TEQUIP:奴隷:ビデオ撮影
			;性交
			IF TEQUIP:奴隷:Ｖセックス
				LOCALS = LV1/
			ELSEIF TEQUIP:奴隷:バイブ
				LOCALS = LV3/
				;その他
			ELSE
				LOCALS = LV4/
			ENDIF
			TSTR:1 += LOCALS
		ENDIF
		;貞操観念
		IF TALENT:奴隷:貞操 > 0
			IF !TALENT:奴隷:恋慕
				TIMES SOURCE:奴隷:情愛 , 0.60
				TIMES SOURCE:奴隷:反感 , 10.00
			ELSEIF !ASSIPLAY
				TIMES SOURCE:奴隷:情愛 , 1.20
				TIMES SOURCE:奴隷:反感 , 0.50
			ENDIF
			;貞操無頓着
		ELSEIF TALENT:奴隷:貞操 < 0
			TIMES SOURCE:奴隷:情愛 , 0.60
			TIMES SOURCE:奴隷:反感 , 0.50
		ENDIF
		;陥落素質による処理
		;MASTERがT奴隷ETの膣に挿入している
		IF TEQUIP:奴隷:Ｖセックス == MASTER
			;親愛かつ相手が主人
			IF TALENT:奴隷:恋慕 > 1
				PRINTFORML %CALLNAME:奴隷%は%CALLNAME:MASTER%が初めての相手である事にこの上ない幸せを感じているようだ……
				TCVAR:奴隷:反発刻印取得抑制 = 1
				TIMES SOURCE:奴隷:情愛 , 2.00
				TIMES SOURCE:奴隷:反感 , 0.10
				;恋慕かつ相手が主人
			ELSEIF TALENT:奴隷:恋慕
				PRINTFORML %CALLNAME:奴隷%は%CALLNAME:MASTER%に処女を捧げられた事に幸福を感じているようだ……
				TCVAR:奴隷:反発刻印取得抑制 = 1
				TIMES SOURCE:奴隷:情愛 , 2.00
				TIMES SOURCE:奴隷:反感 , 0.30
			ENDIF
		ENDIF
		TALENT:奴隷:処女 = 0
	ENDIF
	;-------------------------------------------------
	;再生処女喪失のチェック
	;-------------------------------------------------
	IF TALENT:奴隷:処女 == 2 && EXP_UP(1,奴隷)
		PRINTW 再生処女喪失
		;処女喪失フラグ(今回のコマンド)
		TCVAR:奴隷:破瓜 = 2
		;ビデオ撮影なら記録
		IF TEQUIP:奴隷:ビデオ撮影
			;性交
			IF TEQUIP:奴隷:Ｖセックス
				LOCALS = LV1/
			ELSEIF TEQUIP:奴隷:バイブ
				LOCALS = LV3/
				;その他
			ELSE
				LOCALS = LV4/
			ENDIF
			TSTR:1 += LOCALS
		ENDIF
		IF TEQUIP:奴隷:バイブ
			TRYCALL COM417_Lost_Virgin(奴隷,-1)
		ELSE
			TRYCALL COM417_Lost_Virgin(奴隷,TEQUIP:奴隷:Ｖセックス)
			; 訪問者相手にＶ処女喪失時に記録
			IF 相手 == 人物_訪問者
				CFLAG:奴隷:NTR処女喪失 = 人物_訪問者
			ELSE
				CFLAG:奴隷:NTR処女喪失 = 0
			ENDIF
		ENDIF
		TALENT:奴隷:処女 = 0
	ENDIF

@LOST_VIRGIN_A(奴隷, 相手)
	#DIM 奴隷
	#DIM 相手
	;アナル処女の記録
	IF EXP_UP(21,奴隷)
		TRYCALL memorial_a_append(奴隷, 相手)
	ENDIF

@LOST_VIRGIN_M(奴隷, 相手)
	#DIM 奴隷
	#DIM 相手
	;ファーストキスの記録
	IF EXP_UP(27,奴隷)
		;奴隷=0,相手=-1の時は相手をTARGETとする。
		IF 奴隷 == 0 && 相手 == -1
			相手 = TARGET
		ENDIF
		TRYCALL memorial_m_append(奴隷, 相手)
	ENDIF


	
