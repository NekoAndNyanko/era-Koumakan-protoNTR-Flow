@EVENT_WC_COUNTER_NTRreversal_SOURCE(ARG)
;;ARG=誰？
;;寝取られたあなたを奪還すると決めた際
#DIM 奪還者
	奪還者 = ARG
	LOCAL:20 = 0
	LOCAL:21 = 0
	LOCAL:22 = 0
	LOCAL:23 = 0
	;;屈服度と好感度の差を求めて差に応じて、屈服↓好感↑
	LOCAL:20 = CFLAG:奪還者:屈服度 - CFLAG:奪還者:好感度
	LOCAL:21 = CFLAG:奪還者:好感度 - CFLAG:奪還者:屈服度
	IF LOCAL:20 < 0
		IF LOCAL:21 < 100
			LOCAL:22 = (LOCAL:21 / 10) + 10
			LOCAL:23 = (LOCAL:21 / 10) + 1
		ELSEIF LOCAL:21 < 1000
			LOCAL:22 = (LOCAL:21 / 100) + 50
			LOCAL:23 = (LOCAL:21 / 100) + 10
		ELSE
			LOCAL:22 = (LOCAL:21 / 1000) + 100
			LOCAL:23 = (LOCAL:21 / 1000) + 20
		ENDIF
	ELSE
		IF LOCAL:20 < 300
			LOCAL:22 = (LOCAL:20 / 20) + 10
			LOCAL:23 = (LOCAL:20 / 20) + 1
		ELSEIF LOCAL:20 < 1000
			LOCAL:22 = (LOCAL:20 / 200) + 50
			LOCAL:23 = (LOCAL:20 / 200) + 10
		ELSE
			LOCAL:22 = (LOCAL:20 / 2000) + 100
			LOCAL:23 = (LOCAL:20 / 2000) + 20
		ENDIF
	ENDIF
	CFLAG:奪還者:屈服度 -= LOCAL:22
	CFLAG:奪還者:好感度 += LOCAL:23
	LOCAL:20 = 0
	LOCAL:21 = 0
	LOCAL:22 = 0
	LOCAL:23 = 0
RETURN


;;==============================================================================
@EVENT_WC_COUNTER_NTRagree_SOURCE
;;寝取られたあなたに対して、管理人放棄
	LOCAL:20 = 0
	LOCAL:21 = 0
	LOCAL:22 = 0
	LOCAL:23 = 0
	;;女の子の屈服度に応じて、女の子の好感↓、あなたの屈服↑
	IF CFLAG:TARGET:屈服度 < 1000
		LOCAL:22 = (LOCAL:21 / 100) + 1
		LOCAL:23 = (LOCAL:21 / 100) + 1
	ELSEIF CFLAG:TARGET:屈服度 < 3000
		LOCAL:22 = (LOCAL:21 / 300) + 10
		LOCAL:23 = (LOCAL:21 / 300) + 5
	ELSEIF CFLAG:TARGET:屈服度 < 6000
		LOCAL:22 = (LOCAL:21 / 600) + 50
		LOCAL:23 = (LOCAL:21 / 600) + 10
	ELSEIF CFLAG:TARGET:屈服度 < 9000
		LOCAL:22 = (LOCAL:21 / 900) + 100
		LOCAL:23 = (LOCAL:21 / 900) + 15
	ELSE
		LOCAL:22 = (LOCAL:21 / 1000) + 200
		LOCAL:23 = (LOCAL:21 / 1000) + 20
	ENDIF
	CFLAG:MASTER:屈服度 += LOCAL:22
	CFLAG:TARGET:好感度 -= LOCAL:23
	LOCAL:20 = 0
	LOCAL:21 = 0
	LOCAL:22 = 0
	LOCAL:23 = 0
RETURN


