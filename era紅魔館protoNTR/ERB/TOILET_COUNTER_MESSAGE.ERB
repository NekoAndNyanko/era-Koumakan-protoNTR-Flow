@EVENT_WC_COUNTER_MESSAGE15								;;尻を撫でる
	PRINTFORM %CALLNAME:TARGET%
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1		;;虐待
			PRINTFORM が無遠慮な手つきで、%CALLNAME:MASTER%の
			PRINTDATAL
				DATAFORM 顎先に指をかけ、軽く上を向かせる。
				DATAFORM 手首を取り、軽く捻りあげる。
			ENDDATA
		CASE 2		;;冷淡
			PRINTFORM が無造作な仕草で、%CALLNAME:MASTER%の
			PRINTDATA
				DATAFORM 首
				DATAFORM 腰
				DATAFORM 腕
			ENDDATA
			PRINTFORML に触れる。
		CASE 3		;;快虐
			PRINTFORM が、ペチペチと%CALLNAME:MASTER%の
			PRINTDATA
				DATAFORM 頬
				DATAFORM お尻
				DATAFORM 腕
			ENDDATA
			PRINTFORML を軽く手のひらで打った。
		CASE 6		;;愛玩
			PRINTFORM が優しい手つきで、%CALLNAME:MASTER%の
			PRINTDATA
				DATAFORM 頬
				DATAFORM お尻
				DATAFORM 胸元
			ENDDATA
			PRINTFORML を撫で回す。
		CASE 7,8		;;同情／子供
			PRINTFORM の手が、そっと%CALLNAME:MASTER%の
			PRINTDATA
				DATAFORM 腕
				DATAFORM 腰
				DATAFORM 手
			ENDDATA
			PRINTFORML に触れる。
		CASEELSE		;;性戯／誘惑
			PRINTFORM の手が、衣服の上からそっと%CALLNAME:MASTER%の
			PRINTDATA
				DATAFORM お尻
				DATAFORM 胸元
				DATAFORM 下腹部
			ENDDATA
			PRINTFORML を撫で回す。
	ENDSELECT
	;;
	;;女の子の台詞を挟んで
	;;押し倒す
	IF SELECTCOM == 350
		TRYCALLFORM KOJO_WC_COUNTER_com350_K{NO:TARGET}	;;「押し倒す」のときの汎用
	ELSE
		TRYCALLFORM KOJO_WC_COUNTER15_K{NO:TARGET}	;;尻を撫でる
	ENDIF
	;;
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1		;;虐待
			IF ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW それだけのことでも、%CALLNAME:MASTER%の体は期待に疼いてしまった。
			ELSE
				PRINTFORMW %CALLNAME:MASTER%は、ただジッと%CALLNAME:TARGET%にされるがままになっていた。
			ENDIF
		CASE 2		;;冷淡
			IF TALENT:MASTER:淫乱 || ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW %CALLNAME:MASTER%の体が、物足りなさを憶えてかすかに疼いた。
			ELSE
				PRINTFORMW %CALLNAME:MASTER%は、ただジッと%CALLNAME:TARGET%にされるがままになっていた。
			ENDIF
		CASE 3		;;快虐
			IF ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW 痛みなどまるでないのに、%CALLNAME:MASTER%の体は期待に熱くなり始めた。
			ELSE
				PRINTFORMW %CALLNAME:MASTER%は、ただジッと%CALLNAME:TARGET%にされるがままになっていた。
			ENDIF
		CASE 6		;;愛玩
			PRINTFORM %CALLNAME:TARGET%の優しい手つきを受け入れながらも、
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW 今の我が身を思って%CALLNAME:MASTER%は俯いてしまった。
			ELSEIF TALENT:MASTER:淫乱 || ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW 物足りなさを感じる自分の体に、%CALLNAME:MASTER%は呆れていた。
			ELSE
				PRINTFORMW 自分のようなものに良いのだろうかと、戸惑ってしまった。
			ENDIF
		CASE 7,8		;;同情／子供
			IF TALENT:MASTER:淫乱 || ABL:MASTER:マゾっ気 >= 2
				PRINTFORML 優しく触れられることに戸惑いながらも、%CALLNAME:MASTER%の体は物足りなさに疼き始めた。
				IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
					PRINTFORMW そんな自分の体をつくづく浅ましいと思いながらも、同時にその思いがさらに%CALLNAME:MASTER%を興奮させた。
				ELSE
					PRINTFORMW そんな自分にも優しい%CALLNAME:TARGET%に、%CALLNAME:MASTER%は申し訳なさのようなものを感じていた。
				ENDIF
			ELSE
				PRINTFORM %CALLNAME:MASTER%は静かな表情で%CALLNAME:TARGET%の手を受け入れ
				IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
					PRINTFORMW ながら、良心が疼くのを感じていた。
				ELSEIF TALENT:MASTER:淫乱 || ABL:MASTER:マゾっ気 >= 2
					PRINTFORMW ながらも、物足りなさに体が疼いてしまった。
				ELSE
					PRINTFORMW 、されるがままになっていた。
				ENDIF
			ENDIF
		CASEELSE		;;性戯／誘惑
			IF TALENT:MASTER:淫乱
				PRINTFORMW そっと触れられただけにもかかわらず、%CALLNAME:MASTER%の体が火照り始めた。
			ELSE
				PRINTFORMW %CALLNAME:MASTER%は、ただジッと%CALLNAME:TARGET%にされるがままになっていた。
			ENDIF
	ENDSELECT
RETURN 1


;-----------------------------
@EVENT_WC_COUNTER_MESSAGE16						;;囁く
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1,2,3		;;虐待／冷淡／快虐
			PRINTDATAL
				DATAFORM %CALLNAME:MASTER%の耳元に寄せられた%CALLNAME:TARGET%の唇から、冷たい言葉が紡がれ吹き込まれた。
				DATAFORM %CALLNAME:MASTER%の顎に指をかけて軽く上向かせ、%CALLNAME:TARGET%は口元を笑みの形に歪めた。
			ENDDATA
		CASEELSE	;;性戯／誘惑／愛玩／同情／子供
