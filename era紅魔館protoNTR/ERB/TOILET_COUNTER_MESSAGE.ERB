@EVENT_WC_COUNTER_MESSAGE_bottomCLOTH_OUT(ARG)	;;下半身一旦脱がされ／貞操帯は履いてちゃだめ
;;ARG=狙い（=1：Ｃ／=2：Ｖ／=3：Ａ）
	;;ボディースーツ／スクール水着／レオタード
	IF EQUIP:MASTER:ボディースーツ == 1 || EQUIP:MASTER:レオタード == 1 || EQUIP:MASTER:レオタード == 2
		;;ボディースーツ
		IF EQUIP:MASTER:ボディースーツ == 1
			PRINTFORM ボディースーツ
		;;スクール水着
		ELSEIF EQUIP:MASTER:レオタード == 1
			PRINTFORM スクール水着
		;;レオタード
		ELSEIF EQUIP:MASTER:レオタード == 2
			PRINTFORM レオタード
		ENDIF
		PRINTFORM をずらし、
	;;ズボンか半ズボンかブルマかスパッツ
	ELSEIF EQUIP:MASTER:下半身上着１ == 1 || EQUIP:MASTER:下半身上着２ == 1 || EQUIP:MASTER:下半身上着２ == 2 || EQUIP:MASTER:下半身上着１ == 2
		PRINTFORM 穿いている
		;;ズボン
		IF EQUIP:MASTER:下半身上着１ == 1
			PRINTFORM ズボン
		;;半ズボン
		ELSEIF EQUIP:MASTER:下半身上着２ == 1
			PRINTFORM 半ズボン
		;;ブルマ
		ELSEIF EQUIP:MASTER:下半身上着２ == 2
			PRINTFORM ブルマ
		;;スパッツ
		ELSEIF EQUIP:MASTER:下半身上着１ == 2
			PRINTFORM スパッツ
		ENDIF
		;;ぱんつはいてないか、Ａ狙いで絆創膏
		IF !EQUIP:MASTER:下半身下着２ || EQUIP:MASTER:下半身下着１ == 100 || (ARG == 3 && (EQUIP:MASTER:下半身下着２ == 2 || EQUIP:MASTER:下半身下着１ == 2))
			PRINTFORM を下ろし
		;;はいてる
		ELSE
			;;ばんそうこう（Ａ狙い以外）
			IF (EQUIP:MASTER:下半身下着２ == 2 || EQUIP:MASTER:下半身下着１ == 2) && ARG != 3
				PRINTFORM を下ろし、
			ELSE
				PRINTFORM と
			ENDIF
			IF EQUIP:MASTER:下半身下着２
				PRINTFORM %PANTSNAME(EQUIP:MASTER:下半身下着２)%を
			ELSE
				PRINTFORM %PANTSNAME(EQUIP:MASTER:下半身下着１)%を
			ENDIF
			;;ばんそうこう（Ａ狙い以外）
			IF (EQUIP:MASTER:下半身下着２ == 2 || EQUIP:MASTER:下半身下着１ == 2) && ARG != 3
				PRINTFORM 外すして
			ELSE
				PRINTFORM まとめて膝まで下ろし
			ENDIF
		ENDIF
		PRINTFORM 、
	ELSE
		;;スカートかワンピース
		IF (EQUIP:MASTER:スカート >= 1 && EQUIP:MASTER:スカート <= 4) || EQUIP:MASTER:ワンピース == 1
			PRINTFORM スカートの中に手を入れ
		;;チャイナ服
		ELSEIF EQUIP:MASTER:ワンピース == 2
			PRINTFORM チャイナ服のスリットから手を入れ
		;;着物
		ELSEIF EQUIP:MASTER:着物 == 1
			PRINTFORM 着物の裾を割り開いて捲り上げ
		;;袴か裳
		ELSEIF EQUIP:MASTER:下半身上着１ == 3 || EQUIP:MASTER:スカート == 5
			;;袴
			IF EQUIP:MASTER:下半身上着１ == 3
				PRINTFORM 袴
			;;裳
			ELSEIF EQUIP:MASTER:スカート == 5
				PRINTFORM 裳
			ENDIF
			PRINTFORM の腰紐をほどいて脱がせ
		ENDIF
		IF (EQUIP:MASTER:スカート >= 1 && EQUIP:MASTER:スカート <= 5) || EQUIP:MASTER:ワンピース == 1 || EQUIP:MASTER:ワンピース == 2 || EQUIP:MASTER:着物 == 1 || EQUIP:MASTER:下半身上着１ == 3
			;;ぱんつはいてないか、Ａ狙いで絆創膏
			IF !EQUIP:MASTER:下半身下着２ || EQUIP:MASTER:下半身下着１ == 100 || (ARG == 3 && (EQUIP:MASTER:下半身下着２ == 2 || EQUIP:MASTER:下半身下着１ == 2))
				PRINTFORM ると、
			ELSE
				PRINTFORM 、
			ENDIF
		ENDIF
		;;はいてる
		IF EQUIP:MASTER:下半身下着２ || EQUIP:MASTER:下半身下着１
			;;ばんそうこう（Ａ狙い以外）
			IF (EQUIP:MASTER:下半身下着２ == 2 || EQUIP:MASTER:下半身下着１ == 2) && ARG != 3
				PRINTFORM 着けている
			ELSE
				PRINTFORM 穿いている
			ENDIF
			IF EQUIP:MASTER:下半身下着２
				PRINTFORM %PANTSNAME(EQUIP:MASTER:下半身下着２)%を
			ELSE
				PRINTFORM %PANTSNAME(EQUIP:MASTER:下半身下着１)%を
			ENDIF
			;;ばんそうこう（Ａ狙い以外）
			IF (EQUIP:MASTER:下半身下着２ == 2 || EQUIP:MASTER:下半身下着１ == 2) && ARG != 3
				PRINTFORM 手早く外
			ELSE
				PRINTFORM 膝まで下ろ
			ENDIF
			PRINTFORM し、
		ENDIF
	ENDIF
RETURN


