@EVENT_WC_COUNTER_MESSAGE(枝番)
#DIM 枝番

SIF TARGET <= 0
	RETURN 0
CALL DATUI_MESSAGE
RESULT = 0

IF 枝番
	FOR 枝番,1,4
		TRYCALLFORM EVENT_WC_COUNTER_MESSAGE{TCVAR:20}_{枝番}
	NEXT
ELSE
	TRYCALLFORM EVENT_WC_COUNTER_MESSAGE{TCVAR:20}
ENDIF
SIF RESULT
	PRINTL

;-----------------------------
@EVENT_WC_COUNTER_MESSAGE10								;;色っぽい仕草
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1,2		;;虐待／冷淡
			;;会話
			IF SELECTCOM == 300
				PRINTFORML 話しかけた%CALLNAME:MASTER%をしばらく冷めた目で眺めてから、%CALLNAME:TARGET%はツイと視線をそらせる。
			;;押し倒す
			ELSEIF SELECTCOM == 350
				PRINTFORML %CALLNAME:MASTER%が覆い被さっても、%CALLNAME:TARGET%の冷たい目は変わらなかった。
			ELSE
				PRINTFORML %CALLNAME:TARGET%は刺すような冷たい視線を、%CALLNAME:MASTER%へと向けている。
			ENDIF
		CASE 7		;;同情
			;;会話
			IF SELECTCOM == 300 || SELECTCOM == 350
				PRINTFORML %CALLNAME:TARGET%は柔らかく微笑みながら、%CALLNAME:MASTER%に顔を寄せてきた。
			ELSE
				PRINTFORML %CALLNAME:TARGET%は柔らかな微笑みを浮かべて、%CALLNAME:MASTER%へと小さく一歩、身を寄せた。
			ENDIF
		CASE 8		;;子供
			;;会話
			IF SELECTCOM == 300
				PRINTFORML %CALLNAME:TARGET%は嬉しげに微笑みながら、%CALLNAME:MASTER%に顔を寄せてきた。
			;;押し倒す
			ELSEIF SELECTCOM == 350
				PRINTFORML %CALLNAME:TARGET%は何かの遊びと思っているかのように楽しげに笑いながら、%CALLNAME:MASTER%を迎え入れるように手を差し伸べる。
			ELSE
				PRINTFORML %CALLNAME:TARGET%は柔らかな微笑みを浮かべて、%CALLNAME:MASTER%へと小さく一歩、身を寄せた。
			ENDIF
		CASEELSE		;;快虐／性戯／誘惑／愛玩
			;;会話
			IF SELECTCOM == 300
				PRINTFORML %CALLNAME:TARGET%は艶やかな笑みを浮かべて、%CALLNAME:MASTER%に顔を寄せた。
				PRINTFORML ほんの少し首をひねり顔を寄せれば、%CALLNAME:TARGET%の頬に、唇にさえ、キスできそうなほど近くに……
			;;お茶を淹れる
			ELSEIF SELECTCOM == 301
				PRINTFORML 差し出された紅茶を受け取りながら、%CALLNAME:TARGET%は艶やかな笑みを浮かべた。
				PRINTFORML チラリと舌先を見せてから、そっとカップの縁に唇をそえて一口飲むと
				PRINTFORML %CALLNAME:TARGET%はわずかな紅茶に濡れて艶を増した唇を、薄めに開く。
			;;スキンシップ
			ELSEIF SELECTCOM == 302
				PRINTFORM 触れ合いを求める%CALLNAME:MASTER%の手をそっと取り、%CALLNAME:TARGET%は
				PRINTDATA
					DATAFORM 胸元に風を入れる様な仕草で、
					DATAFORM 悩ましげに眉を寄せながら、
				ENDDATA
				PRINTFORML 服の襟元を少し乱れさせた。
				PRINTFORM %CALLNAME:MASTER%の視線が自分の胸元へと向いていることを確認すると、%CALLNAME:TARGET%は
				PRINTDATA
					DATAFORM 満足げな
					DATAFORM 得意げな
					DATAFORM 艶めかしい
				ENDDATA
				PRINTFORML 笑みを浮かべる。
			;;食事か食事をふるまう
			ELSEIF SELECTCOM == 414 || SELECTCOM == 415
				PRINTDATAL
					DATAFORM %CALLNAME:TARGET%は唾液に濡れ光る舌先を出して、ちろりと手にした食べ物を舐めた。
					DATAFORM わざと%CALLNAME:MASTER%に見せつけるようにして、%CALLNAME:TARGET%はぽっかりと開けた口にゆっくりと食べ物を運んでいる。
				ENDDATA
			;;押し倒す
			ELSEIF SELECTCOM == 350
				PRINTFORML 自分を押し倒してきた%CALLNAME:MASTER%を見つめる%CALLNAME:TARGET%の瞳が、いっそう妖しく輝いた。
			ELSE
				PRINTFORML %CALLNAME:TARGET%は艶やかな笑みを浮かべて、%CALLNAME:MASTER%の目の前に立った。
				PRINTFORML %CALLNAME:MASTER%の視線が自分に向いていることを確かめると、
				;;スカート状の物をはいてる
				IF EQUIP:MASTER:9 || !TEQUIP:MASTER:0 > 3
					;;裳
					IF EQUIP:MASTER:スカート == 5
						PRINTFORML %CALLNAME:TARGET%は自らの裳をつまみ、その裾をゆっくりと引き上げ始める。
						PRINTFORML 足首まで隠していた裾がするすると上がっていき、白い脛が、膝が見えて、そして……
					;;スカート
					ELSE
					PRINTFORML %CALLNAME:TARGET%は自らのスカートをつまみ、その裾をゆっくりと引き上げ始める。
						;;ミニスカート
						IF EQUIP:MASTER:スカート == 1
							PRINTFORML 短いスカートから、すぐに太ももが露わになり、その上の……
						;;ロングスカート
						ELSEIF EQUIP:MASTER:スカート == 3
							PRINTFORML 長い丈のスカートから、するすると白い脛が、膝が見えて、そして……
						;;タイトスカート
						ELSEIF EQUIP:MASTER:スカート == 4
							PRINTFORML タイトスカートがずり上げられ、すぐに太ももが露わになり、その上の……
						;;スカートかワンピース
						ELSE
							PRINTFORML スカートが持ち上げられるにしたがって、柔らかそうな太ももまで見えてきて……
						ENDIF
					ENDIF
					PRINTFORML と言うところで、ピタリと止まった。
				;;スカートじゃない
				ELSE
					;;巨乳以上
					IF TALENT:TARGET:バストサイズ > 0
						PRINTFORML %CALLNAME:TARGET%はその豊かな胸を誇張するように、下側で腕を組むと、ぎゅっと寄せてみせる。
					ELSE
					;;そうでもない
						PRINTFORML その曲線を意識させるためか、%CALLNAME:TARGET%は自らの太ももから腰にかけて、するするとその手を這わせる。
					ENDIF
				ENDIF
			ENDIF
	ENDSELECT
	;;
	;;女の子の台詞を挟んで
	;;押し倒す
	IF SELECTCOM == 350
		TRYCALLFORM KOJO_WC_COUNTER_com350_K{NO:TARGET}	;;「押し倒す」のときの汎用
	ELSE
		TRYCALLFORM KOJO_WC_COUNTER10_K{NO:TARGET}	;;色っぽい仕草
	ENDIF
	;;
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1,2		;;虐待／冷淡
			PRINTFORM %CALLNAME:TARGET%の冷たい態度を当然の
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW ものと感じると同時に、裏切った後ろめたさに%CALLNAME:MASTER%は居たたまれなくなった。
			ELSEIF TALENT:MASTER:淫乱
				PRINTFORMW ものと感じながらも、つい「それ以上」を想像して、%CALLNAME:MASTER%はそっと熱い溜め息をこぼした。
			ELSEIF ABL:MASTER:露出癖 >= 2
				PRINTFORMW ものと感じながらも、その視線にもっと肌を舐められいと、%CALLNAME:MASTER%の体温がわずかに上昇した。
			ELSEIF ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW ものと感じながらも、%CALLNAME:MASTER%はその冷たいあしらいに興奮していた。
			ELSE
				PRINTFORMW ことと感じ、%CALLNAME:MASTER%は静かに頭を下げた。
			ENDIF
		CASE 7		;;同情
			PRINTFORM %CALLNAME:TARGET%の優しさ
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW に、%CALLNAME:MASTER%はかえって申し訳ない思いを強く感じていた。
			ELSEIF TALENT:MASTER:淫乱
				PRINTFORMW につい甘えて求めそうになる自分の淫蕩さを、%CALLNAME:MASTER%は心の中で戒めた。
			ELSE
				PRINTFORMW に戸惑ってしまい、%CALLNAME:MASTER%はつい視線を外して自らの足下を見てしまった。
			ENDIF
		CASE 8		;;子供
			PRINTFORM 自分にも無邪気に応じてくれる%CALLNAME:TARGET%に対して、%CALLNAME:MASTER%は
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW 申し訳なさが心を埋め尽くしていくのを感じた。
			ELSEIF TALENT:MASTER:淫乱
				PRINTFORMW 我が身と比較して申し訳なさと浅ましい興奮を同時に味わっていた。
			ELSEIF ABL:MASTER:露出癖 >= 2 || ABL:MASTER:マゾっ気
				PRINTFORMW 温かみと同時に、ほんの少し物足りなさを感じていた。
			ELSE
				PRINTFORMW それだけで心が温かなもので満たされるように感じていた。
			ENDIF
		CASEELSE		;;快虐／性戯／誘惑／愛玩
			PRINTFORM %CALLNAME:TARGET%の仕草につい頬を上気させ、%CALLNAME:MASTER%は
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW 自分のようなものの視線で汚してはいけないと、視線を下げた。
			ELSEIF TALENT:MASTER:淫乱
				PRINTFORMW 自分で良いのだろうかと思いつつも、視線をそらせなかった。
			ELSEIF ABL:MASTER:露出癖 >= 2
				PRINTFORMW 自分からも何かすべきかと迷ってしまった。
			ELSEIF ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW からかわれているのだと理解し、そのことにほんのりと興奮した。
			ELSE
				PRINTFORMW 戸惑いながら視線を足下へと向けた。
			ENDIF

	ENDSELECT
RETURN 1


