@EVENT_WC_COUNTER_MESSAGE_withPETTING(ARG)
;;ARG = 同席者のキャラ番号
	#DIM 尻狙い
	#DIM Ｂ狙い
	#DIM Ｃ狙い
	#DIM Ｖ狙い
	#DIM Ａ狙い
	#DIM 狙う部位

	尻狙い = 0
	Ｂ狙い = 0
	Ｃ狙い = 0
	Ｖ狙い = 0
	Ａ狙い = 0
	狙う部位 = 0

	;;尻にボーナス
		尻狙い += 1
	;;小さいと触りやすいので下半身狙い
	IF TALENT:ARG:体型 < TALENT:MASTER:体型
		尻狙い += 1
		Ｃ狙い += 1
		Ｖ狙い += 1
		Ａ狙い += 1
	ENDIF
	;;倒錯的だとＡ
	SIF TALENT:ARG:倒錯的
		Ａ狙い += 1
	;;小悪魔だとＣとＶ
	IF TALENT:ARG:小悪魔
		Ｃ狙い += 1
		Ｖ狙い += 1
	ENDIF
	;;幼児だとＢ
	SIF TALENT:ARG:幼児／幼児退行
		Ｂ狙い += 1
	;;幼稚だとＢ
	SIF TALENT:ARG:幼稚
		Ｂ狙い += 1
	;;あなたが非処女だとＶ
	SIF !TALENT:MASTER:処女
		Ｖ狙い += 1
	;;濡れてるほどＶ＆Ａ
	IF PALAM:MASTER:潤滑 >= PALAMLV:5
		Ｖ狙い += 2
		Ａ狙い += 1
	ELSEIF PALAM:MASTER:潤滑 >= PALAMLV:3
		Ｖ狙い += 1
	ELSEIF PALAM:MASTER:潤滑 == PALAMLV:0
		Ｖ狙い -= 1
	ENDIF
	;;開発度合いでＶ
	IF ABL:MASTER:Ｖ感覚 >= 5
		Ｖ狙い += 2
	ELSEIF ABL:MASTER:Ｖ感覚 >= 3
		Ｖ狙い += 1
	ELSEIF ABL:MASTER:Ｖ感覚 == 0
		Ｖ狙い -= 1
	ENDIF
	;;開発度合いでＡ
	IF ABL:MASTER:Ａ感覚 >= 5
		Ａ狙い += 2
	ELSEIF ABL:MASTER:Ａ感覚 >= 3
		Ａ狙い += 1
	ELSEIF ABL:MASTER:Ａ感覚 == 0
		Ａ狙い -= 1
	ENDIF
	;;あなたが絶倫だとＣ
	SIF TALENT:MASTER:絶倫
		Ｃ狙い += 1
	;;あなたが母乳体質だとＢ
	SIF TALENT:MASTER:母乳体質
		Ｂ狙い += 1
	;;あなたの乳サイズに従って、Ｂ
	SELECTCASE TALENT:MASTER:バストサイズ
		CASE -2
			Ｂ狙い -= 2
		CASE -1
			Ｂ狙い -= 1
		CASE 1
			Ｂ狙い += 1
		CASE 2
			Ｂ狙い += 2
	ENDSELECT
	;;あなたの弱いところを責めたがる
	SIF TALENT:MASTER:自慰狂い
		Ｃ狙い += 1
	SIF TALENT:MASTER:淫壺
		Ｖ狙い += 1
	SIF TALENT:MASTER:尻穴狂い
		Ａ狙い += 1
	SIF TALENT:MASTER:淫乳
		Ｂ狙い += 1
	SELECTCASE TALENT:MASTER:Ｂ感度
		CASE -2
			Ｂ狙い -= 2
		CASE -1
			Ｂ狙い -= 1
		CASE 1
			Ｂ狙い += 1
		CASE 2
			Ｂ狙い += 2
	ENDSELECT
	SELECTCASE TALENT:MASTER:Ｃ感度
		CASE -2
			Ｃ狙い -= 2
		CASE -1
			Ｃ狙い -= 1
		CASE 1
			Ｃ狙い += 1
		CASE 2
			Ｃ狙い += 2
	ENDSELECT
	SELECTCASE TALENT:MASTER:Ｖ感度
		CASE -2
			Ｖ狙い -= 2
		CASE -1
			Ｖ狙い -= 1
		CASE 1
			Ｖ狙い += 1
		CASE 2
			Ｖ狙い += 2
	ENDSELECT
	SELECTCASE TALENT:MASTER:Ａ感度
		CASE -2
			Ａ狙い -= 2
		CASE -1
			Ａ狙い -= 1
		CASE 1
			Ａ狙い += 1
		CASE 2
			Ａ狙い += 2
	ENDSELECT
	;;ズボン系と袴と裳は、尻以外の下半身にマイナス
	IF (EQUIP:MASTER:下半身上着１ >= 1 && EQUIP:MASTER:下半身上着１ <= 3) || EQUIP:MASTER:下半身上着２ == 1 || EQUIP:MASTER:スカート == 5
		Ｃ狙い -= 1
		Ｖ狙い -= 1
		Ａ狙い -= 1
	ENDIF
