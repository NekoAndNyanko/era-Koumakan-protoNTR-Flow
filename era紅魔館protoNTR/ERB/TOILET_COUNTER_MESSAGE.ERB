@WC_VA_FITTING(ARG:0,ARG:1,ARG:2)
;;ARG   = 入れる人
;;ARG:1 = 入れられる人
;;ARG:2 = 入れる穴（0=Ｖ／1=Ａ）
#FUNCTION
#DIM 挿入者
#DIM 被害者
#DIM 穴具合
	挿入者 = ARG:0
	被害者 = ARG:1
	IF ARG:2 == 1
		穴具合 = BASE:被害者:Ａ緩さ
	ELSE
		穴具合 = BASE:被害者:Ｖ緩さ
	ENDIF
	LOCAL:10 = 0

	SELECTCASE CFLAG:挿入者:Ｐ大きさ
		CASE -2	;;短小
			SELECTCASE 穴具合
				CASE IS >= 250	;;ゆるゆる／がばがば／ぽっかり
					LOCAL:10 = 0		;;かなり緩い
				CASE IS >= 100	;;きゅっきゅっ
					LOCAL:10 = 1		;;すんなり
				CASEELSE		;;ぎちぎち
					LOCAL:10 = 2		;;いい締め付け
			ENDSELECT
		CASE -1	;;小さめ
			SELECTCASE 穴具合
				CASE IS >= 450	;;がばがば／ぽっかり
					LOCAL:10 = 0		;;かなり緩い
				CASE IS >= 250	;;ゆるゆる
					LOCAL:10 = 1		;;すんなり
				CASE IS >= 100	;;きゅっきゅっ
					LOCAL:10 = 2		;;いい締め付け
				CASEELSE		;;ぎちぎち
					LOCAL:10 = 3		;;ちょっと狭い
			ENDSELECT
		CASE 0	;;普通
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 0		;;かなり緩い
				CASE IS >= 450	;;がばがば
					LOCAL:10 = 1		;;すんなり
				CASE IS >= 250	;;ゆるゆる
					LOCAL:10 = 2		;;いい締め付け
				CASE IS >= 100	;;きゅっきゅっ
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
		CASE 1	;;大きめ
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 1		;;すんなり
				CASE IS >= 450	;;がばがば
					LOCAL:10 = 2		;;いい締め付け
				CASE IS >= 250	;;ゆるゆる
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;きゅっきゅっ／ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
		CASE 2	;;馬並み
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 2		;;いい締め付け
				CASE IS >= 450	;;がばがば
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;ゆるゆる／きゅっきゅっ／ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
	ENDSELECT

	LOCAL:10 -= TALENT:被害者:体型
	SIF LOCAL:10 < 0
		LOCAL:10 = 0

RETURNF LOCAL:10


