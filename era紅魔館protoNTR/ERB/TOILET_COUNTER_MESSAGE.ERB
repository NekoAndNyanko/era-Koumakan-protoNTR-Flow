@EVENT_WC_COUNTER_MESSAGE_InsertITEM_OUT	;;挿入系アイテム外し描写
	LOCAL:20 = 0
	LOCAL:21 = 0
	LOCAL:22 = 0
	LOCAL:23 = 0
	LOCAL:24 = 0

	IF (TALENT:TARGET:管理人 || TARGET == 人物_レミリア)
		SIF CFLAG:MASTER:WC_バイブ装着 != 0
			LOCAL:21 = 1	;;バイブ外す
		SIF CFLAG:MASTER:WC_アナルバイブ装着 != 0
			LOCAL:23 = 1	;;アナルバイブ外す
		SIF CFLAG:MASTER:WC_ローターＶ装着 != 0
			LOCAL:22 = 1	;;ローターＶ外す
		SIF CFLAG:MASTER:WC_ローターＡ装着 != 0
			LOCAL:24 = 1	;;ローターＡ外す
	ELSE
		SIF CFLAG:MASTER:WC_バイブ装着 == 1 && CFLAG:MASTER:WC_道具Ｖ装着者 == TARGET
			LOCAL:21 = 1	;;バイブ外す
		SIF CFLAG:MASTER:WC_アナルバイブ装着 == 1 && CFLAG:MASTER:WC_道具Ａ装着者 == TARGET
			LOCAL:23 = 1	;;アナルバイブ外す
		SIF CFLAG:MASTER:WC_ローターＶ装着 == 1 && CFLAG:MASTER:WC_道具Ｖ装着者 == TARGET
			LOCAL:22 = 1	;;ローターＶ外す
		SIF CFLAG:MASTER:WC_ローターＡ装着 == 1 && CFLAG:MASTER:WC_道具Ａ装着者 == TARGET
			LOCAL:24 = 1	;;ローターＡ外す
	ENDIF

	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1,2,3		;;虐待／冷淡／快虐
			IF LOCAL:21 == 1	;;バイブ外す
				PRINTFORM バイブ
				LOCAL:20 = 1
			ENDIF
			IF LOCAL:23 == 1	;;アナルバイブ外す
				IF LOCAL:20 == 1
					PRINTFORM 、次いで
					LOCAL:20 = 0
				ENDIF
				PRINTFORM アナルバイブ
			LOCAL:20 = 1
			ENDIF
			IF LOCAL:22 == 1 || LOCAL:24 == 1	;;ローターＶＡ外す
				IF LOCAL:20 == 1
					PRINTFORM を、そして
					LOCAL:20 = 0
				ENDIF
				PRINTFORM ローター
			ENDIF
			PRINTFORML を、勢いよく引き抜いた。
			LOCAL:20 = 0
			;;アナルバイブ外す
			IF LOCAL:23 == 1 && ABL:MASTER:Ａ感覚 >= ABL:MASTER:Ｖ感覚
				PRINTFORM 肛門がめくられるような強い摩擦感に%CALLNAME:MASTER%は
				IF ABL:MASTER:Ａ感覚 >= 2
					PRINTFORML 甘く尾を引く震え声を漏らし続けた。
				ELSE
					PRINTFORML 引きつった声を上げて腰を跳ねさせた。
				ENDIF
			ELSEIF LOCAL:21 == 1	;;バイブ外す
				PRINTFORM 膣壁を強く擦られる刺激と、膣内を満たしていたものが消えた喪失感に、%CALLNAME:MASTER%は
				IF ABL:MASTER:Ｖ感覚 >= 2
					PRINTFORML 切なげな鼻声を漏らした。
				ELSE
					PRINTFORML 震える吐息を漏らした。
				ENDIF
			ELSE
				PRINTFORML いやらしい水音を立てて異物が引き抜かれた瞬間、%CALLNAME:MASTER%はびくりと体を震わせた。
			ENDIF
			LOCAL:20 = 0
		CASE 7,8		;;同情／子供
			IF LOCAL:21 == 1	;;バイブ外す
				PRINTFORM バイブをゆっくりと引き抜いていく。
				IF ABL:MASTER:Ｖ感覚 >= 2
					PRINTFORM 焦らされているような物足りない
				ELSE
					PRINTFORM 穏やかに異物が膣内を動いていく
				ENDIF
				PRINTFORM 感触に、%CALLNAME:MASTER%
				IF ABL:MASTER:Ｖ感覚 >= 2
					PRINTFORML の腰がついつい動きそうになる。
				ELSE
					PRINTFORML はごく小さな鼻声を漏らしてしまう。
				ENDIF
				PRINTFORM 静かにバイブが引き抜かれ
				IF ABL:MASTER:Ｖ感覚 >= 2
					PRINTFORML た後もしばらくは、%CALLNAME:MASTER%のヴァギナは物欲しげに小さく開いた口をひくひくと収縮させていた。
				ELSE
					PRINTFORML ると、%CALLNAME:MASTER%のヴァギナは二、三度ほど痙攣するような震えを見せてから、静かにその口を閉じた。
				ENDIF
				LOCAL:20 = 1
			ENDIF
			IF LOCAL:23 == 1	;;アナルバイブ外す
				IF LOCAL:20 == 1
					PRINTFORML 続いて、アナルバイブも同じようにゆっくりと引き抜かれ始めた。
					LOCAL:20 = 0
				ELSE
					PRINTFORML アナルバイブをゆっくりと引き抜いていった。
				ENDIF
				PRINTFORM 連なった球体が一つ一つ肛門を広げながら抜けていくたびに、%CALLNAME:MASTER%は
				IF ABL:MASTER:Ａ感覚 >= 2
					PRINTFORML 息を震わせ小さく喘ぐ。
				ELSE
					PRINTFORML ごく小さな呻き声を漏らす。
				ENDIF
				LOCAL:20 = 1
			ENDIF
			IF LOCAL:22 == 1 || LOCAL:24 == 1	;;ローターＶＡ外す
				IF LOCAL:20 == 1
					PRINTFORM %CALLNAME:TARGET%は、
					LOCAL:20 = 0
				ENDIF
				PRINTFORML 軽くローターのコードを引いて動いていないことを確かめると、そっと引き抜いた。
				PRINTFORML 微かな水音を立てて異物が引き抜かれた瞬間、びくりと%CALLNAME:MASTER%の体が震えた。
				LOCAL:20 = 1
			ENDIF
			LOCAL:20 = 0
		CASEELSE		;;性戯／誘惑／愛玩
			IF LOCAL:21 == 1	;;バイブ外す
				PRINTFORML バイブに手を添えると、ゆっくりと上下に動かし始めた。
				PRINTFORM %CALLNAME:MASTER%の
				IF ABL:MASTER:Ｖ感覚 >= 2
					PRINTFORM 甘く切なげな喘ぎと、いやらしく踊る様な
				ELSE
					PRINTFORM 小さく抑えた鼻声と、ぴくぴくと震える
				ENDIF
				PRINTFORML 腰の動きに、%CALLNAME:TARGET%は
				IF CFLAG:TARGET:WC_応答分類 == 6		;;愛玩
					PRINTFORML 愛おしげな笑みを浮かべる。
				ELSE
					PRINTFORML 薄く笑みを浮かべる。
				ENDIF
				LOCAL:20 = 1
			ENDIF
			IF LOCAL:23 == 1	;;アナルバイブ外す
				IF LOCAL:20 == 1
					PRINTFORM さらに、
				ENDIF
				PRINTFORM %CALLNAME:MASTER%のお尻がくわえ込んでいるアナルバイブ
				IF LOCAL:20 == 1
					PRINTFORM も、同時に動かし始めた。
					LOCAL:20 = 0
				ELSE
					PRINTFORML を、ゆっくりと引き抜き始めた。
				ENDIF
				PRINTFORM 連なっている球体が一つ、また一つと抜けていくたび、%CALLNAME:MASTER%
				IF ABL:MASTER:Ａ感覚 >= 2
					PRINTFORML の体に独特の快感が広がり、歓喜の震えが走る。
				ELSE
					PRINTFORML は息を詰めて排泄にも似たその異物感に耐える。
				ENDIF
				LOCAL:20 = 1
			ENDIF
			IF LOCAL:22 == 1 || LOCAL:24 == 1	;;ローターＶＡ外す
				IF LOCAL:20 == 1
					PRINTFORM %CALLNAME:TARGET%はもう一方の手で
					LOCAL:20 = 0
				ENDIF
				PRINTFORML コードをくんっくんっと軽く引き、%CALLNAME:MASTER%の中のローターを震わせた。
				PRINTFORML 軽く引かれるたびに、%CALLNAME:MASTER%が小さな震え声を漏らす。
				LOCAL:20 = 1
			ENDIF
			PRINTFORML %CALLNAME:TARGET%の手で最後まで%CALLNAME:MASTER%から快感を引き出しながら、小さな塊はじわじわとその体内から出て行った。
			LOCAL:20 = 0
	ENDSELECT
RETURN


