			ENDIF
		;;上記以外（Ｂが一番開発されてる）
		ELSE
			SETBIT CFLAG:TARGET:WC_箇所埋まり,4
			SETBIT CFLAG:TARGET:WC_箇所埋まり,5
		ENDIF
		;同席者のループは最大64まで
		FOR LOOP_CHR,1,CHARANUM
			SIF LOOP_CHR >= 64
				BREAK
			SIF TCVAR:TARGET:位置関係 == 1 || (GETBIT(CFLAG:WC_同席者,LOOP_CHR) && TCVAR:LOOP_CHR:位置関係 == 1)
				SETBIT CFLAG:TARGET:WC_箇所埋まり,0
			SIF TCVAR:TARGET:位置関係 == 2 || (GETBIT(CFLAG:WC_同席者,LOOP_CHR) && TCVAR:LOOP_CHR:位置関係 == 2)
				SETBIT CFLAG:TARGET:WC_箇所埋まり,1
		NEXT
	ENDIF

	;同席者のループは最大64まで
	FOR LOOP_CHR,1,CHARANUM
		SIF LOOP_CHR >= 64
			BREAK
		SIF GETBIT(CFLAG:WC_同席者,LOOP_CHR) && CFLAG:LOOP_CHR:好感度 >= 500
			TRYCALL EVENT_WC_COUNTER_MESSAGE_withPETTING(LOOP_CHR)
	NEXT
RETURN


