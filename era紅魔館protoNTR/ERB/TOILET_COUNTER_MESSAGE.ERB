@EVENT_WC_COUNTER_MESSAGE11								;;見つめる
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1,2		;;虐待／冷淡
			PRINTFORML %CALLNAME:TARGET%の冷たい視線と、%CALLNAME:MASTER%の視線がピタリと合わさる。
		CASE 7,8		;;同情／子供
			PRINTFORML %CALLNAME:TARGET%が、チラチラと何か言いたそうに%CALLNAME:MASTER%へと視線を向けている。
		CASEELSE		;;快虐／性戯／誘惑／愛玩
			PRINTFORML %CALLNAME:TARGET%の妖しく輝く目が、ピタリと%CALLNAME:MASTER%の目元を見つめている。
	ENDSELECT
	;;
	;;ここは台詞いらないかな？
	;;
	SELECTCASE CFLAG:TARGET:WC_応答分類
		CASE 1,2		;;虐待／冷淡
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORML 後ろめたさからか、つい%CALLNAME:MASTER%は俯いてしまった。
			ELSEIF ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW %CALLNAME:TARGET%の冷たい視線に、%CALLNAME:MASTER%の背筋にゾクリとしたものが走った。
			ELSE
				PRINTFORMW 何か自分に落ち度があったのだろうかと、%CALLNAME:MASTER%は落ち着かない気持ちになった。
			ENDIF
		CASE 7,8		;;同情／子供
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW その様子に答える言葉もなく、%CALLNAME:MASTER%はついつい俯いてしまった。
			ELSE
				PRINTFORMW なにかご用があるのなら、自分からうかがった方がいいのだろうかと、%CALLNAME:MASTER%は小さく首をかしげた。
			ENDIF
		CASEELSE		;;快虐／性戯／誘惑／愛玩
			PRINTFORML %CALLNAME:MASTER%の鼓動が一度、ドキリと強く打つ。
			PRINTFORM 何か命じてくださるのかもしれないと考え
			IF TALENT:MASTER:公衆便所 || TALENT:MASTER:NTR
				PRINTFORMW るが、後ろめたさからか%CALLNAME:MASTER%の足は動かなかった。
			ELSEIF TALENT:MASTER:淫乱 || ABL:MASTER:マゾっ気 >= 2
				PRINTFORMW た途端、%CALLNAME:MASTER%の股間が熱く疼いた。
			ELSE
				PRINTFORMW 、%CALLNAME:TARGET%のそばへ行くべきか%CALLNAME:MASTER%は少し迷った。
			ENDIF
	ENDSELECT
RETURN 1


