;服装セットに着替える：男装
;@return 1:着替え成功,0:着替え失敗
@CLOTHES_Preset_jentle(着用者,下着変更=1)
	#DIM 着用者
	#DIM 下着変更
	CALL CLOTHES_Preset_NUDE(着用者,下着変更)
	EQUIP:着用者:衣服着用 = 1
	EQUIP:着用者:靴 = 1
	EQUIP:着用者:靴下 = 1
	IF HAS_VAGINA(着用者)
		;女の子なら下着は女の子用を
		CALL CLOTHES_Change_Knickers(着用者,下着変更)
		;女の子ならサラシをつける
		IF 下着変更
			EQUIP:着用者:上半身下着２ = 5
		ENDIF
	ELSE
		;男の子ならトランクス
		IF 下着変更
			EQUIP:着用者:下半身下着２ = 101
		ENDIF
	ENDIF
	EQUIP:着用者:下半身上着１ = 1
	EQUIP:着用者:上半身上着１ = 4
	CFLAG:着用者:寝間着着用 = 0
RETURN 1

