;-------------------------------------------------
;ダブル素股
;奉仕、レズ系派生コマンド
;-------------------------------------------------
@SCOM10

TRYCALL COM417_is_Protected(PLAYER,TARGET,娼婦紋_恥丘)
SIF RESULT
	RETURN 0

;-------------------------------------------------
;COM別FLAGの設定
;-------------------------------------------------
;行為者
TCVAR:PLAYER:行為者 = TARGET
;TCVAR:0 射精対象フラグ（ビット0=TARGET 1=PLAYER 2=MASTER 3=ASSI）
;TCVAR:1 予備射精対象フラグ（ビットを使いたくない人用、CFLAG:100に翻訳されます）	（1=TARGET 2=PLAYER 3=MASTER 4=ASSI 5=TARGET+ASSI 6=MASTER+ASSI）
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）
;二人目
LOCAL = MASTER_POSE(2,1,1)

LOCALS = {TARGET}/{LOCAL}/
CSTR:PLAYER:10 += LOCALS
TCVAR:MASTER:2 = 6
;強度
TCVAR:MASTER:4 = 1400

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 40
DOWNBASE:気力 += 160

SOURCE:快Ｃ = 170
SOURCE:情愛 = 100
SOURCE:性行動 = 300
SOURCE:達成 = 100
SOURCE:屈従 = 600
SOURCE:逸脱 = 200
SOURCE:反感 = 200

;上のほうで計算した汚れデータ
SOURCE:不潔 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) / 3 * 10 + 60

;消費体力、気力
DOWNBASE:LOCAL:体力 += 40
DOWNBASE:LOCAL:気力 += 160

SOURCE:LOCAL:快Ｃ = 170
SOURCE:LOCAL:情愛 = 50
SOURCE:LOCAL:性行動 = 1500
SOURCE:LOCAL:達成 = 50
SOURCE:LOCAL:屈従 = 300
SOURCE:LOCAL:逸脱 = 200
SOURCE:LOCAL:反感 = 200

;上のほうで計算した汚れデータ
SOURCE:LOCAL:不潔 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, LOCAL) / 3 * 10 + 60
;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 400
SIF TALENT:LOCAL:自己愛 < 0 || TALENT:LOCAL:抵抗
	SOURCE:LOCAL:鬱屈 += 400
;-------------------------------------------------
;汚れの処理
;-------------------------------------------------
;射精に関係なく行われる処理はここから
;奴隷のＶ⇔調教者のＰの汚れが移動
STAIN:Ｖ |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:Ｖ
;助手のＶ⇔調教者のＰの汚れが移動
STAIN:ASSI:Ｖ |= STAIN:PLAYER:Ｐ
STAIN:PLAYER:Ｐ |= STAIN:ASSI:Ｖ
;奴隷のＶ⇔助手のＶの汚れが移動
STAIN:Ｖ |= STAIN:ASSI:Ｖ
STAIN:ASSI:Ｖ |= STAIN:Ｖ

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 3
ENDIF
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,LOCAL)
	EXP:LOCAL:(50 + HETEROSEX(TARGET,LOCAL)) += 3
	EXP:TARGET:(50 + HETEROSEX(TARGET,LOCAL)) += 3
ENDIF
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(LOCAL,PLAYER)
	EXP:LOCAL:(50 + HETEROSEX(LOCAL,PLAYER)) += 3
	EXP:PLAYER:(50 + HETEROSEX(LOCAL,PLAYER)) += 3
ENDIF


RETURN 1

@CAN_SCOM10(ARG)
SKIPDISP ARG
PRINTL ダブル素股
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 2
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:精液中毒 * 1
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快楽刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快楽刻印 * 2
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 2,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 恥じらい
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 快感の否定
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:性別嗜好 & 1 && !(HAS_VAGINA(PLAYER)) && (!TALENT:恋慕 || PLAYER != MASTER)
	PRINT  - 
	TFLAG:実行値 -= 7
	PRINT 男嫌い
	PRINTV '(,7,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;淫乱
IF TALENT:淫乱 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 3
	PRINT 淫乱
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 3
ENDIF
;娼婦
IF TALENT:淫乱 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 3
	PRINT 娼婦
	PRINTV '(,3,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;調教者がふたなり
IF GET_SEXNAME(PLAYER) == "ふたなり"
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 8
	PRINT ふたなり
	PRINTV '(,8,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;媚薬
IF TEQUIP:媚薬
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 媚薬
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;素股では汚れの影響が少なめ
LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) / 3
;汚れあり
IF LOCAL
	PRINT  - 
	TFLAG:実行値 -= LOCAL
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:汚臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,LOCAL,')
	TFLAG:実行値表示用フラグ = 1
ENDIF


;合計を表示(25以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 25
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0
RETURN 1

