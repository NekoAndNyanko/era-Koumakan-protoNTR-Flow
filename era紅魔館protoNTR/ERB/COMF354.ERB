@COM354
;邪魔しない
;%NTR_NAME(0)%と住人１人同席、かつ主人が空気の時の時間進行用コマンド
;会話
IF FLAG:25 == 0
	IF TALENT:NTR
		PRINTFORML %CALLNAME:TARGET%は%CALLNAME:MASTER%を無視して%NTR_NAME(0)%との会話を楽しんでいる
	ELSE
		PRINTFORML %CALLNAME:TARGET%に話しかけようとしたが%NTR_NAME(0)%に気おされて言葉が出てこない
	ENDIF
;キス
ELSEIF FLAG:25 == 1
	IF TALENT:NTR
		PRINTFORML 愕然としている%CALLNAME:MASTER%の反応を楽しみながら
		PRINTFORML %NTR_NAME(0)%とキスをしている%CALLNAME:TARGET%を呆然と眺めている
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%の目の前で%NTR_NAME(0)%とキスしている%CALLNAME:TARGET%を呆然と眺めている
		ELSE
			PRINTFORML キスしている%CALLNAME:TARGET%を止めようとするが%NTR_NAME(0)%に気おされて体が動かない
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 20 + 20 * ABL:MASTER:欲望 + 10 * TALENT:TARGET:浮気公認
;愛撫 or 胸愛撫
ELSEIF FLAG:25 == 2 || FLAG:25 == 3
	IF TALENT:NTR
		PRINTFORML %CALLNAME:MASTER%を無視して%NTR_NAME(0)%の愛撫を受け入れている%CALLNAME:TARGET%を呆然と眺めている
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%の目の前で%NTR_NAME(0)%に愛撫されている%CALLNAME:TARGET%を呆然と眺めている
		ELSE
			PRINTFORML %CALLNAME:TARGET%に愛撫している%NTR_NAME(0)%を止めようとするが気おされて体が動かない
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 30 + 30 * ABL:MASTER:欲望 + 20 * TALENT:TARGET:浮気公認
;フェラ or 69
ELSEIF FLAG:25 == 4 || FLAG:25 == 5 || FLAG:25 == 6
	IF TALENT:NTR
		PRINTFORML %CALLNAME:MASTER%を無視して%NTR_NAME(0)%に奉仕する%CALLNAME:TARGET%を呆然と眺めている
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%の目の前で%NTR_NAME(0)%に奉仕する%CALLNAME:TARGET%を呆然と眺めている
		ELSE
			PRINTFORML %CALLNAME:TARGET%にペニスを押し付けている%NTR_NAME(0)%を止めようとするが気おされて体が動かない
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 50 + 50 * ABL:MASTER:欲望 + 30 * TALENT:TARGET:浮気公認
;セーックス！おああなる
ELSE
	IF TALENT:NTR
		PRINTFORML %CALLNAME:MASTER%は見せ付けるかの様にまぐわっている二人を呆然と眺めている
	ELSE
		IF TALENT:TARGET:浮気公認
			PRINTFORML %CALLNAME:MASTER%は%CALLNAME:TARGET%と%NTR_NAME(0)%の情事に言葉が出てこない	
		ELSE
			PRINTFORML %CALLNAME:MASTER%は%CALLNAME:TARGET%と%NTR_NAME(0)%の情事にショックを受けて言葉が出てこない	
		ENDIF
	ENDIF
	SOURCE:MASTER:欲情 += 100 + 100 * ABL:MASTER:欲望 + 50 * TALENT:TARGET:浮気公認
ENDIF

LOSEBASE:気力 += 100
DOWNBASE:MASTER:気力 += 100

TIME += 5
RETURN 1

