;-------------------------------------------------
;足扱き
;奉仕系コマンド、レベル4
;-------------------------------------------------
@COM85
;-------------------------------------------------
;行為者
;-------------------------------------------------
TCVAR:PLAYER:行為者 = TARGET
;TCVAR:2 射精箇所フラグ（-1=コンドーム 1=膣内 2=アナル 3=手淫 4=口淫 5=パイズリ 6=素股 7=足コキ 8=体表 9=アナル奉仕 10=道具 11=触手）

LOCALS = {TARGET}/
CSTR:PLAYER:10 += LOCALS
TCVAR:PLAYER:2 = 7
;強度
TCVAR:PLAYER:4 = 1500
;奉仕快楽
TFLAG:30 = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;消費体力、気力
DOWNBASE:体力 += 10
DOWNBASE:気力 += 150

SOURCE:性行動 = 700
SOURCE:達成 = 500
SOURCE:屈従 = 800
SOURCE:逸脱 = 400
SOURCE:反感 = 400

;上のほうで計算した汚れデータ
SOURCE:不潔 = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) / 3 * 10 + 50

;-------------------------------------------------
;特殊ソース処理
;-------------------------------------------------
;抑圧、抵抗持ちには、抑鬱ソース追加
SIF TALENT:自己愛 < 0 || TALENT:抵抗
	SOURCE:鬱屈 += 600
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:(50 + HETEROSEX(TARGET,PLAYER)) += 4
	EXP:PLAYER:(50 + HETEROSEX(TARGET,PLAYER)) += 4
ENDIF
;サドは嗜虐快楽
SIF TALENT:サド || ABL:サドっ気
EXP:嗜虐快楽経験 ++

RETURN 1

@CAN_COM85(ARG)
SKIPDISP ARG
PRINTL 足扱き
;-------------------------------------------------
;実行判定処理
;-------------------------------------------------
TFLAG:実行値 = 0
TFLAG:実行値表示用フラグ = 0

;すべての命令に共通の要素を考慮
;(従順が高いと命令に従いやすいなど)
CALL COM_ORDER

;ABL:欲望
IF ABL:欲望
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:欲望 * 1
	PRINT 欲望
	PRINTV 'LV,ABL:欲望,'(,ABL:欲望 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:奉仕精神 * 4
	PRINT 奉仕精神
	PRINTV 'LV,ABL:奉仕精神,'(,ABL:奉仕精神 * 4,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;ABL:精液中毒
IF ABL:精液中毒
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += ABL:精液中毒 * 1
	PRINT 精液中毒
	PRINTV 'LV,ABL:精液中毒,'(,ABL:精液中毒 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

IF MARK:快楽刻印
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += MARK:快楽刻印 * 1
	PRINT 快楽刻印
	PRINTV 'LV,MARK:快楽刻印,'(,MARK:快楽刻印 * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情,5)
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += GETPALAMLV(PALAM:欲情,5) * 1
	PRINT 欲情
	PRINTV 'LV,GETPALAMLV(PALAM:欲情,5),'(,GETPALAMLV(PALAM:欲情,5) * 1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 恥じらい
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭鈍感
IF TALENT:汚臭耐性 > 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 1
	PRINT 汚臭鈍感
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;汚臭敏感
IF TALENT:汚臭耐性 < 0
	PRINT  - 
	TFLAG:実行値 -= 1
	PRINT 汚臭敏感
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;献身的
IF TALENT:献身的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 6
	PRINT 献身的
	PRINTV '(,6,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	TFLAG:実行値 -= 3
	PRINT 快感の否定
	PRINTV '(,1,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:性別嗜好 & 1 && !(HAS_VAGINA(PLAYER)) && (!TALENT:恋慕 || PLAYER != MASTER)
	PRINT  - 
	TFLAG:実行値 -= 8
	PRINT 男嫌い
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;恋慕
IF TALENT:恋慕 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 恋慕
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF
;親愛
IF TALENT:恋慕 > 1 && ASSIPLAY == 0
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 5
	PRINT 親愛
	PRINTV '(,5,')
	TFLAG:実行値表示用フラグ = 5
ENDIF

;倒錯的
IF TALENT:倒錯的
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 4
	PRINT 倒錯的
	PRINTV '(,4,')
	TFLAG:実行値表示用フラグ = 4
ENDIF
;サド
IF TALENT:サド
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 4
	PRINT サド
	PRINTV '(,4,')
	TFLAG:実行値表示用フラグ = 4
ENDIF
;小悪魔
IF TALENT:小悪魔
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 4
	PRINT 小悪魔
	PRINTV '(,4,')
	TFLAG:実行値表示用フラグ = 4
ENDIF
;調教者がふたなり
IF GET_SEXNAME(PLAYER) == "ふたなり"
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 8
	PRINT ふたなり
	PRINTV '(,8,')
	TFLAG:実行値表示用フラグ = 1
ENDIF

;足扱きでは汚れがそれほど影響しない
LOCAL = GET_STAINCOUNT(PLAYER, 部位_Ｐ, TARGET) / 3
;汚れあり
IF LOCAL
	PRINT  - 
	TFLAG:実行値 -= LOCAL
	PRINT 汚れあり
	;汚臭鈍感
	IF TALENT:汚臭耐性 > 0
		PRINT 、汚臭鈍感
	ELSEIF TALENT:汚臭耐性 < 0
		PRINT 、汚臭敏感
	ENDIF
	PRINTV '(,LOCAL,')
	TFLAG:実行値表示用フラグ = 1
ENDIF
;鍵を持っていると実行可能
IF FLAG:貞操帯鍵購入フラグ == TARGET
	SIF TFLAG:実行値表示用フラグ
		PRINT  + 
	TFLAG:実行値 += 100
	PRINT 貞操帯の鍵
	PRINTV '(,100,')
	TFLAG:実行値表示用フラグ = 100
ENDIF	


;合計を表示(22以上で実行)
PRINT  = 
PRINTV TFLAG:実行値

LOCAL = 22
SIF TFLAG:実行値 < LOCAL
	PRINT  < 
SIF TFLAG:実行値 == LOCAL
	PRINT  = 
SIF TFLAG:実行値 > LOCAL
	PRINT  > 
PRINT 実行値
PRINTV LOCAL

WAIT
SKIPDISP 0
;実行できない
SIF TFLAG:実行値 < LOCAL
	RETURN 0
RETURN 1
