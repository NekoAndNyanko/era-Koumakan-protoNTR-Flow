;体験の記録を表示
@PRINT_STATE_MEMORIAL(キャラ番号)
	#DIM キャラ番号
	PRINTFORML %"ファーストキス："+CSTR:キャラ番号:Ｍ初体験の相手%
	PRINTFORML %"処女喪失      ："+CSTR:キャラ番号:Ｖ初体験の相手%
	PRINTFORML %"アナル処女喪失："+CSTR:キャラ番号:Ａ初体験の相手%
RETURN 0

;-------------------------------------------------------------------------------
;体のなにがし
