@PRINT_STATE_MARK(奴隷)
	#DIM  奴隷
	#DIM  刻印番号
	#DIM  刻印番号最大値
	#DIM  セパレータ
	#DIM  改行
	#DIM  単行文字数
	#DIMS 合計文字列
	#DIMS 単行文字列
	IF 刻印番号最大値 == 0
		刻印番号最大値 = VARSIZE("MARK")
		FOR 刻印番号, 刻印番号最大値-1,-1,-1
			IF MARKNAME:刻印番号 == ""
				刻印番号最大値 = 刻印番号
				BREAK
			ENDIF
		NEXT
	ENDIF
	
	セパレータ = 0
	単行文字数 = 0
	合計文字列 = %""%
	FOR 刻印番号,0,刻印番号最大値
		IF MARK:奴隷:刻印番号 && (MARKNAME:刻印番号 != "")
			単行文字列 = %"  "*セパレータ+GET_MARK_LEVEL_OWNERNAME(奴隷,刻印番号)%
			IF (STRLENS(単行文字列)+単行文字数) > STRLENS(DRAWLINESTR)
				セパレータ = 0
				合計文字列 = %合計文字列+"<br>"+単行文字列%
				単行文字数 = STRLENS(単行文字列)
			ELSE
				セパレータ = 1
				合計文字列 = %合計文字列+単行文字列%
				単行文字数 += STRLENS(単行文字列)
			ENDIF
		ENDIF
	NEXT
	
	HTML_PRINT 合計文字列
	
RETURN 0
