;-------------------------------------------------------------------------------
;キャラクターの能力表示を行う
;@param ARG:0 表示するキャラクター番号
;@param ARG:1 宝珠を表示 1:する 0:しない
;@param ARG:2 何処から呼び出したか 1:SHOP 0:それ以外
;@return 1:xxx,0:xxx
@PRINT_STATE(奴隷, 宝珠表示, inSHOP)
	#DIM 奴隷
	#DIM 宝珠表示
	#DIM inSHOP
	PRINTL
	PRINTL
	PRINTL
	DRAWLINE
	PRINTS NAME:奴隷
	;成人以外の子供のみ表示される
	SIF (NO:奴隷 == 148 || NO:奴隷 == 149)
		PRINTFORM %子供の成長状態(奴隷)%
	PRINTFORML (好感度:{CFLAG:奴隷:好感度})
	PRINTFORM %NTR_NAME(0)%からの脅迫
	PRINTFORML (屈服度:{CFLAG:奴隷:屈服度})(脅迫度:{CFLAG:奴隷:脅迫度})
	;@出産まで後何日
	IF TALENT:奴隷:妊娠
		PRINTFORML (%N_T_R(CFLAG:奴隷:にんっしんっ - 1, 1)%の子を出産まで残り{CFLAG:奴隷:出産日 - DAY}日)
	ENDIF
	;出産まで後何日ここまで
	PRINT 体力:
	BAR BASE:奴隷:体力,MAXBASE:奴隷:体力,16
	PRINTFORM ({BASE:奴隷:体力,4}/{MAXBASE:奴隷:体力,4}) 
	PRINT 気力:
	BAR BASE:奴隷:気力,MAXBASE:奴隷:気力,16
	PRINTFORM ({BASE:奴隷:気力,4}/{MAXBASE:奴隷:気力,4})
	PRINTL
	PRINTL
	;行あわせ準備
	LOCAL:1 = LINECOUNT
	IF LOCAL == 0
		PRINTL ＝＝メイン情報１＝＝
		CALL PRINT_STATE_CLOTH(奴隷)
		CALL PRINT_STATE_ABL(奴隷)
		PRINTL
		CALL PRINT_STATE_TALENT(奴隷)
		IF 奴隷 != MASTER
			PRINTL
			PRINTL □ 刻印 □---------------------------------------------------------------------
			CALL PRINT_STATE_MARK(奴隷)
		ELSE
			PRINTL
		ENDIF
		CALL PRINT_STATE_EXP(奴隷)
		SIF 奴隷 != MASTER && !宝珠表示
			CALL PRINT_STATE_JUEL(奴隷)
		SIF 宝珠表示
			CALL PRINT_STATE_EX(奴隷)
		PRINTL
		TRYCALL PRINT_RELATION(奴隷)
	ELSEIF LOCAL == 1
		PRINTL ＝＝メイン情報２＝＝
		PRINTL □ 体 □-----------------------------------------------------------------------
		TRYCALL PRINT_STATE_BODY(奴隷)
		PRINTL
		PRINTL □ その他 □-------------------------------------------------------------------
		TRYCALL PRINT_STATE_MEMORIAL(奴隷)
		TRYCALL PRINT_STATE_PROTECTION(奴隷)
		;従者イベント,ERB
		IF GETBIT(FLAG:雑多設定,2) && 奴隷 == MASTER
			PRINTL
			FOR LOCAL:5, 1, 8
				SIF CFLAG:(LOCAL:5):従者イベント１ < 10
					CONTINUE
				TRYCALL PRINT_STATE_ZYUSYAEVENT(LOCAL:5)
			NEXT
		ENDIF
		;料理の状態
		IF FLAG:料理 && 奴隷 == MASTER
			PRINT 手持ち料理：
			IF TCVAR:MASTER:306 == 2
				PRINT ちょっと臭う
			ELSEIF TCVAR:MASTER:306 == 1
				PRINT 冷めた
			ENDIF
			PRINTFORML %GETDISHNAME(FLAG:料理)%
		ENDIF
		PRINTL
	ELSEIF LOCAL == 2
		PRINTL ＝＝親子情報１＝＝
		PRINTL
		PRINTL □ 兄弟関係 □-----------------------------------------------------------------
		TRYCALL PRINT_PARENT_3(奴隷)
		PRINTL
		PRINTL □ 親 □-----------------------------------------------------------------------
		TRYCALL PRINT_PARENT(奴隷)
		PRINTL
		PRINTL □ 子供 □---------------------------------------------------------------------
		TRYCALL PRINT_PARENT_2(奴隷)
		PRINTL
	ELSEIF LOCAL == 3
		PRINTL ＝＝親子情報２＝＝
		PRINTL
		PRINTL □ 出産表 □-------------------------------------------------------------------
		TRYCALL PRINT_PARENT_4(奴隷)
		PRINTL
	ENDIF
	;行あわせ処理
	IF LINECOUNT - LOCAL:1 < 40
		LOCAL:1 = 40 - (LINECOUNT - LOCAL:1)
		REPEAT LOCAL:1
			PRINTL
		REND
	ENDIF
	DRAWLINE
	IF LOCAL != 0
		PRINTC [1] 前のページへ
	ELSE
		PRINTC 　
	ENDIF
	IF LOCAL != 3
		PRINTC [2] 次のページへ
	ELSE
		PRINTC 　
	ENDIF
	PRINTL
	IF inSHOP
		IF 奴隷 != 0
			PRINTC [3] 前のキャラへ
		ELSE
			PRINTC 　
		ENDIF
		IF 奴隷 != CHARANUM-1
			PRINTC [4] 次のキャラへ
		ELSE
			PRINTC 　
		ENDIF
	ENDIF
	PRINTL
	PRINTC [9] 戻る
	PRINTL
	$LOOP_STATE
	ONEINPUT
	IF RESULT == 1 && LOCAL != 0
		LOCAL --
		RESTART
	ELSEIF RESULT == 2 && LOCAL != 3
		LOCAL ++
		RESTART
	ELSEIF RESULT == 3 && 奴隷 != 0
		奴隷 --
		RESTART
	ELSEIF RESULT == 4 && 奴隷 != CHARANUM-1
		奴隷 ++
		RESTART
	ELSEIF RESULT != 9
		GOTO LOOP_STATE
	ENDIF

;-------------------------------------------------------------------------------
;FIXME:関数の説明を書いてください
;@return 1:xxx,0:xxx
@PRINT_STATE_CLOTH(ARG)
	PRINTL
	PRINTL □ 服装 □---------------------------------------------------------------------
	VARSET LOCAL
	IF !EQUIP:ARG:衣服着用
		PRINTL 全裸
		RETURN 0
	ENDIF
	FOR LOCAL,1,20
		;下半身下着１
		IF ARG != MASTER && LOCAL == 5 && GETBIT(FLAG:雑多設定,1) && Unknown下着(ARG, LOCAL)
			IF GETBIT(FLAG:匂いフェチ,1) 
				PRINTFORML [%"？下着？ "%]
			ELSE
				PRINTFORML [%"？下着？ "%]
			ENDIF
			LOCAL:1 ++
			;下半身下着２
		ELSEIF  ARG != MASTER && LOCAL == 6 && GETBIT(FLAG:雑多設定,1) && Unknown下着(ARG, LOCAL)
			;装着の有無に限らず確認が取れていない場合何も表示されないようにする
			IF GETBIT(FLAG:匂いフェチ,1)
			ELSE
			ENDIF
			LOCAL:1 ++
		ELSEIF EQUIP:ARG:LOCAL
			IF GETBIT(FLAG:匂いフェチ,1) 
				PRINTFORML [%IRUINAME_SMELL(EQUIP:ARG:LOCAL)%%CLOTHNAME(LOCAL,EQUIP:ARG:LOCAL)%]
			ELSE
				PRINTFORML [%CLOTHNAME(LOCAL,EQUIP:ARG:LOCAL)%]
			ENDIF
			LOCAL:1 ++
		ENDIF
	NEXT
	SIF !LOCAL:1
		PRINTL 全裸

;-------------------------------------------------------------------------------
;FIXME:関数の説明を書いてください
;@return 1:xxx,0:xxx
@PRINT_STATE_ABL(ARG)
	PRINTL
	PRINTL □ 能力 □---------------------------------------------------------------------
	LOCAL:1 = 0
	FOR LOCAL,0,50
		SIF !STRLENS(ABLNAME:LOCAL)
			CONTINUE
		SIF LOCAL > 50 && ARG != MASTER
			CONTINUE
		;同性愛関係
		IF !(HAS_VAGINA(ARG)) && (LOCAL == 17 || LOCAL == 32)
			CONTINUE
		ELSEIF HAS_VAGINA(ARG) && (LOCAL == 18 || LOCAL == 33)
			CONTINUE
		ENDIF		
		PRINTFORM %ABLNAME:LOCAL,12%:{ABL:ARG:LOCAL,5,LEFT} 
		LOCAL:1 ++
		SIF LOCAL:1 % 4 == 0
			PRINTL 
	NEXT
	PRINTL
	PRINTL □ 性技 □---------------------------------------------------------------------
	LOCAL:1 = 0
	FOR LOCAL,50,60
		SIF !STRLENS(ABLNAME:LOCAL)
			CONTINUE
		SIF !(HAS_VAGINA(ARG)) && (LOCAL == 52 || LOCAL == 54)
			CONTINUE
		PRINTFORM %ABLNAME:LOCAL,12%:{ABL:ARG:LOCAL,5,LEFT} 
		LOCAL:1 ++
		SIF LOCAL:1 % 4 == 0
			PRINTL 
	NEXT

;-------------------------------------------------------------------------------
;FIXME:関数の説明を書いてください
;@return 1:xxx,0:xxx
@PRINT_STATE_TALENT(ARG)
	PRINTL □ 素質 □---------------------------------------------------------------------
	LOCAL:1 = 0
	LOCALS = ""
	;TALENT:200(名無し)は表示させない
	FOR LOCAL,0,200
		IF STRLENS(GET_TALENTNAME(LOCAL,TALENT:ARG:LOCAL))
			;童貞
			SIF LOCAL == 1 && !(HAS_PENIS(ARG))
				CONTINUE
			LOCALS += GET_TALENTNAME(LOCAL,TALENT:ARG:LOCAL)
			PRINTFORM [%GET_TALENTNAME(LOCAL,TALENT:ARG:LOCAL)%]
			IF STRLENS(LOCALS) > 50
				LOCALS = ""
				PRINTL
			ENDIF
		ENDIF
	NEXT

;-------------------------------------------------------------------------------
;娼婦紋の表示（あれば）
@PRINT_STATE_PROTECTION(ARG)
	VARSET LOCALS
	IF CFLAG:ARG:娼婦紋 == 0
		RETURN 0
	ENDIF
	IF GETBIT(CFLAG:ARG:娼婦紋, 娼婦紋_恥丘)
		IF LOCALS:0 != ""
			LOCALS:0 = %LOCALS:0 + "、"%
		ENDIF
		LOCALS:0 = %LOCALS:0 + "恥丘"%
	ENDIF
	IF GETBIT(CFLAG:ARG:娼婦紋, 娼婦紋_太股)
		IF LOCALS:0 != ""
			LOCALS:0 = %LOCALS:0 + "、"%
		ENDIF
		LOCALS:0 = %LOCALS:0 + "太腿"%
	ENDIF
	IF GETBIT(CFLAG:ARG:娼婦紋, 娼婦紋_左尻)
		IF LOCALS:0 != ""
			LOCALS:0 = %LOCALS:0 + "、"%
		ENDIF
		LOCALS:0 = %LOCALS:0 + "左尻"%
	ENDIF
	IF GETBIT(CFLAG:ARG:娼婦紋, 娼婦紋_胸元)
		IF LOCALS:0 != ""
			LOCALS:0 = %LOCALS:0 + "、"%
		ENDIF
		LOCALS:0 = %LOCALS:0 + "胸元"%
	ENDIF
	IF GETBIT(CFLAG:ARG:娼婦紋, 娼婦紋_右頬)
		IF LOCALS:0 != ""
			LOCALS:0 = %LOCALS:0 + "、"%
		ENDIF
		LOCALS:0 = %LOCALS:0 + "右頬"%
	ENDIF
	;子宮内が他人の精子で満たされているかどうかチェック
	CALL COM417_GetFillingRateByPublicSperm(ARG)
	SELECTCASE RESULT:1 * RESULT:0
		CASE IS >= 200
			PRINTFORML %"鮮やかなピンク色の娼婦紋("+LOCALS:0+")"%
		CASE 100 TO 199
			PRINTFORML %"ピンク色の娼婦紋("+LOCALS:0+")"%
		CASE 50 TO 99
			PRINTFORML %"赤色の娼婦紋("+LOCALS:0+")"%
		CASE 1 TO 49
			PRINTFORML %"赤紫色の娼婦紋("+LOCALS:0+")"%
		CASEELSE
			PRINTFORML %"紫色の娼婦紋("+LOCALS:0+")"%
	ENDSELECT
RETURN 0

;-------------------------------------------------
;関数名:GET_TALENTNAME
;概　要:素質名取得関数
;引　数:ARG:0…TALENT登録番号
;　　　:ARG:1…TALENTの中身
;戻り値:素質名
;備　考:文中関数
;素質の名前を返す。
;-------------------------------------------------
@GET_TALENTNAME(ARG,ARG:1)
	#FUNCTIONS
	SELECTCASE ARG
			;処女
		CASE 0
			SIF ARG:1
				RETURNF "処女"
			;童貞
		CASE 1
			SIF !ARG:1
				RETURNF "童貞"
			;性別
		CASE 2
			IF ARG:1 == 3
				RETURNF "ふたなり"
			ELSEIF ARG:1 == 2
				RETURNF "オトコ"
			ENDIF
			;淫乱
		CASE 4
			IF ARG:1 == 1
				RETURNF "淫乱"
			ELSEIF ARG:1 == 2
				RETURNF "娼婦"
			ENDIF
			;度胸
		CASE 10
			IF ARG:1 == -1
				RETURNF "臆病"
			ELSEIF ARG:1 == 1
				RETURNF "気丈"
			ENDIF
			;態度
		CASE 11
			IF ARG:1 == -1
				RETURNF "素直"
			ELSEIF ARG:1 == 1
				RETURNF "反抗的"
			ENDIF
			;応答
		CASE 12
			IF ARG:1 == -1
