		;V感覚が高いと絶頂経験加算（強絶頂扱い）
		;CALL NTR_V_EXP_PLUS(被害者)
	ENDIF
	;Ａ側処理
	IF -1 != 加害者A
		TEQUIP:被害者:Ａセックス = 加害者A
		IF 加害者A == 人物_訪問者
			CALL NTR_ADD_SURRENDER(被害者, (5 + RAND:5))
		ELSEIF 被害者 != MASTER
			CFLAG:被害者:好感度 += (5 + RAND:5)
		ELSE
			CFLAG:加害者A:好感度 += (5 + RAND:5)
		ENDIF
		BASE:(被害者):体力 = MAX(0,BASE:被害者:体力 - 50)
		;屈服度が高い場合は抵抗しないので気力消費が少ない？
		BASE:被害者:気力 = MAX(0,BASE:被害者:気力-(150-LIMIT(0,70,CFLAG:被害者:屈服度/10)))
		SOURCE:被害者:快Ａ = 400
		FOR iLoop,0,経過時間,5
			CALL 締り具合変動, 被害者, 3, 1
		NEXT
		;SOURCE:被害者:情愛 = 150
		;SOURCE:被害者:苦痛 = 500
		;SOURCE:被害者:露出 = 50
		;SOURCE:被害者:反感 = 300
		;SIF TALENT:被害者:自己愛 < 0 || TALENT:(被害者):抵抗
		;	SOURCE:被害者:鬱屈 += 300
		;SIF TALENT:被害者:貞操 > 0 && CFLAG:(被害者):屈服度 < 1000
		;	SOURCE:被害者:反感 += 1000
		;IF TALENT:被害者:処女 == 1
		;	SOURCE:被害者:苦痛 += 2000
		;	SOURCE:被害者:反感 += 3000
		;ELSEIF TALENT:被害者:処女 == 2
		;	SOURCE:被害者:苦痛 += 500
		;	SOURCE:被害者:反感 += 1000
		;ENDIF
		EXP:被害者:Ａ経験 ++
		EXP:被害者:Ａ性交経験 ++
		IF 加害者A == 人物_訪問者 && 訪問者_Ｐ大きさ == 2
			EXP:被害者:Ａ拡張経験 += 1
			PRINTFORML %CALLNAME:被害者%のアヌスは、%NTR_NAME(0)%の大きすぎるペニスによって拡張されている…
			IF ( TALENT:被害者:マゾ || ABL:被害者:マゾっ気 > 2 ) && EXP:被害者:Ａ拡張経験 < 450
				CUP:被害者:欲情 += 500
				CUP:被害者:苦痛 += 250
			ELSEIF EXP:被害者:Ａ拡張経験 < 450
				CUP:被害者:苦痛 += 500
			ENDIF
		ENDIF
		IF ABL:被害者:Ａ感覚 > RAND:5
			EXP:被害者:Ａ絶頂経験 ++
			EXP:被害者:絶頂経験 ++
		ENDIF
		;A射精処理
		CALL EJACULATION_A(被害者, 加害者A)
		IF 加害者A == 人物_訪問者
			;A処女喪失記録
			CALL LOST_VIRGIN_A(被害者, 加害者A)
			CALL NTR_ADD_SURRENDER(被害者, (15 + RAND:10))
		ELSE
			CALL LOST_VIRGIN_A(被害者, 加害者A)
			TALENT:加害者A:童貞 |= 2
		ENDIF
	ENDIF
RETURN

