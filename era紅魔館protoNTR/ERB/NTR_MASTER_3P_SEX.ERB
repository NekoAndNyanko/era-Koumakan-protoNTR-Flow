;女あなたと住人と訪問者の3P
;
;欲求不満度の意味
; 欲求不満度:1	主人公の欲求不満度(十の位が住人に対して、一の位が訪問者に対して)
; 欲求不満度:2	住人の欲求不満度(十の位が主人公に対して、一の位がもう一人に対して)
; 欲求不満度:3	訪問者の欲求不満度(同じく)

;性交回数の意味
; 性交回数:0 訪問者×主人公 V性交回数
; 性交回数:1 訪問者×主人公 A性交回数
; 性交回数:2 訪問者×住人 V性交回数
; 性交回数:3 訪問者×住人 A性交回数
; 性交回数:4 訪問者 素股射精回数
; 性交回数:5 住人×主人公 V性交回数
; 性交回数:6 住人×主人公 A性交回数
; 性交回数:7 主人公×住人 V性交回数
; 性交回数:8 主人公×住人 A性交回数
;
;FLAG:1840の意味　3人の行動
;   0 : 開始→キス
; 100 : キス（訪問者、住人）→愛撫、主人公参加 
; 101 : キス（訪問者、主人公）→愛撫、住人参加 
; 102 : キス（住人、主人公）→愛撫、訪問者参加 
; 200 : 愛撫→本番、訪問者×主人公＋住人
; 201 : 愛撫→本番、訪問者×住人＋主人公
; 202 : 愛撫→本番、住人＆主人公＋訪問者×主人公
; 203 : 愛撫→本番、住人＆主人公＋訪問者×住人
; 300 : 本番→本番2、住人×主人公 or 訪問者×住人
; 301 : 本番→本番2、主人公×住人 or 訪問者×主人公
; 302 : 本番→本番2、訪問者×主人公→訪問者×住人
; 303 : 本番→本番2、訪問者×住人→訪問者×主人公
; 304 : 本番→本番2、訪問者×主人公＋住人 or 訪問者×住人＋主人公
; 400 : 本番2→本番3、住人(双)×主人公→訪問者×主人公
; 401 : 本番2→本番3、訪問者×主人公→訪問者×住人
; 402 : 本番2→本番3、主人公(双)×住人→訪問者×住人
; 403 : 本番2→本番3、訪問者×主人公→訪問者×住人
; 404 : 本番2→本番3、訪問者×主人公→訪問者×主人公＆住人
; 405 : 本番2→本番3、訪問者×住人→訪問者×住人＆主人公
; 500 : 本番4、主人公＆住人 or 訪問者
; 501 : 本番4、訪問者＆主人公 or 住人
; 502 : 本番4、住人＆主人公 or 訪問者
; 503 : 本番4、訪問者＆主人公 or 住人
; 600 : 終了、主人公
; 601 : 終了、訪問者
; 602 : 終了、住人
; 603 : 終了、訪問者(素股含む)
;

@NTR_MASTER_3P_SEX(奴隷)
	#DIM 奴隷
	#DIM 欲求不満度,4
	#DIM 挿入部位,2
	#DIM 性交回数,9
	#DIM 経過時間
	#DIM 累積時間
	#DIM WK_A
	#DIM WK_B

	FLAG:1840 = 0
	欲求不満度:1 = 0
	欲求不満度:2 = 0
	欲求不満度:3 = 0
	; 二人状態と違って逃げられると困るのでワンループで全部済ませる
WHILE 1
IF FLAG:1840 == 0
	経過時間 = 10
	;   0 : 開始→キス
	CALL NTR_MASTER_3P_SEX_0(経過時間, 奴隷, 欲求不満度)
	TIME += 経過時間
ELSEIF FLAG:1840/100 == 1
	PRINTL
	経過時間 = RAND:10 + 10
	IF FLAG:1840 == 100
		; 100 : キス（訪問者、住人）→愛撫、主人公参加 
		CALL NTR_MASTER_3P_SEX_100(経過時間, 奴隷, 欲求不満度)
	ELSEIF FLAG:1840 == 101
		; 101 : キス（訪問者、主人公）→愛撫、住人参加 
		CALL NTR_MASTER_3P_SEX_101(経過時間, 奴隷, 欲求不満度)
	ELSE
		; 102 : キス（住人、主人公）→愛撫、訪問者参加 
		CALL NTR_MASTER_3P_SEX_102(経過時間, 奴隷, 欲求不満度)
	ENDIF
	TIME += 経過時間
ELSEIF FLAG:1840/100 == 2
	PRINTL
	経過時間 = RAND:10 + 20
	IF FLAG:1840 == 200
		; 200 : 愛撫→本番、訪問者×主人公＋住人
		CALL NTR_MASTER_3P_SEX_200(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ELSEIF FLAG:1840 == 201
		; 201 : 愛撫→本番、訪問者×住人＋主人公
		CALL NTR_MASTER_3P_SEX_201(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ELSEIF FLAG:1840 == 202 || FLAG:1840 == 203
		; 202 : 愛撫→本番、住人＆主人公＋訪問者×主人公
		; 203 : 愛撫→本番、住人＆主人公＋訪問者×住人
		; 202と203は同一関数、内部で分岐
		CALL NTR_MASTER_3P_SEX_202(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ENDIF
	TIME += 経過時間
ELSEIF FLAG:1840/100 == 3
	経過時間 = RAND:10 + 20
	IF FLAG:1840 == 300
		; 300 : 本番→本番、住人×主人公 or 訪問者×住人
		CALL NTR_MASTER_3P_SEX_300(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ELSEIF FLAG:1840 == 301
		; 301 : 本番→本番、主人公×住人 or 訪問者×主人公
		CALL NTR_MASTER_3P_SEX_301(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ELSEIF FLAG:1840 == 302
		; 302 : 本番→本番、訪問者×主人公→訪問者×住人
		CALL NTR_MASTER_3P_SEX_302(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ELSEIF FLAG:1840 == 303
		; 303 : 本番→本番、訪問者×住人→訪問者×主人公
		CALL NTR_MASTER_3P_SEX_303(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ELSEIF FLAG:1840 == 304
		; 304 : 本番→本番、訪問者×主人公＋住人 or 訪問者×住人＋主人公
		CALL NTR_MASTER_3P_SEX_304(経過時間, 奴隷, 欲求不満度, 挿入部位)
	ENDIF
ELSEIF FLAG:1840/100 == 4
	性交回数:0 = 0
	性交回数:1 = 0
	性交回数:2 = 0
	性交回数:3 = 0
	性交回数:4 = 0
	経過時間 = RAND:10 + 20
	IF FLAG:1840 == 400
		; 400 : 本番2→本番3、住人(双)×主人公→訪問者×主人公
		CALL NTR_MASTER_3P_SEX_400(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
	ELSEIF FLAG:1840 == 401
		; 401 : 本番2→本番3、訪問者×主人公→訪問者×住人
		CALL NTR_MASTER_3P_SEX_401(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
	ELSEIF FLAG:1840 == 402
		; 402 : 本番2→本番3、主人公(双)×住人→訪問者×住人
		CALL NTR_MASTER_3P_SEX_402(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
	ELSEIF FLAG:1840 == 403
		; 403 : 本番2→本番3、訪問者×主人公→訪問者×住人
		CALL NTR_MASTER_3P_SEX_403(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
	ELSEIF FLAG:1840 == 404
		; 404 : 本番2→本番3、訪問者×主人公→訪問者×主人公＆住人
		CALL NTR_MASTER_3P_SEX_404(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
	ELSEIF FLAG:1840 == 405
		; 405 : 本番2→本番3、訪問者×住人→訪問者×住人＆主人公
		CALL NTR_MASTER_3P_SEX_405(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
	ENDIF
ELSEIF FLAG:1840/100 == 5
	累積時間 = 0
	WHILE 1
		経過時間 = RAND:10 + 10
		累積時間 += 経過時間 
		IF FLAG:1840 == 500
			; 500 : 本番4、主人公＆住人 or 訪問者
			CALL NTR_MASTER_3P_SEX_500(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
		ELSEIF FLAG:1840 == 501
			; 501 : 本番4、訪問者＆主人公 or 住人
			CALL NTR_MASTER_3P_SEX_501(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
		ELSEIF FLAG:1840 == 502
			; 502 : 本番4、住人＆主人公 or 訪問者
			CALL NTR_MASTER_3P_SEX_502(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
		ELSEIF FLAG:1840 == 503
			; 503 : 本番4、訪問者＆主人公 or 住人
			CALL NTR_MASTER_3P_SEX_503(経過時間, 奴隷, 欲求不満度, 性交回数, 挿入部位)
		ENDIF
		IF FLAG:1840 >= 600
			; 次の段階に移行
			BREAK
		ENDIF
		IF 性交回数:0 + 性交回数:2 > 100
			PRINTL
			PRINTFORMW ループ回数が100回を越えました。エラーの可能性があるのでループを抜けます。
			BREAK
		ENDIF
	WEND
ELSEIF FLAG:1840/100 == 6
	PRINTFORMW 　　　　　　・
	PRINTFORMW 　　　　　　・
	PRINTFORMW 　　　　　　・
	IF FLAG:1840 == 600
		CALL NTR_MASTER_3P_SEX_600(累積時間, 奴隷, 欲求不満度, 性交回数)
	ELSEIF FLAG:1840 == 601
		CALL NTR_MASTER_3P_SEX_601(累積時間, 奴隷, 欲求不満度, 性交回数)
	ELSEIF FLAG:1840 == 602
		CALL NTR_MASTER_3P_SEX_602(累積時間, 奴隷, 欲求不満度, 性交回数)
	ELSEIF FLAG:1840 == 603
		CALL NTR_MASTER_3P_SEX_603(累積時間, 奴隷, 欲求不満度, 性交回数)
	ENDIF
	BASE:奴隷:怒り = 0
	BREAK
ENDIF
WEND
RETURN

