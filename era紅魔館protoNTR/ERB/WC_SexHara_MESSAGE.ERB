			PRINTFORM セーラー服
		;;ブレザー
		ELSEIF EQUIP:MASTER:上半身上着２ == 3
			PRINTFORM ブレザー
		;;上着
		ELSE
			PRINTFORM 上着
		ENDIF
		PRINTFORM の中へ手を差し入れると、
	ENDIF
	;;ブラジャー／スポーツブラ／ヌーブラ／サラシ
	IF (EQUIP:MASTER:上半身下着２ >= 1 && EQUIP:MASTER:上半身下着２ <= 3) || EQUIP:MASTER:上半身下着２ == 5
		;;ブラジャー
		IF EQUIP:MASTER:上半身下着２ == 1
			PRINTFORM ブラジャー
		;;スポーツブラ
		ELSEIF EQUIP:MASTER:上半身下着２ == 2
			PRINTFORM スポーツブラ
		;;ヌーブラ
		ELSEIF EQUIP:MASTER:上半身下着２ == 3
			PRINTFORM ヌーブラ
		;;サラシ
		ELSEIF EQUIP:MASTER:上半身下着２ == 5
			PRINTFORM サラシ
		ENDIF
		PRINTFORM をずらして
	ENDIF
RETURN


;;-------------------------------------------------------------------------------
;;屈服度の上昇値を決定
;;訪問者は、とりあえず男としておく
@WC_SURRENDER(ARG,ARG:1)
;;ARG   = 対象者
;;ARG:1 = プレイ内容
		;;= 1,ライト
		;;= 2,ソフト
		;;= 3,ハード
		;;= 4,特殊
#FUNCTION
#DIM LOOP_CHR
#DIM 対象者
#DIM プレイ内容
#DIM 上昇予定値
#DIM 訪問者凄味
	対象者 = ARG
	プレイ内容 = ARG:1
	上昇予定値 = 0
	訪問者凄味 = 0

	FOR LOOP_CHR,0,CHARANUM
		訪問者凄味 += MARK:LOOP_CHR:浮気快楽刻印
	NEXT

	上昇予定値 = プレイ内容 * 5
	上昇予定値 += TALENT:対象者:NTR + TALENT:対象者:公衆便所 - TALENT:対象者:度胸 - TALENT:対象者:態度 - TALENT:対象者:応答 - TALENT:対象者:プライド - TALENT:対象者:貞操

	SELECTCASE TALENT:対象者:性別嗜好
		CASE 1,3	;;男嫌い／人間不信
			上昇予定値 -= 1
	ENDSELECT

	IF !HAS_VAGINA(対象者)
		SELECTCASE ABL:対象者:ホモっ気
			CASE 0
				上昇予定値 -= 1
			CASE 1
				
			CASE 2
				上昇予定値 += 1
			CASE 3
				上昇予定値 += 2
			CASE 4
				上昇予定値 += 3
			CASEELSE	;;5以上
				上昇予定値 += 4
		ENDSELECT
	ENDIF

	IF CFLAG:対象者:WC_訪問者セクハラ失敗 == 1
		上昇予定値 += プレイ内容
		SIF 上昇予定値 < 0
			上昇予定値 = 0
	ELSE
		IF 対象者 == MASTER
			上昇予定値 += CFLAG:対象者:WC_訪問者受け入れ具合 + LIMIT(訪問者凄味 / 5,1,9)
		ELSE
			上昇予定値 += MARK:対象者:浮気快楽刻印 + LIMIT(訪問者凄味 / 5,1,9)
		ENDIF

		IF 上昇予定値 < プレイ内容
			SELECTCASE プレイ内容
				CASE 1
					上昇予定値 = プレイ内容 + 1
				CASE 2
					上昇予定値 = プレイ内容 + 2
				CASE 3
					上昇予定値 = プレイ内容 + 3
			ENDSELECT
		ENDIF
	ENDIF

RETURNF 上昇予定値


;;-------------------------------------------------------------------------------
;;プレイにおいて、敏感ゆえに責められる部分をチョイス
@WC_sensPOINT(ARG,ARG:1)
;;ARG   = 被害者
;;ARG:1 = 加害者
#FUNCTION
#DIM 被害者
#DIM 加害者
#DIM Ｃ点
#DIM Ｖ点
#DIM Ａ点
#DIM Ｂ点
#DIM ホモレズ判断
	;;=0,異性
	;;=1,同性(レズ)
	;;=2,同性(ゲイ)
#DIM 敏感箇所
	;;=0,Ｃ
	;;=1,Ｖ
	;;=2,Ａ
	;;=3,Ｂ

	被害者 = ARG
	加害者 = ARG:1
	Ｃ点 = 0
	Ｖ点 = 0
	Ａ点 = 0
	Ｂ点 = 0
	SELECTCASE 加害者
		CASE 人物_客,人物_訪問者
			;;オトコとして扱う
			IF !HAS_VAGINA(被害者)
				ホモレズ判断 = 2
			ELSE
				ホモレズ判断 = 0
			ENDIF
		CASEELSE
			ホモレズ判断 = HETEROSEX(加害者,被害者)
	ENDSELECT
	敏感箇所 = 0

	;;[EX]は、「イッたばかりで敏感」的な意味合い
	;;[EXP]は、「何度もイッた経験がある」的な意味合い
	Ｃ点 += ABL:被害者:Ｃ感覚 + TALENT:被害者:Ｃ感度 + EXP:被害者:Ｃ経験 + EX:被害者:Ｃ絶頂 + ABL:被害者:自慰中毒
	Ｖ点 += ABL:被害者:Ｖ感覚 + TALENT:被害者:Ｖ感度 + EXP:被害者:Ｖ経験 + EX:被害者:Ｖ絶頂
	Ａ点 += ABL:被害者:Ａ感覚 + TALENT:被害者:Ａ感度 + EXP:被害者:Ａ経験 + EX:被害者:Ａ絶頂 + ABL:被害者:サドっ気
	Ｂ点 += ABL:被害者:Ｂ感覚 + TALENT:被害者:Ｂ感度 + EXP:被害者:Ｂ経験 + EX:被害者:Ｂ絶頂

	;;おっぱいが並以上なら、Ｂに加点。「感じやすいから」ではなく、「目立つから狙われる」という意味合い
	SIF TALENT:被害者:バストサイズ >= 0 && HAS_VAGINA(被害者)
		Ｂ点 += (TALENT:被害者:バストサイズ + 1) * 2

	;;淫乱非処女なら、Ｖに加点
	SIF !TALENT:被害者:処女 && HAS_VAGINA(被害者) && TALENT:被害者:淫乱
		Ｖ点 += EXP:被害者:Ｖ性交経験 + EXP:被害者:浮気Ｖ性交経験

	;;オトコ同士の場合、加害者は被害者を牝扱いする
	IF ホモレズ判断 == 2
		Ａ点 += ABL:被害者:ホモっ気 + ABL:被害者:ゲイ中毒
		Ｃ点 -= ABL:被害者:ホモっ気 + ABL:被害者:ゲイ中毒
	ENDIF

	;;処女かオトコなら、Ｖの値をゼロに
	SIF TALENT:被害者:処女 || !HAS_VAGINA(被害者)
		Ｖ点 = 0

	IF Ｃ点 >= Ｂ点 && Ｃ点 >= Ｖ点 && Ｃ点 >= Ａ点
		敏感箇所 = 0
	ELSEIF Ｂ点 >= Ｖ点 && Ｂ点 >= Ａ点
		敏感箇所 = 3
	ELSEIF Ｖ点 >= Ａ点
		敏感箇所 = 1
	ELSE
		敏感箇所 = 2
	ENDIF

RETURNF 敏感箇所


