@WC_SexHara_MESSAGE_intercept_CHECK(ARG)
;;ARG=セクハラ内容
	#FUNCTION
	#DIM LOOP_CHR
	#DIM セクハラ内容
	#DIM 成立キャラ
	#DIM 行動者
	セクハラ内容 = ARG
	成立キャラ = 0
	行動者 = 0

	;同席者判定は最大64まで
	FOR LOOP_CHR,1,CHARANUM
		SIF LOOP_CHR >= 64
			BREAK
		SIF !GETBIT(CFLAG:MASTER:WC_同席者,LOOP_CHR)
			CONTINUE
		;;恋慕の時は、屈服度が好感度の２倍を超えてたら許可
		SIF TALENT:LOOP_CHR:恋慕 && CFLAG:LOOP_CHR:屈服度 > CFLAG:LOOP_CHR:好感度 * 2
			CONTINUE
		;;管理人の時は、屈服度が好感度を超えてたら許可
		SIF TALENT:LOOP_CHR:管理人 && CFLAG:LOOP_CHR:屈服度 > CFLAG:LOOP_CHR:好感度
			CONTINUE
		;;それ以外の時は、屈服度が好感度の半分を超えてたら許可
		SIF CFLAG:LOOP_CHR:屈服度 * 2 > CFLAG:LOOP_CHR:好感度
			CONTINUE
		SETBIT 成立キャラ,LOOP_CHR
	NEXT

	IF セクハラ内容 == 13
		FOR LOOP_CHR,1,CHARANUM
			SIF LOOP_CHR >= 64
				BREAK
			SIF LOOP_CHR > 7
				CONTINUE
			SIF GETBIT(成立キャラ,LOOP_CHR) && CFLAG:LOOP_CHR:WC_訪問者飲尿許可 == 3
				CLEARBIT 成立キャラ,LOOP_CHR
		NEXT
	ENDIF

	IF CFLAG:(CFLAG:MASTER:WC_管理人は誰):現在位置 == CFLAG:MASTER:現在位置 && CFLAG:MASTER:WC_管理人は誰 != 0
		SELECTCASE CFLAG:MASTER:WC_管理人は誰
			CASE 1
				SIF GETBIT(成立キャラ,(人物_美鈴))
					行動者 = 人物_美鈴
			CASE 2
				SIF GETBIT(成立キャラ,(人物_小悪魔))
					行動者 = 人物_小悪魔
			CASE 3
				SIF GETBIT(成立キャラ,(人物_パチュリー))
					行動者 = 人物_パチュリー
			CASE 4
				SIF GETBIT(成立キャラ,(人物_咲夜))
					行動者 = 人物_咲夜
			CASE 5
				SIF GETBIT(成立キャラ,(人物_レミリア))
					行動者 = 人物_レミリア
			CASE 6
				SIF GETBIT(成立キャラ,(人物_フラン))
					行動者 = 人物_フラン
			CASE 7
				SIF GETBIT(成立キャラ,(人物_子悪魔))
					行動者 = 人物_子悪魔
		ENDSELECT
	ENDIF

	IF 行動者 == 0 && TARGET < 64
		SIF GETBIT(成立キャラ,TARGET)
			行動者 = TARGET
	ENDIF

	IF 行動者 == 0
		IF GETBIT(成立キャラ,(人物_レミリア))
			行動者 = 人物_レミリア
		ELSEIF GETBIT(成立キャラ,(人物_フラン))
			行動者 = 人物_フラン
		ELSEIF GETBIT(成立キャラ,(人物_パチュリー))
			行動者 = 人物_パチュリー
		ELSEIF GETBIT(成立キャラ,(人物_咲夜))
			行動者 = 人物_咲夜
		ELSEIF GETBIT(成立キャラ,(人物_美鈴))
			行動者 = 人物_美鈴
		ELSEIF GETBIT(成立キャラ,(人物_小悪魔))
			行動者 = 人物_小悪魔
		ELSEIF GETBIT(成立キャラ,(人物_子悪魔))
			行動者 = 人物_子悪魔
		ENDIF
	ENDIF

	IF 行動者 == 0
		FOR LOOP_CHR,8,CHARANUM
			SIF LOOP_CHR >= 64
				BREAK
			SIF LOOP_CHR < 8
				CONTINUE
			SIF 行動者 != 0
				BREAK
			SIF GETBIT(成立キャラ,LOOP_CHR)
				行動者 = LOOP_CHR
				BREAK
		NEXT
	ENDIF

RETURNF 行動者


