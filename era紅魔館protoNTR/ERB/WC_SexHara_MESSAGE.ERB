;-----------------------------
@WC_SH_VA_FITTING(ARG,ARG:1,ARG:2)
;;ARG   = 入れる人のＰサイズ
		;;0 = 短小
		;;1 = 小さめ
		;;2 = 普通
		;;3 = 巨根
		;;4 = 訪問者並（訪問者さんは、通常表記の「馬並み」として扱う）
		;;5 = 訪問者超え
;;ARG:1 = 入れられる人
;;ARG:2 = 入れる穴（0=Ｖ／1=Ａ）
#FUNCTION
#DIM 挿入Ｐ
#DIM 被害者
#DIM 穴具合
	挿入Ｐ = ARG
	被害者 = ARG:1
	IF ARG:2 == 1
		穴具合 = BASE:被害者:Ａ緩さ
	ELSE
		穴具合 = BASE:被害者:Ｖ緩さ
	ENDIF
	LOCAL:10 = 0

	SELECTCASE 挿入Ｐ
		CASE 0	;;短小
			SELECTCASE 穴具合
				CASE IS >= 250	;;ゆるゆる／がばがば／ぽっかり
					LOCAL:10 = 0		;;かなり緩い
				CASE IS >= 100	;;きゅっきゅっ
					LOCAL:10 = 1		;;すんなり
				CASEELSE		;;ぎちぎち
					LOCAL:10 = 2		;;いい締め付け
			ENDSELECT
		CASE 1	;;小さめ
			SELECTCASE 穴具合
				CASE IS >= 450	;;がばがば／ぽっかり
					LOCAL:10 = 0		;;かなり緩い
				CASE IS >= 250	;;ゆるゆる
					LOCAL:10 = 1		;;すんなり
				CASE IS >= 100	;;きゅっきゅっ
					LOCAL:10 = 2		;;いい締め付け
				CASEELSE		;;ぎちぎち
					LOCAL:10 = 3		;;ちょっと狭い
			ENDSELECT
		CASE 2	;;普通
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 0		;;かなり緩い
				CASE IS >= 450	;;がばがば
					LOCAL:10 = 1		;;すんなり
				CASE IS >= 250	;;ゆるゆる
					LOCAL:10 = 2		;;いい締め付け
				CASE IS >= 100	;;きゅっきゅっ
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
		CASE 3	;;巨根
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 1		;;すんなり
				CASE IS >= 450	;;がばがば
					LOCAL:10 = 2		;;いい締め付け
				CASE IS >= 250	;;ゆるゆる
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;きゅっきゅっ／ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
		CASE 4	;;訪問者並み
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 2		;;いい締め付け
				CASE IS >= 450	;;がばがば
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;ゆるゆる／きゅっきゅっ／ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
		CASE 5	;;訪問者越え
			SELECTCASE 穴具合
				CASE IS >= 750	;;ぽっかり
					LOCAL:10 = 3		;;ちょっと狭い
				CASEELSE		;;がばがば／ゆるゆる／きゅっきゅっ／ぎちぎち
					LOCAL:10 = 4		;;きっつい
			ENDSELECT
	ENDSELECT

	LOCAL:10 -= TALENT:被害者:体型
	SIF LOCAL:10 < 0
		LOCAL:10 = 0

RETURNF LOCAL:10


