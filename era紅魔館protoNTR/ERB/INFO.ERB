;-------------------------------------------------------------------------------
;パラメーターを表示する
;@param ARG:0 表示するキャラクター番号
@INFO_PrintPalams(奴隷)
#DIM 奴隷
#DIM ParamNo
#DIM ParamLv
#DIM NextSource
#DIM DispCount
	DispCount = 0
	FOR ParamNo, 0, 101
		;パラメータ名が無いものは処理しない
		IF !STRLENS(PALAMNAME:ParamNo)
			CONTINUE
		ENDIF
		;何度も使うので現在のPALAMLVを保持
		ParamLv = GETPALAMLV(PALAM:奴隷:ParamNo,15)
		;パラメータ名と現在のレベルを表示
		PRINTFORM %" "+PALAMNAME:ParamNo + "Lv"%{ParamLv,2}
		PRINTFORM %"["%
		;PALAMLVが15以上なら上限扱い
		IF ParamLv >= 15
			PRINT ********
		ELSE
			;次のレベルまでどれくらいかを表示(0x00BB使いたいなあ。≫の半角サイズ)
			PRINTFORM %UNICODE(0x002A)*LIMIT(  8 * (PALAM:奴隷:ParamNo - PALAMLV:ParamLv) / (PALAMLV:(ParamLv+1) - PALAMLV:ParamLv), 0, 8)%
			PRINTFORM %UNICODE(0x002E)*LIMIT(8-8 * (PALAM:奴隷:ParamNo - PALAMLV:ParamLv) / (PALAMLV:(ParamLv+1) - PALAMLV:ParamLv), 0, 8)%
		ENDIF
		PRINTFORM %"]"%
		IF LOCAL:0 == 9 && PALAM:奴隷:ParamNo >= 20000
			;潤滑(9)が十分にある場合だけ特殊表示
			PRINTFORM %"摩擦 0 "%
		ELSE
			;それ以外はSI接頭語を付けて表示
			PRINTFORM %INFO_ToSIPrefixed(PALAM:奴隷:ParamNo),7%
		ENDIF
		DispCount++
		SIF DispCount % PRINTCPERLINE() == 0
			PRINTL 
	NEXT
	PRINTL
RETURN 0

;-------------------------------------------------------------------------------
;数値に対して「整数部3桁+少数部2桁+SI接頭語」を返す。
;@return 1:xxx,0:xxx
@INFO_ToSIPrefixed(ARG:0)
	#FUNCTIONS
	;Int64の最大値は9,223,372,036,854,775,807であるため、対応はエクサまでで良い。
	SELECTCASE ARG:0
		CASE IS >= 1000000000000000000
			RETURNF TOSTR(ARG:0,"###,,,,,,.00E")
		CASE IS >= 1000000000000000
			RETURNF TOSTR(ARG:0,"###,,,,,.00P")
		CASE IS >= 1000000000000
			RETURNF TOSTR(ARG:0,"###,,,,.00T")
		CASE IS >= 1000000000
			RETURNF TOSTR(ARG:0,"###,,,.00G")
		CASE IS >= 1000000
			RETURNF TOSTR(ARG:0,"###,,.00M")
		CASE IS >= 1000
			RETURNF TOSTR(ARG:0,"###,.00k")
		CASEELSE
			RETURNF TOSTR(ARG:0,"##0    ")
	ENDSELECT
RETURNF ""

