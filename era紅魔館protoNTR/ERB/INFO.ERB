;関数名:SORT_CFLAG
;CFLAG:ARGの昇順でRESULT:1にキャラ番号を代入する
;ARG CFLAGの番号
@SORT_CFLAG(ARG)
	VARSET LOCAL
	VARSET RESULT
	FOR LOCAL:10,1,CHARANUM
		LOCAL:1 = 0
		FOR LOCAL,1,CHARANUM
			SIF MATCH(RESULT,LOCAL)
				CONTINUE
			IF CFLAG:LOCAL:ARG >= LOCAL:1
				LOCAL:1 = CFLAG:LOCAL:ARG
				LOCAL:2 = LOCAL
			ENDIF
		NEXT
		RESULT:(LOCAL:10) = LOCAL:2
	NEXT

;-------------------------------------------------------------------------------
;キャラの態度(馴れ合い強度)を決定する。
;基本的に「そのキャラがどれくらい積極的に行動してくるか」を示すもの。
;強度 0:スキンシップまで ,1:軽いセクハラしてくる ,2:セクハラしてくる ,3+:押し倒してくる
;@return 0
@CHARA_ATTITUDE
	#DIM 馴れ合い上限値
	#DIM 好感度基準値
	#DIM 仮強度
	#DIM 奴隷
	;0:ループ用キャラ番号、1:馴れ合い基準値、2:最大馴れ合いランク制限
	VARSET 奴隷
	FOR 奴隷,1,CHARANUM
		;このタイミングで全員の好感度マイナスを０に補正
		CFLAG:奴隷:好感度 = MAX(0,CFLAG:奴隷:好感度)
		;同時に全員の屈服度マイナスを０に補正
		CFLAG:奴隷:屈服度 = MAX(0,CFLAG:奴隷:屈服度)
		;好感度補正、基準は0
		好感度基準値 = 0
		好感度基準値 += TALENT:奴隷:管理人 * 10                           ;管理人(10)
		好感度基準値 += TALENT:奴隷:恋慕 * 10                             ;恋慕(10)
		好感度基準値 += GET_MARK_LEVEL(奴隷,刻印番号_屈服刻印,MASTER) * 3 ;屈服刻印(LV×3)
		好感度基準値 += GET_MARK_LEVEL(奴隷,刻印番号_快楽刻印,MASTER) * 3 ;快楽刻印(LV×3)
		好感度基準値 += MIN(ABL:奴隷:親密 * 10, 50)                       ;親密(LV×10、最大50)
		好感度基準値 += MIN(ABL:奴隷:欲望 * 10, 50)                       ;欲望(LV×10、最大50)
		好感度基準値 += GETPALAMLV(PALAM:奴隷:欲情,5) * 5                 ;欲情(LV×5)
		好感度基準値 += GETPALAMLV(PALAM:奴隷:好意,5) * 5                 ;好意(LV×5)
		好感度基準値 += BASE:奴隷:ムード / 50                             ;ムード(最大30)
		好感度基準値 += (1000 - BASE:奴隷:理性) / 30                      ;理性の低さ(最大33)
		好感度基準値 += 5*GETBIT(CFLAG:奴隷:既成事実,1)                   ;既成事実:うふふ(5)、うふふはセクハラしやすさには影響しない。

		;基本的に馴れ合い強度の上限は1
		馴れ合い上限値 = 1
		馴れ合い上限値 += GETBIT(CFLAG:奴隷:既成事実,0) ;既成事実:告白がある場合は最大ランク+1
		馴れ合い上限値 += (TALENT:奴隷:淫乱 > 0)        ;淫乱/娼婦なら積極的にセクハラしてくる。上限ランクが増える。

		IF TALENT:MASTER:肉便器 || CFLAG:奴隷:MASTERの弱味
			;MASTERの弱味を握っているなら積極的にセクハラしてくる。基準値に+5して上限ランクを+2に
			;「あなた」が肉便器の場合は、弱味を握られているものとして扱う。基準値に+5して上限ランクを+2に
			好感度基準値 += 5
			馴れ合い上限値 += 2
		ELSE
			;弱みを握っていないなら欲情補正(0.5〜1.0倍)
			好感度基準値 = 好感度基準値 * (5000 + MIN(PALAM:奴隷:欲情,5000)) / 10000
		ENDIF

		IF CFLAG:奴隷:怒り || CFLAG:奴隷:睡眠
			;怒り or 睡眠なら最大ランクは０
			馴れ合い上限値 = 0
		ENDIF
		
		;馴れ合い基準値50(一線超えない場合60)毎に仮ランク１、NTRや公衆便所がある場合は仮ランクが減る
		;NTRや公衆便所(管理状態)でも、我慢できなくなれば的な表現。
		仮強度 = MAX( 好感度基準値 / (50+TALENT:奴隷:一線越えない*10) - (TALENT:奴隷:NTR + TALENT:奴隷:公衆便所) , 0 )
		;最大ランクまでに切り詰める
		CFLAG:奴隷:馴れ合い強度度 = MIN( 仮強度 , 馴れ合い上限値 )
		;DEBUGPRINTFORML %CALLNAME:奴隷+"(CFLAG:馴れ合い強度度"%{CFLAG:奴隷:馴れ合い強度度}%")"%
	NEXT
RETURN 0

;-------------------------------------------------------------------------------
;情事の発覚
@AFFAIR_DISCLOSURE(ARG, ARG:1)
	;ARG 入室してきたキャラの番号　ARG:1 0なら同室、1なら覗き
	VARSET LOCAL
	LOCAL:1 = IN_ROOM("MAX", CFLAG:MASTER:現在位置, "CFLAG", GETNUM(CFLAG, "うふふ"), 1)
	IF (LOCAL:1 && CFLAG:(LOCAL:1):うふふ) || CFLAG:MASTER:うふふ == 3
		IF ARG:1 == 1 && 天候(天候値) == "霧" && OPENPLACE(CFLAG:MASTER:現在位置) == 2
		ELSEIF ARG:1 == 1 && 天候(天候値) == "暗闇"
		ELSE
			DRAWLINE
			PRINTFORM %CALLNAME:ARG%に
			IF  CFLAG:MASTER:うふふ == 3
				PRINTFORMW %CALLNAME:PLAYER%の情事を見られてしまった！
			ELSE
				PRINTFORMW %CALLNAME:(LOCAL:1)%との情事を見られてしまった！
				;傷心判定
				SIF TALENT:ARG:恋慕 && !TALENT:ARG:親愛 && !TALENT:ARG:淫乱 && !TALENT:ARG:NTR && !TALENT:ARG:公衆便所
					LOCAL:4 = 1
			ENDIF
			IF LOCAL:4 == 1
				CALL 素質傷心取得, ARG
			ELSEIF !GETBIT(CFLAG:ARG:MASTERの弱味 , 1)
				PRINTFORMW %CALLNAME:ARG%に弱みを握られた
			ENDIF
			IF CFLAG:ARG:馴れ合い強度度 < 2
				IF !ARG:1
					;見た人
					CALL KOJO_EVENT(1,ARG,LOCAL:1)
					;見られた人（最も地位の高いTARGETで代表）
					CALL KOJO_EVENT(2,LOCAL:1,ARG)
				ENDIF
				CALL TOUCH_SET(0,0,0,1)
				DRAWLINE
				LOCAL:2 = 1
				;謹慎処分とか弱味とかの処理を入れる予定
			ELSEIF !ARG:1
				PRINTFORMW %CALLNAME:ARG%は負けじと%CALLNAME:MASTER%に体を摺り寄せてきた
				CFLAG:ARG:うふふ = CFLAG:(LOCAL:1):うふふ
				;見た人
				CALL KOJO_EVENT(3,ARG,LOCAL:1)
				;見られた人（最も地位の高いTARGETで代表）
				CALL KOJO_EVENT(4,LOCAL:1,ARG)
				DRAWLINE
				RETURN 0
			ENDIF
			SIF !LOCAL:4 && !GETBIT(CFLAG:ARG:MASTERの弱味 , 1)
				SETBIT CFLAG:ARG:MASTERの弱味 ,1
		ENDIF
	ENDIF
	IF LOCAL:2
		FOR LOCAL,0,CHARANUM
			CFLAG:LOCAL:うふふ = 0
		NEXT
		RETURN 1
	ENDIF
	;通常遭遇
	;こちらからも見える位置にいる、もしくは同室
	IF !CFLAG:ARG:睡眠 && (!ARG:1 || CAN_MOVE(CFLAG:MASTER:現在位置, CFLAG:ARG:現在位置) == 2)
		;さっきから居るのは分かっていたか否か
		IF CFLAG:ARG:前ターン位置 == CFLAG:ARG:現在位置
			CALL 自慰中断, ARG
			IF CAN_MOVE(CFLAG:MASTER:前ターン位置, CFLAG:ARG:現在位置) == 2
				PRINTFORML %CALLNAME:ARG+"が"+GETPLACENAME(CFLAG:ARG:現在位置)+"に居る"%
				SIF !TEQUIP:ARG:0 == 0 && TEQUIP:ARG:1 == 0
					PRINTFORML %CALLNAME:ARG+"は半裸の上半身を晒け出している…"%
				SIF TEQUIP:ARG:0 == 0 && !TEQUIP:ARG:1 == 0
					PRINTFORML %CALLNAME:ARG+"は半裸の下半身を晒け出している…"%
				SIF TEQUIP:ARG:0 == 0 && TEQUIP:ARG:1 == 0
					PRINTFORML %CALLNAME:ARG+"は服を着ていない…"%
				CALL INFO_PrintPortrayal(ARG)
			ELSE
				PRINTFORML %CALLNAME:ARG+"が"+GETPLACENAME(CFLAG:ARG:現在位置)+"に居た"%
				SIF !TEQUIP:ARG:0 == 0 && TEQUIP:ARG:1 == 0
					PRINTFORML %CALLNAME:ARG+"は半裸の上半身を晒け出している…"%
				SIF TEQUIP:ARG:0 == 0 && !TEQUIP:ARG:1 == 0
					PRINTFORML %CALLNAME:ARG+"は半裸の下半身を晒け出している…"%
				SIF TEQUIP:ARG:0 == 0 && TEQUIP:ARG:1 == 0
					PRINTFORML %CALLNAME:ARG+"は服を着ていない…"%
				CALL INFO_PrintPortrayal(ARG)
			ENDIF
			WAIT
			LOCAL:3 = 1
			;さっきから居るのは分かってて相手が移動した場合は別の場所で表示が出る
		ELSEIF CAN_MOVE(CFLAG:MASTER:前ターン位置, CFLAG:ARG:前ターン位置) < 2 && CFLAG:ARG:前ターン位置 != CFLAG:MASTER:前ターン位置
			PRINTFORMW %CALLNAME:ARG%が%GETPLACENAME(CFLAG:ARG:現在位置)%に来た
			SIF !TEQUIP:ARG:0 == 0 && TEQUIP:ARG:1 == 0
				PRINTFORML %CALLNAME:ARG+"は半裸の上半身を晒け出している…"%
			SIF TEQUIP:ARG:0 == 0 && !TEQUIP:ARG:1 == 0
				PRINTFORML %CALLNAME:ARG+"は半裸の下半身を晒け出している…"%
			SIF TEQUIP:ARG:0 == 0 && TEQUIP:ARG:1 == 0
				PRINTFORML %CALLNAME:ARG+"は服を着ていない…"%
			LOCAL:3 = 2
		ENDIF
		IF !ARG:1 && LOCAL:3
			IF CFLAG:ARG:情事目撃 && CFLAG:(CFLAG:ARG:情事目撃):現在位置 != CFLAG:ARG:現在位置
				PRINTFORMW %CALLNAME:ARG%に先の%CALLNAME:(CFLAG:ARG:情事目撃)%との一件について問い詰められた…
				CALL KOJO_EVENT(0, ARG, 2)
				CFLAG:ARG:情事目撃 = 0
			ELSE
				CALL KOJO_EVENT(0, ARG, LOCAL:3 - 1)
			ENDIF
		ENDIF
	ENDIF

