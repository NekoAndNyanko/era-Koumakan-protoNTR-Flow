;-------------------------------------------------------------------------------
;対象の描写を表示
@INFO_PrintPortrayal(奴隷)
	#DIM 奴隷
	#DIMS 描写文章
	描写文章 = %""%
	IF GETBIT(CFLAG:奴隷:娼婦紋, 娼婦紋_右頬)
		IF 描写文章 != ""
			描写文章 = %描写文章+"、"%
		ENDIF
		CALL COM417_GetFillingRateByPublicSperm(奴隷)
		SELECTCASE RESULT:1 * RESULT:0
			CASE IS >= 200
				描写文章 = %描写文章+"頬には鮮やかなピンク色の娼婦紋が見え"%
			CASE 100 TO 199
				描写文章 = %描写文章+"頬にはピンク色の娼婦紋が見え"%
			CASE 50 TO 99
				描写文章 = %描写文章+"頬には赤色の娼婦紋が見え"%
			CASE 1 TO 49
				描写文章 = %描写文章+"頬には赤紫色の娼婦紋が見え"%
			CASEELSE
				描写文章 = %描写文章+"頬には紫色の娼婦紋が見え"%
		ENDSELECT
	ENDIF
	IF TALENT:奴隷:母乳体質 && BASE:奴隷:母乳 > MAXBASE:奴隷:母乳
		IF 描写文章 != ""
			描写文章 = %描写文章+"、"%
		ENDIF
		SELECTCASE TALENT:奴隷:バストサイズ
			CASE IS > 0
				描写文章 = %描写文章+"豊満な胸は母乳で張ってい"%
			CASE IS == 0
				描写文章 = %描写文章+"胸は母乳で張ってい"%
			CASE IS < 0
				描写文章 = %描写文章+"控えめな胸は母乳で張ってい"%
			CASEELSE
		ENDSELECT
	ENDIF
	IF 描写文章 != ""
		描写文章 = %CALLNAME:奴隷+"の"+描写文章+"る"%
	ENDIF
	PRINTFORML %描写文章%
RETURN 0
