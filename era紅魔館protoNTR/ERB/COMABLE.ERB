	;猫舌の調教者の場合は技巧Lv2以上必要
	SIF TALENT:PLAYER:猫舌 && ABL:PLAYER:技巧 < 2
		RETURN 0

	;下半身着衣
	SIF TEQUIP:0 & 4 || TEQUIP:0 & 2
