	Ａ装入者 = MASTER_POSE(7,1,1)	; MASTERのＰ→Ａ装入者取得
	IF Ｖ装入者
		対象者 = Ｖ装入者
	ELSEIF Ａ装入者
		対象者 = Ａ装入者
	ENDIF
	SIF !TFLAG:100
		RETURN 0
	SIF GLOBAL_COMABLE(71)
		RETURN RESULT
	;調教者がペニス所有時、ある程度勃起してないとダメ
	SIF HAS_PENIS(PLAYER) && BASE:PLAYER:勃起 < 1000 && MASTER_POSE(6,1,1) == 0
		RETURN 0
	;TARGET以外の誰かに挿入してないとダメ
	SIF !対象者 || 対象者 == TARGET
		RETURN 0
	;相手が処女で、従順4以下・レズっ気4以下の場合はできない（サドならＯＫ）
	IF 対象者 && TALENT:対象者:処女
		SIF (ABL:従順 <= 4 || ABL:レズっ気 <= 4) && TALENT:サド == 0
			RETURN 0
	ENDIF
	;潤滑不足で、従順3以下・レズっ気3以下のASSIの場合はできない（サドならＯＫ）
	IF 対象者 && PALAM:対象者:潤滑 < PALAMLV:2
		SIF (ABL:従順 <= 3 || ABL:レズっ気 <= 3) && TALENT:サド == 0
			RETURN 0
	ENDIF
	;オトコだとダメ
	SIF 対象者 && !(TALENT:対象者:2 & 1)
		RETURN 0
	;ペニスがないとだめ
	SIF !HAS_PENIS(TARGET) && ITEM:PBAND == 0
		RETURN 0
	;バイブ装着中はダメ
	SIF TEQUIP:対象者:13
		RETURN 0
	;膣鏡を使用しているとダメ
	SIF TEQUIP:対象者:膣鏡
		RETURN 0
	;アナルバイブ使用中は不可
	SIF TEQUIP:対象者:14
		RETURN 0
	;アナルビーズ使用中は不可
	SIF TEQUIP:対象者:15
		RETURN 0
	;普通の浣腸使用中は不可
	SIF TEQUIP:対象者:21
		RETURN 0
	;バルーン使用中は不可
	SIF TEQUIP:対象者:22
		RETURN 0
	;電極使用中は不可
	SIF TEQUIP:対象者:23
		RETURN 0
	;新妻プレイ中はダメ
	SIF TEQUIP:対象者:33 || TEQUIP:33
		RETURN 0
	;体格差がある場合は主人が禁断の知識持ちじゃないとダメ
	IF TALENT:MASTER:禁断の知識 == 0
		SIF ABS(TALENT:体型 - TALENT:対象者:体型) > 2
			RETURN 0
		SIF ABS(TALENT:体型 - TALENT:対象者:体型) > 2
			RETURN 0
	ENDIF
	;一定のＡ経験が必要
	SIF EXP:対象者:2 < EXPLV:3
		RETURN 0
	;下半身着衣
	SIF TEQUIP:対象者:0 > 1
		RETURN 0
RETURN 1


