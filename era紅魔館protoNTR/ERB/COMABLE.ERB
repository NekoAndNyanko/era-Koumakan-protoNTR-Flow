;0番台,０．愛撫系コマンド
;-------------------------------------------------
@COM_ABLE0
;愛撫実行判定
	SIF !TFLAG:100
		RETURN 0
	;一括管理
	SIF GLOBAL_COMABLE(0)
		RETURN RESULT
RETURN 1

@COM_ABLE1
;クンニ実行判定
	SIF !TFLAG:100
		RETURN 0
	;一括管理
	SIF GLOBAL_COMABLE(1)
		RETURN RESULT
	;膣鏡を使用しているとダメ
	SIF TEQUIP:膣鏡
		RETURN 0
	;性器が汚れているとMASTERの場合クンニはできない（調教者が汚れ無視か愛液、母乳のよごれならＯＫ）
	SIF STAIN:Ｖ >= 汚れ_ペニス && (STAIN:Ｖ != 汚れ_母乳 && STAIN:Ｖ != 汚れ_母乳_愛液) && ASSIPLAY == 0 && TALENT:MASTER:汚臭耐性 < 2
		RETURN 0
	;性器が汚れており、汚臭敏感で従順3以下のASSIの場合クンニはできない（調教者が汚れ無視か愛液、母乳のよごれならＯＫ）
	IF STAIN:Ｖ >= 汚れ_ペニス && (STAIN:Ｖ != 汚れ_母乳 && STAIN:Ｖ != 汚れ_母乳_愛液) && ASSIPLAY
		SIF ABL:ASSI:従順 <= 3 && TALENT:ASSI:汚臭耐性 < 0
			RETURN 0
	ENDIF
	;下半身着衣
	SIF TEQUIP:0 & 4
		RETURN 0
RETURN 1

@COM_ABLE2
;フェラする実行判定
	SIF !TFLAG:100
		RETURN 0
	;一括管理
	SIF GLOBAL_COMABLE(2)
		RETURN RESULT
	;性器が汚れているとMASTERの場合フェラするはできない（調教者が汚れ無視か愛液、Ｐ、母乳のよごれならＯＫ）
	SIF STAIN:Ｐ >= 汚れ_精液 && (STAIN:Ｐ != 汚れ_母乳_ペニス && STAIN:Ｐ != 汚れ_母乳_ペニス_愛液) && ASSIPLAY == 0 && TALENT:MASTER:汚臭耐性 < 2
		RETURN 0
	;性器が汚れており、汚臭敏感で従順3以下のASSIの場合フェラはできない（調教者が汚れ無視か愛液、Ｐ、母乳のよごれならＯＫ）
	IF STAIN:Ｐ >= 汚れ_精液 && (STAIN:Ｐ != 汚れ_母乳_ペニス && STAIN:Ｐ != 汚れ_母乳_ペニス_愛液) && ASSIPLAY
		SIF ABL:ASSI:従順 <= 3 && TALENT:ASSI:汚臭耐性 < 0
			RETURN 0
	ENDIF
	;猫舌の調教者の場合は技巧Lv2以上必要
	SIF TALENT:PLAYER:猫舌 && ABL:PLAYER:技巧 < 2
		RETURN 0
	;オトコかふたなりじゃないとダメ
	SIF !HAS_PENIS(TARGET)
		RETURN 0
	;対象者がペニス用貞操帯を着けていたらダメ
	SIF CFLAG:TARGET:ペニス用貞操帯着用 != 0
		RETURN 0
	;下半身着衣
	SIF TEQUIP:0 & 4
		RETURN 0
	;オナホール使用中はダメ
	SIF TEQUIP:12
		RETURN 0
RETURN 1

@COM_ABLE3
;指挿入れ実行判定
	IF !TFLAG:100
		RETURN 0
	ENDIF
	;一括管理
	IF GLOBAL_COMABLE(3)
		RETURN RESULT
	ENDIF
	;オトコだとダメ
	IF !HAS_VAGINA(TARGET)
		RETURN 0
	ENDIF
	;潤滑不足で、従順3以下・レズっ気3以下のASSIの場合アナル愛撫はできない（サドならＯＫ）
	IF PALAM:潤滑 < PALAMLV:2 && ASSIPLAY
		SIF (ABL:ASSI:従順 <= 3 || ABL:ASSI:レズっ気 <= 3) && !TALENT:ASSI:サド
			RETURN 0
	ENDIF

	;膣鏡を使用しているとダメ
	SIF TEQUIP:膣鏡
		RETURN 0
	;下半身着衣
	SIF TEQUIP:0 & 4
		RETURN 0
RETURN 1

