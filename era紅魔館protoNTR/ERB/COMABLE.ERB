;-------------------------------------------------
;60番台,３．セックス系コマンド
;-------------------------------------------------
@COM_ABLE60
;正常位実行判定
	SIF !TFLAG:100
		RETURN 0
	;一括管理
	SIF GLOBAL_COMABLE(60)
		RETURN RESULT
	;調教者がペニス所有時、ある程度勃起してないとダメ
	SIF HAS_PENIS(PLAYER) && BASE:PLAYER:勃起 < 1000 && MASTER_POSE(6,1,1) == 0
		RETURN 0
	;バイブ挿入中はダメ
	SIF TEQUIP:13
		RETURN 0
	;膣鏡を使用しているとダメ
	SIF TEQUIP:膣鏡
		RETURN 0
	;オトコだとダメ
	SIF !HAS_VAGINA(TARGET)
		RETURN 0
	;体格差がある場合は主人が禁断の知識持ちじゃないとダメ
	SIF ABS(TALENT:体型 - TALENT:PLAYER:体型) > 2 && !TALENT:PLAYER:禁断の知識
		RETURN 0
	;調教者がオトコ、半人半妖、ふたなり、もしくはペニスバンドがないとダメ
	SIF !HAS_PENIS(PLAYER) && ITEM:PBAND == 0
		RETURN 0
	;処女で、従順4以下・レズっ気4以下のASSIの場合はできない（サドならＯＫ）
	IF TALENT:処女 && ASSIPLAY
		SIF (ABL:ASSI:従順 <= 4 || ABL:ASSI:レズっ気 <= 4) && TALENT:ASSI:サド == 0
			RETURN 0
	ENDIF
	;処女かつ潤滑不足はだめ
	SIF PALAM:潤滑 < PALAMLV:2 && TALENT:処女
			RETURN 0

	;下半身着衣
	SIF TEQUIP:0 & 4
		RETURN 0
	;お風呂場プレイ中はプレイマットが無いとダメ
	IF TEQUIP:31
		SIF ITEM:20 == 0 && NOITEM == 0
			RETURN 0
	ENDIF
	;背面座位からはダメ
	SIF TEQUIP:体位 == 5
		RETURN 0
RETURN 1

