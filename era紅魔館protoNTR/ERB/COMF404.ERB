;-------------------------------------------------
;覗く
;-------------------------------------------------
@COM404
TFLAG:現在覗いている場所 = 0
CALL COM404_01_TargetSelect()
IF 0 == RESULT
	; やっぱやめた
	RETURN 0
ENDIF
; 覗いたら時間経過
TIME += 3
WAIT
RETURN 1

;---------------------------------------------------------------------------------------
; 覗く部屋を選ぶ
@COM404_01_TargetSelect()
#DIM targetArea
#DIM LOOP_POS	 ; ループ変数：場所
DRAWLINE
PRINTFORML どの部屋を覗きますか？
PRINTL [0] - やめる
FOR LOOP_POS,0,MAXROOM()
	SIF CAN_MOVE(CFLAG:MASTER:現在位置, LOOP_POS)
		PRINTFORML [{LOOP_POS}] - %GETPLACENAME(LOOP_POS)%
NEXT
$INPUT_LABEL
INPUT
targetArea = RESULT
IF 0 ==targetArea
	RETURN 0
ENDIF
IF !CAN_MOVE(CFLAG:MASTER:現在位置, targetArea) 
	;不適切なtargetAreaならやり直し
	GOTO INPUT_LABEL
ENDIF

CALL COM404_02_tryPeeping(targetArea)

RETURN 1

;---------------------------------------------------------------------------------------
; 覗いてみる
@COM404_02_tryPeeping(targetArea)
#DIM targetArea
#DIM isSoundOnly
IF !CAN_MOVE(CFLAG:MASTER:現在位置, targetArea)
	PRINTFORML %GETPLACENAME(CFLAG:MASTER:現在位置)%から%GETPLACENAME(targetArea)%は覗けない。
	RETURN 0
ENDIF

IF 天候(天候値) == "暗闇"
	PRINTL しかし辺りは真っ暗で何も見えなかった……。
	RETURN 0
ENDIF

IF 天候(天候値) == "霧" && OPENPLACE(targetArea) == 2
	PRINTL しかし霧がかかっていて何も見えなかった……。
	RETURN 0
ENDIF

; 移動可能な場所であれば扉を開けようとする
IF CAN_MOVE(CFLAG:MASTER:現在位置, targetArea) == 1
	IF tryOpenDoor(MASTER, targetArea)
		PRINTFORML %CALLNAME:MASTER + "は音を立てないように扉を少しだけ開き、" + GETPLACENAME(targetArea) + "の様子をうかがった……。"%
		isSoundOnly = 0
	ELSEIF canUnLock(MASTER, targetArea)
		PRINTFORML %CALLNAME:MASTER + "は鍵を使って扉を少しだけ開き、" + GETPLACENAME(targetArea) + "の様子をうかがった……。"%
		isSoundOnly = 0
	ELSE
		IF isLockedByBolt(targetArea)
			PRINTFORM どうやら内側から閂がかけられているらしく、
		ELSE
			PRINTFORM 鍵がかかっていて、
		ENDIF
		PRINTFORML 扉を開けることができなかった。
		IF IS_NTR_SHOW_ALL()
			PRINTFORML %CALLNAME:MASTER + "は仕方なく、採光用の小さな窓から" + GETPLACENAME(targetArea) + "の様子をうかがった……。"%
			isSoundOnly = 0
		ELSE
			PRINTFORML %CALLNAME:MASTER + "は仕方なく、耳を寄せて" + GETPLACENAME(targetArea) + "の様子をうかがった……。"%
			HTML_PRINT @"<font color='#" +TOSTR(色設定_テキスト_備考,"x") +"'>ＮＴＲ筒抜けモードを有効にすると、採光窓から中を覗く事ができるようになります。</font>"
			isSoundOnly = 1
		ENDIF
	ENDIF
	WAIT
ENDIF

CALL COM404_03_searchCharacter(targetArea, isSoundOnly)
RETURN 1

;---------------------------------------------------------------------------------------
; 誰かいないか探す
@COM404_03_searchCharacter(targetArea, isSoundOnly)
#DIM targetArea
#DIM isSoundOnly
#DIM 登録番号
#DIM 見付けたキャラ
#DIM 見付けた人数
#DIMS 見付けたキャラ説明

IF targetArea == FLAG:訪問者の現在位置
	IF 場所_訪問者宅 != FLAG:訪問者の現在位置
		CALL LOOK_WITH_VISITOR(targetArea, isSoundOnly)
		RETURN 1
	ENDIF
ENDIF

; 自慰しているキャラを探す
見付けたキャラ = 0
FOR 登録番号, 1, CHARANUM
	IF CFLAG:登録番号:現在位置 == targetArea
		IF CFLAG:登録番号:自慰中 > 0
			見付けたキャラ = 登録番号
			BREAK
		ENDIF
	ENDIF
NEXT
IF INRANGE(見付けたキャラ, 1, CHARANUM)
	CALL COM404_04_watchMasturbation(targetArea, isSoundOnly, 見付けたキャラ)
	RETURN 1
ENDIF

; 自慰しているキャラがいない場合は、通常の発見判定
見付けた人数 = 0
見付けたキャラ説明 = %""%
PRINTFORM %GETPLACENAME(targetArea) + "には、"%
FOR 登録番号, 1, CHARANUM
	IF CFLAG:登録番号:現在位置 == targetArea
		IF 0 == 見付けたキャラ
			見付けたキャラ = 登録番号
			見付けたキャラ説明 += CALLNAME:登録番号
		ELSE
			見付けたキャラ説明 += " と "+CALLNAME:登録番号
		ENDIF
		見付けた人数++
	ENDIF
NEXT
IF FLAG:侵入者の現在位置 == targetArea
	IF 見付けた人数
		見付けたキャラ説明 += " と こいし"
	ELSE
		見付けたキャラ説明 += "こいし"
	ENDIF
	見付けた人数++
ENDIF
IF FLAG:訪問者の現在位置 == targetArea
	IF 見付けた人数
		見付けたキャラ説明 += " と " + NTR_NAME(0)
	ELSE
		見付けたキャラ説明 += NTR_NAME(0)
	ENDIF
	見付けた人数++
ENDIF
見付けたキャラ説明 += "がいる。"

IF 0 == 見付けた人数
	PRINTL 誰もいなかった……。
	RETURN 1
ENDIF

IF !isSoundOnly
	PRINTFORML %見付けたキャラ説明%
ELSE
	IF 1 < 見付けた人数
		PRINTFORML %CALLNAME:見付けたキャラ + "がいて、誰かと話しているようだ……。"%
	ELSE
		PRINTFORML %CALLNAME:見付けたキャラ + "がいるようだが、声を聴くことはできなさそうだ……。"%
	ENDIF
ENDIF

IF BATHROOM(targetArea)
	SOURCE:PLAYER:欲情 = 10
ENDIF

IF  1 == 見付けた人数
	IF EQUIP:見付けたキャラ:スカート
		IF 5 != EQUIP:見付けたキャラ:スカート
			;スカート着用で、それが袴でないなら、パンチラの可能性がある
			CALL CHK_PANCHIRA(targetArea, 見付けたキャラ)
		ENDIF
	ENDIF
	IF EQUIP:見付けたキャラ:ワンピース == 1
		;ワンピース着用で、それが普通のワンピースなら、パンチラの可能性がある
		CALL CHK_PANCHIRA(targetArea, 見付けたキャラ)
	ENDIF
ENDIF
RETURN 1

@COM404_04_watchMasturbation(targetArea, isSoundOnly, 自慰中のキャラ)
#DIM targetArea
#DIM isSoundOnly
#DIM 自慰中のキャラ
#DIM 自慰中に求めた名前
CALL LOOK_MASTURBATION(自慰中のキャラ, isSoundOnly)
自慰中に求めた名前 = RESULT
	;自慰を見た事によるソースの獲得
SOURCE:PLAYER:欲情 = 10
SELECTCASE 自慰中に求めた名前
	CASE 1
		;あなたの名前を呼びながら
		SOURCE:PLAYER:欲情 += 10
	CASE -1
			;訪問者の名前
		SELECTCASE TALENT:自慰中のキャラ:浮気公認
			CASE IS >= 浮気_性交公認
				PRINTFORML %CALLNAME:MASTER + "は、" + MSG_RE_LOVER(自慰中のキャラ) + "であるはずの" + CALLNAME:自慰中のキャラ + "が" + NTR_NAME(0) + "を求めている事に激しい興奮を覚えた。"%
				SOURCE:PLAYER:欲情 += 10
			CASE IS >= 浮気_浮気公認
				PRINTFORML %CALLNAME:MASTER + "は、" + MSG_RE_LOVER(自慰中のキャラ) + "であるはずの" + CALLNAME:自慰中のキャラ + "が" + NTR_NAME(0) + "を求めている事に興奮を覚えた。"%
				SOURCE:PLAYER:欲情 += 5
			CASEELSE
				SOURCE:PLAYER:欲情 -= 5
		ENDSELECT
	CASEELSE
		SOURCE:PLAYER:欲情 += 5
ENDSELECT
RETURN 1

;覗いた先に訪問者と住人がいた場合の処理
;現状は１対１でないと詳細はわからないものとする。
;ハーレム状態でも覗けるべきか。
;@RETURN 0:表示なし　1:表示あり
@LOOK_WITH_VISITOR(targetArea, isSoundOnly)
#DIM targetArea
#DIM LOOP_CHR
#DIM CNT_CHR
#DIM isSoundOnly
#DIMS 訪問者との描写
#DIMS 訪問者との音声

訪問者との描写 = %""%
訪問者との音声 = %""%

IF FLAG:訪問者の現在位置 != targetArea
	RETURN 0
ENDIF

; 訪問者と同じ場所にいる住人の人数を数える
CNT_CHR = 0
FOR LOOP_CHR, 1, CHARANUM
	IF CFLAG:LOOP_CHR:現在位置 == targetArea
		CNT_CHR++
	ENDIF
NEXT

IF CNT_CHR == 0
	;同じ場所にいる住人が０人の場合何もなし
	RETURN 0
ENDIF

IF CNT_CHR == 1
	;同じ場所にいる住人が１人の場合詳細を覗ける
	FOR LOOP_CHR, 1, CHARANUM
		IF CFLAG:LOOP_CHR:現在位置 == targetArea
			訪問者との描写 += "【" + GETPLACENAME(targetArea) + "】で"
			IF FLAG:睡姦フラグ == 1
				訪問者との描写 += NTR_NAME(0) + "が睡眠中の" + CALLNAME:LOOP_CHR + "を襲っていた"
			ELSE
				CALL COM404_VisitorDetail(LOOP_CHR, 訪問者との描写, 訪問者との音声)
				IF isSoundOnly 
					PRINTFORML %訪問者との音声%
				ELSE
					PRINTFORML %訪問者との描写%
					;覗きバレ時見せつけ(浮気公認Lvで頻度が変化)
					IF ( 1 + TALENT:LOOP_CHR:浮気公認 ) > RAND(4)
						CALL NTR_SHOW_PEEP(LOOP_CHR)
					ENDIF
				ENDIF
			ENDIF
		ENDIF
	NEXT
ELSE
	;同じ場所にいる住人が２人以上の場合概略を覗ける。覗きバレ見せつけなし
	FOR LOOP_CHR,1,CHARANUM
		IF CFLAG:LOOP_CHR:現在位置 == targetArea && !CFLAG:LOOP_CHR:睡眠
			CALL COM404_VisitorDetail(LOOP_CHR, 訪問者との描写, 訪問者との音声)
			IF isSoundOnly 
				PRINTFORML %訪問者との音声%
			ELSE
				PRINTFORML %訪問者との描写%
			ENDIF
		ENDIF
	NEXT
ENDIF
RETURN 1

@COM404_VisitorDetail(キャラ, 訪問者との描写, 訪問者との音声)
#DIM キャラ
#DIMS REF 訪問者との描写
#DIMS REF 訪問者との音声
SELECTCASE CFLAG:キャラ:訪問者行為
	CASE NTR_行為_なし
		IF FLAG:ハーレム時カウンタ
			訪問者との描写 += NTR_NAME(0) + "は" + CALLNAME:キャラ + "を抱き寄せた"
		ELSE
			訪問者との描写 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "が話をしていた"
		ENDIF
		訪問者との音声 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "の声が聞こえた…話をしているようだ"
	CASE NTR_行為_キス
		訪問者との描写 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "がキスをしていた"
		訪問者との音声 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "の声が聞こえた…キスをしているようだ"
	CASE NTR_行為_愛撫
		訪問者との描写 += NTR_NAME(0) + "が" + CALLNAME:キャラ + "の体を丹念に愛撫していた"
		訪問者との音声 += CALLNAME:キャラ + "のかすかな喘ぎ声が聞こえた…" + NTR_NAME(0) + "に愛撫されているようだ" 
	CASE NTR_行為_胸愛撫
		訪問者との描写 += NTR_NAME(0) + "が" + CALLNAME:キャラ + "の胸を優しく撫でていた"
		訪問者との音声 += CALLNAME:キャラ + "のかすかな喘ぎ声が聞こえた…" + NTR_NAME(0) + "に愛撫されているようだ" 
	CASE NTR_行為_フェラチオ
		訪問者との描写 += CALLNAME:キャラ + "が" + NTR_NAME(0) + "のペニスを咥えていた"
		訪問者との音声 += NTR_NAME(0) + "の声が聞こえた…" + CALLNAME:キャラ + "にフェラチオをさせているようだ"
	CASE NTR_行為_シックスナイン
		訪問者との描写 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "がお互いの性器を舐めあっていた"
		訪問者との音声 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "の声が聞こえた…互いに性器を舐め合っているようだ"
	CASE NTR_行為_素股
		訪問者との描写 += CALLNAME:キャラ + "が" + NTR_NAME(0) + "に素股をしていた"
		訪問者との音声 += NTR_NAME(0) + "の声が聞こえた…" + CALLNAME:キャラ + "に素股で腰を振らせているようだ"
	CASE NTR_行為_Ｖ正常位 TO NTR_行為_Ｖ背面立位
		訪問者との描写 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "がまぐわっていた"
		訪問者との音声 += NTR_NAME(0) + "の声と、" + CALLNAME:キャラ + "の喘ぎ声が聞こえた…セックスをしているようだ"
	CASE NTR_行為_Ａ正常位 TO NTR_行為_Ａ背面立位
		訪問者との描写 += NTR_NAME(0) + "と" + CALLNAME:キャラ + "がアナルセックスをしていた"
		訪問者との音声 += NTR_NAME(0) + "の声と、" + CALLNAME:キャラ + "の喘ぎ声が聞こえた…アナルセックスをしているようだ"
	CASEELSE
		PRINTW バグです
ENDSELECT
RETURN 1

;パンチラ可否チェック
@CHK_PANCHIRA(targetArea, TGT_CHR)
#DIM targetArea	; 覗いてる先の場所
#DIM TGT_CHR	; 覗いてる先の人物
#DIM POS		; 覗いてる側の場所

POS =  CFLAG:MASTER:現在位置		; 覗いてる側の場所
	;広間から踊り場を見上げたとき1/3でパンチラ
IF POS == 場所_広間 && targetArea == 場所_二階踊り場 && !RAND:3
	CALL LOOK_PANCHIRA(TGT_CHR, MASTER, targetArea, 0)
ENDIF
	;踊り場から二階廊下を見上げたとき1/4でパンチラ
IF POS == 場所_二階踊り場 && targetArea == 場所_二階廊下 && !RAND:4 
	CALL LOOK_PANCHIRA(TGT_CHR, MASTER, targetArea, 0)
ENDIF
	;庭に面した一階から庭を見たとき1/3でパンチラ
IF POS == 場所_食堂 || POS == 場所_納屋 || POS == 場所_屋外トイレ
	IF targetArea == 場所_庭 && !RAND:3 
		CALL LOOK_PANCHIRA(TGT_CHR, MASTER, targetArea, 1)
	ENDIF
ENDIF
	;湖に面した一階から湖を見たとき1/3でパンチラ
IF POS == 場所_正門 
	IF targetArea == 場所_湖 && !RAND:3 
		CALL LOOK_PANCHIRA(TGT_CHR, MASTER, targetArea, 1)
	ENDIF
ENDIF
	;湖南部に面した一階から湖を見たとき1/3でパンチラ
IF POS == 場所_チルノの家 || POS == 場所_大妖精の家 
	IF targetArea == 場所_湖南部 && !RAND:3 
		CALL LOOK_PANCHIRA(TGT_CHR, MASTER, targetArea, 1)
	ENDIF
ENDIF
RETURN

;パンチラ処理
@LOOK_PANCHIRA(被害者, 加害者, targetArea, FLG_POS)
#DIM 被害者		; 見られた人
#DIM 加害者		; 見た人(998:訪問者, 997:こいし)
#DIM targetArea	; 覗いた先の場所
#DIM FLG_POS	; 場所フラグ 0:踊り場、二階廊下 1:庭、湖
	;踊り場 or 二階廊下
IF FLG_POS == 0
		;見た人がMASTER
	IF 被害者 != MASTER && 加害者 == MASTER
		PRINTFORML %GETPLACENAME(targetArea)%を見上げる形になってしまったため%CALLNAME:被害者%の
		CALL LOOK_PANCHIRA_MSG1(被害者, 加害者, targetArea)
		SIF 0 == CFLAG:被害者:ぱんつ確認 
			CFLAG:被害者:ぱんつ確認 = 2
		;見られた人がMASTER
	ELSEIF 被害者 == MASTER && 加害者 != MASTER
		PRINTFORM  たまたま%GETPLACENAME(targetArea)%を見上げる場所にいた
		CALL LOOK_PANCHIRA_MSG2(被害者, 加害者, targetArea)
		;その他の組み合わせは現在未対応
	ELSE
	ENDIF
	;庭 or 湖
ELSEIF FLG_POS == 1
		;見た人がMASTER
	IF 被害者 != MASTER && 加害者 == MASTER
		PRINTFORML %GETPLACENAME(targetArea)%には%CALLNAME:被害者%がいたが、悪戯な風が吹いたため
		CALL LOOK_PANCHIRA_MSG1(被害者, 加害者, targetArea)
		;見られた人がMASTER
	ELSEIF 被害者 == MASTER && 加害者 != MASTER
			;その他の組み合わせは現在未対応
		PRINTFORML 悪戯な風が吹いたためスカートがめくれあがってしまい、
		PRINTFORM  たまたま%GETPLACENAME(targetArea)%を見ることのできる場所にいた
		CALL LOOK_PANCHIRA_MSG2(被害者, 加害者, targetArea)
	ELSE
	ENDIF
ENDIF

RETURN 0

