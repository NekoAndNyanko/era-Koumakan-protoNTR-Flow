;自慰覗きイベント
;戻り値：0…誰かの名前　1…あなたの名前　-1…訪問者の名前
@LOOK_MASTURBATION(奴隷, isSoundOnly)
#DIM 奴隷
#DIM 貞操帯着用中
#DIM 自慰部位１
#DIM 自慰感覚
#DIM 戻り値
#DIMS 名前
#DIM isSoundOnly
#DIMS 自慰行為映像
#DIMS 自慰行為音声
	;自慰行動保存 Bit 0=行為C 1=行為V 2=行為A 3=行為B 4=2点行為 5=絶頂

貞操帯着用中 = (下着名１_２(奴隷)=="貞操帯")
自慰行為映像 = %"中では、" + CALLNAME:奴隷 + "が"%
自慰行為音声 = %"中からは、" + CALLNAME:奴隷 + "が"%
IF 貞操帯着用中
	自慰感覚 = 0
	IF !GETBIT(CFLAG:奴隷:自慰行動保存,4)
			;単一部位
		IF GETBIT(CFLAG:奴隷:自慰行動保存,0)
			自慰行為映像 += "クリトリスに触れようと"+下着名１_２(奴隷)+"の表面を引っ掻いて"
		ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,1)
			自慰行為映像 += "膣内に触れようと"+下着名１_２(奴隷)+"の表面を引っ掻いて"
		ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,2)
			自慰行為映像 += "アナルに触れようと"+下着名１_２(奴隷)+"の表面を引っ掻いて"
		ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,3)
			自慰感覚 = ABL:奴隷:Ｂ感覚
			IF ABL:奴隷:Ｂ感覚 >= 4
				自慰行為映像 += "乳首を指先で軽くつまんで刺激を与え"
			ELSEIF TALENT:奴隷:バストサイズ >= 0
				自慰行為映像 += "胸を揉んで時激し"
			ELSE
				自慰行為映像 += "に指を当てて撫でる様に愛撫し"
			ENDIF
		ENDIF
	ELSE
			;複数部位
		IF GETBIT(CFLAG:奴隷:自慰行動保存,3)
				;胸とセット
			IF GETBIT(CFLAG:奴隷:自慰行動保存,0)
				自慰行為映像 += "クリトリスに触れようと"+下着名１_２(奴隷)+"の表面を引っ掻きながら、"
			ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,1)
				自慰行為映像 += "膣内に触れようと"+下着名１_２(奴隷)+"の表面を引っ掻きながら、"
			ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,2)
				自慰行為映像 += "アナルに触れようと"+下着名１_２(奴隷)+"の表面を引っ掻きながら、"
			ENDIF
			IF ABL:奴隷:Ｂ感覚 >= 4
				自慰行為映像 += "乳首を指先で軽くつまんで刺激を与え"
			ELSEIF TALENT:奴隷:バストサイズ >= 0
				自慰行為映像 += "胸を揉んで時激し"
			ELSE
				自慰行為映像 += "に指を当てて撫でる様に愛撫し"
			ENDIF
			自慰感覚 = ABL:奴隷:Ｂ感覚
		ELSE
				;胸以外のセット、つまり刺激無し
			IF GETBIT(CFLAG:奴隷:自慰行動保存,0)
				自慰部位１ = 0
				自慰行為映像 += "クリトリスと"
			ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,1)
				自慰部位１ = 1
				自慰行為映像 += "膣内と"
			ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,2)
				自慰部位１ = 2
				自慰行為映像 += "アナルと"
			ENDIF

			IF 自慰部位１ != 0 && GETBIT(CFLAG:奴隷:自慰行動保存,0)
				自慰行為映像 += "クリトリスに触れようと"+下着名１_２(奴隷)+"の表面を引っ掻いて"
			ELSEIF 自慰部位１ != 1 && GETBIT(CFLAG:奴隷:自慰行動保存,1)
				自慰行為映像 += "膣内に触れようと"+下着名１_２(奴隷)+"の表面を引っ掻いて"
			ELSEIF 自慰部位１ != 2 && GETBIT(CFLAG:奴隷:自慰行動保存,2)
				自慰行為映像 += "アナルに触れようと"+下着名１_２(奴隷)+"の表面を引っ掻いて"
			ENDIF
		ENDIF
	ENDIF
ELSE
		;貞操帯なし、普通の自慰
	IF GETBIT(CFLAG:奴隷:自慰行動保存,0)
		自慰行為映像 += "クリトリス"
		自慰部位１ = 0
		自慰感覚 = ABL:奴隷:Ｃ感覚
		IF !GETBIT(CFLAG:奴隷:自慰行動保存,4)
			IF 自慰感覚 >= 4
				自慰行為映像 += "を指先で軽くつまんで刺激を与え"
			ELSE
				自慰行為映像 += "を軽くなぞる様に愛撫し"
			ENDIF
		ENDIF
	ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,1)
		自慰行為映像 += "膣内"
		自慰部位１ = 1
		自慰感覚 = ABL:奴隷:Ｖ感覚
		IF !GETBIT(CFLAG:奴隷:自慰行動保存,4)
			IF 自慰感覚 >= 4
				自慰行為映像 += "に指を２本入れて膣内を執拗にかき混ぜ"
			ELSE
				自慰行為映像 += "に指を入れて膣内を刺激し"
			ENDIF
		ENDIF
	ELSEIF GETBIT(CFLAG:奴隷:自慰行動保存,2)
		自慰行為映像 +=  "アナル"
		自慰部位１ = 2
		自慰感覚 = ABL:奴隷:Ａ感覚
		IF !GETBIT(CFLAG:奴隷:自慰行動保存,4)
			IF 自慰感覚 >= 4
				自慰行為映像 +=  "に指を２本入れて腸内を執拗にかき混ぜ"
			ELSE
				自慰行為映像 +=  "に指を入れて腸内を刺激し"
			ENDIF
		ENDIF
	ELSE
		IF 自慰感覚 >= 4
			自慰行為映像 +=  "乳首"
		ELSE
			自慰行為映像 +=  "胸"
		ENDIF
		自慰部位１ = 3
		自慰感覚 = ABL:奴隷:Ｂ感覚
		IF !GETBIT(CFLAG:奴隷:自慰行動保存,4)
			IF 自慰感覚 >= 4
				自慰行為映像 +=  "を指先で軽くつまんで刺激を与え"
			ELSE
				IF TALENT:奴隷:バストサイズ >= 0
					自慰行為映像 +=  "を揉んで時激し"
				ELSE
					自慰行為映像 +=  "に指を当てて撫でる様に愛撫し"
				ENDIF
			ENDIF
		ENDIF
	ENDIF
	IF GETBIT(CFLAG:奴隷:自慰行動保存,4)
		自慰行為映像 +=  "と"
		IF 自慰部位１ != 0 && GETBIT(CFLAG:奴隷:自慰行動保存,0)
			自慰行為映像 +=  "クリトリス"
			自慰感覚 = MAX(自慰感覚, ABL:奴隷:Ｃ感覚)
		ELSEIF 自慰部位１ != 1 && GETBIT(CFLAG:奴隷:自慰行動保存,1)
			自慰行為映像 +=  "膣内"
			自慰感覚 = MAX(自慰感覚, ABL:奴隷:Ｖ感覚)
		ELSEIF 自慰部位１ != 2 && GETBIT(CFLAG:奴隷:自慰行動保存,2)
			自慰行為映像 +=  "アナル"
			自慰感覚 = MAX(自慰感覚, ABL:奴隷:Ａ感覚)
		ELSEIF 自慰部位１ != 3 && GETBIT(CFLAG:奴隷:自慰行動保存,3)
			自慰行為映像 +=  "胸"
			自慰感覚 = MAX(自慰感覚, ABL:奴隷:Ｂ感覚)
		ENDIF
		自慰行為映像 +=  "を同時に弄り"
	ENDIF
	PRINTL

ENDIF

名前 = %"誰か"%
戻り値 = 0
IF TALENT:奴隷:恋慕
	戻り値 = 1
	SIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られ寸前)
		戻り値 = -1
	SIF NTR_CHK_FAVORABLY(奴隷, FAV_寝取られそう) && RAND:2 == 0
		戻り値 = -1
	SIF NTR_CHK_FAVORABLY(奴隷, FAV_主人より高い) && RAND:3 == 0
		戻り値 = -1
